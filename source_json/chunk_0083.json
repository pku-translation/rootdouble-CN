{
  "000060": {
    "context": "000060",
    "code": "0x55",
    "string": "√After Chapter5"
  },
  "000070": {
    "context": "000070",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「いや、そんな事より先生。サリュが言っていた事を、守部たちに伝えた方がいいんじゃないか？」"
  },
  "000105": {
    "context": "000105",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "「……なんのために？」"
  },
  "000116": {
    "context": "000116",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「決まってるだろう。サリュが言うには、オレにも何か怪しいところがある……」"
  },
  "000127": {
    "context": "000127",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「その事を守部とおっさんに伝えれば、あんたに向けられた疑惑の目が、オレの方に向くじゃないか」"
  },
  "000132": {
    "context": "000132",
    "code": "0x45",
    "developer_comment": "",
    "string": "渡瀬が言うと、恵那は目を丸くした。"
  },
  "000161": {
    "context": "000161",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "「呆れた……まさかそれ、私のためって事？」"
  },
  "000172": {
    "context": "000172",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「あぁ、いけないか？」"
  },
  "000201": {
    "context": "000201",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "「いけないわよ。何でも話せばいいってもんじゃないわ。ただでも私の拳銃のせいで、みんな殺気立ってしまってるのに……」"
  },
  "000230": {
    "context": "000230",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "「っていうかだいたいね、さっきもあなたは私を庇うべきじゃなかったのよ」"
  },
  "000259": {
    "context": "000259",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "「銃の事を黙っていたのは、私のミスだったんだから」"
  },
  "000270": {
    "context": "000270",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "「何の根拠もなく私の言葉を信じたあなたは、すでにあの２人に疑われかけてるかもしれないのよ？」"
  },
  "000281": {
    "context": "000281",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "「だからこれ以上――」"
  },
  "000292": {
    "context": "000292",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「いや、根拠ならあったさ」"
  },
  "000321": {
    "context": "000321",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "「え？」"
  },
  "000332": {
    "context": "000332",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「オレが先生を『信じられる』と判断したからだ」"
  },
  "000361": {
    "context": "000361",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "「……！」"
  },
  "000390": {
    "context": "000390",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "「まだ、そんな事を言うなんて――」"
  },
  "000401": {
    "context": "000401",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "「私が何のために、父の話をあなたにしたと思ってるの？」"
  },
  "000406": {
    "context": "000406",
    "code": "0x45",
    "developer_comment": "",
    "string": "恵那がイライラした様子で言う。"
  },
  "000435": {
    "context": "000435",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "「いい、隊長さん？　今このラボでは確実に何かが起きてる」"
  },
  "000446": {
    "context": "000446",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "「さっきの爆弾の事もそうだし、三ノ宮さんが逃げたのだって、きっと私たちの知らない理由があるのよ」"
  },
  "000457": {
    "context": "000457",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "「悪意を持った誰かが、裏で動いているに違いないわ」"
  },
  "000486": {
    "context": "000486",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "「気を抜くと――本当に命を落としかねないわよ」"
  },
  "000497": {
    "context": "000497",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「……っ」"
  },
  "000508": {
    "context": "000508",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "「生き残るためには、周囲のあらゆるものを疑ってかかりなさい」"
  },
  "000519": {
    "context": "000519",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "「周りの人も、この施設の事も。場合によっては、自分自身の見聞きしたものも。先入観に騙されてるかもしれないから」"
  },
  "000530": {
    "context": "000530",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「……ずいぶん、教師らしからぬ事を言うんだな」"
  },
  "000559": {
    "context": "000559",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "「失望した？」"
  },
  "000570": {
    "context": "000570",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "「でも、教師と言うのは思いのほか懐疑的なものなのよ。なんでもかんでも信じてちゃ、護るべき生徒も護れないもの」"
  },
  "000581": {
    "context": "000581",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「そうか……強いんだな、教師ってのは」"
  },
  "000586": {
    "context": "000586",
    "code": "0x45",
    "developer_comment": "",
    "string": "渡瀬が感心したようにそう言うと、恵那は思い切りため息をついた。"
  },
  "000615": {
    "context": "000615",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "「はぁ……ダメよ、隊長さん。私の今の言葉だって、ちゃんと疑ってかからなくちゃ」"
  },
  "000626": {
    "context": "000626",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「……そうだな、信じれば騙されるかもしれないしな」"
  },
  "000637": {
    "context": "000637",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「先生の親父さんの話を聞くと、『信じる』って事には、リスクがついて回るって事になるもんな」"
  },
  "000666": {
    "context": "000666",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "「わかってるんじゃない。こんな状況だと、余計にそうなのよ？」"
  },
  "000671": {
    "context": "000671",
    "code": "0x45",
    "developer_comment": "",
    "string": "恵那が責めるように睨む。"
  },
  "000676": {
    "context": "000676",
    "code": "0x45",
    "developer_comment": "",
    "string": "渡瀬はその瞳を見返し、きっぱりと言った。"
  },
  "000687": {
    "context": "000687",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「それでもオレは、先生のことを信じるよ」"
  },
  "000716": {
    "context": "000716",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "「えっ……」"
  },
  "000727": {
    "context": "000727",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「リスク抜きの『信義』なんて、嘘っぱちだろ」"
  },
  "000738": {
    "context": "000738",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「自分だけ安全なところにいて『信じる』だの『信じない』だの、そんなのはなんの力も持たない言葉だ」"
  },
  "000749": {
    "context": "000749",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「だからこれからも先生が疑われれば、無条件に信じるし、庇う」"
  },
  "000760": {
    "context": "000760",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「……きっと先生の親父さんも、そうするはずだよ」"
  },
  "000765": {
    "context": "000765",
    "code": "0x45",
    "developer_comment": "",
    "string": "渡瀬は静かな声で告げた。"
  },
  "000770": {
    "context": "000770",
    "code": "0x45",
    "developer_comment": "",
    "string": "恵那の父の人生を、肯定したいという想いも込めて。"
  },
  "000775": {
    "context": "000775",
    "code": "0x45",
    "developer_comment": "",
    "string": "すると恵那が、どこか切なげに笑う。"
  },
  "000804": {
    "context": "000804",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "「まったく、本当に馬鹿なんだから……」"
  },
  "000809": {
    "context": "000809",
    "code": "0x45",
    "developer_comment": "",
    "string": "それ以上、恵那は何も言わなくなった。"
  },
  "000814": {
    "context": "000814",
    "code": "0x45",
    "developer_comment": "",
    "string": "そうこうしている内に、渡瀬たちは第６エリアに到着した。"
  }
}
