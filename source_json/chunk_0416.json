[
  {
    "index": 50,
    "code": "0x55",
    "developer_comment": "",
    "string": "√Double Chapter7"
  },
  {
    "index": 86,
    "code": "0x85",
    "developer_comment": "",
    "string": "　%N　%N　%N　%N　%N"
  },
  {
    "index": 171,
    "code": "0x45",
    "developer_comment": "【――守部洵との記憶――】",
    "string": "――ラボでの事件から、ちょうど１年経ったある日の事――"
  },
  {
    "index": 176,
    "code": "0x45",
    "developer_comment": "【――守部洵との記憶――】",
    "string": "渡瀬は風見と共に、鹿鳴市を訪れていた。"
  },
  {
    "index": 181,
    "code": "0x45",
    "developer_comment": "【――守部洵との記憶――】",
    "string": "大切な用事を果たすために。"
  },
  {
    "index": 186,
    "code": "0x45",
    "developer_comment": "【――守部洵との記憶――】",
    "string": "だが、その用事に前に寄るところがある。"
  },
  {
    "index": 252,
    "code": "0x45",
    "developer_comment": "【――守部洵との記憶――】",
    "string": "それは、鹿鳴市中央消防署――"
  },
  {
    "index": 257,
    "code": "0x45",
    "developer_comment": "【――守部洵との記憶――】",
    "string": "渡瀬たちの古巣だった。"
  },
  {
    "index": 282,
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「あの子、遅いわねぇ……１２時に出てくるっていったのに」"
  },
  {
    "index": 311,
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「まさか、出場でもかかったのかしら」"
  },
  {
    "index": 336,
    "code": "0x45",
    "developer_comment": "風見",
    "string": "風見が署の玄関を見ながら、心配そうに呟く。"
  },
  {
    "index": 341,
    "code": "0x45",
    "developer_comment": "風見",
    "string": "渡瀬は苦笑して答えた。"
  },
  {
    "index": 354,
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「そんな事ないだろ。おおかた司令長に捕まってるんじゃないか？」"
  },
  {
    "index": 387,
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "渡瀬たちは今日、ここで洵と待ち合わせしていたのだ。"
  },
  {
    "index": 392,
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "例の用事には、洵も同行する。"
  },
  {
    "index": 397,
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "とはいえ今日は洵は仕事があるので、昼休みだけ抜けてくるという話だった。"
  },
  {
    "index": 404,
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "と、ちょうどその時――"
  },
  {
    "index": 453,
    "code": "0x45",
    "developer_comment": "洵",
    "string": "「ごめんごめん、遅くなった！」"
  },
  {
    "index": 458,
    "code": "0x45",
    "developer_comment": "洵",
    "string": "署の中から、洵が飛び出してくるのが見えた。"
  },
  {
    "index": 469,
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「おぉ、守部！」"
  },
  {
    "index": 527,
    "code": "0x45",
    "developer_comment": "洵",
    "string": "「いやぁ久しぶり、２人とも！」"
  },
  {
    "index": 532,
    "code": "0x45",
    "developer_comment": "洵",
    "string": "洵は渡瀬たちに駆け寄り、にこにこと言った。"
  },
  {
    "index": 568,
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「元気そうね、洵」"
  },
  {
    "index": 597,
    "code": "0x45",
    "developer_comment": "洵",
    "string": "「元気元気、っていうかもー大変だよぉ」"
  },
  {
    "index": 626,
    "code": "0x45",
    "developer_comment": "洵",
    "string": "「なにしろシリウスも２人殉職して、２人辞めちゃったから、人材不足でねぇ」"
  },
  {
    "index": 637,
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「……そうか」"
  },
  {
    "index": 642,
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "その言葉を聞き、渡瀬の胸に痛みが走る。"
  },
  {
    "index": 671,
    "code": "0x45",
    "developer_comment": "洵",
    "string": "「あー、そうやってしょげないの」"
  },
  {
    "index": 700,
    "code": "0x45",
    "developer_comment": "洵",
    "string": "「司令長たちも最初は、悲しんだり混乱したりしてたけど……」"
  },
  {
    "index": 729,
    "code": "0x45",
    "developer_comment": "洵",
    "string": "「今はもうすっかり立ち直って、シリウス再建のために頑張ってるから」"
  },
  {
    "index": 759,
    "code": "0x45",
    "developer_comment": "洵",
    "string": "「あたしも『シリウスの長い１日』を生き残った女として、隊を引っ張ってるよ」"
  },
  {
    "index": 770,
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「へぇ、頼もしいじゃないか」"
  },
  {
    "index": 824,
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "あの日のラボへの出場は、消防関係者の中で『シリウスの長い１日』と呼ばれ、半ば伝説的に扱われているそうだ。"
  },
  {
    "index": 829,
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "それを経験した事は、洵のキャリアに大きな星をつけたのだろう。"
  },
  {
    "index": 868,
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "当の洵はそれをおごる事なく、にやりと笑って言う。"
  },
  {
    "index": 897,
    "code": "0x45",
    "developer_comment": "洵",
    "string": "「それより聞いたよ隊長～、いよいよお姉ちゃんと結婚だって？」"
  },
  {
    "index": 908,
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「む……お前、知ってたのか」"
  },
  {
    "index": 937,
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「当たり前じゃない、一番に報告したわよ」"
  },
  {
    "index": 948,
    "code": "0x45",
    "developer_comment": "洵",
    "string": "「プロポーズの言葉はなんだったのかな？　ねぇどうなの、隊長――」"
  },
  {
    "index": 977,
    "code": "0x45",
    "developer_comment": "洵",
    "string": "「っと、もう『隊長』じゃないんだよね。なんて呼べばいい？」"
  },
  {
    "index": 988,
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「なんでもいいさ。『笠鷺さん』とかでいいんじゃないか」"
  },
  {
    "index": 1017,
    "code": "0x45",
    "developer_comment": "洵",
    "string": "「それだと、お姉ちゃんとかぶっちゃうじゃん？　お姉ちゃんも今じゃ『笠鷺風見』なんだから」"
  },
  {
    "index": 1028,
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「じゃあどうするってんだよ」"
  },
  {
    "index": 1057,
    "code": "0x45",
    "developer_comment": "洵",
    "string": "「そうだなぁ。うん、やっぱここは――」"
  },
  {
    "index": 1086,
    "code": "0x45",
    "developer_comment": "洵",
    "string": "「『お兄ちゃん』だね！」"
  },
  {
    "index": 1097,
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「お、お兄ちゃん！？」"
  },
  {
    "index": 1126,
    "code": "0x45",
    "developer_comment": "洵",
    "string": "「そりゃそうだよ、お姉ちゃんのダンナはお兄ちゃんでしょ？」"
  },
  {
    "index": 1137,
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「お兄ちゃんねぇ……なんか呼ばれ慣れないっていうか、急にこんなでかい妹ができると戸惑うっていうか……」"
  },
  {
    "index": 1166,
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「いいじゃないの、あなた」"
  },
  {
    "index": 1177,
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「私と縁を結ぶという事は、洵とも縁を結ぶという事よ？」"
  },
  {
    "index": 1206,
    "code": "0x45",
    "developer_comment": "洵",
    "string": "「そうだよお兄ちゃん、納得しなさい」"
  },
  {
    "index": 1211,
    "code": "0x45",
    "developer_comment": "洵",
    "string": "女２人に詰め寄られ、渡瀬は思わずたじろいだ。"
  },
  {
    "index": 1222,
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「……あぁもう、わかったよ！　好きに呼べ好きに！」"
  },
  {
    "index": 1251,
    "code": "0x45",
    "developer_comment": "洵",
    "string": "「了解！　じゃああたしは妹なんだから、守部じゃなくて『洵』と呼びなさい」"
  },
  {
    "index": 1262,
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「偉そうな妹だな！　もうなんでもいいよ！」"
  },
  {
    "index": 1267,
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "渡瀬の言葉に、洵と風見が笑った。"
  },
  {
    "index": 1272,
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "やれやれと首を振る彼に、洵が問いを重ねる。"
  },
  {
    "index": 1301,
    "code": "0x45",
    "developer_comment": "洵",
    "string": "「ところでお兄ちゃん、これからどうやってお姉ちゃん養うつもりなの？　仕事は？」"
  },
  {
    "index": 1312,
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「あぁ、あんな大事件の中心人物になった以上、もうマトモな職には就きづらいからな」"
  },
  {
    "index": 1323,
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「先生の手引きで、超科局に入る事にした」"
  },
  {
    "index": 1352,
    "code": "0x45",
    "developer_comment": "洵",
    "string": "「超科局？　まさかコミュニケーター管理官になるの？」"
  },
  {
    "index": 1363,
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「そうさ。なにしろ俺の妻は、レベル６コミュニケーターだからな」"
  },
  {
    "index": 1374,
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「夫が管理官なら、色々と安心だろ？」"
  },
  {
    "index": 1403,
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「そう。私は今後、この人の監視と保護を受けるのよ」"
  },
  {
    "index": 1414,
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「一生、一番そばでな」"
  },
  {
    "index": 1443,
    "code": "0x45",
    "developer_comment": "洵",
    "string": "「そうなんだ……」"
  },
  {
    "index": 1472,
    "code": "0x45",
    "developer_comment": "洵",
    "string": "「……じゃあ、シリウスにはもう戻ってこないんだね？」"
  },
  {
    "index": 1498,
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「っ……」"
  },
  {
    "index": 1503,
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "その言葉が渡瀬の心を打った。"
  },
  {
    "index": 1508,
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "寂しげな顔をする洵に、渡瀬は静かな声で答える。"
  },
  {
    "index": 1519,
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「……戻れないさ」"
  },
  {
    "index": 1524,
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "鹿鳴市の公務員になるなど不可能だ。"
  },
  {
    "index": 1529,
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "あの事件を境に、渡瀬の立場ははっきりと変わってしまった。"
  },
  {
    "index": 1534,
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "かつての職場に、自分の過去に、どれだけ愛着があったとしても戻る事はできない。"
  },
  {
    "index": 1545,
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「……今でも時々、ここで過ごした日々を、懐かしく思う事はあるけどな」"
  },
  {
    "index": 1574,
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「そうね……」"
  },
  {
    "index": 1579,
    "code": "0x45",
    "developer_comment": "風見",
    "string": "風見も署の方に目をやって言った。"
  },
  {
    "index": 1584,
    "code": "0x45",
    "developer_comment": "風見",
    "string": "亘の遺志を継いで、消防官になってから、ずっとここに勤めてきた。"
  },
  {
    "index": 1589,
    "code": "0x45",
    "developer_comment": "風見",
    "string": "人命を救うため、誰かの未来を護るため、何度も死地に飛び込んだ。"
  },
  {
    "index": 1594,
    "code": "0x45",
    "developer_comment": "風見",
    "string": "その意思は消えていない。"
  },
  {
    "index": 1599,
    "code": "0x45",
    "developer_comment": "風見",
    "string": "立場や仕事が変わっても、決して――"
  },
  {
    "index": 1610,
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「……なぁ、洵。俺はもうレスキュー隊員じゃないが……」"
  },
  {
    "index": 1621,
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「これからも誰かを護るために、働き続けるつもりだよ」"
  },
  {
    "index": 1632,
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「お前と一緒には働けないが、それでも気持ちは一緒だ」"
  },
  {
    "index": 1637,
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "渡瀬がそう言うと、洵は微笑んで――"
  },
  {
    "index": 1666,
    "code": "0x45",
    "developer_comment": "洵",
    "string": "「……うん、わかってる」"
  },
  {
    "index": 1671,
    "code": "0x45",
    "developer_comment": "洵",
    "string": "渡瀬の胸に顔を埋めた。"
  },
  {
    "index": 1799,
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「……洵？」"
  },
  {
    "index": 1804,
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "名を呼ぶと、洵は囁くように言った。"
  },
  {
    "index": 1815,
    "code": "0x45",
    "developer_comment": "洵",
    "string": "「……ねぇ、隊長、副隊長」"
  },
  {
    "index": 1826,
    "code": "0x45",
    "developer_comment": "洵",
    "string": "「２人がシリウスを辞めても、離れて暮らす事になっても――」"
  },
  {
    "index": 1837,
    "code": "0x45",
    "developer_comment": "洵",
    "string": "「あたしたちは、仲間だよ」"
  },
  {
    "index": 1848,
    "code": "0x45",
    "developer_comment": "洵",
    "string": "「同じ釜のゴハン食べて、何度も助けて助けられて、一緒に死線をくぐってきた……」"
  },
  {
    "index": 1853,
    "code": "0x45",
    "developer_comment": "洵",
    "string": "――それは仲間という言葉だけでは言い尽くせない。"
  },
  {
    "index": 1858,
    "code": "0x45",
    "developer_comment": "洵",
    "string": "バディという言葉でさえ足りない。"
  },
  {
    "index": 1863,
    "code": "0x45",
    "developer_comment": "洵",
    "string": "互いに誰より大切な――"
  },
  {
    "index": 1874,
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「……家族だな」"
  },
  {
    "index": 1885,
    "code": "0x45",
    "developer_comment": "洵",
    "string": "「そう、そういう事！」"
  },
  {
    "index": 1930,
    "code": "0x45",
    "developer_comment": "洵",
    "string": "そう言って洵は、涙を拭った。"
  },
  {
    "index": 1935,
    "code": "0x45",
    "developer_comment": "洵",
    "string": "その顔には吹っ切れたような笑みが浮かんでいる。"
  },
  {
    "index": 1946,
    "code": "0x45",
    "developer_comment": "洵",
    "string": "「これからよろしくね、お兄ちゃん！」"
  },
  {
    "index": 1957,
    "code": "0x45",
    "developer_comment": "洵",
    "string": "「２人が帰る場所は、ここにもあるからさ。何かあったら寄ってってね」"
  },
  {
    "index": 1968,
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「あぁ、もちろんだ」"
  },
  {
    "index": 1979,
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「可愛い妹の顔見に、ちょくちょく寄らせてもらうよ」"
  },
  {
    "index": 1989,
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "渡瀬はそう言って、洵の頭をくしゃくしゃと撫でる。"
  },
  {
    "index": 1994,
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "彼女は撫でられるに任せ、幸福そうに微笑んだ……。"
  }
]