{
  "000050": {
    "context": "000050",
    "code": "0x55",
    "string": "√Double Chapter7"
  },
  "000086": {
    "context": "000086",
    "code": "0x85",
    "string": "　%N　%N　%N　%N　%N"
  },
  "000090": {
    "context": "000090",
    "code": "0x47",
    "developer_comment": "",
    "string": "【――守部洵との記憶――】"
  },
  "000171": {
    "context": "000171",
    "code": "0x45",
    "developer_comment": "",
    "string": "――ラボでの事件から、ちょうど１年経ったある日の事――"
  },
  "000176": {
    "context": "000176",
    "code": "0x45",
    "developer_comment": "",
    "string": "渡瀬は風見と共に、鹿鳴市を訪れていた。"
  },
  "000181": {
    "context": "000181",
    "code": "0x45",
    "developer_comment": "",
    "string": "大切な用事を果たすために。"
  },
  "000186": {
    "context": "000186",
    "code": "0x45",
    "developer_comment": "",
    "string": "だが、その用事に前に寄るところがある。"
  },
  "000252": {
    "context": "000252",
    "code": "0x45",
    "developer_comment": "",
    "string": "それは、鹿鳴市中央消防署――"
  },
  "000257": {
    "context": "000257",
    "code": "0x45",
    "developer_comment": "",
    "string": "渡瀬たちの古巣だった。"
  },
  "000282": {
    "context": "000282",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「あの子、遅いわねぇ……１２時に出てくるっていったのに」"
  },
  "000311": {
    "context": "000311",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「まさか、出場でもかかったのかしら」"
  },
  "000336": {
    "context": "000336",
    "code": "0x45",
    "developer_comment": "",
    "string": "風見が署の玄関を見ながら、心配そうに呟く。"
  },
  "000341": {
    "context": "000341",
    "code": "0x45",
    "developer_comment": "",
    "string": "渡瀬は苦笑して答えた。"
  },
  "000354": {
    "context": "000354",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「そんな事ないだろ。おおかた司令長に捕まってるんじゃないか？」"
  },
  "000387": {
    "context": "000387",
    "code": "0x45",
    "developer_comment": "",
    "string": "渡瀬たちは今日、ここで洵と待ち合わせしていたのだ。"
  },
  "000392": {
    "context": "000392",
    "code": "0x45",
    "developer_comment": "",
    "string": "例の用事には、洵も同行する。"
  },
  "000397": {
    "context": "000397",
    "code": "0x45",
    "developer_comment": "",
    "string": "とはいえ今日は洵は仕事があるので、昼休みだけ抜けてくるという話だった。"
  },
  "000404": {
    "context": "000404",
    "code": "0x45",
    "developer_comment": "",
    "string": "と、ちょうどその時――"
  },
  "000453": {
    "context": "000453",
    "code": "0x45",
    "developer_comment": "洵",
    "string": "「ごめんごめん、遅くなった！」"
  },
  "000458": {
    "context": "000458",
    "code": "0x45",
    "developer_comment": "",
    "string": "署の中から、洵が飛び出してくるのが見えた。"
  },
  "000469": {
    "context": "000469",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「おぉ、守部！」"
  },
  "000527": {
    "context": "000527",
    "code": "0x45",
    "developer_comment": "洵",
    "string": "「いやぁ久しぶり、２人とも！」"
  },
  "000532": {
    "context": "000532",
    "code": "0x45",
    "developer_comment": "",
    "string": "洵は渡瀬たちに駆け寄り、にこにこと言った。"
  },
  "000568": {
    "context": "000568",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「元気そうね、洵」"
  },
  "000597": {
    "context": "000597",
    "code": "0x45",
    "developer_comment": "洵",
    "string": "「元気元気、っていうかもー大変だよぉ」"
  },
  "000626": {
    "context": "000626",
    "code": "0x45",
    "developer_comment": "洵",
    "string": "「なにしろシリウスも２人殉職して、２人辞めちゃったから、人材不足でねぇ」"
  },
  "000637": {
    "context": "000637",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「……そうか」"
  },
  "000642": {
    "context": "000642",
    "code": "0x45",
    "developer_comment": "",
    "string": "その言葉を聞き、渡瀬の胸に痛みが走る。"
  },
  "000671": {
    "context": "000671",
    "code": "0x45",
    "developer_comment": "洵",
    "string": "「あー、そうやってしょげないの」"
  },
  "000700": {
    "context": "000700",
    "code": "0x45",
    "developer_comment": "洵",
    "string": "「司令長たちも最初は、悲しんだり混乱したりしてたけど……」"
  },
  "000729": {
    "context": "000729",
    "code": "0x45",
    "developer_comment": "洵",
    "string": "「今はもうすっかり立ち直って、シリウス再建のために頑張ってるから」"
  },
  "000759": {
    "context": "000759",
    "code": "0x45",
    "developer_comment": "洵",
    "string": "「あたしも『シリウスの長い１日』を生き残った女として、隊を引っ張ってるよ」"
  },
  "000770": {
    "context": "000770",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「へぇ、頼もしいじゃないか」"
  },
  "000824": {
    "context": "000824",
    "code": "0x45",
    "developer_comment": "",
    "string": "あの日のラボへの出場は、消防関係者の中で『シリウスの長い１日』と呼ばれ、半ば伝説的に扱われているそうだ。"
  },
  "000829": {
    "context": "000829",
    "code": "0x45",
    "developer_comment": "",
    "string": "それを経験した事は、洵のキャリアに大きな星をつけたのだろう。"
  },
  "000868": {
    "context": "000868",
    "code": "0x45",
    "developer_comment": "",
    "string": "当の洵はそれをおごる事なく、にやりと笑って言う。"
  },
  "000897": {
    "context": "000897",
    "code": "0x45",
    "developer_comment": "洵",
    "string": "「それより聞いたよ隊長～、いよいよお姉ちゃんと結婚だって？」"
  },
  "000908": {
    "context": "000908",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「む……お前、知ってたのか」"
  },
  "000937": {
    "context": "000937",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「当たり前じゃない、一番に報告したわよ」"
  },
  "000948": {
    "context": "000948",
    "code": "0x45",
    "developer_comment": "洵",
    "string": "「プロポーズの言葉はなんだったのかな？　ねぇどうなの、隊長――」"
  },
  "000977": {
    "context": "000977",
    "code": "0x45",
    "developer_comment": "洵",
    "string": "「っと、もう『隊長』じゃないんだよね。なんて呼べばいい？」"
  },
  "000988": {
    "context": "000988",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「なんでもいいさ。『笠鷺さん』とかでいいんじゃないか」"
  },
  "001017": {
    "context": "001017",
    "code": "0x45",
    "developer_comment": "洵",
    "string": "「それだと、お姉ちゃんとかぶっちゃうじゃん？　お姉ちゃんも今じゃ『笠鷺風見』なんだから」"
  },
  "001028": {
    "context": "001028",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「じゃあどうするってんだよ」"
  },
  "001057": {
    "context": "001057",
    "code": "0x45",
    "developer_comment": "洵",
    "string": "「そうだなぁ。うん、やっぱここは――」"
  },
  "001086": {
    "context": "001086",
    "code": "0x45",
    "developer_comment": "洵",
    "string": "「『お兄ちゃん』だね！」"
  },
  "001097": {
    "context": "001097",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「お、お兄ちゃん！？」"
  },
  "001126": {
    "context": "001126",
    "code": "0x45",
    "developer_comment": "洵",
    "string": "「そりゃそうだよ、お姉ちゃんのダンナはお兄ちゃんでしょ？」"
  },
  "001137": {
    "context": "001137",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「お兄ちゃんねぇ……なんか呼ばれ慣れないっていうか、急にこんなでかい妹ができると戸惑うっていうか……」"
  },
  "001166": {
    "context": "001166",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「いいじゃないの、あなた」"
  },
  "001177": {
    "context": "001177",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「私と縁を結ぶという事は、洵とも縁を結ぶという事よ？」"
  },
  "001206": {
    "context": "001206",
    "code": "0x45",
    "developer_comment": "洵",
    "string": "「そうだよお兄ちゃん、納得しなさい」"
  },
  "001211": {
    "context": "001211",
    "code": "0x45",
    "developer_comment": "",
    "string": "女２人に詰め寄られ、渡瀬は思わずたじろいだ。"
  },
  "001222": {
    "context": "001222",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「……あぁもう、わかったよ！　好きに呼べ好きに！」"
  },
  "001251": {
    "context": "001251",
    "code": "0x45",
    "developer_comment": "洵",
    "string": "「了解！　じゃああたしは妹なんだから、守部じゃなくて『洵』と呼びなさい」"
  },
  "001262": {
    "context": "001262",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「偉そうな妹だな！　もうなんでもいいよ！」"
  },
  "001267": {
    "context": "001267",
    "code": "0x45",
    "developer_comment": "",
    "string": "渡瀬の言葉に、洵と風見が笑った。"
  },
  "001272": {
    "context": "001272",
    "code": "0x45",
    "developer_comment": "",
    "string": "やれやれと首を振る彼に、洵が問いを重ねる。"
  },
  "001301": {
    "context": "001301",
    "code": "0x45",
    "developer_comment": "洵",
    "string": "「ところでお兄ちゃん、これからどうやってお姉ちゃん養うつもりなの？　仕事は？」"
  },
  "001312": {
    "context": "001312",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「あぁ、あんな大事件の中心人物になった以上、もうマトモな職には就きづらいからな」"
  },
  "001323": {
    "context": "001323",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「先生の手引きで、超科局に入る事にした」"
  },
  "001352": {
    "context": "001352",
    "code": "0x45",
    "developer_comment": "洵",
    "string": "「超科局？　まさかコミュニケーター管理官になるの？」"
  },
  "001363": {
    "context": "001363",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「そうさ。なにしろ俺の妻は、レベル６コミュニケーターだからな」"
  },
  "001374": {
    "context": "001374",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「夫が管理官なら、色々と安心だろ？」"
  },
  "001403": {
    "context": "001403",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「そう。私は今後、この人の監視と保護を受けるのよ」"
  },
  "001414": {
    "context": "001414",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「一生、一番そばでな」"
  },
  "001443": {
    "context": "001443",
    "code": "0x45",
    "developer_comment": "洵",
    "string": "「そうなんだ……」"
  },
  "001472": {
    "context": "001472",
    "code": "0x45",
    "developer_comment": "洵",
    "string": "「……じゃあ、シリウスにはもう戻ってこないんだね？」"
  },
  "001498": {
    "context": "001498",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「っ……」"
  },
  "001503": {
    "context": "001503",
    "code": "0x45",
    "developer_comment": "",
    "string": "その言葉が渡瀬の心を打った。"
  },
  "001508": {
    "context": "001508",
    "code": "0x45",
    "developer_comment": "",
    "string": "寂しげな顔をする洵に、渡瀬は静かな声で答える。"
  },
  "001519": {
    "context": "001519",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「……戻れないさ」"
  },
  "001524": {
    "context": "001524",
    "code": "0x45",
    "developer_comment": "",
    "string": "鹿鳴市の公務員になるなど不可能だ。"
  },
  "001529": {
    "context": "001529",
    "code": "0x45",
    "developer_comment": "",
    "string": "あの事件を境に、渡瀬の立場ははっきりと変わってしまった。"
  },
  "001534": {
    "context": "001534",
    "code": "0x45",
    "developer_comment": "",
    "string": "かつての職場に、自分の過去に、どれだけ愛着があったとしても戻る事はできない。"
  },
  "001545": {
    "context": "001545",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「……今でも時々、ここで過ごした日々を、懐かしく思う事はあるけどな」"
  },
  "001574": {
    "context": "001574",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「そうね……」"
  },
  "001579": {
    "context": "001579",
    "code": "0x45",
    "developer_comment": "",
    "string": "風見も署の方に目をやって言った。"
  },
  "001584": {
    "context": "001584",
    "code": "0x45",
    "developer_comment": "",
    "string": "亘の遺志を継いで、消防官になってから、ずっとここに勤めてきた。"
  },
  "001589": {
    "context": "001589",
    "code": "0x45",
    "developer_comment": "",
    "string": "人命を救うため、誰かの未来を護るため、何度も死地に飛び込んだ。"
  },
  "001594": {
    "context": "001594",
    "code": "0x45",
    "developer_comment": "",
    "string": "その意思は消えていない。"
  },
  "001599": {
    "context": "001599",
    "code": "0x45",
    "developer_comment": "",
    "string": "立場や仕事が変わっても、決して――"
  },
  "001610": {
    "context": "001610",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「……なぁ、洵。俺はもうレスキュー隊員じゃないが……」"
  },
  "001621": {
    "context": "001621",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「これからも誰かを護るために、働き続けるつもりだよ」"
  },
  "001632": {
    "context": "001632",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「お前と一緒には働けないが、それでも気持ちは一緒だ」"
  },
  "001637": {
    "context": "001637",
    "code": "0x45",
    "developer_comment": "",
    "string": "渡瀬がそう言うと、洵は微笑んで――"
  },
  "001666": {
    "context": "001666",
    "code": "0x45",
    "developer_comment": "洵",
    "string": "「……うん、わかってる」"
  },
  "001671": {
    "context": "001671",
    "code": "0x45",
    "developer_comment": "",
    "string": "渡瀬の胸に顔を埋めた。"
  },
  "001799": {
    "context": "001799",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「……洵？」"
  },
  "001804": {
    "context": "001804",
    "code": "0x45",
    "developer_comment": "",
    "string": "名を呼ぶと、洵は囁くように言った。"
  },
  "001815": {
    "context": "001815",
    "code": "0x45",
    "developer_comment": "洵",
    "string": "「……ねぇ、隊長、副隊長」"
  },
  "001826": {
    "context": "001826",
    "code": "0x45",
    "developer_comment": "洵",
    "string": "「２人がシリウスを辞めても、離れて暮らす事になっても――」"
  },
  "001837": {
    "context": "001837",
    "code": "0x45",
    "developer_comment": "洵",
    "string": "「あたしたちは、仲間だよ」"
  },
  "001848": {
    "context": "001848",
    "code": "0x45",
    "developer_comment": "洵",
    "string": "「同じ釜のゴハン食べて、何度も助けて助けられて、一緒に死線をくぐってきた……」"
  },
  "001853": {
    "context": "001853",
    "code": "0x45",
    "developer_comment": "",
    "string": "――それは仲間という言葉だけでは言い尽くせない。"
  },
  "001858": {
    "context": "001858",
    "code": "0x45",
    "developer_comment": "",
    "string": "バディという言葉でさえ足りない。"
  },
  "001863": {
    "context": "001863",
    "code": "0x45",
    "developer_comment": "",
    "string": "互いに誰より大切な――"
  },
  "001874": {
    "context": "001874",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「……家族だな」"
  },
  "001885": {
    "context": "001885",
    "code": "0x45",
    "developer_comment": "洵",
    "string": "「そう、そういう事！」"
  },
  "001930": {
    "context": "001930",
    "code": "0x45",
    "developer_comment": "",
    "string": "そう言って洵は、涙を拭った。"
  },
  "001935": {
    "context": "001935",
    "code": "0x45",
    "developer_comment": "",
    "string": "その顔には吹っ切れたような笑みが浮かんでいる。"
  },
  "001946": {
    "context": "001946",
    "code": "0x45",
    "developer_comment": "洵",
    "string": "「これからよろしくね、お兄ちゃん！」"
  },
  "001957": {
    "context": "001957",
    "code": "0x45",
    "developer_comment": "洵",
    "string": "「２人が帰る場所は、ここにもあるからさ。何かあったら寄ってってね」"
  },
  "001968": {
    "context": "001968",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「あぁ、もちろんだ」"
  },
  "001979": {
    "context": "001979",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「可愛い妹の顔見に、ちょくちょく寄らせてもらうよ」"
  },
  "001989": {
    "context": "001989",
    "code": "0x45",
    "developer_comment": "",
    "string": "渡瀬はそう言って、洵の頭をくしゃくしゃと撫でる。"
  },
  "001994": {
    "context": "001994",
    "code": "0x45",
    "developer_comment": "",
    "string": "彼女は撫でられるに任せ、幸福そうに微笑んだ……。"
  }
}