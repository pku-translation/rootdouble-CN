{
  "000008": {
    "context": "000008",
    "code": "0x55",
    "string": "√After Chapter1"
  },
  "000077": {
    "context": "000077",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「……」"
  },
  "000082": {
    "context": "000082",
    "code": "0x45",
    "developer_comment": "",
    "string": "風見が悔しげに、ＰＤＡをしまう。"
  },
  "000087": {
    "context": "000087",
    "code": "0x45",
    "developer_comment": "",
    "string": "渡瀬たちは呆然として、口々に呟いた。"
  },
  "000098": {
    "context": "000098",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「復旧まで９時間……だって……！？」"
  },
  "000128": {
    "context": "000128",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "「長すぎるわよ！　なんでそんな事に！？」"
  },
  "000159": {
    "context": "000159",
    "code": "0x45",
    "developer_comment": "洵",
    "string": "「メルトダウンを起こした原子炉が、すぐそばにあるってのに……！」"
  },
  "000164": {
    "context": "000164",
    "code": "0x45",
    "developer_comment": "",
    "string": "その環境での９時間――それは途方もなく長く感じられた。"
  },
  "000169": {
    "context": "000169",
    "code": "0x45",
    "developer_comment": "",
    "string": "風見がアテにしていた増援も、全く望めないらしい。"
  },
  "000174": {
    "context": "000174",
    "code": "0x45",
    "developer_comment": "",
    "string": "かすれた声で、渡瀬は尋ねる。"
  },
  "000279": {
    "context": "000279",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「宇喜多さん……」"
  },
  "000308": {
    "context": "000308",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "「……なんだね」"
  },
  "000319": {
    "context": "000319",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「『放射能漏洩防止システム』ってのは、なんなんだ……！？」"
  },
  "000348": {
    "context": "000348",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "「……！」"
  },
  "000359": {
    "context": "000359",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「それのせいで、隔壁が開かないって……なんで人がまだ地下にいるのに、そんなものが作動したんだ？」"
  },
  "000370": {
    "context": "000370",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「メルトダウンって、いったいなんなんだ！？」"
  },
  "000381": {
    "context": "000381",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「教えてくれ！　オレも漠然としか、その恐ろしさを知らないんだよ！」"
  },
  "000469": {
    "context": "000469",
    "code": "0x45",
    "developer_comment": "",
    "string": "宇喜多はためらいがちに皆を見回し、そして呟く。"
  },
  "000498": {
    "context": "000498",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "「うむ……知らない人もいるだろうから、説明しておいたほうがよさそうだな……」"
  },
  "000527": {
    "context": "000527",
    "code": "0x45",
    "developer_comment": "ユウリ",
    "string": "「……っ」"
  },
  "000571": {
    "context": "000571",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "「極めてかいつまんで説明するが、いいかね？」"
  },
  "000582": {
    "context": "000582",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「ああ……頼む」"
  },
  "000587": {
    "context": "000587",
    "code": "0x45",
    "developer_comment": "",
    "string": "渡瀬が言うと、宇喜多は静かに語り始めた。"
  },
  "000616": {
    "context": "000616",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "「メルトダウンとはつまり――原子炉の炉心が異常な高温になり、融解してしまう現象の事だ」"
  },
  "000627": {
    "context": "000627",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "「通常そんな事が起こらないように、原子炉の中で燃焼している核燃料は、絶えず冷却されるべきなのだが……」"
  },
  "000656": {
    "context": "000656",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "「冷却が適切になされなかった場合、炉心の耐熱性を上回る熱量が発生し、原子炉そのものが破損する」"
  },
  "000667": {
    "context": "000667",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "「そして灼熱した核燃料が、外部に放出されるんだ」"
  },
  "000678": {
    "context": "000678",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「Ｎエリアで起こっている『大火災』ってやつの原因はそれか？」"
  },
  "000689": {
    "context": "000689",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "「あぁ。恐らく莫大な熱量を持つ核燃料が、周囲の可燃物を炎上させたんだな」"
  },
  "000718": {
    "context": "000718",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "「溶け落ちた核燃料は、摂氏３０００度超の凄まじい高温を発し、あらゆるものを燃やしてしまう」"
  },
  "000747": {
    "context": "000747",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「今も火災が続いていると言う事は、破損の程度は大きいのでしょうか？」"
  },
  "000776": {
    "context": "000776",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "「そう考えて間違いはないだろう」"
  },
  "000787": {
    "context": "000787",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "「自動消火システムが破壊されている事から見るに、爆発なども起きている可能性がある」"
  },
  "000816": {
    "context": "000816",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "「核燃料が何らかの水分に触れれば、水蒸気爆発などが起こるからね。Ｎエリアは焦熱と爆風の地獄と化しているはずさ」"
  },
  "000821": {
    "context": "000821",
    "code": "0x45",
    "developer_comment": "",
    "string": "宇喜多は引きつった笑みを浮かべ、そう答えた。"
  },
  "000850": {
    "context": "000850",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "「……そして核燃料とは、言うまでもなく放射性物質だ」"
  },
  "000879": {
    "context": "000879",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "「原子炉から放出されたそれは、放射線を周囲に撒き散らす」"
  },
  "000890": {
    "context": "000890",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "「実例を挙げるなら――そう、『チェルノブイリ原発事故』というのを知ってるかね？」"
  },
  "000901": {
    "context": "000901",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「いや……」"
  },
  "000906": {
    "context": "000906",
    "code": "0x45",
    "developer_comment": "",
    "string": "聞いた事があるようなないような言葉だった。宇喜多はため息混じりに続ける。"
  },
  "000935": {
    "context": "000935",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "「もう半世紀近くも前にロシアで起きた、人類史上最大のクライシスだよ」"
  },
  "000964": {
    "context": "000964",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "「原子力発電所で使用されていた原子炉がメルトダウンし、水蒸気爆発が発生」"
  },
  "000975": {
    "context": "000975",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "「１０ｔにも及ぶ放射性物質が、原子炉建屋の外に撒き散らされたんだ」"
  },
  "000986": {
    "context": "000986",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "「これはヒロシマ型原爆の５００倍の量にあたり、それによる死者は膨大な数にのぼった」"
  },
  "000997": {
    "context": "000997",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "「原発職員や事故処理にあたった消防士など、３０００人が死亡。そしてその後も増え続けた」"
  },
  "001008": {
    "context": "001008",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "「最終的な死者数は、今となってはわからないが――数十万人にも及ぶとさえ言われている」"
  },
  "001019": {
    "context": "001019",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「数十万！？　なんでそんなに死者が……！」"
  },
  "001048": {
    "context": "001048",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "「その原因がまさに、放射性物質なのさ」"
  },
  "001059": {
    "context": "001059",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "「『死の灰』とも呼ばれるそれは、爆発と火災により空に舞いあがって、雨と共に地表に降り注いだんだ」"
  },
  "001088": {
    "context": "001088",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "「わかるかね？　真に恐ろしいのはこの点だ。今この地下に満ちている放射線が、周辺地域全体に拡がるという点が……！」"
  },
  "001099": {
    "context": "001099",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「……その、死の灰ってのが漏れると、施設の外も汚染されちまうって事か？」"
  },
  "001128": {
    "context": "001128",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "「その通りだ。事実チェルノブイリ原発の周辺は、以後９００年に渡り人の住めない土地となった」"
  },
  "001139": {
    "context": "001139",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "「ゆえにメルトダウン発生時の緊急処置というのは、過去に国内外で発生したＮ災害の教訓を活かし――」"
  },
  "001150": {
    "context": "001150",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "「放射性物質の拡散を抑える事を、第一義に考える風潮が強まった」"
  },
  "001161": {
    "context": "001161",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "「だからこの施設では、メルトダウンが起きたら、全ての隔壁が自動で閉まるようになっているのさ」"
  },
  "001172": {
    "context": "001172",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "「『ケースＮ』というのはつまり、隔壁強制閉鎖を知らせるコードだよ」"
  },
  "001183": {
    "context": "001183",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「……そういう事だったのか……！」"
  },
  "001188": {
    "context": "001188",
    "code": "0x45",
    "developer_comment": "",
    "string": "確かに原子炉を堅牢無比の複合鋼板で覆ってしまえば、万が一メルトダウンが発生した時も対処しやすいだろう。"
  },
  "001193": {
    "context": "001193",
    "code": "0x45",
    "developer_comment": "",
    "string": "これが例えば普通の鉄筋コンクリートでできた施設なら、水蒸気爆発などが起きれば施設が崩壊しかねない。"
  },
  "001198": {
    "context": "001198",
    "code": "0x45",
    "developer_comment": "",
    "string": "それを避けるためには、施設そのものを巨大な重金属の箱と化し、有事の際には蓋を閉じて、密閉してしまうべきかもしれない。"
  },
  "001203": {
    "context": "001203",
    "code": "0x45",
    "developer_comment": "",
    "string": "その考えは理解できるが――"
  },
  "001214": {
    "context": "001214",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「……でも、閉じ込められた人たちはどうなるんだ？」"
  },
  "001243": {
    "context": "001243",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "「そうよ、脱出口はないの！？」"
  },
  "001254": {
    "context": "001254",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "「エアダクトとか……今こうして呼吸ができるという事は、空気の通り道はあるんでしょう？」"
  },
  "001283": {
    "context": "001283",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "「いや、エアダクト内の各所には、放射性物質濾過装置が設置されている。地上に続く箇所や、各エリアを隔てるところにね」"
  },
  "001294": {
    "context": "001294",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "「当然、人は通れない。エアダクトからの脱出は不可能だ」"
  },
  "001305": {
    "context": "001305",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「そこまで念入りに！？」"
  },
  "001334": {
    "context": "001334",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "「そうとも。どこの原子力関連施設もそうだが、安全面には極めて配慮してある」"
  },
  "001345": {
    "context": "001345",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "「いまこのラボの地下フロアは、外界と完全に隔絶されている。灰一粒、外に漏らさないようにね」"
  },
  "001356": {
    "context": "001356",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "「だから先ほどの通信で言っていたように、ＡＤを投与しながら、隔壁の開放を待つしか――」"
  },
  "001361": {
    "context": "001361",
    "code": "0x45",
    "developer_comment": "",
    "string": "宇喜多がそう言いかけた時、洵がそれを遮った。"
  },
  "001390": {
    "context": "001390",
    "code": "0x45",
    "developer_comment": "洵",
    "string": "「待ってよ、宇喜多さん……悠長に待ってるわけにはいかないみたいだよ」"
  },
  "001419": {
    "context": "001419",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "「な、なんだね？」"
  },
  "001424": {
    "context": "001424",
    "code": "0x45",
    "developer_comment": "",
    "string": "そう問われると、洵はプロキオンを懐から出して続けた。"
  },
  "001435": {
    "context": "001435",
    "code": "0x45",
    "developer_comment": "洵",
    "string": "「このプロキオンの数値、見て……今は『１６０５ｍＳｖ』になってる」"
  },
  "001446": {
    "context": "001446",
    "code": "0x45",
    "developer_comment": "洵",
    "string": "「だけど隔壁封鎖の直後は、まだ『１００ｍＳｖ』前後だった」"
  },
  "001475": {
    "context": "001475",
    "code": "0x45",
    "developer_comment": "洵",
    "string": "「場所によって、多少上下するみたいだけど……徐々に数値は、上がってるんだよ」"
  },
  "001486": {
    "context": "001486",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「なに！？」"
  },
  "001491": {
    "context": "001491",
    "code": "0x45",
    "developer_comment": "",
    "string": "そう言われて渡瀬も気づいた。"
  },
  "001496": {
    "context": "001496",
    "code": "0x45",
    "developer_comment": "",
    "string": "初めてプロキオンの値を見た時は、数値は『１１００ｍＳｖ』程度だった。"
  },
  "001501": {
    "context": "001501",
    "code": "0x45",
    "developer_comment": "",
    "string": "それからほんのわずかな時間で、確かに数値は上昇している。"
  },
  "001512": {
    "context": "001512",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「ど、どういう事だ宇喜多さん！？　なんで数値が上がるんだ！？」"
  },
  "001523": {
    "context": "001523",
    "code": "0x45",
    "developer_comment": "洵",
    "string": "「施設外に放射性物質を拡散させないようにはなってても、施設内は何の対策もしてないの！？」"
  },
  "001552": {
    "context": "001552",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "「……い、いや、その……」"
  },
  "001581": {
    "context": "001581",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "「……僕にも、わからん……！」"
  },
  "001592": {
    "context": "001592",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「わからないって！？」"
  },
  "001621": {
    "context": "001621",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "「各エリアを隔てるセキュリティゲートも隔壁と同様、放射性物質の拡散を防ぐために役立つはずなんだが……」"
  },
  "001650": {
    "context": "001650",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "「しかし例の爆発や火災のせいで、ゲートや内壁があちこち破損しているのかもしれない……」"
  },
  "001661": {
    "context": "001661",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "「ひょっとしたら、エアダクト内の濾過装置も壊れているのかも……」"
  },
  "001780": {
    "context": "001780",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "「な、なによそれ！？　『安全面への配慮』が聞いてあきれるわ！」"
  },
  "001785": {
    "context": "001785",
    "code": "0x45",
    "developer_comment": "",
    "string": "恵那は眉を逆立て、宇喜多に詰め寄った。"
  },
  "001796": {
    "context": "001796",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "「人がまだ地下に残っているのに、勝手に隔壁を封鎖しておいて、しかも開けられない！」"
  },
  "001853": {
    "context": "001853",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "「放射線はどんどん強くなる！　電話も無線通信も使えない！　エレベーターも動かなければ、消火システムも役立たず！」"
  },
  "001882": {
    "context": "001882",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "「どこを見ても穴だらけじゃないの！　何が放射能漏洩防止システムよ！」"
  },
  "001911": {
    "context": "001911",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "「う、む……た、確かに安全面に多少の不備があった事は認めるが……」"
  },
  "001940": {
    "context": "001940",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "「システムの暴走や、爆発などは想定外の出来事で――」"
  },
  "001969": {
    "context": "001969",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "「その想定外の出来事が起きないようにするのが、『安全面への配慮』でしょう！？」"
  },
  "001988": {
    "context": "001988",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「先生の言う通りだ、宇喜多さん！」"
  },
  "001999": {
    "context": "001999",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「『想定外』ってのは要するに、『想定する気がなかった』って事じゃないのか！？」"
  },
  "002010": {
    "context": "002010",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「結局オレたちの安全はどうなるんだ！？」"
  },
  "002039": {
    "context": "002039",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "「し、しかし！　僕はこのラボの職員だが、セキュリティ関連の責任者というわけではないんだぞ！？」"
  },
  "002050": {
    "context": "002050",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "「怒るのもわかるが、少し落ち着いてくれたまえ！」"
  },
  "002064": {
    "context": "002064",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「ま、待て先生、落ち着け！」"
  },
  "002093": {
    "context": "002093",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "「落ち着けって！？　落ち着いていられるわけがないじゃない！」"
  },
  "002104": {
    "context": "002104",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "「私だけならまだしも、私の生徒まで閉じ込められているのに！」"
  },
  "002161": {
    "context": "002161",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「っ！」"
  },
  "002166": {
    "context": "002166",
    "code": "0x45",
    "developer_comment": "",
    "string": "渡瀬ははっとして、先ほどのモニターを見た。"
  },
  "002211": {
    "context": "002211",
    "code": "0x45",
    "developer_comment": "",
    "string": "――彼らもまた、この劣悪を極める状況下に閉じ込められているのだ。"
  },
  "002216": {
    "context": "002216",
    "code": "0x45",
    "developer_comment": "",
    "string": "救助の手も得られず、火に追われ放射線に怯え、逃げ惑っているに違いない。"
  },
  "002227": {
    "context": "002227",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "「せめて一刻も早く、彼らの元にＡＤを届けなくちゃ……！」"
  },
  "002232": {
    "context": "002232",
    "code": "0x45",
    "developer_comment": "",
    "string": "泣きそうな声で恵那が言う。"
  },
  "002237": {
    "context": "002237",
    "code": "0x45",
    "developer_comment": "",
    "string": "性格がキツい女に見えたが――やはり、そこは教師らしい。生徒たちが心配で仕方ないのだろう。"
  },
  "002290": {
    "context": "002290",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「先生、すぐに彼らを探しに行こう！」"
  },
  "002319": {
    "context": "002319",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "「ええ！」"
  },
  "002324": {
    "context": "002324",
    "code": "0x45",
    "developer_comment": "",
    "string": "渡瀬たちが部屋を飛び出そうとした時、鋭い声が響いた。"
  },
  "002354": {
    "context": "002354",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「２人とも、お待ち下さい！」"
  },
  "002383": {
    "context": "002383",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "「橘さん！？　どうして止めるの？」"
  },
  "002421": {
    "context": "002421",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「彼らを捜索する前に……まだ１つ、踏まえておかねばならない問題が残っているからです」"
  },
  "002430": {
    "context": "002430",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "（だが、それは……！）"
  },
  "002454": {
    "context": "002454",
    "code": "0x45",
    "developer_comment": "",
    "string": "そう思った時、風見が渡瀬の気持ちを代弁した。"
  },
  "002465": {
    "context": "002465",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「いえ、椿山さん……そこにもまた１つ問題があります」"
  },
  "002494": {
    "context": "002494",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "「え……？」"
  },
  "002523": {
    "context": "002523",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「彼らと合流できたとしても、全員が生き延びるためには、１時間ごとに９本のＡＤが必要でしょう」"
  },
  "002552": {
    "context": "002552",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「ですが我々に残されたＡＤは――わずか５本しかありません」"
  },
  "002563": {
    "context": "002563",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「彼らに届けるどころか、このままでは我々６人だけでも、次の１時間を生き延びる事もできないんですよ……！」"
  },
  "002568": {
    "context": "002568",
    "code": "0x45",
    "developer_comment": "",
    "string": "その言葉を聞き、皆が息を呑んだ。"
  },
  "002597": {
    "context": "002597",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "「ご、５本って……それしかないの！？」"
  },
  "002626": {
    "context": "002626",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「申し訳ありません……このような事態を、予想していなかったものですから」"
  },
  "002655": {
    "context": "002655",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "「準備が足りなすぎるわよ！　あなたたち、レスキュー隊なんでしょ！？」"
  },
  "002684": {
    "context": "002684",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「……申し訳、ありません」"
  },
  "002739": {
    "context": "002739",
    "code": "0x45",
    "developer_comment": "",
    "string": "罵倒と謝罪、追及と対立――それらの言葉を聞きながら、渡瀬は奥歯を噛み締めていた。"
  },
  "002745": {
    "context": "002745",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "（どこまでひどい状況なんだ……！）"
  },
  "002750": {
    "context": "002750",
    "code": "0x45",
    "developer_comment": "",
    "string": "全員揃って被曝を防ぎ続けるためには、１時間に最低６本ずつのＡＤが必要になる。"
  },
  "002755": {
    "context": "002755",
    "code": "0x45",
    "developer_comment": "",
    "string": "しかも要救助者を１人助ければ、その分必要になるＡＤは増える。"
  },
  "002760": {
    "context": "002760",
    "code": "0x45",
    "developer_comment": "",
    "string": "ユウリを救助した直後は、ＡＤはそこまで不足していたわけではなかったが――"
  },
  "002766": {
    "context": "002766",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "（先生や宇喜多さんがそこに加わった事で……いよいよ足りなくなってしまったわけか！）"
  },
  "002771": {
    "context": "002771",
    "code": "0x45",
    "developer_comment": "",
    "string": "なぜこれほど最悪な事態になってしまったのか、その理由はわからないし、考えている暇もない。"
  },
  "002776": {
    "context": "002776",
    "code": "0x45",
    "developer_comment": "",
    "string": "だが、今するべき事ははっきりしている。"
  },
  "002834": {
    "context": "002834",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「つまり、オレたちは『３名の要救助者』を捜索しつつ――」"
  },
  "002845": {
    "context": "002845",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「『人数分のＡＤ』を見つけなきゃいけないってわけか……！」"
  },
  "002874": {
    "context": "002874",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「えぇ、それもできるだけ早くです」"
  },
  "002879": {
    "context": "002879",
    "code": "0x45",
    "developer_comment": "",
    "string": "風見はモニターを見て答える。"
  },
  "002890": {
    "context": "002890",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「彼らの『累積被曝量』が、致死量を超える前に……！」"
  },
  "002901": {
    "context": "002901",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「累積被曝量？」"
  },
  "002906": {
    "context": "002906",
    "code": "0x45",
    "developer_comment": "",
    "string": "渡瀬が問い返すと、風見は早口で説明した。"
  },
  "002935": {
    "context": "002935",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「放射線を受けた量というものは、体の中に蓄積していきます。その合計値が累積被曝量です」"
  },
  "002946": {
    "context": "002946",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「プロキオンに表示されるのは、『その場所にいた時、１時間あたりどれくらい被曝するかの量』」"
  },
  "002957": {
    "context": "002957",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「つまり現在の環境下では、『１時間あたり１６０５ｍＳｖ被曝する』という事です」"
  },
  "002968": {
    "context": "002968",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「累積被曝量が２０００ｍＳｖに至った場合、致死率は約５パーセント。４０００ｍＳｖなら、致死率は約５０パーセント」"
  },
  "002997": {
    "context": "002997",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「そして８０００ｍＳｖに至れば、９９パーセント死亡します」"
  },
  "003008": {
    "context": "003008",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「つまり累積被曝量が上がるほど、死のリスクも加速度的に高まるのです」"
  },
  "003019": {
    "context": "003019",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「そして一度累積した被曝量は、すぐには減る事はありません」"
  },
  "003024": {
    "context": "003024",
    "code": "0x45",
    "developer_comment": "",
    "string": "と言う事は今の環境下で、ＡＤを投与していなかった場合――"
  },
  "003029": {
    "context": "003029",
    "code": "0x45",
    "developer_comment": "",
    "string": "２時間半で約４０００ｍＳｖ、５時間で約８０００ｍＳｖ被曝する。"
  },
  "003035": {
    "context": "003035",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "（ＡＤがなければ皆、数時間ともたず死んでしまうのか……！）"
  },
  "003040": {
    "context": "003040",
    "code": "0x45",
    "developer_comment": "",
    "string": "そう思う渡瀬に、宇喜多が言葉を重ねる。"
  },
  "003086": {
    "context": "003086",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "「だがＡＤは４０００ｍＳｖまでの放射線量を、ほぼ完全に無害化する」"
  },
  "003115": {
    "context": "003115",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "「さらに８０００ｍＳｖまでの環境下でも、延命効果は極めて高い」"
  },
  "003126": {
    "context": "003126",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "「ＡＤさえ投与しておけば、累積被曝量は増加しないんだ」"
  },
  "003137": {
    "context": "003137",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「それは間違いないのか？　できれば確証が欲しいんだが――」"
  },
  "003166": {
    "context": "003166",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "「信じ難いだろうが、全面的に信用してくれ。ＡＤは我がラボの最高傑作だ」"
  },
  "003177": {
    "context": "003177",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "「安定ヨウ素剤、キレート剤、プルシアンブルー等９種の抗放射性物質薬に、医療用ナノマシンを加えた、全身被曝予防剤――」"
  },
  "003206": {
    "context": "003206",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "「それは静脈から注射する事で全身を巡り、細胞・血液・神経・遺伝子を保護する」"
  },
  "003217": {
    "context": "003217",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "「胃や肺などの粘膜にも３分で行き渡り、放射性物質の吸収を完全にカット」"
  },
  "003246": {
    "context": "003246",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "「さらに各放射性物質と化合し、無害化した上で、汗・尿等と共に対外に排出する」"
  },
  "003257": {
    "context": "003257",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "「その劇的な有効性を知っているから、僕もこんなに落ち着いているんだ。さもなきゃもっとパニックになってるさ」"
  },
  "003268": {
    "context": "003268",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「そ……そうか！」"
  },
  "003273": {
    "context": "003273",
    "code": "0x45",
    "developer_comment": "",
    "string": "宇喜多の口調と表情には、嘘はなさそうだった。"
  },
  "003278": {
    "context": "003278",
    "code": "0x45",
    "developer_comment": "",
    "string": "要はＡＤを投与し続けている限り、生存できる。"
  },
  "003283": {
    "context": "003283",
    "code": "0x45",
    "developer_comment": "",
    "string": "しかしＡＤが切れた瞬間から、時間経過と共に死が近づくという事だ。"
  },
  "003314": {
    "context": "003314",
    "code": "0x45",
    "developer_comment": "洵",
    "string": "「じゃあこんなところで、ごちゃごちゃ言い合ってる場合じゃないね！」"
  },
  "003343": {
    "context": "003343",
    "code": "0x45",
    "developer_comment": "洵",
    "string": "「施設内に残っているＡＤをかき集めて、夏彦くんたちの元にも届けなきゃ！」"
  },
  "003354": {
    "context": "003354",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「なぁ宇喜多さん！　監視カメラを使って、要救助者を探せないか！？」"
  },
  "003383": {
    "context": "003383",
    "code": "0x45",
    "developer_comment": "洵",
    "string": "「そうだよ、もしくは館内放送で呼び出すとか！」"
  },
  "003412": {
    "context": "003412",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "「いや、それが……放送は使えなくなってるし、カメラもほとんどが壊れてしまっているようなのだよ」"
  },
  "003423": {
    "context": "003423",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「壊れたって、どうして？」"
  },
  "003452": {
    "context": "003452",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "「火災や爆発の影響だろうね……映像記録も、多くのデータが消えている」"
  },
  "003481": {
    "context": "003481",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "「残っているいくつかの映像から察するに、彼らは『第６エリア』方面へ移動していたようだが……」"
  },
  "003492": {
    "context": "003492",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「第６エリアか！」"
  },
  "003497": {
    "context": "003497",
    "code": "0x45",
    "developer_comment": "",
    "string": "この第３エリアからずっと先のエリアに、彼らは向かったという。"
  },
  "003502": {
    "context": "003502",
    "code": "0x45",
    "developer_comment": "",
    "string": "だがそれがわかっただけでも充分だった。"
  },
  "003513": {
    "context": "003513",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「むしろちょうどいい……あの３人以外にも、要救助者がいる可能性がある」"
  },
  "003524": {
    "context": "003524",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「その要救助者を全て助けるためには、全エリアを隅々まで捜索する必要があるからな」"
  },
  "003553": {
    "context": "003553",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「えぇ、第１エリアと第２エリアはすでに捜索済みです」"
  },
  "003564": {
    "context": "003564",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「この第３エリアから、第６エリアまで捜索しながら進めば、全てのエリアを捜索した事になります」"
  },
  "003593": {
    "context": "003593",
    "code": "0x45",
    "developer_comment": "洵",
    "string": "「堂島先輩や桧山先輩も、そのどこかにいるはずだよ」"
  },
  "003622": {
    "context": "003622",
    "code": "0x45",
    "developer_comment": "洵",
    "string": "「あの２人もそれぞれ支給されたＡＤを持ってるし、隊長と並んで有能な人たちだ。合流できれば心強いよ！」"
  },
  "003633": {
    "context": "003633",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「よし！　橘、守部、さっそく行動を開始しよう！」"
  },
  "003638": {
    "context": "003638",
    "code": "0x45",
    "developer_comment": "",
    "string": "そう声を上げる渡瀬たちに、恵那が硬い声で言う。"
  },
  "003670": {
    "context": "003670",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "「わ……私も行くわ！」"
  },
  "003699": {
    "context": "003699",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "「う、うむ。全員で捜索しながら進んだ方が、効率がいいだろう」"
  },
  "003774": {
    "context": "003774",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「しかし、民間の方々に手伝ってもらうわけには――」"
  },
  "003803": {
    "context": "003803",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "「そういうことを言っている場合じゃないでしょう！？」"
  },
  "003832": {
    "context": "003832",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "「そうとも。それに僕はラボ内の事にも詳しい。多少は役にも立てるだろう」"
  },
  "003861": {
    "context": "003861",
    "code": "0x45",
    "developer_comment": "ユウリ",
    "string": "「わたしも……なにもできないかもしれないけど、がんばります」"
  },
  "003866": {
    "context": "003866",
    "code": "0x45",
    "developer_comment": "",
    "string": "宇喜多やユウリまでもが声を上げた。"
  },
  "003891": {
    "context": "003891",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「いや……ここからは橘の言う通り、オレたちレスキューの３人で捜索しよう」"
  },
  "003920": {
    "context": "003920",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "「３人！？　たった３人で、この広いラボを捜索するというのかね！？」"
  },
  "003949": {
    "context": "003949",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "「そうよ、そんなの時間がかかり過ぎるわ！」"
  },
  "003954": {
    "context": "003954",
    "code": "0x45",
    "developer_comment": "",
    "string": "皆の批判が渡瀬へ集中しかけた時、洵が声を上げた。"
  },
  "003983": {
    "context": "003983",
    "code": "0x45",
    "developer_comment": "洵",
    "string": "「隊長、副隊長……今はルールより実を取ろう。みんなもやる気になってくれてるし」"
  },
  "003998": {
    "context": "003998",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「先生たちの言う通りだ、橘……こうして言い争ってる時間が惜しい」"
  },
  "004027": {
    "context": "004027",
    "code": "0x45",
    "developer_comment": "洵",
    "string": "「そうだよ副隊長、今はルールより実を取ろう」"
  },
  "004042": {
    "context": "004042",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「ユウリだってこう言ってくれてるんだ……橘！」"
  },
  "004047": {
    "context": "004047",
    "code": "0x45",
    "developer_comment": "",
    "string": "ラボの広さを考えれば、捜索の手は多い方がいいだろう。"
  },
  "004052": {
    "context": "004052",
    "code": "0x45",
    "developer_comment": "",
    "string": "それに勇気を振り絞った、ユウリの思いを無駄にはしたくない。"
  },
  "004081": {
    "context": "004081",
    "code": "0x45",
    "developer_comment": "洵",
    "string": "「そうだよ副隊長、今はルールより実を取ろう」"
  },
  "004090": {
    "context": "004090",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "（橘、どうするんだ……？）"
  },
  "004095": {
    "context": "004095",
    "code": "0x45",
    "developer_comment": "",
    "string": "渡瀬が風見へ視線を向けた時、洵が威勢良く言った。"
  },
  "004124": {
    "context": "004124",
    "code": "0x45",
    "developer_comment": "洵",
    "string": "「副隊長、今はルールより実を取ろう！　みんなもやる気になってくれてるしさ」"
  },
  "004153": {
    "context": "004153",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「……確かに、そうせざるを得ませんね……！」"
  },
  "004158": {
    "context": "004158",
    "code": "0x45",
    "developer_comment": "",
    "string": "一瞬の逡巡の末、風見もうなずく。"
  },
  "004163": {
    "context": "004163",
    "code": "0x45",
    "developer_comment": "",
    "string": "――６人の意思は、ある目的のためにまとまった。"
  },
  "004168": {
    "context": "004168",
    "code": "0x45",
    "developer_comment": "",
    "string": "それは即ち、『生き残る事』。"
  },
  "004173": {
    "context": "004173",
    "code": "0x45",
    "developer_comment": "",
    "string": "そして『要救助者を救う事』。"
  },
  "004178": {
    "context": "004178",
    "code": "0x45",
    "developer_comment": "",
    "string": "２つの目的を叶えるため、すぐにでも動かなければならない。"
  },
  "004189": {
    "context": "004189",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「だったら急ごう、具体的にどう捜索する？」"
  },
  "004200": {
    "context": "004200",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「プロキオンもセキュリティカードも２つしかありませんから、ここは２手にわかれて捜索しましょう」"
  },
  "004229": {
    "context": "004229",
    "code": "0x45",
    "developer_comment": "洵",
    "string": "「チーム分けは？」"
  },
  "004376": {
    "context": "004376",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「バランスを考えて、まずは私と隊長とユウリさんが組みましょう」"
  },
  "004387": {
    "context": "004387",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「了解だ！」"
  },
  "004416": {
    "context": "004416",
    "code": "0x45",
    "developer_comment": "ユウリ",
    "string": "「わ……わかりました」"
  },
  "004528": {
    "context": "004528",
    "code": "0x45",
    "developer_comment": "洵",
    "string": "「じゃあこっちは、あたしと椿山さんと宇喜多さんだね？」"
  },
  "004539": {
    "context": "004539",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "「えぇ」"
  },
  "004550": {
    "context": "004550",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "「僕も異論なしだ」"
  },
  "004657": {
    "context": "004657",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「では、我々は内周棟を捜索しますので、守部のチームは外周棟をお願いします」"
  },
  "004686": {
    "context": "004686",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「捜索対象は全部で４つ――」"
  },
  "004697": {
    "context": "004697",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「『要救助者』と『ＡＤ』」"
  },
  "004708": {
    "context": "004708",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「そしてできれば、第５エリアに進むための『レベル５のセキュリティカード』と、『脱出経路』です」"
  },
  "004719": {
    "context": "004719",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「ＡＤは鉄製の『メディカルボックス』に入ってます。通路などにも落ちているかもしれませんので、入念にチェックを」"
  },
  "004748": {
    "context": "004748",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「時間が差し迫っているので急いで……！　しかし決して焦らず、身の安全に重々留意して下さい！」"
  },
  "004759": {
    "context": "004759",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「それでは皆さん、よろしくお願いします！」"
  },
  "004767": {
    "context": "004767",
    "code": "0x45",
    "developer_comment": "全員",
    "string": "「おう！」"
  },
  "004772": {
    "context": "004772",
    "code": "0x45",
    "developer_comment": "",
    "string": "その声に従い、渡瀬たちは監視ルームを飛び出した。"
  }
}