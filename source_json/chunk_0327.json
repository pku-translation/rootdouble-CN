{
  "000050": {
    "context": "000050",
    "code": "0x55",
    "string": "√Double Chapter3"
  },
  "000074": {
    "context": "000074",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「な……！」"
  },
  "000118": {
    "context": "000118",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「え、な、何！？」"
  },
  "000129": {
    "context": "000129",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "「何だ！？」"
  },
  "000134": {
    "context": "000134",
    "code": "0x45",
    "developer_comment": "",
    "string": "鉄扉の向こうで、金切り音は響き続けていた。"
  },
  "000140": {
    "context": "000140",
    "code": "0x45",
    "developer_comment": "",
    "string": "……やがてそれが、不意に止まる。"
  },
  "000146": {
    "context": "000146",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "（なんだ……なんなんだ！？）"
  },
  "000196": {
    "context": "000196",
    "code": "0x45",
    "developer_comment": "",
    "string": "不吉な予感を覚え、夏彦は鉄扉を見た。"
  },
  "000201": {
    "context": "000201",
    "code": "0x45",
    "developer_comment": "",
    "string": "火災や爆発にも耐えるという、極めて頑丈そうな扉を。"
  },
  "000206": {
    "context": "000206",
    "code": "0x45",
    "developer_comment": "",
    "string": "しかし、それには――鍵がついていない！"
  },
  "000226": {
    "context": "000226",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「悠里、おじさん！　この扉、開かないようにはできないんですか！？」"
  },
  "000255": {
    "context": "000255",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「で、できないよ！」"
  },
  "000284": {
    "context": "000284",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "「避難用のシェルターに、内側から閉める鍵があるものか！」"
  },
  "000295": {
    "context": "000295",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「じゃあ扉を押さえて――！」"
  },
  "000300": {
    "context": "000300",
    "code": "0x45",
    "developer_comment": "",
    "string": "夏彦がそう言いかけた時。"
  },
  "000365": {
    "context": "000365",
    "code": "0x45",
    "developer_comment": "",
    "string": "重い音を立て、扉が開いた。"
  },
  "000430": {
    "context": "000430",
    "code": "0x45",
    "developer_comment": "？？",
    "string": "「……やっぱ隠し部屋か、これ」"
  },
  "000443": {
    "context": "000443",
    "code": "0x45",
    "developer_comment": "",
    "string": "現れたのは、洵だった。"
  },
  "000449": {
    "context": "000449",
    "code": "0x45",
    "developer_comment": "",
    "string": "シェルター内を見回し、宇喜多を見て言う。"
  },
  "000460": {
    "context": "000460",
    "code": "0x45",
    "developer_comment": "洵",
    "string": "「宇喜多さん。姿が見えないと思ったら、こんなところに――って……！」"
  },
  "000471": {
    "context": "000471",
    "code": "0x45",
    "developer_comment": "洵",
    "string": "「夏彦くんに、ユウリちゃん？　それにましろちゃんまで！？」"
  },
  "000482": {
    "context": "000482",
    "code": "0x45",
    "developer_comment": "洵",
    "string": "「なんであんたたちがここに……」"
  },
  "000493": {
    "context": "000493",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「い、いやその……」"
  },
  "000498": {
    "context": "000498",
    "code": "0x45",
    "developer_comment": "",
    "string": "見据えられ、夏彦は思わず言葉を濁す。"
  },
  "000503": {
    "context": "000503",
    "code": "0x45",
    "developer_comment": "",
    "string": "洵は夏彦とましろの傷に目を止め――そして奥歯を噛んだ。"
  },
  "000514": {
    "context": "000514",
    "code": "0x45",
    "developer_comment": "洵",
    "string": "「……宇喜多さん、アンタ……」"
  },
  "000573": {
    "context": "000573",
    "code": "0x45",
    "developer_comment": "洵",
    "string": "「あたしだけならまだしも、子供たちにも手を出したのかい！！」"
  },
  "000584": {
    "context": "000584",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "「！」"
  },
  "000589": {
    "context": "000589",
    "code": "0x45",
    "developer_comment": "",
    "string": "宇喜多がぎくりと身を強張らせる。"
  },
  "000594": {
    "context": "000594",
    "code": "0x45",
    "developer_comment": "",
    "string": "夏彦は慌てて割って入った。"
  },
  "000605": {
    "context": "000605",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「ま、待って下さい洵さん！」"
  },
  "000616": {
    "context": "000616",
    "code": "0x45",
    "developer_comment": "洵",
    "string": "「なんで止めるの！？　このおっさん、あたしを襲ったんだよ！？」"
  },
  "000627": {
    "context": "000627",
    "code": "0x45",
    "developer_comment": "洵",
    "string": "「それにあんたたちだって、全身傷だらけじゃないか！　このおっさんに撃たれたんだろ！？」"
  },
  "000638": {
    "context": "000638",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「違うんです、おじさんは悪くないんです！」"
  },
  "000649": {
    "context": "000649",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「だ、だからその……やめて下さい……」"
  },
  "000694": {
    "context": "000694",
    "code": "0x45",
    "developer_comment": "",
    "string": "夏彦と悠里に言われ、洵はきょとんとした。"
  },
  "000705": {
    "context": "000705",
    "code": "0x45",
    "developer_comment": "洵",
    "string": "「どうして止めるの……？」"
  },
  "000716": {
    "context": "000716",
    "code": "0x45",
    "developer_comment": "洵",
    "string": "「おかしいよ２人とも……犯人を庇うなんて……」"
  },
  "000727": {
    "context": "000727",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「だから違うんです！　おじさんは、なんていうか、操られていただけなんですよ！」"
  },
  "000738": {
    "context": "000738",
    "code": "0x45",
    "developer_comment": "洵",
    "string": "「そんなバカな言い訳――」"
  },
  "000743": {
    "context": "000743",
    "code": "0x45",
    "developer_comment": "",
    "string": "洵はそう言いかけて、何かに気付いた表情を浮かべる。"
  },
  "000754": {
    "context": "000754",
    "code": "0x45",
    "developer_comment": "洵",
    "string": "「あぁ……そういう事だったんだ？」"
  },
  "000765": {
    "context": "000765",
    "code": "0x45",
    "developer_comment": "洵",
    "string": "「そのおっさんとあんたたちは、グルだったんだね？」"
  },
  "000776": {
    "context": "000776",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「な、なんでそんな事に！？」"
  },
  "000787": {
    "context": "000787",
    "code": "0x45",
    "developer_comment": "洵",
    "string": "「だってそうでしょ？　途中から姿を消したユウリちゃんと、探しても探しても見つからなかった夏彦くんたち」"
  },
  "000798": {
    "context": "000798",
    "code": "0x45",
    "developer_comment": "洵",
    "string": "「あんたらは宇喜多さんと組んで、みんなを殺そうとしていたんだ」"
  },
  "000809": {
    "context": "000809",
    "code": "0x45",
    "developer_comment": "洵",
    "string": "「ＡＤの数を考えれば、閉じ込められた９人全員は生き残れない。だからあんたたちは誰か３人を殺して、生き残ろうとしたんだ」"
  },
  "000820": {
    "context": "000820",
    "code": "0x45",
    "developer_comment": "洵",
    "string": "「そう考えれば、全部ツジツマがあうよね？」"
  },
  "000825": {
    "context": "000825",
    "code": "0x45",
    "developer_comment": "",
    "string": "洵の口調におかしなところはなかった。理性的ですらあった。"
  },
  "000830": {
    "context": "000830",
    "code": "0x45",
    "developer_comment": "",
    "string": "ただその語る内容のみが、完全に理性を失っている。"
  },
  "000841": {
    "context": "000841",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「じゅ、洵さん……！」"
  },
  "000847": {
    "context": "000847",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "（あの洵さんが、こんな風になるなんて……！）"
  },
  "000852": {
    "context": "000852",
    "code": "0x45",
    "developer_comment": "",
    "string": "改めて被験体Ｎの力に戦慄した。"
  },
  "000857": {
    "context": "000857",
    "code": "0x45",
    "developer_comment": "",
    "string": "洵は狂気に満ちた目で、夏彦たちを睨みつけて言う。"
  },
  "000907": {
    "context": "000907",
    "code": "0x45",
    "developer_comment": "洵",
    "string": "「堂島先輩たちを殺したのも、ＡＤが消えたのも、あんたたちのせいでしょ？」"
  },
  "000918": {
    "context": "000918",
    "code": "0x45",
    "developer_comment": "洵",
    "string": "「どうしてそんな事をしたの？　ねぇ、どうして？」"
  },
  "000929": {
    "context": "000929",
    "code": "0x45",
    "developer_comment": "洵",
    "string": "「あの人たちは、人の命を救うために生きた、本物のレスキュー隊員だったのに……」"
  },
  "000940": {
    "context": "000940",
    "code": "0x45",
    "developer_comment": "洵",
    "string": "「どうして先輩たちを殺したんだよッ！？」"
  },
  "000945": {
    "context": "000945",
    "code": "0x45",
    "developer_comment": "",
    "string": "洵はエンジンカッターを手に迫ってきた。"
  },
  "001065": {
    "context": "001065",
    "code": "0x45",
    "developer_comment": "",
    "string": "宇喜多が弾かれたように銃を向ける。"
  },
  "001076": {
    "context": "001076",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "「やめろ、それ以上近づくな！」"
  },
  "001081": {
    "context": "001081",
    "code": "0x45",
    "developer_comment": "",
    "string": "洵は足を止めて、小馬鹿にしたように鼻を鳴らした。"
  },
  "001136": {
    "context": "001136",
    "code": "0x45",
    "developer_comment": "洵",
    "string": "「へぇ～、カッコイイ事言うね？　あたしから奪った弾がその中に入ってるのかな？」"
  },
  "001156": {
    "context": "001156",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "「いや、別の銃だ。弾倉も入った、ちゃんとした銃さ」"
  },
  "001167": {
    "context": "001167",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "「この子たちに手を出すなら、撃つ」"
  },
  "001187": {
    "context": "001187",
    "code": "0x45",
    "developer_comment": "洵",
    "string": "「なるほど……ま、色々言いたいことはあるけど」"
  },
  "001198": {
    "context": "001198",
    "code": "0x45",
    "developer_comment": "洵",
    "string": "「やれるもんならやってみなよ」"
  },
  "001218": {
    "context": "001218",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "「本当に撃つぞ……！」"
  },
  "001274": {
    "context": "001274",
    "code": "0x45",
    "developer_comment": "洵",
    "string": "「あんたには撃てないよ。あんたの行動には芯がないもん」"
  },
  "001309": {
    "context": "001309",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "「何……？」"
  },
  "001329": {
    "context": "001329",
    "code": "0x45",
    "developer_comment": "洵",
    "string": "「どうせ殺すつもりなら、あたしから弾を奪う時、ついでに殺しちゃえばよかったんだ」"
  },
  "001364": {
    "context": "001364",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "「な、何を言っているんだ！？　そんな事できるか！」"
  },
  "001384": {
    "context": "001384",
    "code": "0x45",
    "developer_comment": "洵",
    "string": "「自分を心から信じてはいないから、あたしを生かしておいたんでしょ？」"
  },
  "001477": {
    "context": "001477",
    "code": "0x45",
    "developer_comment": "洵",
    "string": "「結局善も悪も中途半端な、弱い人間なんだよあんたは」"
  },
  "001512": {
    "context": "001512",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "「だ……黙れ！」"
  },
  "001517": {
    "context": "001517",
    "code": "0x45",
    "developer_comment": "",
    "string": "宇喜多がそう叫ぶと共に――"
  },
  "001574": {
    "context": "001574",
    "code": "0x45",
    "developer_comment": "",
    "string": "銃声が鳴り響いた。"
  },
  "001585": {
    "context": "001585",
    "code": "0x02",
    "string": "jump-script 328"
  }
}
