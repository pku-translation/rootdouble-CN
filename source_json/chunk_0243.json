  "000635": {
    "context": "000635",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "それに視覚が聴覚が触覚が嗅覚が味覚が――痛覚が。"
  },
  "000640": {
    "context": "000640",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "肉体が、意識が、ここにある。"
  },
  "000646": {
    "context": "000646",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "（ぼくはまだ、死んでいない……？）"
  },
  "000651": {
    "context": "000651",
    "code": "0x45",
    "developer_comment": "夏彦",
