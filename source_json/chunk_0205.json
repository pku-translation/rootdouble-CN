  "003607": {
    "context": "003607",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「あたたっ、ごめんごめんサリュ！」"
  },
  "003618": {
    "context": "003618",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「悪かった、脅かしすぎた！　冗談だよ冗談！」"
  },
  "003629": {
    "context": "003629",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「そうそう、こんなのただの噂だって！」"
  },
  "003717": {
    "context": "003717",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「……ならいい。でも今度また同じ話をしたら、本気で怒る」"
