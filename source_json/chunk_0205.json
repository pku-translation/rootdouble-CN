  "002170": {
    "context": "002170",
    "code": "0x45",
    "developer_comment": "",
    "string": "夏彦はそう前置きして、話し始めた。"
  },
  "002181": {
    "context": "002181",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「――当時ぼくらはよくここに集まって、『鹿鳴市の七不思議』の謎を解こうって盛り上がってたんだよ」"
  },
  "002210": {
    "context": "002210",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「ナナフシギ？」"
  },
  "002239": {
    "context": "002239",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「この街に伝わる、７つの不思議な噂のコト」"
  },
  "002268": {
    "context": "002268",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「例えばどんなものがあるの？」"
