  "002067": {
    "context": "002067",
    "code": "0x45",
    "developer_comment": "",
    "string": "その目には過去の映像が浮かんでいるのかもしれない。"
  },
  "002096": {
    "context": "002096",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「また、２人の昔話？」"
  },
  "002125": {
    "context": "002125",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「あ、ごめん、退屈かな？」"
  },
  "002154": {
    "context": "002154",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「そんな事はない。私も聞きたい」"
