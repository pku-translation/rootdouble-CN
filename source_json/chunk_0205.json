  "002798": {
    "context": "002798",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「うん」"
  },
  "002827": {
    "context": "002827",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「そりゃ当然だよねぇ。なにしろ脳のしくみ自体が、そもそもよくわかっていないんだもの」"
  },
  "002856": {
    "context": "002856",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「なのに独学でＢＣを学んで、間違ったやり方で訓練をしたりしてると……」"
  },
  "002885": {
    "context": "002885",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「脳におかしな負荷がかかって、それが蓄積して……」"
  },
  "002914": {
    "context": "002914",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「やがてその人は、心が壊れてしまうらしいんだよ……！」"
  },
  "002943": {
    "context": "002943",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「……っ」"
