  "002301": {
    "context": "002301",
    "code": "0x45",
    "developer_comment": "",
    "string": "――あの時も同じだった。"
  },
  "002306": {
    "context": "002306",
    "code": "0x45",
    "developer_comment": "",
    "string": "待っていろと言ったのに、悠里もついてきてしまった。"
