  "002724": {
    "context": "002724",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「ぎ……ぐ、ぅうぅううッ……！」"
  },
  "002753": {
    "context": "002753",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「大丈夫夏彦！？　しっかりして！」"
