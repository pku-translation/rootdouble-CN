  "002371": {
    "context": "002371",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「悠里、きちゃだめだよ！　火事がおきてるんだぞ！？」"
  },
  "002382": {
    "context": "002382",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「わ、わたしだって、はかせ助けたいもん……！」"
  },
  "002411": {
    "context": "002411",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「ひこりんやはかせが危ない目にあってるのに、ひとりでまってるのやだもん！」"
  },
  "002422": {
    "context": "002422",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「……！」"
  },
  "002427": {
    "context": "002427",
    "code": "0x45",
    "developer_comment": "",
    "string": "悠里の声はいつもと違っていた。"
  },
  "002432": {
    "context": "002432",
    "code": "0x45",
    "developer_comment": "",
    "string": "そんな力強い彼女の声を聞くのは初めてだった。"
  },
  "002437": {
    "context": "002437",
    "code": "0x45",
    "developer_comment": "",
    "string": "だから、９年前の夏彦は――"
  },
  "002448": {
    "context": "002448",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「……わかった……」"
  },
  "002453": {
    "context": "002453",
    "code": "0x45",
    "developer_comment": "",
    "string": "そう答えてしまったのだ。"
  },
  "002464": {
    "context": "002464",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「じゃあ、いっしょにいこう！　２人で母さんをたすけよう！」"
  },
  "002493": {
    "context": "002493",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「うん！」"
  },
  "002631": {
    "context": "002631",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "（――どうしてこんなに、何もかも符合するんだ！？）"
