  "002084": {
    "context": "002084",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「！？」"
  },
  "002122": {
    "context": "002122",
    "code": "0x45",
    "developer_comment": "",
    "string": "背後に、ましろとサリュがいる事に気付いた。"
