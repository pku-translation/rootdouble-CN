  "002173": {
    "context": "002173",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「単独行動は危険！」"
  },
  "002184": {
    "context": "002184",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「そんな事はわかってるよ！　でも――」"
  },
  "002189": {
    "context": "002189",
    "code": "0x45",
    "developer_comment": "",
    "string": "夏彦がそう言いかけた瞬間、"
  },
  "002194": {
    "context": "002194",
    "code": "0x45",
    "developer_comment": "",
    "string": "またも過去の出来事が――！"
