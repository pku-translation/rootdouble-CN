  "000744": {
    "context": "000744",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「そ、そういうものか？」"
  },
  "000755": {
    "context": "000755",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「そういうもの。あなたたちの名前も意味深い」"
