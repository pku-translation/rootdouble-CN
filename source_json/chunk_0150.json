  "000609": {
    "context": "000609",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "唐突な質問の連続に、間抜けな声が漏れる。"
  },
  "000620": {
    "context": "000620",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「さ……さぁ？　知らないけど、意味なんかあるのかな」"
  },
  "000631": {
    "context": "000631",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「夏に生まれたの？」"
  },
  "000642": {
    "context": "000642",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「あぁ、うん。６月生まれ。だからかな？」"
  },
  "000671": {
    "context": "000671",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「そう」"
  },
  "000677": {
