  "000806": {
    "context": "000806",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「気づいてないもなにも……」"
  },
  "000811": {
    "context": "000811",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "全く意味がわからない。"
  },
  "000840": {
    "context": "000840",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「意味があるといえば、『サリュ』というあだ名もそう」"
