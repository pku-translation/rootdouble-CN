  "000766": {
    "context": "000766",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「なんの話？」"
  },
  "000795": {
    "context": "000795",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「気づいてない？　まぁ偶然だろうとは思うけど」"
