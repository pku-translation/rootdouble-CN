{
  "000368": {
    "code": "0x55",
    "developer_comment": "",
    "string": "√After Chapter0"
  },
  "000372": {
    "code": "0x86",
    "developer_comment": "",
    "string": "They are gathered in the jail so that the captive is gathered, and confined by the prison."
  },
  "000373": {
    "code": "0x86",
    "developer_comment": "",
    "string": "After a lot of days pass, they are punished．"
  },
  "000374": {
    "code": "0x86",
    "developer_comment": "",
    "string": "（The Old Testament / Isaiah 24:22）"
  },
  "000376": {
    "code": "0x45",
    "developer_comment": "",
    "string": "――その轟音は、終末を報せる鐘のように――"
  },
  "000381": {
    "code": "0x45",
    "developer_comment": "",
    "string": "夜明けの研究所に響き渡った。"
  },
  "000489": {
    "code": "0x45",
    "developer_comment": "研究員１",
    "string": "「な……なんの音だ！？」"
  },
  "000497": {
    "code": "0x45",
    "developer_comment": "研究員２",
    "string": "「わかりません！？　第１エリアの方から――」"
  },
  "000505": {
    "code": "0x45",
    "developer_comment": "研究員１",
    "string": "「確認を！　早く！」"
  },
  "000513": {
    "code": "0x45",
    "developer_comment": "研究員２",
    "string": "「あ、いけません！　増幅中にゲートを開けては――！」"
  },
  "000620": {
    "code": "0x45",
    "developer_comment": "研究員１",
    "string": "「――――ッ！？」"
  },
  "000716": {
    "code": "0x45",
    "developer_comment": "自動放送",
    "string": "「――ただいま地下１階第１エリアで、激しい振動を感知しました」"
  },
  "000721": {
    "code": "0x45",
    "developer_comment": "自動放送",
    "string": "――そして轟音は、１つに止まらない。"
  },
  "000906": {
    "code": "0x45",
    "developer_comment": "自動放送",
    "string": "「――ただいま地下２階第３エリアで、激しい振動を感知しました」"
  },
  "000911": {
    "code": "0x45",
    "developer_comment": "自動放送",
    "string": "断続的に響く音と、激しい揺れ。"
  },
  "001098": {
    "code": "0x45",
    "developer_comment": "自動放送",
    "string": "「――ただいま地下１階第２エリアで、火災が発生しました」"
  },
  "001103": {
    "code": "0x45",
    "developer_comment": "自動放送",
    "string": "吹きぬける爆風、そして人々の叫び声――"
  },
  "001194": {
    "code": "0x45",
    "developer_comment": "自動放送",
    "string": "「――ただいま地下２階第２エリアで、激しい振動を感知しました」"
  },
  "001275": {
    "code": "0x86",
    "developer_comment": "自動放送",
    "string": "自動放送"
  },
  "001276": {
    "code": "0x86",
    "developer_comment": "自動放送",
    "string": "「施設内のセキュリティを、『避難モード』に移行。Ｎエリアを除く全ゲートを開放します」"
  },
  "001288": {
    "code": "0x45",
    "developer_comment": "自動放送",
    "string": "――その場にいた者たちは、まだ誰も理解していない。"
  },
  "001293": {
    "code": "0x45",
    "developer_comment": "自動放送",
    "string": "ここでなにが起きたのかを。"
  },
  "001298": {
    "code": "0x45",
    "developer_comment": "自動放送",
    "string": "戸惑いと喧騒の中、割れた声が響く。"
  },
  "001309": {
    "code": "0x45",
    "developer_comment": "アナウンス",
    "string": "「き、緊急放送です！」"
  },
  "001317": {
    "code": "0x45",
    "developer_comment": "アナウンス",
    "string": "「ただいま施設内で、原因不明の爆発が発生しました！」"
  },
  "001322": {
    "code": "0x45",
    "developer_comment": "アナウンス",
    "string": "それは、真実の断片であり――"
  },
  "001327": {
    "code": "0x45",
    "developer_comment": "アナウンス",
    "string": "破滅を告げる声だった。"
  },
  "001420": {
    "code": "0x86",
    "developer_comment": "アナウンス",
    "string": "――２０３０年　０９月　１６日"
  },
  "001499": {
    "code": "0x86",
    "developer_comment": "アナウンス",
    "string": "０６時　１９分――"
  },
  "001577": {
    "code": "0x86",
    "developer_comment": "アナウンス",
    "string": "鹿鳴市郊外の、科学研究施設"
  },
  "001654": {
    "code": "0x86",
    "developer_comment": "アナウンス",
    "string": "『原子力生物学機構・第６研究所』にて"
  },
  "001733": {
    "code": "0x86",
    "developer_comment": "アナウンス",
    "string": "――爆発事故、発生――"
  },
  "001927": {
    "code": "0x86",
    "developer_comment": "アナウンス",
    "string": "自動放送"
  },
  "001928": {
    "code": "0x86",
    "developer_comment": "アナウンス",
    "string": "「繰り返します。施設内のセキュリティを、『避難モード』に移行。Ｎエリアを除く全ゲートを開放します。職員の皆様は、落ち着いて地上へ避難して下さい」"
  },
  "001930": {
    "code": "0x45",
    "developer_comment": "アナウンス",
    "string": "自動放送が狂ったように、警告を叫ぶ。"
  },
  "001974": {
    "code": "0x45",
    "developer_comment": "アナウンス",
    "string": "それは起こりえないはずの事故だった。"
  },
  "001982": {
    "code": "0x45",
    "developer_comment": "警備員１",
    "string": "「消火システムが作動しない！？」"
  },
  "001990": {
    "code": "0x45",
    "developer_comment": "警備員２",
    "string": "「なぜだ……くそッ！　所員の避難を急がせろ！」"
  },
  "002052": {
    "code": "0x45",
    "developer_comment": "警備員２",
    "string": "誰もがわけもわからず、口々にわめき散らす。"
  },
  "002060": {
    "code": "0x45",
    "developer_comment": "研究員１",
    "string": "「非常階段が崩れ落ちただと！？」"
  },
  "002068": {
    "code": "0x45",
    "developer_comment": "研究員２",
    "string": "「エレベーターも全て停止しています！　避難の術がありません！」"
  },
  "002076": {
    "code": "0x45",
    "developer_comment": "研究員３",
    "string": "「バカな……なんでそんな事に！」"
  },
  "002084": {
    "code": "0x45",
    "developer_comment": "研究員２",
    "string": "「と、とにかく避難を……急いで！」"
  },
  "002089": {
    "code": "0x45",
    "developer_comment": "研究員２",
    "string": "だがその騒ぎを一喝するように、冷静な声が響いた。"
  },
  "002188": {
    "code": "0x45",
    "developer_comment": "女科学者",
    "string": "「みんな、落ち着きなさい！」"
  },
  "002197": {
    "code": "0x45",
    "developer_comment": "研究員１",
    "string": "「博士！？」"
  },
  "002208": {
    "code": "0x45",
    "developer_comment": "女科学者",
    "string": "「忘れたの？　有事の際には即座に、所轄の消防署に通報が届くシステムになってる事を」"
  },
  "002219": {
    "code": "0x45",
    "developer_comment": "女科学者",
    "string": "「ものの数分で、救助隊が到着するはずよ」"
  },
  "002227": {
    "code": "0x45",
    "developer_comment": "研究員２",
    "string": "「数分で！？　そんな、早すぎる！」"
  },
  "002238": {
    "code": "0x45",
    "developer_comment": "女科学者",
    "string": "「彼らは通常の消防隊員ではないもの」"
  },
  "002249": {
    "code": "0x45",
    "developer_comment": "女科学者",
    "string": "「特別高度救助隊『シリウス』――」"
  },
  "002326": {
    "code": "0x45",
    "developer_comment": "女科学者",
    "string": "「我が市が誇る、レスキュー隊の精鋭たちよ」"
  },
  "002507": {
    "code": "0x45",
    "developer_comment": "女科学者",
    "string": "――０６時　２６分"
  },
  "002512": {
    "code": "0x45",
    "developer_comment": "女科学者",
    "string": "レスキュー隊、現場に到着――"
  },
  "002527": {
    "code": "0x45",
    "developer_comment": "女性隊員１",
    "string": "「隊長！　Ａチーム全員揃いました！」"
  },
  "002589": {
    "code": "0x45",
    "developer_comment": "女性隊員１",
    "string": "防護服を着て並ぶ隊員たちの前に、同じ服を着た男が立つ。"
  },
  "002643": {
    "code": "0x45",
    "developer_comment": "隊長",
    "string": "「よし、全員そのまま聞け！」"
  },
  "002651": {
    "code": "0x45",
    "developer_comment": "隊長",
    "string": "「現在このラボの中には、無数の人が取り残されている！」"
  },
  "002659": {
    "code": "0x45",
    "developer_comment": "隊長",
    "string": "「そして、施設の性質上――これは通常の出場ではない」"
  },
  "002668": {
    "code": "0x45",
    "developer_comment": "隊長",
    "string": "「この現場ではなにが起こるかわからない。どんな不測の事態もだ」"
  },
  "002723": {
    "code": "0x45",
    "developer_comment": "女性隊員２",
    "string": "「……っ」"
  },
  "002728": {
    "code": "0x45",
    "developer_comment": "女性隊員２",
    "string": "新人隊員らしき若い女が、防護服の中で息を呑んだ。"
  },
  "002733": {
    "code": "0x45",
    "developer_comment": "女性隊員２",
    "string": "隊長は彼女を見て続ける。"
  },
  "002759": {
    "code": "0x45",
    "developer_comment": "隊長",
    "string": "「俺たちの任務は、人を救助する事――いつもそうだったように、今日もそれは変わらない」"
  },
  "002767": {
    "code": "0x45",
    "developer_comment": "隊長",
    "string": "「だがお前たちは、レスキュー隊員である前に人間だ」"
  },
  "002775": {
    "code": "0x45",
    "developer_comment": "隊長",
    "string": "「お前たちが生還すれば、それだけで１名の命を救った事になる」"
  },
  "002783": {
    "code": "0x45",
    "developer_comment": "隊長",
    "string": "「以上に留意し、救助活動に従事しろ。それでは幸運を祈る！」"
  },
  "002801": {
    "code": "0x45",
    "developer_comment": "隊員たち",
    "string": "「おう！」"
  },
  "002856": {
    "code": "0x45",
    "developer_comment": "隊員たち",
    "string": "――そして彼らは、その現場に駆け込んだ。"
  },
  "002861": {
    "code": "0x45",
    "developer_comment": "隊員たち",
    "string": "そこでなにが待ち受けているかも知らずに。"
  },
  "002866": {
    "code": "0x45",
    "developer_comment": "隊員たち",
    "string": "後に『シリウスの長い１日』と呼ばれる出場が――"
  },
  "002871": {
    "code": "0x45",
    "developer_comment": "隊員たち",
    "string": "こうして幕を開けた。"
  },
  "002959": {
    "code": "0x45",
    "developer_comment": "隊長",
    "string": "「橘は守部と！　堂島は桧山とツーマンセルだ！」"
  },
  "002967": {
    "code": "0x45",
    "developer_comment": "隊長",
    "string": "「Ｂチームとの連絡を欠かすな！　連携を常に取り続けろ！」"
  },
  "002989": {
    "code": "0x45",
    "developer_comment": "隊長",
    "string": "――彼らには信念があった。"
  },
  "002994": {
    "code": "0x45",
    "developer_comment": "隊長",
    "string": "自分に課せられた任務を、なんとしてもやり遂げる事。"
  },
  "002999": {
    "code": "0x45",
    "developer_comment": "隊長",
    "string": "だが――事態は彼らの想定を遥かに上回る。"
  },
  "003004": {
    "code": "0x45",
    "developer_comment": "隊長",
    "string": "予想外の困難が、幾度となく彼らを襲った。"
  },
  "003116": {
    "code": "0x45",
    "developer_comment": "女性隊員１",
    "string": "「守部！　地下にまだ人が――！」"
  },
  "003124": {
    "code": "0x45",
    "developer_comment": "女性隊員２",
    "string": "「くっ……い、行こう副隊長！」"
  },
  "003163": {
    "code": "0x45",
    "developer_comment": "隊員１",
    "string": "「笠鷺、すぐに地下に来てくれ！」"
  },
  "003171": {
    "code": "0x45",
    "developer_comment": "隊員２",
    "string": "「時間がありません、急いで！」"
  },
  "003179": {
    "code": "0x45",
    "developer_comment": "隊長",
    "string": "「わかった、そこで待ってろ！」"
  },
  "003306": {
    "code": "0x45",
    "developer_comment": "女性隊員１",
    "string": "「……繋がらない！？」"
  },
  "003339": {
    "code": "0x45",
    "developer_comment": "女性隊員２",
    "string": "「え！？」"
  },
  "003347": {
    "code": "0x45",
    "developer_comment": "女性隊員１",
    "string": "「おかしいです……隊長に何が……！？」"
  },
  "003561": {
    "code": "0x45",
    "developer_comment": "隊員１",
    "string": "「ここが『Ｎエリア』か……」"
  },
  "003569": {
    "code": "0x45",
    "developer_comment": "隊員２",
    "string": "「この先に、例の２人が閉じ込められてるっていうんですか？」"
  },
  "003577": {
    "code": "0x45",
    "developer_comment": "隊長",
    "string": "「そうさ……だから行こう」"
  },
  "003703": {
    "code": "0x45",
    "developer_comment": "隊長",
    "string": "「あの時、俺たちは誰も救えなかった……」"
  },
  "003717": {
    "code": "0x45",
    "developer_comment": "隊長",
    "string": "「その贖罪をする時が、ようやく来たんだよ」"
  },
  "003837": {
    "code": "0x45",
    "developer_comment": "隊長",
    "string": "――だが、その決意さえも踏みにじられる――"
  },
  "003923": {
    "code": "0x45",
    "developer_comment": "隊長",
    "string": "「バカな……誰もいないだって！？」"
  },
  "003931": {
    "code": "0x45",
    "developer_comment": "隊員１",
    "string": "「騙されたのか、俺たちは！？」"
  },
  "003939": {
    "code": "0x45",
    "developer_comment": "隊員２",
    "string": "「なぜ……誰に！？」"
  },
  "003991": {
    "code": "0x45",
    "developer_comment": "隊員２",
    "string": "「畜生……どうしてこんな事に……！」"
  },
  "003999": {
    "code": "0x45",
    "developer_comment": "隊長",
    "string": "「悔やむなお前ら、今は後悔する時じゃない」"
  },
  "004050": {
    "code": "0x45",
    "developer_comment": "隊長",
    "string": "「まだ任務は……続いているんだ」"
  },
  "004077": {
    "code": "0x86",
    "developer_comment": "隊長",
    "string": "自動放送"
  },
  "004078": {
    "code": "0x86",
    "developer_comment": "隊長",
    "string": "「ただいま、『ケースＮ』が発生しました。緊急事態につき、セキュリティを通常モードに移行。地上に続く全隔壁を閉鎖します」"
  },
  "004235": {
    "code": "0x45",
    "developer_comment": "隊長",
    "string": "「……俺がその娘を探しに行く」"
  },
  "004243": {
    "code": "0x45",
    "developer_comment": "隊員１",
    "string": "「バカな、１人でか！？」"
  },
  "004251": {
    "code": "0x45",
    "developer_comment": "隊長",
    "string": "「行くしかないだろう！　お前たちは後を頼む！」"
  },
  "004259": {
    "code": "0x45",
    "developer_comment": "隊長",
    "string": "「２０分待って戻らなかったら、俺を置いて行け！」"
  },
  "004305": {
    "code": "0x45",
    "developer_comment": "隊長",
    "string": "――後悔を飲み込み、彼は走った。"
  },
  "004310": {
    "code": "0x45",
    "developer_comment": "隊長",
    "string": "己の信念を貫くために――"
  },
  "004403": {
    "code": "0x45",
    "developer_comment": "隊長",
    "string": "「はぁっ、はぁっ……」"
  },
  "004417": {
    "code": "0x45",
    "developer_comment": "隊長",
    "string": "「ようやく見つけた……君はこのラボの関係者だな？」"
  },
  "004468": {
    "code": "0x45",
    "developer_comment": "？？",
    "string": "「あ、あなたは……？」"
  },
  "004476": {
    "code": "0x45",
    "developer_comment": "隊長",
    "string": "「レスキュー隊だ。君たちを救助に来た」"
  },
  "004535": {
    "code": "0x45",
    "developer_comment": "隊長",
    "string": "――そうして彼は、１人の少女を探し出し――"
  },
  "004592": {
    "code": "0x45",
    "developer_comment": "隊長",
    "string": "「間に合うか……？」"
  },
  "004639": {
    "code": "0x45",
    "developer_comment": "隊長",
    "string": "「まだあいつらは、いるはずだ……」"
  },
  "004755": {
    "code": "0x45",
    "developer_comment": "隊長",
    "string": "「きっと、この先に……！」"
  },
  "004828": {
    "code": "0x45",
    "developer_comment": "隊長",
    "string": "――苦痛に耐え、なお歩いた――"
  },
  "004877": {
    "code": "0x45",
    "developer_comment": "隊長",
    "string": "――そして、その任務の果てに――"
  },
  "004954": {
    "code": "0x45",
    "developer_comment": "隊長",
    "string": "「ぐッ！？」"
  },
  "005013": {
    "code": "0x45",
    "developer_comment": "隊長",
    "string": "――彼は、『怪物』に出会った。"
  },
  "005089": {
    "code": "0x45",
    "developer_comment": "隊長",
    "string": "「――なッ！？」"
  },
  "005094": {
    "code": "0x45",
    "developer_comment": "隊長",
    "string": "怪物が男の手首を掴む。"
  },
  "005102": {
    "code": "0x45",
    "developer_comment": "？？",
    "string": "「……はっ」"
  },
  "005107": {
    "code": "0x45",
    "developer_comment": "？？",
    "string": "怪物は彼を見て、笑っていた。"
  },
  "005112": {
    "code": "0x45",
    "developer_comment": "？？",
    "string": "壊れた腕時計が、視界の端に映る。"
  },
  "005117": {
    "code": "0x45",
    "developer_comment": "？？",
    "string": "言い知れない恐怖が、彼の背筋を凍りつかせた瞬間――"
  },
  "005125": {
    "code": "0x45",
    "developer_comment": "隊長",
    "string": "「ッ！！」"
  },
  "005199": {
    "code": "0x45",
    "developer_comment": "隊長",
    "string": "脳を握りつぶされるような激痛が、彼を襲った。"
  },
  "005207": {
    "code": "0x45",
    "developer_comment": "隊長",
    "string": "「がぁあああッ！！」"
  },
  "005212": {
    "code": "0x45",
    "developer_comment": "隊長",
    "string": "体験した事の無い痛みに、男は絶叫した。"
  },
  "005217": {
    "code": "0x45",
    "developer_comment": "隊長",
    "string": "痛みの正体が掴めず、男は激しく混乱する。"
  },
  "005225": {
    "code": "0x45",
    "developer_comment": "隊長",
    "string": "「い、今のは……！？」"
  },
  "005233": {
    "code": "0x45",
    "developer_comment": "隊長",
    "string": "「まさか、お前も……頭の中を！！」"
  },
  "005238": {
    "code": "0x45",
    "developer_comment": "隊長",
    "string": "鍛え上げた肉体も、その力の前ではまるで無力だった。"
  },
  "005244": {
    "code": "0x45",
    "developer_comment": "隊長",
    "string": "（ば……化物め！）"
  },
  "005249": {
    "code": "0x45",
    "developer_comment": "隊長",
    "string": "それでも彼が己の任務を貫こうと、それに挑もうとした時――"
  },
  "005326": {
    "code": "0x45",
    "developer_comment": "隊長",
    "string": "「ッ！！！」"
  },
  "005331": {
    "code": "0x45",
    "developer_comment": "隊長",
    "string": "再び頭痛が走り抜け、彼の信念は砕かれた。"
  },
  "005339": {
    "code": "0x45",
    "developer_comment": "隊長",
    "string": "「あ……あ……」"
  },
  "005344": {
    "code": "0x45",
    "developer_comment": "隊長",
    "string": "彼は頭を押さえ、後ずさる。"
  },
  "005349": {
    "code": "0x45",
    "developer_comment": "隊長",
    "string": "怪物の表情が禍々しく歪み、その視線が彼を貫いた。"
  },
  "005357": {
    "code": "0x45",
    "developer_comment": "隊長",
    "string": "「うあああああっ！」"
  },
  "005411": {
    "code": "0x45",
    "developer_comment": "隊長",
    "string": "彼はわき目も振らず逃げ出した。"
  },
  "005416": {
    "code": "0x45",
    "developer_comment": "隊長",
    "string": "逃げなければ殺される。自分が消滅してしまう。"
  },
  "005421": {
    "code": "0x45",
    "developer_comment": "隊長",
    "string": "痛む頭を抱え、彼は必死で逃げた。"
  },
  "005470": {
    "code": "0x45",
    "developer_comment": "隊長",
    "string": "（逃げるんだ……少しでも遠くへ！）"
  },
  "005525": {
    "code": "0x45",
    "developer_comment": "隊長",
    "string": "怪物の力の及ばないところまで。"
  },
  "005574": {
    "code": "0x45",
    "developer_comment": "隊長",
    "string": "だが、それも虚しく――"
  },
  "005626": {
    "code": "0x45",
    "developer_comment": "隊長",
    "string": "弾けるような痛みと共に、目の前に閃光が瞬いた。"
  },
  "005634": {
    "code": "0x45",
    "developer_comment": "隊長",
    "string": "「が……はッ……！」"
  },
  "005689": {
    "code": "0x45",
    "developer_comment": "隊長",
    "string": "――それが彼の最後の知覚だった。"
  },
  "005694": {
    "code": "0x45",
    "developer_comment": "隊長",
    "string": "パチンとスイッチを落とすように、"
  },
  "005699": {
    "code": "0x45",
    "developer_comment": "隊長",
    "string": "彼の意識は闇に消えた。"
  },
  "005704": {
    "code": "0x45",
    "developer_comment": "隊長",
    "string": "――――――"
  },
  "005709": {
    "code": "0x45",
    "developer_comment": "隊長",
    "string": "――――"
  },
  "005714": {
    "code": "0x45",
    "developer_comment": "隊長",
    "string": "――"
  },
  "005833": {
    "code": "0x45",
    "developer_comment": "隊長",
    "string": "――やがて、静止した時の彼方で――"
  },
  "005838": {
    "code": "0x45",
    "developer_comment": "隊長",
    "string": "目覚めよ、と呼ぶ声が聞こえた。"
  },
  "005843": {
    "code": "0x45",
    "developer_comment": "隊長",
    "string": "あるいはそんな記憶があった。"
  },
  "005848": {
    "code": "0x45",
    "developer_comment": "隊長",
    "string": "白く塗られた墓の下で、彼はその声を聞き……"
  },
  "005853": {
    "code": "0x45",
    "developer_comment": "隊長",
    "string": "そして時が、動き出した。"
  }
}