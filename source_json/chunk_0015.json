{
  "000046": {
    "context": "000046",
    "code": "0x55",
    "developer_comment": "",
    "string": "√After Chapter1"
  },
  "000060": {
    "context": "000060",
    "code": "0x45",
    "developer_comment": "",
    "string": "渡瀬たちが内周棟に戻ると、恵那たちが迎え入れてくれた。"
  },
  "000125": {
    "context": "000125",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "「な、何かあったの？　爆発音みたいのが聞こえたけど」"
  },
  "000136": {
    "context": "000136",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「いや、バックドラフトってのを起こしちまってな……」"
  },
  "000141": {
    "context": "000141",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "――渡瀬は外周棟の状況を、恵那たちに報告した。"
  },
  "000171": {
    "context": "000171",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "「外周棟で、それほどの火災が……！？」"
  },
  "000182": {
    "context": "000182",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「どうした宇喜多さん？　そんなに驚いて」"
  },
  "000211": {
    "context": "000211",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "「いや……想像していた以上に、火災が広がっていると思ってね」"
  },
  "000242": {
    "context": "000242",
    "code": "0x45",
    "developer_comment": "洵",
    "string": "「だね。こんなに広がるとわかっていたら、隊長も宇喜多さんを置きざりにしていかなかっただろうし」"
  },
  "000253": {
    "context": "000253",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「オレもひどい事をしたもんだ……すまないな、宇喜多さん」"
  },
  "000282": {
    "context": "000282",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "「それはもういい。それより、捜索を急いだほうがいいんじゃないかね？」"
  },
  "000311": {
    "context": "000311",
    "code": "0x45",
    "developer_comment": "洵",
    "string": "「じゃあ次は地下１階を……って、あれ？」"
  },
  "000316": {
    "context": "000316",
    "code": "0x45",
    "developer_comment": "洵",
    "string": "そう言いかけて、洵が不意に怪訝そうな顔をした。"
  },
  "000345": {
    "context": "000345",
    "code": "0x45",
    "developer_comment": "洵",
    "string": "「椿山さん、宇喜多さん……ユウリちゃんは？」"
  },
  "000374": {
    "context": "000374",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "「――え！？」"
  },
  "000413": {
    "context": "000413",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "全員が、はっとしてあたりを見回した。"
  },
  "000418": {
    "context": "000418",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "だがそこにはユウリの姿がない。"
  },
  "000481": {
    "context": "000481",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「ど、どこにいったんだユウリは！？」"
  },
  "000510": {
    "context": "000510",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "「わからないわ！　さっきまで一緒にいたのに！」"
  },
  "000539": {
    "context": "000539",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "「ついさっきまでは、目を離さずにいたんだ！　本当だ！」"
  },
  "000568": {
    "context": "000568",
    "code": "0x45",
    "developer_comment": "洵",
    "string": "「逃げたの！？　どうして！？」"
  },
  "000597": {
    "context": "000597",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「わかりませんが、まずいですよこれは……ここは１人で出歩くには危険すぎます！」"
  },
  "000688": {
    "context": "000688",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "「……っ！」"
  },
  "000693": {
    "context": "000693",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "風見の言葉に、恵那が鋭く息を呑む。"
  },
  "000698": {
    "context": "000698",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "その表情が、自分の失態を責めているように渡瀬には見えた。"
  },
  "000709": {
    "context": "000709",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「先生、すぐ探しに行くぞ！」"
  },
  "000738": {
    "context": "000738",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "「そ、そうね……まだ遠くへは行ってないはずだわ！」"
  },
  "000780": {
    "context": "000780",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "そう言って視線を交わすと、２人は同時に駆け出した。"
  },
  "000834": {
    "context": "000834",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "すぐに後ろから、他のメンバーが動き出す声も聞こえて来る。"
  },
  "000845": {
    "context": "000845",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「守部、宇喜多さん、我々も探しに行きますよ！」"
  },
  "000856": {
    "context": "000856",
    "code": "0x45",
    "developer_comment": "洵",
    "string": "「了解！」"
  },
  "000867": {
    "context": "000867",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "「ああ、急ごう！」"
  },
  "000950": {
    "context": "000950",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "走りながら、渡瀬は恵那に尋ねる。"
  },
  "000961": {
    "context": "000961",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「先生、ユウリはどこに行ったと思う！？」"
  },
  "000990": {
    "context": "000990",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "「わ、わからないわ！　だけど――セキュリティゲートが開いた音とかは聞こえなかったと思う……」"
  },
  "000995": {
    "context": "000995",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "そう言われて、渡瀬は気付いた。"
  },
  "001000": {
    "context": "001000",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "セキュリティゲートも各研究室の扉も、開閉する時は大きな音が鳴る事に。"
  },
  "001006": {
    "context": "001006",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "（――このエリアの外にいくには、セキュリティゲートを通る必要がある）"
  },
  "001012": {
    "context": "001012",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "（そしてユウリは恐らくカードを持っていない。このエリアの外には行っていないだろう）"
  },
  "001023": {
    "context": "001023",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「先生、その辺にある小部屋の扉が開いた音は聞いたか？」"
  },
  "001052": {
    "context": "001052",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "「それも聞いてないわ！」"
  },
  "001063": {
    "context": "001063",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「という事は――」"
  },
  "001068": {
    "context": "001068",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "答えはもう１つしかなかった。"
  },
  "001073": {
    "context": "001073",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "恵那がはっと頭上を見上げる。"
  },
  "001103": {
    "context": "001103",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "「――きっと上の階に向かったんだわ！」"
  },
  "001108": {
    "context": "001108",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "階段室の扉は、機械仕掛けのスライド扉ではなく、通常の防火扉だ。"
  },
  "001113": {
    "context": "001113",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "音を立てないように注意すれば、皆に気付かれずに外に出られるだろう。"
  },
  "001124": {
    "context": "001124",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「行こう先生！」"
  },
  "001153": {
    "context": "001153",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "「ええ！」"
  },
  "001158": {
    "context": "001158",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "２人は頷き合い、非常階段へと向かった。"
  },
  "001245": {
    "context": "001245",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "そして階段室に入ると――"
  },
  "001256": {
    "context": "001256",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「いた！」"
  },
  "001261": {
    "context": "001261",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "薄闇の中に、ユウリの姿があった。"
  },
  "001401": {
    "context": "001401",
    "code": "0x45",
    "developer_comment": "ユウリ",
    "string": "「ッ！」"
  },
  "001406": {
    "context": "001406",
    "code": "0x45",
    "developer_comment": "ユウリ",
    "string": "ユウリがびくっと身をすくませる。"
  },
  "001417": {
    "context": "001417",
    "code": "0x45",
    "developer_comment": "ユウリ",
    "string": "渡瀬たちは階段を駆け上がり、ユウリの腕を掴んだ。"
  },
  "001533": {
    "context": "001533",
    "code": "0x45",
    "developer_comment": "ユウリ",
    "string": "「あ……」"
  },
  "001544": {
    "context": "001544",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「ユウリ、勝手にいなくなるなよ……心配したぞ」"
  },
  "001549": {
    "context": "001549",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "渡瀬はユウリの肩に優しく触れる。"
  },
  "001554": {
    "context": "001554",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "彼女は叱られた犬のように、しゅんとしていた。"
  },
  "001640": {
    "context": "001640",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "「ユウリさん、どうして１人で逃げたりしたの？」"
  },
  "001669": {
    "context": "001669",
    "code": "0x45",
    "developer_comment": "ユウリ",
    "string": "「あ、いえ……」"
  },
  "001680": {
    "context": "001680",
    "code": "0x45",
    "developer_comment": "ユウリ",
    "string": "「あの……わたし、怖くって……」"
  },
  "001691": {
    "context": "001691",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「怖い？」"
  },
  "001720": {
    "context": "001720",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "「怖いって……何が？」"
  },
  "001749": {
    "context": "001749",
    "code": "0x45",
    "developer_comment": "ユウリ",
    "string": "「そ、それは……」"
  },
  "001760": {
    "context": "001760",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「ユウリ、別に怒ったりしないから、先生の質問に答えるんだ」"
  },
  "001765": {
    "context": "001765",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "渡瀬は掴んでいた腕を離し、優しく尋ねた。"
  },
  "001770": {
    "context": "001770",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "するとユウリが観念したように、おずおずと口を動かす。"
  },
  "001799": {
    "context": "001799",
    "code": "0x45",
    "developer_comment": "ユウリ",
    "string": "「そ、その……さっきの爆発音が……」"
  },
  "001810": {
    "context": "001810",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "「爆発音？　それじゃあユウリさんはその音に驚いて、ここまで逃げてきたって言うの？」"
  },
  "001821": {
    "context": "001821",
    "code": "0x45",
    "developer_comment": "ユウリ",
    "string": "「は、はい……」"
  },
  "001844": {
    "context": "001844",
    "code": "0x45",
    "developer_comment": "ユウリ",
    "string": "渡瀬は恵那と顔を見合わせた。"
  },
  "001855": {
    "context": "001855",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「よほど怖がりなんだな……」"
  },
  "001866": {
    "context": "001866",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "「……えぇ」"
  },
  "001910": {
    "context": "001910",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "渡瀬は苦笑しつつ、階段室の外に向けて声を飛ばした。"
  },
  "001921": {
    "context": "001921",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「おーいみんな！　ユウリがいたぞー！」"
  },
  "001938": {
    "context": "001938",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "――するとやがて、風見たちが駆け寄ってくる音が聞こえた。"
  },
  "001943": {
    "context": "001943",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "到着するなり、風見が鋭く言う。"
  },
  "001989": {
    "context": "001989",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「ダメでしょうユウリさん！　１人で行動しては！」"
  },
  "002053": {
    "context": "002053",
    "code": "0x45",
    "developer_comment": "ユウリ",
    "string": "「！！」"
  },
  "002064": {
    "context": "002064",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「危険だって言ってるじゃないですか！？　あんまり勝手なことすると、私も怒りますよ！？」"
  },
  "002093": {
    "context": "002093",
    "code": "0x45",
    "developer_comment": "ユウリ",
    "string": "「あ、あの……ごめ……」"
  },
  "002098": {
    "context": "002098",
    "code": "0x45",
    "developer_comment": "ユウリ",
    "string": "風見に強く叱られ、ユウリが身と声を震わせる。"
  },
  "002103": {
    "context": "002103",
    "code": "0x45",
    "developer_comment": "ユウリ",
    "string": "渡瀬は慌てて口を挟んだ。"
  },
  "002114": {
    "context": "002114",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「ちょ、ちょっと待ってくれ橘！　あまり叱らないでやってくれるか？」"
  },
  "002143": {
    "context": "002143",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「なんですって？」"
  },
  "002154": {
    "context": "002154",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「ユウリはオレの起こしたバックドラフトの爆炎に驚いて、思わず逃げてしまったらしいんだ」"
  },
  "002165": {
    "context": "002165",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「つまり言ってみりゃ、オレのせいなんだよ。だからその……怒鳴るのもほどほどに……」"
  },
  "002170": {
    "context": "002170",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "さっき自分も叱られた手前、渡瀬がおずおずと言うと――風見は困ったような表情を浮かべた。"
  },
  "002199": {
    "context": "002199",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「そうですか……しかし１人で行動したら、もっと危険ですよ」"
  },
  "002230": {
    "context": "002230",
    "code": "0x45",
    "developer_comment": "洵",
    "string": "「そうだよユウリちゃん。あたしらと一緒にいるのが、一番安全だよ」"
  },
  "002259": {
    "context": "002259",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "「僕も素人だがね、プロの言う事は聞いておいた方がいいと思う」"
  },
  "002288": {
    "context": "002288",
    "code": "0x45",
    "developer_comment": "ユウリ",
    "string": "「す、すみません……」"
  },
  "002293": {
    "context": "002293",
    "code": "0x45",
    "developer_comment": "ユウリ",
    "string": "大人たちに口々に叱られ、すっかりユウリは萎縮してしまったようだ。"
  },
  "002304": {
    "context": "002304",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「まぁまぁ……どのみち地下１階に上がるところだったんだし、手間が省けたじゃないか」"
  },
  "002333": {
    "context": "002333",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「隊長は甘いです！　こういう事が続くと、助かる命も助けられません！」"
  },
  "002344": {
    "context": "002344",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「それはそうだが、でももうわかったよな、ユウリも？」"
  },
  "002355": {
    "context": "002355",
    "code": "0x45",
    "developer_comment": "ユウリ",
    "string": "「……はい」"
  },
  "002360": {
    "context": "002360",
    "code": "0x45",
    "developer_comment": "ユウリ",
    "string": "ユウリは消え入りそうな声で言った。"
  },
  "002365": {
    "context": "002365",
    "code": "0x45",
    "developer_comment": "ユウリ",
    "string": "さすがの風見も、多少バツが悪そうな表情を浮かべる。"
  },
  "002394": {
    "context": "002394",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「……わかればいいのです。それでは行きましょうか」"
  },
  "002437": {
    "context": "002437",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "風見はユウリの手を引き、階段を上がっていった。"
  },
  "002442": {
    "context": "002442",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "洵と宇喜多がそれに続く。"
  },
  "002447": {
    "context": "002447",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "そして渡瀬たちも、階段を上り始めた時――"
  },
  "002512": {
    "context": "002512",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "「……隊長さん」"
  },
  "002517": {
    "context": "002517",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "恵那が渡瀬の手を取って、囁いた。"
  },
  "002528": {
    "context": "002528",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「ん、どうした？」"
  },
  "002539": {
    "context": "002539",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "「ユウリさんだけど……さっき追いかけてきたあなたを見て、びくっとしてなかった？」"
  },
  "002550": {
    "context": "002550",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「え？　あぁ、確かに……」"
  },
  "002555": {
    "context": "002555",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "恵那に言われて、渡瀬はふと思いだした。"
  },
  "002560": {
    "context": "002560",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "先ほどのユウリの反応と、そしてそれまでの彼女の態度を。"
  },
  "002571": {
    "context": "002571",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「……っていうか実はさ、最初に会った時から、なんか嫌われてるみたいなんだよ」"
  },
  "002582": {
    "context": "002582",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "「嫌われてる？　どうして？」"
  },
  "002593": {
    "context": "002593",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「いや、オレにもわからん……なんでなんだろう」"
  },
  "002598": {
    "context": "002598",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "渡瀬は自分に何か問題があるのかと思い始めた。"
  },
  "002603": {
    "context": "002603",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "恵那の顔を見て尋ねる。"
  },
  "002614": {
    "context": "002614",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「なぁ先生、オレの顔って怖いかな？」"
  },
  "002643": {
    "context": "002643",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "「怖いって……別に？　まぁフツーの顔だわよ。若干マヌケな感じの」"
  },
  "002654": {
    "context": "002654",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「ぐっ……い、言うねェ」"
  },
  "002659": {
    "context": "002659",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "恵那の毒舌に心を痛めながら、渡瀬は問いを重ねる。"
  },
  "002670": {
    "context": "002670",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「しかし、どうやら顔が問題ってわけじゃないみたいだな。他に原因があるのかな？」"
  },
  "002681": {
    "context": "002681",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "「なんでしょうね。思い当たる事は？」"
  },
  "002692": {
    "context": "002692",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「うーん、なにもないんだが……」"
  },
  "002703": {
    "context": "002703",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「そういや先生は教師だったな。生徒にわけもなく嫌われる事ってあるか？」"
  },
  "002726": {
    "context": "002726",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "渡瀬が尋ねると、恵那はきょとんとした。"
  },
  "002737": {
    "context": "002737",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "「そんな事あるわけないじゃない。私生徒に愛されてるし」"
  },
  "002748": {
    "context": "002748",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「……あ、そう。じゃあその秘訣は？」"
  },
  "002777": {
    "context": "002777",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "「そうね。まぁ強いて言うなら、子供たちに惜しみなく愛を注ぐ事かしら」"
  },
  "002788": {
    "context": "002788",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「愛ぃ？」"
  },
  "002799": {
    "context": "002799",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "「そうよ。愛」"
  },
  "002828": {
    "context": "002828",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "「常に子供たちの事を慮り、目を配る……手間を惜しまず、労をいとわずね」"
  },
  "002839": {
    "context": "002839",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "「多少押し付けがましくても、相手の事をちゃんと心から考えていれば、きっと気持ちも伝わるはずだから」"
  },
  "002850": {
    "context": "002850",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「ふーん、そういうものかね」"
  },
  "002855": {
    "context": "002855",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "普段の恵那と生徒たちの関係を知らないからなんとも言えないが、頭には留めておこうと思った。"
  },
  "002860": {
    "context": "002860",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "同時に、恵那に反撃するネタがある事にも気付き、渡瀬はそれを口にする。"
  },
  "002871": {
    "context": "002871",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「……でも先生よ。『常に子供に目を配ってる』割には、ユウリが逃げたことには気付かなかったみたいだな？」"
  },
  "002900": {
    "context": "002900",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "「痛いところつくわね。だからちょっと落ち込んでたのよ」"
  },
  "002911": {
    "context": "002911",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「まぁこれからはお互い、ユウリには常に目を配っておこうぜ」"
  },
  "002940": {
    "context": "002940",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "「ふん、あなたに言われなくてもわかってるわよ」"
  },
  "002945": {
    "context": "002945",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "恵那がそう言った時、渡瀬たちは階段を上りきり――"
  },
  "002950": {
    "context": "002950",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "地下１階フロアに着いた。"
  },
  "002964": {
    "context": "002964",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「探そうみんな！」"
  },
  "003040": {
    "context": "003040",
    "code": "0x45",
    "developer_comment": "全員",
    "string": "「ああ！」"
  },
  "003045": {
    "context": "003045",
    "code": "0x45",
    "developer_comment": "全員",
    "string": "そう言って５人は、散り散りにユウリの捜索を始めた。"
  },
  "003093": {
    "context": "003093",
    "code": "0x45",
    "developer_comment": "全員",
    "string": "渡瀬はひとまず、第２エリア方面に走った。"
  },
  "003141": {
    "context": "003141",
    "code": "0x45",
    "developer_comment": "全員",
    "string": "だがそこにはユウリの姿はない。"
  },
  "003152": {
    "context": "003152",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「くっ、こっちじゃないか！」"
  },
  "003200": {
    "context": "003200",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "ひとまず引き返し、第３エリアに戻る。"
  },
  "003278": {
    "context": "003278",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "第３エリアには、風見と恵那の姿があった。"
  },
  "003289": {
    "context": "003289",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「いたか！？」"
  },
  "003369": {
    "context": "003369",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "「見つからないわ！」"
  },
  "003380": {
    "context": "003380",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「彼女はどこへ……！？」"
  },
  "003385": {
    "context": "003385",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "渡瀬がユウリの逃げた先について考えた時、洵の声が聞こえた。"
  },
  "003429": {
    "context": "003429",
    "code": "0x45",
    "developer_comment": "洵",
    "string": "「いたー！　いたよみんな！」"
  },
  "003434": {
    "context": "003434",
    "code": "0x45",
    "developer_comment": "洵",
    "string": "声は非常階段の方から聞こえてきていた。"
  },
  "003445": {
    "context": "003445",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「上の階だ！」"
  },
  "003450": {
    "context": "003450",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "渡瀬はそう言って、非常階段に向けて走り出した。"
  },
  "003455": {
    "context": "003455",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "それに風見と恵那が続く。"
  },
  "003509": {
    "context": "003509",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "そして階段室に入り、階段を駆け上がると――"
  },
  "003520": {
    "context": "003520",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「いた！」"
  },
  "003525": {
    "context": "003525",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "そこにユウリの姿があった。"
  },
  "003592": {
    "context": "003592",
    "code": "0x45",
    "developer_comment": "ユウリ",
    "string": "「あ……」"
  },
  "003597": {
    "context": "003597",
    "code": "0x45",
    "developer_comment": "ユウリ",
    "string": "ユウリは暗がりに佇み、怯えたように渡瀬を見ていた。"
  },
  "003704": {
    "context": "003704",
    "code": "0x45",
    "developer_comment": "ユウリ",
    "string": "傍らには洵と宇喜多の姿がある。"
  },
  "003709": {
    "context": "003709",
    "code": "0x45",
    "developer_comment": "ユウリ",
    "string": "その時、風見が――鋭く叫んだ。"
  },
  "003839": {
    "context": "003839",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「ダメでしょうユウリさん！　１人で行動しては！」"
  },
  "003903": {
    "context": "003903",
    "code": "0x45",
    "developer_comment": "ユウリ",
    "string": "「！！」"
  },
  "003914": {
    "context": "003914",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「危険だって言ってるじゃないですか！？　あんまり勝手なことすると、私も怒りますよ！？」"
  },
  "003943": {
    "context": "003943",
    "code": "0x45",
    "developer_comment": "ユウリ",
    "string": "「あ、あの……ごめ……」"
  },
  "003948": {
    "context": "003948",
    "code": "0x45",
    "developer_comment": "ユウリ",
    "string": "風見に強く叱られ、ユウリが身と声を震わせる。"
  },
  "003959": {
    "context": "003959",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「ちょ、ちょっと落ち着け橘！」"
  },
  "003990": {
    "context": "003990",
    "code": "0x45",
    "developer_comment": "洵",
    "string": "「いや、副隊長が正しいよ」"
  },
  "004001": {
    "context": "004001",
    "code": "0x45",
    "developer_comment": "洵",
    "string": "「ユウリちゃん、どうして椿山さんたちのもとを離れたの？」"
  },
  "004030": {
    "context": "004030",
    "code": "0x45",
    "developer_comment": "ユウリ",
    "string": "「えっ、と……」"
  },
  "004035": {
    "context": "004035",
    "code": "0x45",
    "developer_comment": "ユウリ",
    "string": "ユウリはしばらく、答えに窮していたようだが、やがて観念したように答えた。"
  },
  "004064": {
    "context": "004064",
    "code": "0x45",
    "developer_comment": "ユウリ",
    "string": "「爆発の音が聞こえて……それで怖くなっちゃって……」"
  },
  "004093": {
    "context": "004093",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "「わかるけど……１人で行動したら、もっと危ないわよ」"
  },
  "004122": {
    "context": "004122",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "「みんなと一緒にいるより、安全だとでも思ったのかね？」"
  },
  "004151": {
    "context": "004151",
    "code": "0x45",
    "developer_comment": "ユウリ",
    "string": "「す、すみません……」"
  },
  "004156": {
    "context": "004156",
    "code": "0x45",
    "developer_comment": "ユウリ",
    "string": "大人たちに口々に叱られ、すっかりユウリは萎縮してしまったようだ。"
  },
  "004167": {
    "context": "004167",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「まぁまぁ……どのみち地下１階に上がるところだったんだし、手間が省けたじゃないか」"
  },
  "004196": {
    "context": "004196",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「隊長は甘いです！　こういう事が続くと、助かる命も助けられません！」"
  },
  "004207": {
    "context": "004207",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「それはそうだが、でももうわかったよな、ユウリも？」"
  },
  "004218": {
    "context": "004218",
    "code": "0x45",
    "developer_comment": "ユウリ",
    "string": "「……はい」"
  },
  "004223": {
    "context": "004223",
    "code": "0x45",
    "developer_comment": "ユウリ",
    "string": "肩を落とすユウリに苦笑しつつ、渡瀬たちは階段室を出て――"
  },
  "004228": {
    "context": "004228",
    "code": "0x45",
    "developer_comment": "ユウリ",
    "string": "地下１階に上がった。"
  }
}