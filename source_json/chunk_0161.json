{
  "000077": {
    "context": "000077",
    "code": "0x55",
    "string": "√Before Chapter3"
  },
  "000085": {
    "context": "000085",
    "code": "0x45",
    "developer_comment": "",
    "string": "――買い物を終えた夏彦たちは、バスに乗って帰宅した。"
  },
  "000090": {
    "context": "000090",
    "code": "0x45",
    "developer_comment": "",
    "string": "そして連れ立って、玄関を潜る。"
  },
  "000306": {
    "context": "000306",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「あ、おかえりなさ～い」"
  },
  "000311": {
    "context": "000311",
    "code": "0x45",
    "developer_comment": "",
    "string": "家に戻ると、悠里が夏彦たちを迎え入れてくれた。"
  },
  "000322": {
    "context": "000322",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「ただいま、悠里」"
  },
  "000351": {
    "context": "000351",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「ただいまーちゃん」"
  },
  "000380": {
    "context": "000380",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「……ただいま」"
  },
  "000385": {
    "context": "000385",
    "code": "0x45",
    "developer_comment": "",
    "string": "サリュもおずおずと答えを返す。"
  },
  "000408": {
    "context": "000408",
    "code": "0x45",
    "developer_comment": "",
    "string": "ましろはほっとしたように笑い、そして声を上げた。"
  },
  "000437": {
    "context": "000437",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「よーし、それじゃご飯作っちゃうね。今日は豪勢なメニューになるよ～？」"
  },
  "000489": {
    "context": "000489",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「あ、手伝おうか？」"
  },
  "000518": {
    "context": "000518",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「いーのいーの、私１人で充分。っていうか今日は、私も本気を出すから……」"
  },
  "000547": {
    "context": "000547",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「キッチンは戦場になる。素人は足手まといだよ」"
  },
  "000558": {
    "context": "000558",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「……！」"
  },
  "000569": {
    "context": "000569",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「わ、わかった、任せよう……」"
  },
  "000598": {
    "context": "000598",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「ふふん、まぁ座って待っててよ」"
  },
  "000631": {
    "context": "000631",
    "code": "0x45",
    "developer_comment": "",
    "string": "ましろはそう言って、キッチンに行った。"
  },
  "000706": {
    "context": "000706",
    "code": "0x45",
    "developer_comment": "",
    "string": "そしておとなしく待つこと、１時間――"
  },
  "000711": {
    "context": "000711",
    "code": "0x45",
    "developer_comment": "",
    "string": "やがてましろが、料理をテーブルにずらりと並べて言った。"
  },
  "000802": {
    "context": "000802",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「はいっ、お待たせ！」"
  },
  "000820": {
    "context": "000820",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「フランスパンと牛レバーのパテ、海老のココットにニース風サラダ、そしてビシソワーズ・スープだよ」"
  },
  "000831": {
    "context": "000831",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「メインディッシュは、シャリアピン風ビフテキ！」"
  },
  "000842": {
    "context": "000842",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「お、おおお～！！」"
  },
  "000847": {
    "context": "000847",
    "code": "0x45",
    "developer_comment": "",
    "string": "夏彦は思わず歓声を上げた。"
  },
  "000852": {
    "context": "000852",
    "code": "0x45",
    "developer_comment": "",
    "string": "５つの皿に、それぞれ見た目も麗しい料理が並んでいる。"
  },
  "000973": {
    "context": "000973",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「……素晴しい」"
  },
  "000984": {
    "context": "000984",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「すごいよましろん、たった小一時間でこれを？」"
  },
  "000995": {
    "context": "000995",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「っていうかすごすぎるだろ。何者なんだお前は……」"
  },
  "001024": {
    "context": "001024",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「日頃の修練と、高圧電磁調理器のたまものだね。まぁ話はあとあと、まずは食べてみてよ！」"
  },
  "001053": {
    "context": "001053",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「了解。夏彦、早く」"
  },
  "001064": {
    "context": "001064",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「あ、あぁ」"
  },
  "001334": {
    "context": "001334",
    "code": "0x45",
    "developer_comment": "",
    "string": "ましろとサリュに促され、夏彦たちは食卓に着く。"
  },
  "001395": {
    "context": "001395",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「それじゃサリュとの出会いを祝って！　いただきまーす！」"
  },
  "001421": {
    "context": "001421",
    "code": "0x45",
    "developer_comment": "３人",
    "string": "「いただきまーす！」"
  },
  "001461": {
    "context": "001461",
    "code": "0x45",
    "developer_comment": "",
    "string": "夏彦たちも元気に言い、そして料理に取り掛かった。"
  },
  "001631": {
    "context": "001631",
    "code": "0x45",
    "developer_comment": "",
    "string": "まずはフランスパンとレバーのパテを、一口食べると――"
  },
  "001642": {
    "context": "001642",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「……うまっ！」"
  },
  "001647": {
    "context": "001647",
    "code": "0x45",
    "developer_comment": "",
    "string": "思わず感嘆の声が漏れた。"
  },
  "001652": {
    "context": "001652",
    "code": "0x45",
    "developer_comment": "",
    "string": "パテの深い味わいが、香ばしいパンの香りと絡み合っている。"
  },
  "001657": {
    "context": "001657",
    "code": "0x45",
    "developer_comment": "",
    "string": "レバー特有の臭みは、全く感じられなかった。"
  },
  "001829": {
    "context": "001829",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「ん～……このココットもおいひ～！　お口の中でほろけるよぅ」"
  },
  "001876": {
    "context": "001876",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「む、サラダも美味……」"
  },
  "001905": {
    "context": "001905",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「ふふふ、お気に召したようですな」"
  },
  "001934": {
    "context": "001934",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「でも特に自信があるのは、やっぱりこのメインデッシュだよ」"
  },
  "002104": {
    "context": "002104",
    "code": "0x45",
    "developer_comment": "",
    "string": "ましろはそう言って、件のビフテキを指差す。"
  },
  "002116": {
    "context": "002116",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「これはとあるオペラ歌手が考案した、お肉をもっとも美味しく食べる調理法らしいよ」"
  },
  "002127": {
    "context": "002127",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「うまいゾっ！　ぜひお試しあれ！」"
  },
  "002138": {
    "context": "002138",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「どれどれ……」"
  },
  "002143": {
    "context": "002143",
    "code": "0x45",
    "developer_comment": "",
    "string": "夏彦たちはそのビフテキを口に運び――"
  },
  "002154": {
    "context": "002154",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「！」"
  },
  "002314": {
    "context": "002314",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「！！」"
  },
  "002400": {
    "context": "002400",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「！！！」"
  },
  "002406": {
    "context": "002406",
    "code": "0x45",
    "developer_comment": "",
    "string": "かっと目を見開いた。"
  },
  "002441": {
    "context": "002441",
    "code": "0x45",
    "developer_comment": "",
    "string": "１口食べた瞬間、芳醇な肉のうまみを感じた。"
  },
  "002446": {
    "context": "002446",
    "code": "0x45",
    "developer_comment": "",
    "string": "見た目は牛を焼いて、刻みタマネギを乗せただけの、ごくシンプルな料理だ。"
  },
  "002451": {
    "context": "002451",
    "code": "0x45",
    "developer_comment": "",
    "string": "だがその肉が、信じがたいほど柔らかいのだ。ナイフがなんの抵抗もなく入る。"
  },
  "002456": {
    "context": "002456",
    "code": "0x45",
    "developer_comment": "",
    "string": "なのに肉の味わいは全く損なわれていない。"
  },
  "002461": {
    "context": "002461",
    "code": "0x45",
    "developer_comment": "",
    "string": "一噛みするごとに肉汁が溢れ、うまみが口中に広がった。"
  },
  "002659": {
    "context": "002659",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「……ましろん。おいしすぎて泣きそうだよぅ」"
  },
  "002670": {
    "context": "002670",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「あぁ、こりゃ今までで一番かもしれない」"
  },
  "002717": {
    "context": "002717",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「おほほ、マエストロとお呼び」"
  },
  "002722": {
    "context": "002722",
    "code": "0x45",
    "developer_comment": "",
    "string": "ましろが胸を張って言う。"
  },
  "002727": {
    "context": "002727",
    "code": "0x45",
    "developer_comment": "",
    "string": "サリュは無言で、そして瞬く間にそれを平らげた。"
  },
  "002738": {
    "context": "002738",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「あれぇ、サリュちゃんもう食べ終わったの？」"
  },
  "002767": {
    "context": "002767",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「見た目によらず、すごい食欲だね」"
  },
  "002772": {
    "context": "002772",
    "code": "0x45",
    "developer_comment": "",
    "string": "サリュはおずおずと、ましろに言う。"
  },
  "002801": {
    "context": "002801",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「……あの、ましろ」"
  },
  "002830": {
    "context": "002830",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「ん、なぁに？」"
  },
  "002859": {
    "context": "002859",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「これ、おかわりは……あるの？」"
  },
  "002870": {
    "context": "002870",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「た、食べるなぁサリュ！　１皿でも結構ボリュームあるのに」"
  },
  "002899": {
    "context": "002899",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「夏彦がおかわりするかもと思って、もう１枚お肉は用意しといたけど……」"
  },
  "002928": {
    "context": "002928",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「……！」"
  },
  "002951": {
    "context": "002951",
    "code": "0x45",
    "developer_comment": "",
    "string": "サリュがちらりと夏彦を見る。その目には懇願の色が浮かんでいた。"
  },
  "002962": {
    "context": "002962",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「――ましろ、その肉をサリュにあげてくれ」"
  },
  "003007": {
    "context": "003007",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「いいの？」"
  },
  "003018": {
    "context": "003018",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「そりゃあね。サリュほどの食べっぷりなら、譲るのもやぶさかではないよ」"
  },
  "003063": {
    "context": "003063",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「……感謝する」"
  },
  "003092": {
    "context": "003092",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「よしきた！　ちょっと待っててね！」"
  },
  "003138": {
    "context": "003138",
    "code": "0x45",
    "developer_comment": "",
    "string": "そう言ってましろは、嬉々としてキッチンに向かった。"
  },
  "003189": {
    "context": "003189",
    "code": "0x45",
    "developer_comment": "",
    "string": "サリュはその２枚目の肉を、今度はじっくりと味わって食べた。"
  },
  "003195": {
    "context": "003195",
    "code": "0x45",
    "developer_comment": "",
    "string": "そうしてやがて、それも全て食べ終え――"
  }
}