  "000005": {
    "context": "000005",
    "code": "0x45",
    "developer_comment": "",
    "string": "その一言が、彼の背中を押した。"
  },
  "000011": {
    "context": "000011",
    "code": "0x45",
    "developer_comment": "男",
    "string": "（オレがあの子を助けるんだ！）"
  },
  "000016": {
    "context": "000016",
    "code": "0x45",
    "developer_comment": "男",
    "string": "決意を胸に、男は火炎満ちる部屋に飛び込んだ。"
  },
  "000159": {
    "context": "000159",
    "code": "0x45",
    "developer_comment": "男",
    "string": "「ぐっ！」"
  },
  "000164": {
    "context": "000164",
    "code": "0x45",
    "developer_comment": "男",
    "string": "だが部屋に入るなり、彼は立ち往生した。"
  },
  "000169": {
    "context": "000169",
    "code": "0x45",
    "developer_comment": "男",
    "string": "焦げ付くような熱波が、彼の歩みを止める。"
  },
  "000218": {
    "context": "000218",
    "code": "0x45",
    "developer_comment": "男",
    "string": "すぐそこに例の少女がいるのだが、そこまで行くのも難しい。"
  },
  "000223": {
    "context": "000223",
    "code": "0x45",
    "developer_comment": "男",
    "string": "その時、すぐ傍で声がした。"
  },
  "000313": {
    "context": "000313",
    "code": "0x45",
    "developer_comment": "女隊員",
    "string": "「なにやってんの隊長！」"
  },
  "000318": {
    "context": "000318",
    "code": "0x45",
    "developer_comment": "女隊員",
    "string": "いつのまにかさっきの女が、彼を追って部屋に入ってきていた。"
  },
  "000329": {
    "context": "000329",
    "code": "0x45",
    "developer_comment": "男",
    "string": "「あ、い、いやその……」"
  },
  "000334": {
    "context": "000334",
    "code": "0x45",
    "developer_comment": "男",
    "string": "そう言いかけた時――"
  },
  "000360": {
    "context": "000360",
    "code": "0x45",
    "developer_comment": "男",
    "string": "「熱ッ！」"
  },
  "000365": {
    "context": "000365",
    "code": "0x45",
    "developer_comment": "男",
    "string": "頭に酷い熱を感じた。"
  },
  "000370": {
    "context": "000370",
    "code": "0x45",
    "developer_comment": "男",
    "string": "一瞬後に、不快な臭いが――"
  },
  "000376": {
    "context": "000376",
    "code": "0x45",
    "developer_comment": "男",
    "string": "（髪が燃えてる！？）"
  },
  "000381": {
    "context": "000381",
    "code": "0x45",
    "developer_comment": "男",
    "string": "それが彼のパニックを誘った。"
  },
  "000407": {
    "context": "000407",
    "code": "0x45",
    "developer_comment": "男",
    "string": "「うわっ！　ああっ！　くそっ！　くそっ！」"
  },
  "000412": {
    "context": "000412",
    "code": "0x45",
    "developer_comment": "男",
    "string": "男は頭をバタバタと叩いて、火を消そうとする。"
  },
  "000441": {
    "context": "000441",
    "code": "0x45",
    "developer_comment": "女隊員",
    "string": "「隊長、なにそんな素人みたいなこと……！？」"
  },
  "000470": {
    "context": "000470",
    "code": "0x45",
    "developer_comment": "女隊員",
    "string": "「副隊長、隊長の様子が変だよ！？」"
  },
  "000481": {
    "context": "000481",
    "code": "0x45",
    "developer_comment": "副隊長",
    "string": "「いま、放水援護を――！」"
  },
  "000514": {
    "context": "000514",
    "code": "0x45",
    "developer_comment": "副隊長",
    "string": "部屋の外で声が響き、続いて放水が来た。"
  },
  "000540": {
    "context": "000540",
    "code": "0x45",
    "developer_comment": "副隊長",
    "string": "水が霧のように、室内に満ちる。"
  },
  "000545": {
    "context": "000545",
    "code": "0x45",
    "developer_comment": "副隊長",
    "string": "だが男の頭の火は消えず、彼は恐慌に陥った。"
  },
  "000560": {
    "context": "000560",
    "code": "0x45",
    "developer_comment": "男",
    "string": "「くそ！　消えろ、消えろ！」"
  },
  "000569": {
    "context": "000569",
    "code": "0x45",
    "developer_comment": "男",
    "string": "少女の救助の事など、もう頭から消えていた。"
  },
  "000578": {
    "context": "000578",
    "code": "0x45",
    "developer_comment": "男",
    "string": "思い余った彼は、放水を直接浴びて火を消そうとする。"
  },
  "000587": {
    "context": "000587",
    "code": "0x45",
    "developer_comment": "男",
    "string": "しかし、放水の射線上に入った瞬間――"
  },
  "000644": {
    "context": "000644",
    "code": "0x45",
    "developer_comment": "男",
    "string": "「がっ！」"
  },
  "000649": {
    "context": "000649",
    "code": "0x45",
    "developer_comment": "男",
    "string": "水の勢いに、弾き飛ばされた。"
  },
  "000700": {
    "context": "000700",
    "code": "0x45",
    "developer_comment": "男",
    "string": "「ッ！！！」"
  },
  "000705": {
    "context": "000705",
    "code": "0x45",
    "developer_comment": "男",
    "string": "男は仰向けに倒れこみ、後頭部を打った。"
  },
  "000716": {
    "context": "000716",
    "code": "0x45",
    "developer_comment": "男",
    "string": "「ぐっ……」"
  },
  "000772": {
    "context": "000772",
    "code": "0x45",
    "developer_comment": "男",
    "string": "急速に目の前が暗くなる。"
  },
  "000792": {
    "context": "000792",
    "code": "0x45",
    "developer_comment": "副隊長",
    "string": "「ああ！　隊長！」"
  },
  "000806": {
    "context": "000806",
    "code": "0x45",
    "developer_comment": "副隊長",
    "string": "遠くで誰かの声が聞こえる。"
  },
  "000826": {
    "context": "000826",
    "code": "0x45",
    "developer_comment": "副隊長",
    "string": "「隊長、どうしてこんな……！」"
  },
  "000840": {
    "context": "000840",
    "code": "0x45",
    "developer_comment": "副隊長",
    "string": "その声もすぐに遠ざかる。"
  },
  "000855": {
    "context": "000855",
    "code": "0x45",
    "developer_comment": "男",
    "string": "（な、なんだこれ……これで終わりか？）"
  },
  "000870": {
    "context": "000870",
    "code": "0x45",
