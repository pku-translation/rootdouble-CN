{
  "000007": {
    "context": "000007",
    "code": "0x55",
    "developer_comment": "",
    "string": "√Before Chapter7"
  },
  "000023": {
    "context": "000023",
    "code": "0x45",
    "developer_comment": "",
    "string": "その建物は、巨大な門と高いフェンスで守られていた。"
  },
  "000028": {
    "context": "000028",
    "code": "0x45",
    "developer_comment": "",
    "string": "夏彦たちはそのフェンスを乗り越え――敷地内へと侵入した。"
  },
  "000077": {
    "context": "000077",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「……着いたか」"
  },
  "000114": {
    "context": "000114",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "視線の先には、巨大な建物があった。"
  },
  "000119": {
    "context": "000119",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "『ラボ』――原子力生物学機構第６研究所。"
  },
  "000124": {
    "context": "000124",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "ここで９年前、夏彦と悠里の心に、消えない傷が刻み込まれたのだ。"
  },
  "000190": {
    "context": "000190",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「……静かだね」"
  },
  "000220": {
    "context": "000220",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「まだテロは起きていないと思われる」"
  },
  "000231": {
    "context": "000231",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「あぁ……！」"
  },
  "000236": {
    "context": "000236",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "夏彦は硬い声で答える。"
  },
  "000242": {
    "context": "000242",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "（くそ、予想はしていたけど……！）"
  },
  "000247": {
    "context": "000247",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "建物を見るだけで冷や汗が流れ、鳥肌が立った。"
  },
  "000252": {
    "context": "000252",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "９年前の恐怖は、今なお生きているらしい。"
  },
  "000258": {
    "context": "000258",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "（……耐えろ、これを乗り越えるんだ）"
  },
  "000263": {
    "context": "000263",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "――そう思った時、鋭い声が響き渡った。"
  },
  "000271": {
    "context": "000271",
    "code": "0x45",
    "developer_comment": "警備員１",
    "string": "「君たち、ここで何やってる！　ここは立ち入り禁止だぞ！」"
  },
  "000276": {
    "context": "000276",
    "code": "0x45",
    "developer_comment": "警備員１",
    "string": "見ればラボの方から、警備員らしき男が２人駆け寄ってきていた。"
  },
  "000281": {
    "context": "000281",
    "code": "0x45",
    "developer_comment": "警備員１",
    "string": "警備員たちはマシンガンらしきものを肩から提げている。"
  },
  "000307": {
    "context": "000307",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「あ、そ、その……！」"
  },
  "000312": {
    "context": "000312",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "それを見たましろが、怯えたように後ずさった。"
  },
  "000317": {
    "context": "000317",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "だが夏彦はましろとサリュを護るように立ち、警備員に答える。"
  },
  "000328": {
    "context": "000328",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「待ってください。ぼくは天川美夜子上級研究員の息子、天川夏彦です」"
  },
  "000336": {
    "context": "000336",
    "code": "0x45",
    "developer_comment": "警備員２",
    "string": "「……なに？」"
  },
  "000341": {
    "context": "000341",
    "code": "0x45",
    "developer_comment": "警備員２",
    "string": "怪訝そうな顔をする若い警備員に、夏彦は学生証を見せて続けた。"
  },
  "000352": {
    "context": "000352",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「緊急の件で、母に会いに来ました。母を呼んでもらえませんか？」"
  },
  "000360": {
    "context": "000360",
    "code": "0x45",
    "developer_comment": "警備員１",
    "string": "「どういう用件だね」"
  },
  "000371": {
    "context": "000371",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「……家族の命の危機です」"
  },
  "000382": {
    "context": "000382",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「母には昨日から連絡がつかず……仕方なく、直接やってきました」"
  },
  "000387": {
    "context": "000387",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "夏彦は嘘を答える事にした。"
  },
  "000392": {
    "context": "000392",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "そうしながらも必死で、エンパシーを試みる。"
  },
  "000397": {
    "context": "000397",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "しかし、すぐに――ましろからテレパシーが飛んできた。"
  },
  "000423": {
    "context": "000423",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "《夏彦、この人たちの心が読めない……！　心開いてないよ》"
  },
  "000429": {
    "context": "000429",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "（まぁそうだろうな……）"
  },
  "000434": {
    "context": "000434",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "警備員たちが内通者か否かに関わらず、突然現れた高校生たちに、心は開かないだろう。"
  },
  "000439": {
    "context": "000439",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "彼らは夏彦たちを、じっと見据えている。"
  },
  "000444": {
    "context": "000444",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "やがて若い方の警備員が、年かさの警備員に耳打ちした。"
  },
  "000452": {
    "context": "000452",
    "code": "0x45",
    "developer_comment": "警備員２",
    "string": "「……天川博士をお呼びしますか？」"
  },
  "000460": {
    "context": "000460",
    "code": "0x45",
    "developer_comment": "警備員１",
    "string": "「……う、む」"
  },
  "000465": {
    "context": "000465",
    "code": "0x45",
    "developer_comment": "警備員１",
    "string": "そのとき不意にポツリポツリと、水滴が落ちてきた。"
  },
  "000521": {
    "context": "000521",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「あ……」"
  },
  "000534": {
    "context": "000534",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "空にはいつの間にか雲がかかり、雨が降り始めた。"
  },
  "000539": {
    "context": "000539",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "警備員が忌々しげに空を見上げる。"
  },
  "000547": {
    "context": "000547",
    "code": "0x45",
    "developer_comment": "警備員１",
    "string": "「雨か……仕方ないな」"
  },
  "000555": {
    "context": "000555",
    "code": "0x45",
    "developer_comment": "警備員１",
    "string": "「エントランスまでは入れてあげよう。そこで天川博士を待つんだ」"
  },
  "000566": {
    "context": "000566",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「は……はい！」"
  },
  "000571": {
    "context": "000571",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "夏彦たちはその言葉に、密かに拳を握った。"
  },
  "000618": {
    "context": "000618",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "――夏彦たちはエントランスに通された。"
  },
  "000623": {
    "context": "000623",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "２人の警備員のうち、年かさの方が言う。"
  },
  "000631": {
    "context": "000631",
    "code": "0x45",
    "developer_comment": "警備員１",
    "string": "「博士を呼んでくる。ここで少し待っていたまえ」"
  },
  "000642": {
    "context": "000642",
    "code": "0x45",
    "developer_comment": "警備員１",
    "string": "彼はエントランスの奥にある、鉄扉の向こうに消えた。"
  },
  "000647": {
    "context": "000647",
    "code": "0x45",
    "developer_comment": "警備員１",
    "string": "取り残された夏彦たちには、若い警備員が張り付いていた。"
  },
  "000659": {
    "context": "000659",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「……ぼくらは連れてってはもらえないんですか？」"
  },
  "000667": {
    "context": "000667",
    "code": "0x45",
    "developer_comment": "警備員２",
    "string": "「外部の人間が入れるのは、このエントランスまでだ」"
  },
  "000675": {
    "context": "000675",
    "code": "0x45",
    "developer_comment": "警備員２",
    "string": "「この施設は至るところに『セキュリティゲート』が設けられていて、所定のカードを持っていないと入れない」"
  },
  "000680": {
    "context": "000680",
    "code": "0x45",
    "developer_comment": "警備員２",
    "string": "男は鉄扉をちらりと見た。"
  },
  "000685": {
    "context": "000685",
    "code": "0x45",
    "developer_comment": "警備員２",
    "string": "その扉の脇には、カードを通すスロットらしきものがある。"
  },
  "000714": {
    "context": "000714",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「天川博士はどこに？」"
  },
  "000722": {
    "context": "000722",
    "code": "0x45",
    "developer_comment": "警備員２",
    "string": "「地下フロアだろう。だが地上より地下はさらにセキュリティが高く、一般人の立ち入りは厳禁だ」"
  },
  "000730": {
    "context": "000730",
    "code": "0x45",
    "developer_comment": "警備員２",
    "string": "「場合によっては、警告なしに発砲・捕縛する事もある」"
  },
  "000738": {
    "context": "000738",
    "code": "0x45",
    "developer_comment": "警備員２",
    "string": "「わかったらここでおとなしく待っているんだね」"
  },
  "000743": {
    "context": "000743",
    "code": "0x45",
    "developer_comment": "警備員２",
    "string": "それだけ警備が厳しければ、テロリストの侵入も許さないだろう。"
  },
  "000749": {
    "context": "000749",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "（でも油断はできない。敵がどう侵入してくるのかわからないんだから……！）"
  },
  "000754": {
    "context": "000754",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "夏彦がそう思った時、ましろのテレパシーが飛んできた。"
  },
  "000830": {
    "context": "000830",
    "code": "0x85",
    "developer_comment": "夏彦",
    "string": "《時間を無駄にできないよね……"
  },
  "000832": {
    "context": "000832",
    "code": "0x85",
    "developer_comment": "夏彦",
    "string": "今のうちに、博士にテレパシー送ってみようか？》"
  },
  "000833": {
    "context": "000833",
    "code": "0x86",
    "developer_comment": "夏彦",
    "string": "ましろ"
  },
  "000834": {
    "context": "000834",
    "code": "0x86",
    "developer_comment": "夏彦",
    "string": "《時間を無駄にできないよね……今のうちに、博士にテレパシー送ってみようか？》"
  },
  "000862": {
    "context": "000862",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "夏彦ははっとして、ましろにうなずいて見せた。"
  },
  "000937": {
    "context": "000937",
    "code": "0x85",
    "developer_comment": "夏彦",
    "string": "《了解……それじゃやってみるよ》"
  },
  "000938": {
    "context": "000938",
    "code": "0x86",
    "developer_comment": "夏彦",
    "string": "ましろ"
  },
  "000939": {
    "context": "000939",
    "code": "0x86",
    "developer_comment": "夏彦",
    "string": "《了解……それじゃやってみるよ》"
  },
  "000968": {
    "context": "000968",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "（あぁ、ぼくも試してみよう）"
  },
  "001016": {
    "context": "001016",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "夏彦はそう思い、精神を集中した。"
  },
  "001047": {
    "context": "001047",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "母の固有脳波形を頭に思い描く。"
  },
  "001052": {
    "context": "001052",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "いつか母自身が言っていた。彼女はサリュと同じ『ＴＹＰＥ４』だ。"
  },
  "001058": {
    "context": "001058",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "（母さんに伝えなければならない事がある……）"
  },
  "001064": {
    "context": "001064",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "（今以上に、この能力が必要とされる時はない。当然テレパシーは使えるはずだ！）"
  },
  "001070": {
    "context": "001070",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "そう思った瞬間、夏彦の五感が不意に消えた。"
  },
  "001075": {
    "context": "001075",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "頭に念じたメッセージが、外に放たれるのを感じる。"
  },
  "001247": {
    "context": "001247",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "《――母さん、聞こえるか！？　ぼくだよ、夏彦だ！》"
  },
  "001258": {
    "context": "001258",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "《いまぼくらは、ラボの地上エントランスにいる》"
  },
  "001269": {
    "context": "001269",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "《ぼくらが来た理由は、昨日言っていた一件に関わってる》"
  },
  "001280": {
    "context": "001280",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "《つまり、テロの事――Ｑがこの施設を狙っている可能性があるんだ》"
  },
  "001291": {
    "context": "001291",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "《Ｑの目的は、この街を滅ぼすことだ。そのためにここの原子炉を狙ってる》"
  },
  "001302": {
    "context": "001302",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "《しかもこの施設の中には、恐らく内通者がいる。宇喜多のおじさんか、あるいは他にも……》"
  },
  "001313": {
    "context": "001313",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "《どれだけ警備を厳しくしても、セキュリティが高くても、内通者がいればテロは成功してしまうかもしれない》"
  },
  "001324": {
    "context": "001324",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "《だから、母さん……今すぐ施設内の全員を外に出して、誰も中に入れないようにしてくれ》"
  },
  "001335": {
    "context": "001335",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "《無茶を言っているのはわかってる。だけどそれしか方法がないんだ！》"
  },
  "001488": {
    "context": "001488",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "――そこまで念じたところで、五感が戻ってきた。"
  },
  "001494": {
    "context": "001494",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "（……よし！）"
  },
  "001499": {
    "context": "001499",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "夏彦はテレパシーが発動した事を確信した。"
  },
  "001504": {
    "context": "001504",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "すぐにまたましろから、テレパシーが飛んでくる。"
  },
  "001580": {
    "context": "001580",
    "code": "0x85",
    "developer_comment": "夏彦",
    "string": "《ねぇ夏彦、テレパシー送ったけど……"
  },
  "001582": {
    "context": "001582",
    "code": "0x85",
    "developer_comment": "夏彦",
    "string": "天川博士に伝わってる感じがしないよ》"
  },
  "001583": {
    "context": "001583",
    "code": "0x86",
    "developer_comment": "夏彦",
    "string": "ましろ"
  },
  "001584": {
    "context": "001584",
    "code": "0x86",
    "developer_comment": "夏彦",
    "string": "《ねぇ夏彦、テレパシー送ったけど……天川博士に伝わってる感じがしないよ》"
  },
  "001618": {
    "context": "001618",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「え？　それってわかるのか？」"
  },
  "001623": {
    "context": "001623",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "思わず声を出してしまった夏彦を、警備員が訝しげに見る。"
  },
  "001631": {
    "context": "001631",
    "code": "0x45",
    "developer_comment": "警備員２",
    "string": "「ん、なにがだね？」"
  },
  "001642": {
    "context": "001642",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「あ、いや、なんでもありません」"
  },
  "001647": {
    "context": "001647",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "慌ててごまかす夏彦に、ましろがテレパシーで続ける。"
  },
  "001704": {
    "context": "001704",
    "code": "0x85",
    "developer_comment": "夏彦",
    "string": "《慣れてくるとね、エンパシーの応用で送信確認もできるの》"
  },
  "001705": {
    "context": "001705",
    "code": "0x86",
    "developer_comment": "夏彦",
    "string": "ましろ"
  },
  "001706": {
    "context": "001706",
    "code": "0x86",
    "developer_comment": "夏彦",
    "string": "《慣れてくるとね、エンパシーの応用で送信確認もできるの》"
  },
  "001739": {
    "context": "001739",
    "code": "0x85",
    "developer_comment": "夏彦",
    "string": "《だけど、なんか変……全く反応がない》"
  },
  "001740": {
    "context": "001740",
    "code": "0x86",
    "developer_comment": "夏彦",
    "string": "ましろ"
  },
  "001741": {
    "context": "001741",
    "code": "0x86",
    "developer_comment": "夏彦",
    "string": "《だけど、なんか変……全く反応がない》"
  },
  "001770": {
    "context": "001770",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "（何！？　母さんに何かあったのか！？）"
  },
  "001830": {
    "context": "001830",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "夏彦は嫌な予感を覚えつつ、エントランスの奥を見る。"
  },
  "001835": {
    "context": "001835",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "――その時、不意に――"
  },
  "001872": {
    "context": "001872",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「ッ！！」"
  },
  "001877": {
    "context": "001877",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "脳裏に９年前の記憶が蘇った。"
  },
  "001982": {
    "context": "001982",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "――それは２０２１年、７月７日。"
  },
  "001987": {
    "context": "001987",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "悠里と共に、この施設に忍び込んだ時の記憶だった。"
  },
  "001992": {
    "context": "001992",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "当時は警備も緩かったようだ。"
  },
  "001997": {
    "context": "001997",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "夏彦達は物資搬入用トラックの荷台に忍び込み、警備員の目を盗んで、エントランスに侵入した。"
  },
  "002057": {
    "context": "002057",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "職員たちは地下にいるのか、エントランスに人気はなかった。"
  },
  "002068": {
    "context": "002068",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「ふ～ん、ここがラボかぁ」"
  },
  "002073": {
    "context": "002073",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "夏彦があたりを見回して言うと、傍らの悠里がささやく。"
  },
  "002084": {
    "context": "002084",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「ね、ねぇ、ひこりん……もっとこそこそしたほうがいいよぅ」"
  },
  "002095": {
    "context": "002095",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「なんだよ悠里、悠里だってさんせいしただろ？」"
  },
  "002106": {
    "context": "002106",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「いつもおしごとばっかりさせられて、たいへんそうな母さんをたすけてあげようってさ」"
  },
  "002135": {
    "context": "002135",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「う、うん、わかってるけど……」"
  },
  "002140": {
    "context": "002140",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "それが夏彦と悠里が、ラボに忍び込んだ理由だった。"
  },
  "002145": {
    "context": "002145",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "当時からすでに天川博士は、ラボの中核をなす研究者となっていて、いつも夜遅くまで働いていた。"
  },
  "002150": {
    "context": "002150",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "それが夏彦には、無理やり働かされているように見えたのだ。"
  },
  "002161": {
    "context": "002161",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「これは母さんをたすけるための、せいぎのさくせんなんだ」"
  },
  "002172": {
    "context": "002172",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「もっとどうどうとしてたほうがいいって」"
  },
  "002201": {
    "context": "002201",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「でもでも……みつかったらおこられちゃうよ」"
  },
  "002212": {
    "context": "002212",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「なんだ、こわがりだな。ほら手ーつないであげるから、いくぞ」"
  },
  "002253": {
    "context": "002253",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "夏彦は悠里の手を握り、エントランスの奥に進む。"
  },
  "002269": {
    "context": "002269",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「母さんにきいたんだ。母さんはちかの、だい５エリアっていうところではたらいてるって」"
  },
  "002280": {
    "context": "002280",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「えらいはかせしか入れないエリアなんだって」"
  },
  "002309": {
    "context": "002309",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「じゃあまずは、ちかにいくかいだんをさがさなくちゃね」"
  },
  "002320": {
    "context": "002320",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「あのとびらのおくだよきっと。いこう悠里」"
  },
  "002325": {
    "context": "002325",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "そして、その時も――夏彦たちは、あの鉄扉に行く手を阻まれたのだ。"
  },
  "002336": {
    "context": "002336",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「あれ……あかない」"
  },
  "002365": {
    "context": "002365",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「どうすればあくのかな……？」"
  },
  "002370": {
    "context": "002370",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "――どうすれば開くのか。"
  },
  "002375": {
    "context": "002375",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "あの時はなぜ、地下に降りられたのか。"
  },
  "002380": {
    "context": "002380",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "夏彦がそれを思い出そうとした時――！"
  },
  "002541": {
    "context": "002541",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「……ぐっ！？」"
  },
  "002546": {
    "context": "002546",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "不意に頭に激痛が走り、思考が途切れた。"
  },
  "002554": {
    "context": "002554",
    "code": "0x45",
    "developer_comment": "警備員２",
    "string": "「む？」"
  },
  "002559": {
    "context": "002559",
    "code": "0x45",
    "developer_comment": "警備員２",
    "string": "警備員の胡乱な声が、遠くで聞こえる。"
  },
  "002564": {
    "context": "002564",
    "code": "0x45",
    "developer_comment": "警備員２",
    "string": "激痛に耐えられず、夏彦は壁にもたれた。"
  },
  "002608": {
    "context": "002608",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「――夏彦！？」"
  },
  "002619": {
    "context": "002619",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「どうしたの！？　気分でも悪いの！？」"
  },
  "002630": {
    "context": "002630",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「あ……頭が……！」"
  },
  "002635": {
    "context": "002635",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "万力で締め付けられているように痛んでいた。"
  },
  "002640": {
    "context": "002640",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "９年前の事を思い出すと同時に。"
  },
  "002646": {
    "context": "002646",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "（どうして……！？　なんでこんなに頭が痛む！？）"
  },
  "002652": {
    "context": "002652",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "（……いや、いいから思い出せ！）"
  },
  "002658": {
    "context": "002658",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "（おかしいじゃないか！　昔と今で警備の体制は変わっても、施設の構造までは変わらないだろ！？）"
  },
  "002664": {
    "context": "002664",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "（どうしてぼくらは地下に降りられたんだ！？）"
  },
  "002669": {
    "context": "002669",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "あの時も夏彦と悠里は、セキュリティカードを持っていなかった。"
  },
  "002674": {
    "context": "002674",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "それでもなんとか鉄扉を開けようと、カードスロットを操作していた時――"
  },
  "002680": {
    "context": "002680",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "（……そうだ）"
  },
  "002686": {
    "context": "002686",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "（あの時は確か、警報が……！）"
  },
  "002691": {
    "context": "002691",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "思い出しかけると同時に、またも頭痛が！"
  },
  "002728": {
    "context": "002728",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「ぐっ、う、うううっ……！」"
  },
  "002733": {
    "context": "002733",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "それに耐えられず、夏彦がふらついた。"
  },
  "002776": {
    "context": "002776",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "――その瞬間――。"
  },
  "002915": {
    "context": "002915",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "足元で、轟音が響いた。"
  }
}