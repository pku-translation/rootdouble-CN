{
  "000041": {
    "context": "000041",
    "code": "0x55",
    "developer_comment": "",
    "string": "√Before Chapter5"
  },
  "000059": {
    "context": "000059",
    "code": "0x45",
    "developer_comment": "",
    "string": "そうして３人の持ち歌も尽きかけた頃――"
  },
  "000064": {
    "context": "000064",
    "code": "0x45",
    "developer_comment": "",
    "string": "隣に座っていたましろが、寝息を立てはじめた。"
  },
  "000141": {
    "context": "000141",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「くぅ……くぅ……」"
  },
  "000146": {
    "context": "000146",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "サリュが歌うのをやめて、言う。"
  },
  "000175": {
    "context": "000175",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「ましろ、寝たの？」"
  },
  "000186": {
    "context": "000186",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「あぁ、疲れてたんだろうな」"
  },
  "000215": {
    "context": "000215",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「……私につきあって、無理して歌っていたのかもしれない」"
  },
  "000226": {
    "context": "000226",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「そんな事ないだろ、こいつも楽しんでたと思うよ」"
  },
  "000231": {
    "context": "000231",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "そして恐らく、ましろも不安を忘れたかったのだろう。"
  },
  "000242": {
    "context": "000242",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「気を遣う事はないよ。ぼくも楽しかったし」"
  },
  "000277": {
    "context": "000277",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「そう……よかった」"
  },
  "000306": {
    "context": "000306",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「私もいい経験になった。２人に感謝する」"
  },
  "000335": {
    "context": "000335",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「歌を思い切り歌うのが、こんなに楽しいとは知らなかったから」"
  },
  "000340": {
    "context": "000340",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "そう言われて、夏彦はふと思った。"
  },
  "000346": {
    "context": "000346",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "（サリュはカラオケ初体験ってより……大声で歌うのが初めてなんじゃ？）"
  },
  "000351": {
    "context": "000351",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "考えてみれば、サリュの過去については、ほとんど何も知らないのだ。"
  },
  "000362": {
    "context": "000362",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「なぁ、サリュ？」"
  },
  "000391": {
    "context": "000391",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「なに？」"
  },
  "000402": {
    "context": "000402",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「サリュって、フランスにいた頃、どんな生活してたんだ？」"
  },
  "000426": {
    "context": "000426",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「――」"
  },
  "000431": {
    "context": "000431",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "夏彦の言葉に、サリュが口をつぐむ。"
  },
  "000442": {
    "context": "000442",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「あ、いや、答えたくなかったらいいんだよ。ちょっと気になっただけだから」"
  },
  "000471": {
    "context": "000471",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「答えたくないわけじゃない。聞きたいなら話す」"
  },
  "000500": {
    "context": "000500",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「もっとも、あまり面白い話ではないけど――」"
  },
  "000505": {
    "context": "000505",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "サリュはそう言って、静かに話し始めた。"
  },
  "000624": {
    "context": "000624",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「私が生まれたのは、フランスの片田舎にある大きな屋敷」"
  },
  "000635": {
    "context": "000635",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「その中に閉じ込められるようにして、私は育った」"
  },
  "000646": {
    "context": "000646",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「え、やっぱサリュってお嬢様だったのか？」"
  },
  "000675": {
    "context": "000675",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「わからないけど、そうなのかもしれない」"
  },
  "000704": {
    "context": "000704",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「私の両親は確か、フランスで原子力関連の企業を経営している……らしい」"
  },
  "000733": {
    "context": "000733",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「そこで私は、なに不自由のない生活をしていた。たくさんの教育係に囲まれ、あらゆる分野の教育を受けて」"
  },
  "000744": {
    "context": "000744",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「あらゆる分野って？」"
  },
  "000773": {
    "context": "000773",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「科学を中心とした様々な学問。英語・日本語・中国語など１５種の外語」"
  },
  "000784": {
    "context": "000784",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「工学などの技術。ピアノや絵画といった芸術の類。果ては器械体操や護身術まで」"
  },
  "000795": {
    "context": "000795",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「……！」"
  },
  "000800": {
    "context": "000800",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "夏彦は驚愕した。"
  },
  "000805": {
    "context": "000805",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "優秀な少女だとは思っていたが、まさかそこまでだったとは。"
  },
  "000834": {
    "context": "000834",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「私は乾いた砂のように、それらの知識を吸収した」"
  },
  "000863": {
    "context": "000863",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「増えていく知識と、習熟していく技術が、私の存在証明。想い出はそれが全て」"
  },
  "000874": {
    "context": "000874",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「外で遊んだりとかは？　近所の友達とかと……」"
  },
  "000903": {
    "context": "000903",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「そんな事はまるでなかった。両親がとても厳しくて、ずっと家の中にいた」"
  },
  "000932": {
    "context": "000932",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「私も外に出る必要は、まるで感じていなかった」"
  },
  "000943": {
    "context": "000943",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「なるほどね……」"
  },
  "000948": {
    "context": "000948",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "育ちがぼくらとは違うんだと、夏彦は思った。"
  },
  "000953": {
    "context": "000953",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "夏彦が外で遊んでいた頃も、ずっとサリュは家で英才教育を受けていたのだ。"
  },
  "000958": {
    "context": "000958",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "サリュの独特の雰囲気や様々な能力は、その生い立ちから来ているのだろう。"
  },
  "000963": {
    "context": "000963",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "だが、娘を全く外に出してくれない両親と言うのは、あまりまともだとは思えない。"
  },
  "000969": {
    "context": "000969",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "（気軽に聞いていい話題じゃなかったかもしれないな……）"
  },
  "000974": {
    "context": "000974",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "複雑な事情があるのか、あるいはサリュの体質に関係があるのか。"
  },
  "000979": {
    "context": "000979",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "そう思いつつも、夏彦は問いを重ねる。"
  },
  "000990": {
    "context": "000990",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「でも今は外に出てるみたいだけど……両親の考えが変わったのか？」"
  },
  "001019": {
    "context": "001019",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「そういうわけじゃない。天川博士に連れ出された」"
  },
  "001030": {
    "context": "001030",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「は？　なんで母さんが？」"
  },
  "001041": {
    "context": "001041",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「そもそもサリュの両親とは、どういう関係？　遠縁の親戚だというのは聞いたけど……」"
  },
  "001070": {
    "context": "001070",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「博士は若い頃、一時期フランスに住んでいたのだけど、そのとき私の家に下宿していたとの事」"
  },
  "001081": {
    "context": "001081",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「その後も度々、私の両親と連絡を取り、私の存在を知ったらしい」"
  },
  "001160": {
    "context": "001160",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「博士は私を『自由にさせてあげたかった』と言っていた」"
  },
  "001171": {
    "context": "001171",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「あの母さんがねぇ……」"
  },
  "001176": {
    "context": "001176",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "にわかには信じがたいが、サリュが嘘をついているとは思えなかった。"
  },
  "001268": {
    "context": "001268",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「博士は両親を説得し、日本へ留学する事を認めさせた。以上が私が博士から聞いた経緯」"
  },
  "001279": {
    "context": "001279",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「だからこんな中途半端な時期に、急に転校してきたのか……」"
  },
  "001293": {
    "context": "001293",
    "code": "0x55",
    "developer_comment": "夏彦",
    "string": "√Before Chapter5"
  },
  "001306": {
    "context": "001306",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「でも、どうしてウチに住む事になったんだ？」"
  },
  "001335": {
    "context": "001335",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「理由の１つは、私には他に住むところがないという事」"
  },
  "001346": {
    "context": "001346",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「もう１つは博士曰く、『私に夏彦を護って欲しい』との事」"
  },
  "001375": {
    "context": "001375",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「前にも言ったけど、近頃この鹿鳴市も治安が悪くなっているから」"
  },
  "001386": {
    "context": "001386",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「治安……確かにな」"
  },
  "001391": {
    "context": "001391",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "数日前にその話を聞いた時は、その言葉の意味はよくわからなかった。"
  },
  "001396": {
    "context": "001396",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "だがそれは今日一日で、かなり現実味を帯びてきた。"
  },
  "001402": {
    "context": "001402",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "（ぼくを護る、か……）"
  },
  "001407": {
    "context": "001407",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "サリュのあの身体能力も、警備員を手玉にとった武術も、全てフランスにいた頃に身につけたものらしい。"
  },
  "001412": {
    "context": "001412",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "しかし母がどの程度、サリュを信頼して任せているかはわからないが――"
  },
  "001418": {
    "context": "001418",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "（護られてばかりじゃ、情けないよな）"
  },
  "001429": {
    "context": "001429",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「……なぁサリュ」"
  },
  "001440": {
    "context": "001440",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「今日、警備員を投げ飛ばしただろ？　あれ、ぼくにもできないかな？」"
  },
  "001469": {
    "context": "001469",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「夏彦が？」"
  },
  "001480": {
    "context": "001480",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「いや、なんだか状況も緊迫してきたしさ。ぼくも自分の身を守れるようになりたいんだよ」"
  },
  "001509": {
    "context": "001509",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「武術は少しかじったくらいでは、役には立たないけど。素人にも使える技はある」"
  },
  "001520": {
    "context": "001520",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「そうか、ぜひ教えてくれ」"
  },
  "001531": {
    "context": "001531",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「了解。では夏彦、私の襟首を掴んで」"
  },
  "001542": {
    "context": "001542",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「あぁ、こうか？」"
  },
  "001547": {
    "context": "001547",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "夏彦がそっと、サリュの襟首に手を伸ばす。"
  },
  "001552": {
    "context": "001552",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "すると次の瞬間――"
  },
  "001576": {
    "context": "001576",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "その手をサリュに掴まれた。"
  },
  "001587": {
    "context": "001587",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「！？」"
  },
  "001616": {
    "context": "001616",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「例えば襟首を掴まれたら、まず相手の手首を掴み返す」"
  },
  "001673": {
    "context": "001673",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「そしてその腕を捻るように、相手の後ろに回り――」"
  },
  "001688": {
    "context": "001688",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「思い切り、引き下げる！」"
  },
  "001762": {
    "context": "001762",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「ぬお！？」"
  },
  "001767": {
    "context": "001767",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "サリュの掛け声と共に、夏彦の視界が反転した。"
  },
  "001824": {
    "context": "001824",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "背中に衝撃が走り、夏彦はうめく。"
  },
  "001835": {
    "context": "001835",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「ぐッ、ぐぅぅ……！」"
  },
  "001840": {
    "context": "001840",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "不思議な感覚だった。"
  },
  "001845": {
    "context": "001845",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "サリュはさほど力を込めていない風なのに、ひょいと投げられてしまった。"
  },
  "001874": {
    "context": "001874",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「大丈夫？」"
  },
  "001885": {
    "context": "001885",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「あ、あぁ……」"
  },
  "001890": {
    "context": "001890",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "よろよろと立ち上がる夏彦に、サリュが説明する。"
  },
  "001947": {
    "context": "001947",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「これが『四方投げ』。人間の反射を利用した、合気道の技」"
  },
  "001976": {
    "context": "001976",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「相手は関節を逆に取られているから、痛みから逃げようとしたら自然と投げられてしまう」"
  },
  "001987": {
    "context": "001987",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「な、なるほど……！」"
  },
  "002016": {
    "context": "002016",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「相手に襟首をつかまれた時に限定すれば、素人でも使える」"
  },
  "002045": {
    "context": "002045",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「だけど忘れないで。護身術において、大事なのはまず何より逃げることだから」"
  },
  "002056": {
    "context": "002056",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「逃げる？」"
  },
  "002085": {
    "context": "002085",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「そう。日本の諺にもあるでしょう？　『生ヒョウホンは怪我の元』」"
  },
  "002096": {
    "context": "002096",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「『生兵法は怪我の元』か？　なんだよ生標本って。なんか怖いよ」"
  },
  "002125": {
    "context": "002125",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「……日本語は難しい」"
  },
  "002136": {
    "context": "002136",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「サリュでもそういう間違いする事あるんだな。なんか安心したよ」"
  },
  "002165": {
    "context": "002165",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「うるさい夏彦。とにかくあくまで護るのは私。夏彦が戦うのは、最後の手段だと思って」"
  },
  "002176": {
    "context": "002176",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「わかった。何かあったら、まずはサリュを頼ることにするよ」"
  },
  "002181": {
    "context": "002181",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "夏彦はサリュの言葉に、素直に頷いた。"
  },
  "002186": {
    "context": "002186",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "護られてばかりではやはり情けないが、サリュの言う事ももっともだからだ。"
  },
  "002198": {
    "context": "002198",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「技のご教授ありがとう。なんかお礼ができればいいんだけど――」"
  },
  "002233": {
    "context": "002233",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「なら、逆に教えてほしい事がある」"
  },
  "002262": {
    "context": "002262",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「さっきは私の過去を語った。今度は夏彦の事を聞きたい」"
  },
  "002273": {
    "context": "002273",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「え、ぼくの何を？」"
  },
  "002302": {
    "context": "002302",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「なんでもいい。例えば家族について」"
  },
  "002331": {
    "context": "002331",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「夏彦の母は天川博士だけど、父はいまどこに？」"
  },
  "002342": {
    "context": "002342",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「あ、聞いてないのか？　父さんはだいぶ前に――お星サマになったよ」"
  },
  "002371": {
    "context": "002371",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「星に……？」"
  },
  "002376": {
    "context": "002376",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "迂遠な言い方では伝わらなかったようだ。夏彦は率直に言い直す。"
  },
  "002387": {
    "context": "002387",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「その、死んじゃったって事さ」"
  },
  "002416": {
    "context": "002416",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「えっ……」"
  },
  "002427": {
    "context": "002427",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「享年３２歳。まだ若かったのに、癌にかかっちゃってさ」"
  },
  "002438": {
    "context": "002438",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「ぼくが５歳の時だから、もう１０年以上も前だ」"
  },
  "002467": {
    "context": "002467",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「……………ごめん」"
  },
  "002478": {
    "context": "002478",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「あーいや、気にしなくていいよ。もう昔のことだし、今さら悲しくなったりはしないから」"
  },
  "002483": {
    "context": "002483",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "わずかな沈黙が流れた。"
  },
  "002488": {
    "context": "002488",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "アリスが何も鳴かないのを見て、サリュはほっとしたように言う。"
  },
  "002517": {
    "context": "002517",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「……夏彦の父は、どういう人だったの？」"
  },
  "002528": {
    "context": "002528",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「それがあんまり、覚えてないんだよなぁ」"
  },
  "002539": {
    "context": "002539",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「ぼくと同様、コミュニケーターだったらしいけど」"
  },
  "002568": {
    "context": "002568",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「父も？」"
  },
  "002579": {
    "context": "002579",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「『コミュニケーター大発現』前に生まれた人としては珍しい、適性度Ｂ・能力レベル４のコミュニケーターだったらしいよ」"
  },
  "002584": {
    "context": "002584",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "母から伝え聞く話をすると、サリュは合点したように言った。"
  },
  "002613": {
    "context": "002613",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「では夏彦の適性度の高さは、父からの遺伝なのね。確か博士は、適性度Ｆのはずだから」"
  },
  "002624": {
    "context": "002624",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「うーん、どうだろう……？　たしかに母さんは適性がないけど」"
  },
  "002653": {
    "context": "002653",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「どういう意味？　夏彦は適性度Ｓなのだから、父からの遺伝と考えるのが自然」"
  },
  "002664": {
    "context": "002664",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「……それなんだけどさ、サリュ」"
  },
  "002669": {
    "context": "002669",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "夏彦はわずかに声を潜めて言った。"
  },
  "002680": {
    "context": "002680",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「実はぼくの適性度って、元からＳだったわけじゃないんだ」"
  },
  "002709": {
    "context": "002709",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「……えっ？」"
  },
  "002720": {
    "context": "002720",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「学校に入る前に計った時は、Ｃだったんだよ」"
  },
  "002731": {
    "context": "002731",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「でも小学校に入ってからもう１度計測すると、Ｓに上昇してたんだ」"
  },
  "002736": {
    "context": "002736",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "その言葉に、サリュの目が丸くなった。"
  },
  "002765": {
    "context": "002765",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「……ちょっと待って、どういう事？」"
  },
  "002794": {
    "context": "002794",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「適性度とは、生まれつき決まっているもので、生涯変わる事はないはず」"
  },
  "002805": {
    "context": "002805",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「そこなんだよ、不思議なのは」"
  },
  "002816": {
    "context": "002816",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「計測した医者も首を捻っていてさ。めったにない事例だって騒ぎになったんだけど――」"
  },
  "002827": {
    "context": "002827",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「結局最終的には、小学校に入る前に計った時の『Ｃ』って結果はただの計測ミスで、正しくは『Ｓ』だったって事になった」"
  },
  "002832": {
    "context": "002832",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "それは今では、ほとんど知ってる人はいない事実だ。"
  },
  "002837": {
    "context": "002837",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "鹿鳴学園の生徒データベースには、夏彦は生来『適性度Ｓ』であると記録され、『適性度Ｃ』だった頃のデータは抹消されている。"
  },
  "002866": {
    "context": "002866",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「……そういう事ってあるの？」"
  },
  "002877": {
    "context": "002877",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「まぁそれから何年経っても『Ｓ』のままだから、そっちが正しいんだろう」"
  },
  "002888": {
    "context": "002888",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「特筆するような事は、そのくらいかな。あとはわりと平凡な生活を送っていたよ」"
  },
  "002918": {
    "context": "002918",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「そう……」"
  },
  "002923": {
    "context": "002923",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "ふと気付くと、サリュの反応が目に見えて鈍くなっていた。"
  },
  "002934": {
    "context": "002934",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「ごめん、つまらないかなこんな話？」"
  },
  "002963": {
    "context": "002963",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「あっ……ち、違う」"
  },
  "002992": {
    "context": "002992",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「実は……さっきからとても物凄く非常に眠い」"
  },
  "003049": {
    "context": "003049",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「え、そうだったのか？」"
  },
  "003078": {
    "context": "003078",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「……夏彦の話を、もっと聞きたくて、頑張っていたんだけど……もう、限界」"
  },
  "003089": {
    "context": "003089",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「いいよいいよ、また今度いくらでも話してやるから」"
  },
  "003118": {
    "context": "003118",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「ならば……私は、もう、寝る……」"
  },
  "003123": {
    "context": "003123",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "サリュの声はまるで電池が切れかけてでもいるように、急速に弱々しくなっていった。"
  },
  "003142": {
    "context": "003142",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「寝るんなら、膝枕でもしてやろうか？」"
  },
  "003171": {
    "context": "003171",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「ありがとう……では、失礼して……」"
  },
  "003222": {
    "context": "003222",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "サリュは夏彦の膝に倒れこみ、目を閉じた。"
  },
  "003233": {
    "context": "003233",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「えっ、あ……」"
  },
  "003244": {
    "context": "003244",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「くー……くー……」"
  },
  "003249": {
    "context": "003249",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "ツッコむ隙もなく、サリュは寝息を立て始めた。"
  },
  "003260": {
    "context": "003260",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「軽い冗談のつもりだったんだけどな……」"
  },
  "003265": {
    "context": "003265",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "そう言いつつ、夏彦はサリュの特質を思い出した。"
  },
  "003270": {
    "context": "003270",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "他人の気持ちがわからないという事は、冗談も通じないのかもしれない。"
  },
  "003276": {
    "context": "003276",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "（……軽い気持ちでからかったりしたら、えらい事になりそうだな）"
  },
  "003281": {
    "context": "003281",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "要はサリュに対しては、常に本気で受け答えすべきなのだろう。"
  },
  "003286": {
    "context": "003286",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "夏彦は彼女の寝顔を見つめつつ、密かにそう思った。"
  },
  "003300": {
    "context": "003300",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「うん、それがいいよ」"
  },
  "003329": {
    "context": "003329",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「ではお休み……」"
  },
  "003380": {
    "context": "003380",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "サリュはぽすんと、ソファに倒れこんだ。"
  },
  "003385": {
    "context": "003385",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "倒れると同時に、すでに寝息を立てている。"
  },
  "003396": {
    "context": "003396",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「くー……くー……」"
  },
  "003401": {
    "context": "003401",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "素晴しい寝つきの早さだった。"
  },
  "003406": {
    "context": "003406",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "目を閉じた瞬間に、すでに寝入ったようだ。"
  },
  "003417": {
    "context": "003417",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「……ふふ、こういうところはまるで子供だな」"
  },
  "003422": {
    "context": "003422",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "夏彦はなんだか愛娘を見るような気持ちで、その寝顔を見つめた。"
  },
  "003427": {
    "context": "003427",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "そして自分もそろそろ寝ようかと思った時――"
  },
  "003432": {
    "context": "003432",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "夏彦はふと、悠里の事が気になった。"
  },
  "003438": {
    "context": "003438",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "（向こうは大丈夫かな……）"
  },
  "003443": {
    "context": "003443",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "夏彦たちが巻いた保安課の連中が、天川家に押しかけて、悠里を詰問したりしてはいないだろうか。"
  },
  "003448": {
    "context": "003448",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "夏彦はだんだん、心配になってきた。"
  },
  "003453": {
    "context": "003453",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "それに夏彦たちが明日まで帰らなかったら、悠里も心配するだろう。"
  },
  "003459": {
    "context": "003459",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "（……１度電話を入れておこう）"
  },
  "003464": {
    "context": "003464",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "ＰＤＡを置いてきたのは、失敗だったかもしれない。"
  },
  "003469": {
    "context": "003469",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "夏彦は音を立てないよう、そっとその部屋を出た。"
  },
  "003573": {
    "context": "003573",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "――店を出た夏彦は、夜の街を見渡した。"
  },
  "003579": {
    "context": "003579",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "（公衆電話はと……）"
  },
  "003630": {
    "context": "003630",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "今どきそんなものはあまりないが、運よく店のすぐ近くに電話ボックスを見つけた。"
  },
  "003699": {
    "context": "003699",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "保安課や監視カメラに注意しながら、そこに駆け込む。"
  },
  "003704": {
    "context": "003704",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "そして受話器を取り、夏彦は自宅の電話番号をプッシュした。"
  },
  "003715": {
    "context": "003715",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "コール音が鳴る。"
  },
  "003720": {
    "context": "003720",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "しかし――悠里が電話に出ない。"
  },
  "003734": {
    "context": "003734",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "（まさか……保安課の連中に、連れて行かれかけたとか？）"
  },
  "003739": {
    "context": "003739",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "そうなれば彼女はパニック障害を起こし、自分を見失うほど泣き叫んでいるはずだ。"
  },
  "003744": {
    "context": "003744",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "コール音が鳴るたび、不安が急速に膨らんでいく。"
  },
  "003750": {
    "context": "003750",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "（出てくれ、悠里……！）"
  },
  "003755": {
    "context": "003755",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "夏彦がそう思った時、ようやく電話が繋がった。"
  },
  "003764": {
    "context": "003764",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "（……？　自分の部屋にいるのか？）"
  },
  "003769": {
    "context": "003769",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "電話はリビングにしかないため、彼女が自室にいれば、コール音が聞こえないという事もありうる。"
  },
  "003774": {
    "context": "003774",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "それでも辛抱強く待つと、やがて電話が繋がった。"
  },
  "003997": {
    "context": "003997",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「もしも～し」"
  },
  "004008": {
    "context": "004008",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「あ、悠里か？」"
  },
  "004037": {
    "context": "004037",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「ひこりん！」"
  },
  "004048": {
    "context": "004048",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「そっち大丈夫か？　あの後、保安課の連中が来たりしてないか？」"
  },
  "004077": {
    "context": "004077",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「ううん、心配しないで。何もなかったよー」"
  },
  "004082": {
    "context": "004082",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "悠里ののんびりした声を聞いて、夏彦は安心した。"
  },
  "004111": {
    "context": "004111",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「ひこりんたちこそ、あのあと大丈夫だった？」"
  },
  "004122": {
    "context": "004122",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「ああ、こっちは問題ない。ましろもサリュも捕まったりはしてないよ」"
  },
  "004151": {
    "context": "004151",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「今、どこにいるの？」"
  },
  "004162": {
    "context": "004162",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「ええと……」"
  },
  "004167": {
    "context": "004167",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "『繁華街のカラオケボックス』と答えそうになって、夏彦は思いとどまった。"
  },
  "004172": {
    "context": "004172",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "盗聴の危険性を感じたからだ。"
  },
  "004177": {
    "context": "004177",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "保安課もそこまではしないだろうが、用心するに越したことはないだろう。"
  },
  "004188": {
    "context": "004188",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「……街のどこかだよ」"
  },
  "004217": {
    "context": "004217",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「そう……」"
  },
  "004222": {
    "context": "004222",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "悠里も夏彦の気持ちを察したのか、深くは追及しなかった。"
  },
  "004233": {
    "context": "004233",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「まぁ心配する事はないけどさ、ちょっと今日は帰れそうにないんだ」"
  },
  "004294": {
    "context": "004294",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「ましろとサリュと一緒に、公園かどこかに野宿しようと思う」"
  },
  "004299": {
    "context": "004299",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "これは嘘だ。仮に盗聴されていた場合の、ちょっとした陽動作戦。"
  },
  "004328": {
    "context": "004328",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「公園？　野宿って事？　女の子２人も連れて大丈夫なの？」"
  },
  "004339": {
    "context": "004339",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「いやぁ、ましろはあれでなかなか肝の据わったヤツだし。サリュもああ見えてワイルドな娘なんだよ」"
  },
  "004368": {
    "context": "004368",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「え～？」"
  },
  "004386": {
    "context": "004386",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「なーに、心配しなくていいって。たまには外で夜を過ごすってのもいいもんだよ」"
  },
  "004415": {
    "context": "004415",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「ん、そうかもね……」"
  },
  "004420": {
    "context": "004420",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "夏彦の言葉に、悠里はふと柔らかい声で呟いた。"
  },
  "004449": {
    "context": "004449",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「……懐かしいね、ひこりん。ずっと前にも、そんな事があったね」"
  },
  "004460": {
    "context": "004460",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「ん？」"
  },
  "004489": {
    "context": "004489",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「覚えてる？　家出の時の事」"
  },
  "004494": {
    "context": "004494",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "悠里に問われる前に、夏彦は思い出していた。"
  },
  "004499": {
    "context": "004499",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "あの家出の結末を――"
  },
  "004510": {
    "context": "004510",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「……覚えてるよ」"
  },
  "004515": {
    "context": "004515",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "そう答える夏彦の胸に、遠い日の情景が蘇った。"
  },
  "004593": {
    "context": "004593",
    "code": "0x55",
    "developer_comment": "夏彦",
    "string": "√Before Chapter5"
  },
  "004763": {
    "context": "004763",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "（……あの頃もう母さんは、悠里を引き取ることを決めていたのかな……）"
  },
  "004768": {
    "context": "004768",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "まさか悠里の両親の死によって、それが実現するとは思わなかっただろうが、ありえる事のように思える。"
  },
  "004773": {
    "context": "004773",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "しかし当時は優しかった母も、やがて冷淡になっていき、めったに家に帰らなくなっていった。"
  },
  "004778": {
    "context": "004778",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "だからこそ夏彦と悠里は、実の家族以上に仲睦まじくなり、心許しあう間柄になったのだ。"
  },
  "004784": {
    "context": "004784",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "（互いに心配しすぎてしまうのは、そのせいだろうな……）"
  },
  "004789": {
    "context": "004789",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "そう思いつつ、夏彦は言う。"
  },
  "004803": {
    "context": "004803",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「ホントホント。昼間も警備員を投げ飛ばしたりしてたしさ」"
  },
  "004832": {
    "context": "004832",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「……ならいいんだけど、あんまり無茶はしないでね」"
  },
  "004837": {
    "context": "004837",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "心配そうな悠里に、夏彦は苦笑して言う。"
  },
  "004866": {
    "context": "004866",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「うーん……送り出しておいてなんだけど……やっぱり後から、だんだん心配になっちゃってね」"
  },
  "004877": {
    "context": "004877",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「そうか……」"
  },
  "004882": {
    "context": "004882",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "夏彦は若干申し訳なくなりつつ、続けた。"
  },
  "004893": {
    "context": "004893",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「……それにしても、悠里がぼくを送り出したのは、少し意外だったよ」"
  },
  "004922": {
    "context": "004922",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「え？」"
  },
  "004933": {
    "context": "004933",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「悠里はぼく以上に、事件とか危険とかそういうのが嫌いだと思ってたからさ」"
  },
  "004944": {
    "context": "004944",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「なんで今回に限って、あんなこと言ったんだい？　どういう心境の変化？」"
  },
  "004949": {
    "context": "004949",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "何気なく尋ねると、悠里は――"
  },
  "005028": {
    "context": "005028",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「……ひこりんが、その言葉を望んでいたからだよ」"
  },
  "005033": {
    "context": "005033",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "静かに、そう答えた。"
  },
  "005044": {
    "context": "005044",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「え……？」"
  },
  "005079": {
    "context": "005079",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「……実は前から、ずっと思ってた事があってね」"
  },
  "005090": {
    "context": "005090",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「ひこりんはさ、本当に今までみたいな穏やかな生活が好きなの？」"
  },
  "005119": {
    "context": "005119",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「わたしに合わせてただけなんじゃない……？」"
  },
  "005124": {
    "context": "005124",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "不意にそんな事を言われ、夏彦は息を呑んだ。"
  },
  "005138": {
    "context": "005138",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「そんな事はないよ。ぼく自身が本当に、穏やかな生活が好きなだけさ」"
  },
  "005167": {
    "context": "005167",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「それじゃあ今回みたいに、平穏が崩れるような事があったら……」"
  },
  "005196": {
    "context": "005196",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「やっぱり、嫌？」"
  },
  "005201": {
    "context": "005201",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "夏彦は少し考え、答えた。"
  },
  "005212": {
    "context": "005212",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「……そりゃあね」"
  },
  "005223": {
    "context": "005223",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「でもしかたないよ、巻き込まれてしまったものは……」"
  },
  "005228": {
    "context": "005228",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "そう答えた途端、いくつか気になる点が出てきた。"
  },
  "005234": {
    "context": "005234",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "（ましろが変な事に首を突っ込まなければ、こんなことにはならなかったとは思うけど……）"
  },
  "005240": {
    "context": "005240",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "（……いや、ましろよりサリュの方が問題か）"
  },
  "005246": {
    "context": "005246",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "（あいつが警備員を投げ飛ばしたりしなけりゃな……）"
  },
  "005251": {
    "context": "005251",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "夏彦がそう思うと、悠里も黙り込んだ。"
  },
  "005275": {
    "context": "005275",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「……」"
  },
  "005286": {
    "context": "005286",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「……あれ、どうした悠里？」"
  },
  "005291": {
    "context": "005291",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "悠里はしばらく黙り込んだ後で、夏彦に言った。"
  },
  "005320": {
    "context": "005320",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「……ひこりん、そんなんじゃダメだよ……！」"
  },
  "005331": {
    "context": "005331",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「え？」"
  },
  "005360": {
    "context": "005360",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「どうして２人を責めるの……！？」"
  },
  "005389": {
    "context": "005389",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「ましろんもサリュちゃんも、一生懸命自分のすべき事をしただけじゃない……！」"
  },
  "005394": {
    "context": "005394",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "そう言われて、夏彦は愕然とした。"
  },
  "005405": {
    "context": "005405",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「ちょ、ちょっと待ってくれ悠里！　ぼくは何も言ってないぞ！」"
  },
  "005434": {
    "context": "005434",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「うぅん、ひこりんは２人を責めているでしょ」"
  },
  "005445": {
    "context": "005445",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「どうしてわかる！？」"
  },
  "005474": {
    "context": "005474",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「わかるよ。だって――」"
  },
  "005502": {
    "context": "005502",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "悠里が何か言いかけた時、不意に通話が切れた。"
  },
  "005513": {
    "context": "005513",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「あ、あれ！？　もしもし！　もしもし！」"
  },
  "005518": {
    "context": "005518",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "夏彦は受話器にまくし立てる。"
  },
  "005523": {
    "context": "005523",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "答えは返ってこない。"
  },
  "005529": {
    "context": "005529",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "（……なんだそれ、どういう意味だ？）"
  },
  "005535": {
    "context": "005535",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "夏彦がそう思った瞬間――"
  },
  "005585": {
    "context": "005585",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "あたりの情景は、幻のように消えた。"
  },
  "005716": {
    "context": "005716",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「そんな事は……ないつもりだけど……」"
  },
  "005727": {
    "context": "005727",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「でも、どうして急にそんな事を言うんだ？」"
  },
  "005751": {
    "context": "005751",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「……」"
  },
  "005762": {
    "context": "005762",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「ぼくが望んでいたって……なんでそう思う？」"
  },
  "005773": {
    "context": "005773",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「……なんとなくだよ」"
  },
  "005778": {
    "context": "005778",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "悠里の言葉は曖昧だったが、確信的に喋っているようにも思えた。"
  },
  "005784": {
    "context": "005784",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "（……ぼくがそれを望んでたって……）"
  },
  "005790": {
    "context": "005790",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "（仮にそうだとしても、なんでそれがわかる？）"
  },
  "005795": {
    "context": "005795",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "そう考えていくうち、１つの疑惑が浮き上がってきた。"
  },
  "005801": {
    "context": "005801",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "（……まさか、悠里は……）"
  },
  "005806": {
    "context": "005806",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "だがその時、悠里がぽつりと呟いた。"
  },
  "005835": {
    "context": "005835",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「……違っていたら、ごめんだけど……」"
  },
  "005864": {
    "context": "005864",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「ひこりんはさ、考えたことない？　『昔の自分に戻りたい』って」"
  },
  "005875": {
    "context": "005875",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「昔の自分に？」"
  },
  "005904": {
    "context": "005904",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「わたしを気遣うあまり、ずっとできなかった事があるんじゃない……？」"
  },
  "005933": {
    "context": "005933",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「自分から危険に踏み込んだりとかさ……」"
  },
  "005944": {
    "context": "005944",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「そんなバカな？　わざわざそんな事しないよ」"
  },
  "005973": {
    "context": "005973",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「それが普通の答えだよね」"
  },
  "006002": {
    "context": "006002",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「だけど、わたしはね、思うんだ」"
  },
  "006031": {
    "context": "006031",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「本当のひこりんは、たとえその道がどれだけ危険だとしても……」"
  },
  "006042": {
    "context": "006042",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「……正しいと思った道を、進む人だって」"
  },
  "006076": {
    "context": "006076",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「あ、おい、悠里！？」"
  },
  "006146": {
    "context": "006146",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "電話はそこで、不意に切れた。"
  },
  "006152": {
    "context": "006152",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "街の喧騒もいつの間にか消えていて、突如静寂が夏彦を包む。"
  },
  "006167": {
    "context": "006167",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "夏彦の腕時計の音だけが、静寂の中に響く。"
  },
  "006173": {
    "context": "006173",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "だがすぐに、その音すらも止んだ。"
  },
  "006179": {
    "context": "006179",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "（……またか）"
  },
  "006391": {
    "context": "006391",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "見れば時計は、やはり壊れていた。"
  },
  "006397": {
    "context": "006397",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "（また現実に戻るのか……）"
  },
  "006402": {
    "context": "006402",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "もうわかっている。"
  },
  "006407": {
    "context": "006407",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "いままで見ていたのは、９月１４日の回想だ。"
  },
  "006412": {
    "context": "006412",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "そして徐々に、過去の回想は、現在進行している現実と近づきつつある。"
  },
  "006418": {
    "context": "006418",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "（時計が壊れた、あの日に……）"
  },
  "006466": {
    "context": "006466",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "完全な静寂の中、夏彦の意識は――９月１６日に戻った。"
  }
}