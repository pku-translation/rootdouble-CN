{
  "000089": {
    "context": "000089",
    "code": "0x55",
    "string": "√Before Chapter6"
  },
  "000116": {
    "context": "000116",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「……！」"
  },
  "000121": {
    "context": "000121",
    "code": "0x45",
    "developer_comment": "",
    "string": "夏彦は昨日の事を、克明に思い出した。"
  },
  "000126": {
    "context": "000126",
    "code": "0x45",
    "developer_comment": "",
    "string": "真実を知り、敵と対峙し、敗北した日の記憶を。"
  },
  "000131": {
    "context": "000131",
    "code": "0x45",
    "developer_comment": "",
    "string": "そして訪れた今日――。"
  },
  "000136": {
    "context": "000136",
    "code": "0x45",
    "developer_comment": "",
    "string": "目の前のゲートは閉ざされ、夏彦は悠里やましろと共に、苦境に陥っていた。"
  },
  "000141": {
    "context": "000141",
    "code": "0x45",
    "developer_comment": "",
    "string": "だが、それでもなお――"
  },
  "000147": {
    "context": "000147",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "（諦めてたまるか……！）"
  },
  "000152": {
    "context": "000152",
    "code": "0x45",
    "developer_comment": "",
    "string": "彼はそう思っていた。"
  },
  "000157": {
    "context": "000157",
    "code": "0x45",
    "developer_comment": "",
    "string": "傍らに立つ悠里が、夏彦にそっと囁く。"
  },
  "000221": {
    "context": "000221",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「……ひこりん、思い出したでしょ」"
  },
  "000232": {
    "context": "000232",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「たくさんの障害を越えて、ひこりんはここにやってきた……」"
  },
  "000261": {
    "context": "000261",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「ひこりんは、過去を乗り越えられる力を手に入れたんだよ」"
  },
  "000272": {
    "context": "000272",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「これから何が起ころうと、ひこりんの心は折れない。どんな事も乗り越えられるはずだよ」"
  },
  "000277": {
    "context": "000277",
    "code": "0x45",
    "developer_comment": "",
    "string": "その言葉が、夏彦の心をなお強くする。"
  },
  "000282": {
    "context": "000282",
    "code": "0x45",
    "developer_comment": "",
    "string": "頭痛はまだ続いていたが、奥歯を噛んでそれに耐えた。"
  },
  "000287": {
    "context": "000287",
    "code": "0x45",
    "developer_comment": "",
    "string": "黙りこくる夏彦に、ましろが声をかけてくる。"
  },
  "000339": {
    "context": "000339",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「夏彦、悠里と何を話しているの！？」"
  },
  "000344": {
    "context": "000344",
    "code": "0x45",
    "developer_comment": "",
    "string": "夏彦は笑みを返し、彼女に答える。"
  },
  "000355": {
    "context": "000355",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「……勇気を貰ってたんだ」"
  },
  "000384": {
    "context": "000384",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「え？」"
  },
  "000395": {
    "context": "000395",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「ましろ、まだ諦めちゃダメだ。脱出の道はきっとある」"
  },
  "000424": {
    "context": "000424",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「で、でも、ゲートが開かないんじゃどこにも……」"
  },
  "000435": {
    "context": "000435",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「いや……『セキュリティカード』を探すんだ。カードさえあれば、第６エリアに行ける」"
  },
  "000532": {
    "context": "000532",
    "code": "0x45",
    "developer_comment": "",
    "string": "見ればゲートの横に、カードを通すスロットがあった。"
  },
  "000537": {
    "context": "000537",
    "code": "0x45",
    "developer_comment": "",
    "string": "夏彦たちはセキュリティカードを持っていないが、それが見つかればゲートも開けられるのだろう。"
  },
  "000564": {
    "context": "000564",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「警報が鳴った直後、施設内はあれだけパニックになっていた」"
  },
  "000575": {
    "context": "000575",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「職員の誰かが、カードを忘れていってもおかしくない！」"
  },
  "000608": {
    "context": "000608",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「そ、そうか！」"
  },
  "000613": {
    "context": "000613",
    "code": "0x45",
    "developer_comment": "",
    "string": "ましろの声に、わずかに元気が戻る。"
  },
  "000618": {
    "context": "000618",
    "code": "0x45",
    "developer_comment": "",
    "string": "一縷の希望かもしれないが、それに賭けるしかなかった。"
  },
  "000624": {
    "context": "000624",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "（絶対カードを見つけてやる……ぼくが２人を、必ず脱出させてみせる！）"
  },
  "000629": {
    "context": "000629",
    "code": "0x45",
    "developer_comment": "",
    "string": "夏彦はそう思い、再び駆け出した。"
  },
  "000762": {
    "context": "000762",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「手分けして探そう！　ましろはそっちの部屋を頼む！」"
  },
  "000791": {
    "context": "000791",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「わ、わかったよ！」"
  },
  "000802": {
    "context": "000802",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「じゃあひこりんは、奥の方の部屋をお願い！」"
  },
  "000813": {
    "context": "000813",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「あぁ、任せろ！」"
  },
  "000818": {
    "context": "000818",
    "code": "0x45",
    "developer_comment": "",
    "string": "夏彦は悠里の声に従い、その部屋に飛び込んだ。"
  },
  "000910": {
    "context": "000910",
    "code": "0x45",
    "developer_comment": "",
    "string": "そこは６畳程度の、小さな個人研究室だった。"
  },
  "000941": {
    "context": "000941",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "（散らかってるな……これならもしかしたら、どこかにカードがあるかもしれない）"
  },
  "000946": {
    "context": "000946",
    "code": "0x45",
    "developer_comment": "",
    "string": "夏彦はさっそく、カードを探し始めた。"
  },
  "000951": {
    "context": "000951",
    "code": "0x45",
    "developer_comment": "",
    "string": "まずは机の引き出しを探る。"
  },
  "000995": {
    "context": "000995",
    "code": "0x45",
    "developer_comment": "",
    "string": "だがそこには、なぜか鉄製の箱が詰まっているだけで、カードらしきものはなかった。"
  },
  "001010": {
    "context": "001010",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「ちっ！」"
  },
  "001024": {
    "context": "001024",
    "code": "0x45",
    "developer_comment": "",
    "string": "引き出しを早々に見限り、机の上に目をやる。"
  },
  "001029": {
    "context": "001029",
    "code": "0x45",
    "developer_comment": "",
    "string": "だがその時、そこにあるものに目が止まった。"
  },
  "001040": {
    "context": "001040",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「え……！？」"
  },
  "001045": {
    "context": "001045",
    "code": "0x45",
    "developer_comment": "",
    "string": "机の上に、デジタルフォトフレームが飾られていた。"
  },
  "001050": {
    "context": "001050",
    "code": "0x45",
    "developer_comment": "",
    "string": "そこに表示されている写真――それは夏彦自身の写真だった。"
  },
  "001061": {
    "context": "001061",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「なんで、ぼくの写真が……！？」"
  },
  "001066": {
    "context": "001066",
    "code": "0x45",
    "developer_comment": "",
    "string": "写真を見ているうち、それを撮られた時の事を思い出した。"
  },
  "001071": {
    "context": "001071",
    "code": "0x45",
    "developer_comment": "",
    "string": "高校の入学日の夕方、珍しく家に帰ってきた母が、ＰＤＡのカメラ機能でさっと撮ったものだ。"
  },
  "001082": {
    "context": "001082",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "（ッ！　まさかこの部屋、母さんの個人研究室か！？）"
  },
  "001087": {
    "context": "001087",
    "code": "0x45",
    "developer_comment": "",
    "string": "夏彦は息を呑んだ。"
  },
  "001092": {
    "context": "001092",
    "code": "0x45",
    "developer_comment": "",
    "string": "だがその写真を見ているうち、なぜか凄まじい違和感が襲ってきた。"
  },
  "001098": {
    "context": "001098",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "（……！？）"
  },
  "001103": {
    "context": "001103",
    "code": "0x45",
    "developer_comment": "",
    "string": "ソファに夏彦が、１人で座っている。"
  },
  "001108": {
    "context": "001108",
    "code": "0x45",
    "developer_comment": "",
    "string": "急に母にＰＤＡを向けられて、戸惑った事を覚えている。"
  },
  "001114": {
    "context": "001114",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "（……おかしい……何か変だ！）"
  },
  "001119": {
    "context": "001119",
    "code": "0x45",
    "developer_comment": "",
    "string": "夏彦は違和感に耐え切れず、写真から目を逸らした。"
  },
  "001124": {
    "context": "001124",
    "code": "0x45",
    "developer_comment": "",
    "string": "すると今度は、戸棚に入っているファイルが目に入った。"
  },
  "001129": {
    "context": "001129",
    "code": "0x45",
    "developer_comment": "",
    "string": "ファイルされた年別に、整理されているらしい。"
  },
  "001134": {
    "context": "001134",
    "code": "0x45",
    "developer_comment": "",
    "string": "その中には、『２０２１年』というファイルが――"
  },
  "001140": {
    "context": "001140",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "（……！）"
  },
  "001145": {
    "context": "001145",
    "code": "0x45",
    "developer_comment": "",
    "string": "そんな事をしている場合ではない。"
  },
  "001150": {
    "context": "001150",
    "code": "0x45",
    "developer_comment": "",
    "string": "だが夏彦は吸い寄せられるように、ファイルに手を伸ばした。"
  },
  "001156": {
    "context": "001156",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "（もしかしたら、ここに何か答えが……！）"
  },
  "001162": {
    "context": "001162",
    "code": "0x45",
    "developer_comment": "",
    "string": "そう思った時、気配を感じた。"
  },
  "001250": {
    "context": "001250",
    "code": "0x45",
    "developer_comment": "",
    "string": "はっとして顔を上げると、部屋の入口に悠里が立っていた。"
  },
  "001267": {
    "context": "001267",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「……悠里……！」"
  },
  "001296": {
    "context": "001296",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「……？」"
  },
  "001301": {
    "context": "001301",
    "code": "0x45",
    "developer_comment": "",
    "string": "悠里は何も言わず、小首を傾げた。"
  },
  "001306": {
    "context": "001306",
    "code": "0x45",
    "developer_comment": "",
    "string": "夏彦は彼女に問いかける。"
  },
  "001317": {
    "context": "001317",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「君はこの事を知っていて、ぼくをこの部屋に入れたのか！？」"
  },
  "001346": {
    "context": "001346",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「わたしがひこりんを……？　違うよ、ひこりんが自分でこの部屋を選んだんだよ」"
  },
  "001357": {
    "context": "001357",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「……？　どういう意味だ？」"
  },
  "001386": {
    "context": "001386",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「９年前も、この部屋に来たでしょ？　火と煙に追われて……」"
  },
  "001397": {
    "context": "001397",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「……！」"
  },
  "001408": {
    "context": "001408",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「あの時、カードは見つけられなかったね。だけど今回は、必ず見つかる」"
  },
  "001419": {
    "context": "001419",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「どうしてわかる！？」"
  },
  "001430": {
    "context": "001430",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「見落としているだけなんだよ。ひこりんはすでに、カードのありかを知ってるはずだよ」"
  },
  "001435": {
    "context": "001435",
    "code": "0x45",
    "developer_comment": "",
    "string": "悠里は夏彦を見つめて言う。"
  },
  "001464": {
    "context": "001464",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「それを探して……ましろんと一緒に逃げて」"
  },
  "001475": {
    "context": "001475",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「ましろと、って……悠里はどうするんだ？」"
  },
  "001504": {
    "context": "001504",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「わたしは一緒に行けないの」"
  },
  "001515": {
    "context": "001515",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「どうして！？」"
  },
  "001544": {
    "context": "001544",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「ひこりんもそろそろ、気付いているんじゃない？」"
  },
  "001549": {
    "context": "001549",
    "code": "0x45",
    "developer_comment": "",
    "string": "悠里は寂しげに微笑んだ。"
  },
  "001555": {
    "context": "001555",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "（……どういう事だ）"
  },
  "001561": {
    "context": "001561",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "（知っているって言うのか……ぼくがその理由を？）"
  },
  "001566": {
    "context": "001566",
    "code": "0x45",
    "developer_comment": "",
    "string": "だが徐々に、夏彦は気づき始めていた。"
  },
  "001571": {
    "context": "001571",
    "code": "0x45",
    "developer_comment": "",
    "string": "それまで見てきたものが、聞いてきた言葉が、感じてきた思いが――"
  },
  "001576": {
    "context": "001576",
    "code": "0x45",
    "developer_comment": "",
    "string": "全ての記憶が少しずつ、真実に近づきつつある事を。"
  },
  "001581": {
    "context": "001581",
    "code": "0x45",
    "developer_comment": "",
    "string": "回想と現実の、時間は重なりつつある事を。"
  },
  "001586": {
    "context": "001586",
    "code": "0x45",
    "developer_comment": "",
    "string": "夏彦を急かすように、時計がまた動き出す。"
  },
  "001591": {
    "context": "001591",
    "code": "0x45",
    "developer_comment": "",
    "string": "――日付表示は『９月１６日』。"
  },
  "001596": {
    "context": "001596",
    "code": "0x45",
    "developer_comment": "",
    "string": "今日の『３時１９分』を指していた。"
  },
  "001625": {
    "context": "001625",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「……ねぇひこりん、今日見聞きした事を思い出して」"
  },
  "001636": {
    "context": "001636",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「そこに真実はあるから……」"
  },
  "001641": {
    "context": "001641",
    "code": "0x45",
    "developer_comment": "",
    "string": "そして悠里のささやきと共に、夏彦の意識は――"
  },
  "001646": {
    "context": "001646",
    "code": "0x45",
    "developer_comment": "",
    "string": "４時間分、過去に遡った。"
  }
}