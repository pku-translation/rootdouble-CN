{
  "000051": {
    "context": "000051",
    "code": "0x55",
    "developer_comment": "",
    "string": "√Before Chapter6"
  },
  "000078": {
    "context": "000078",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「……！」"
  },
  "000083": {
    "context": "000083",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "夏彦は昨日の事を、克明に思い出した。"
  },
  "000088": {
    "context": "000088",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "真実を知り、敵と対峙し、敗北した日の記憶を。"
  },
  "000093": {
    "context": "000093",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "そして訪れた今日――。"
  },
  "000098": {
    "context": "000098",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "目の前のゲートは閉ざされ、夏彦は悠里やましろと共に、苦境に陥っていた。"
  },
  "000103": {
    "context": "000103",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "だが、それでもなお――"
  },
  "000109": {
    "context": "000109",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "（諦めてたまるか……！）"
  },
  "000114": {
    "context": "000114",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "彼はそう思っていた。"
  },
  "000119": {
    "context": "000119",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "傍らに立つ悠里が、夏彦にそっと囁く。"
  },
  "000183": {
    "context": "000183",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「……ひこりん、思い出したでしょ」"
  },
  "000194": {
    "context": "000194",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「たくさんの障害を越えて、ひこりんはここにやってきた……」"
  },
  "000223": {
    "context": "000223",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「ひこりんは、過去を乗り越えられる力を手に入れたんだよ」"
  },
  "000234": {
    "context": "000234",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「これから何が起ころうと、ひこりんの心は折れない。どんな事も乗り越えられるはずだよ」"
  },
  "000239": {
    "context": "000239",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "その言葉が、夏彦の心をなお強くする。"
  },
  "000244": {
    "context": "000244",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "頭痛はまだ続いていたが、奥歯を噛んでそれに耐えた。"
  },
  "000249": {
    "context": "000249",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "黙りこくる夏彦に、ましろが声をかけてくる。"
  },
  "000301": {
    "context": "000301",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「夏彦、悠里と何を話しているの！？」"
  },
  "000306": {
    "context": "000306",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "夏彦は笑みを返し、彼女に答える。"
  },
  "000317": {
    "context": "000317",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「……勇気を貰ってたんだ」"
  },
  "000346": {
    "context": "000346",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「え？」"
  },
  "000357": {
    "context": "000357",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「ましろ、まだ諦めちゃダメだ。脱出の道はきっとある」"
  },
  "000386": {
    "context": "000386",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「で、でも、ゲートが開かないんじゃどこにも……」"
  },
  "000397": {
    "context": "000397",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「いや……『セキュリティカード』を探すんだ。カードさえあれば、第６エリアに行ける」"
  },
  "000494": {
    "context": "000494",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "見ればゲートの横に、カードを通すスロットがあった。"
  },
  "000499": {
    "context": "000499",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "夏彦たちはセキュリティカードを持っていないが、それが見つかればゲートも開けられるのだろう。"
  },
  "000526": {
    "context": "000526",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「警報が鳴った直後、施設内はあれだけパニックになっていた」"
  },
  "000537": {
    "context": "000537",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「職員の誰かが、カードを忘れていってもおかしくない！」"
  },
  "000570": {
    "context": "000570",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「そ、そうか！」"
  },
  "000575": {
    "context": "000575",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "ましろの声に、わずかに元気が戻る。"
  },
  "000580": {
    "context": "000580",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "一縷の希望かもしれないが、それに賭けるしかなかった。"
  },
  "000586": {
    "context": "000586",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "（絶対カードを見つけてやる……ぼくが２人を、必ず脱出させてみせる！）"
  },
  "000591": {
    "context": "000591",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "夏彦はそう思い、再び駆け出した。"
  },
  "000724": {
    "context": "000724",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「手分けして探そう！　ましろはそっちの部屋を頼む！」"
  },
  "000753": {
    "context": "000753",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「わ、わかったよ！」"
  },
  "000764": {
    "context": "000764",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「じゃあひこりんは、奥の方の部屋をお願い！」"
  },
  "000775": {
    "context": "000775",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「あぁ、任せろ！」"
  },
  "000780": {
    "context": "000780",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "夏彦は悠里の声に従い、その部屋に飛び込んだ。"
  },
  "000872": {
    "context": "000872",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "そこは６畳程度の、小さな個人研究室だった。"
  },
  "000903": {
    "context": "000903",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "（散らかってるな……これならもしかしたら、どこかにカードがあるかもしれない）"
  },
  "000908": {
    "context": "000908",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "夏彦はさっそく、カードを探し始めた。"
  },
  "000913": {
    "context": "000913",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "まずは机の引き出しを探る。"
  },
  "000957": {
    "context": "000957",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "だがそこには、なぜか鉄製の箱が詰まっているだけで、カードらしきものはなかった。"
  },
  "000972": {
    "context": "000972",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「ちっ！」"
  },
  "000986": {
    "context": "000986",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "引き出しを早々に見限り、机の上に目をやる。"
  },
  "000991": {
    "context": "000991",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "だがその時、そこにあるものに目が止まった。"
  },
  "001002": {
    "context": "001002",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「え……！？」"
  },
  "001007": {
    "context": "001007",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "机の上に、デジタルフォトフレームが飾られていた。"
  },
  "001012": {
    "context": "001012",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "そこに表示されている写真――それは夏彦自身の写真だった。"
  },
  "001023": {
    "context": "001023",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「なんで、ぼくの写真が……！？」"
  },
  "001028": {
    "context": "001028",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "写真を見ているうち、それを撮られた時の事を思い出した。"
  },
  "001033": {
    "context": "001033",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "高校の入学日の夕方、珍しく家に帰ってきた母が、ＰＤＡのカメラ機能でさっと撮ったものだ。"
  },
  "001044": {
    "context": "001044",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "（ッ！　まさかこの部屋、母さんの個人研究室か！？）"
  },
  "001049": {
    "context": "001049",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "夏彦は息を呑んだ。"
  },
  "001054": {
    "context": "001054",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "だがその写真を見ているうち、なぜか凄まじい違和感が襲ってきた。"
  },
  "001060": {
    "context": "001060",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "（……！？）"
  },
  "001065": {
    "context": "001065",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "ソファに夏彦が、１人で座っている。"
  },
  "001070": {
    "context": "001070",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "急に母にＰＤＡを向けられて、戸惑った事を覚えている。"
  },
  "001076": {
    "context": "001076",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "（……おかしい……何か変だ！）"
  },
  "001081": {
    "context": "001081",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "夏彦は違和感に耐え切れず、写真から目を逸らした。"
  },
  "001086": {
    "context": "001086",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "すると今度は、戸棚に入っているファイルが目に入った。"
  },
  "001091": {
    "context": "001091",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "ファイルされた年別に、整理されているらしい。"
  },
  "001096": {
    "context": "001096",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "その中には、『２０２１年』というファイルが――"
  },
  "001102": {
    "context": "001102",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "（……！）"
  },
  "001107": {
    "context": "001107",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "そんな事をしている場合ではない。"
  },
  "001112": {
    "context": "001112",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "だが夏彦は吸い寄せられるように、ファイルに手を伸ばした。"
  },
  "001118": {
    "context": "001118",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "（もしかしたら、ここに何か答えが……！）"
  },
  "001124": {
    "context": "001124",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "そう思った時、気配を感じた。"
  },
  "001212": {
    "context": "001212",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "はっとして顔を上げると、部屋の入口に悠里が立っていた。"
  },
  "001229": {
    "context": "001229",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「……悠里……！」"
  },
  "001258": {
    "context": "001258",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「……？」"
  },
  "001263": {
    "context": "001263",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "悠里は何も言わず、小首を傾げた。"
  },
  "001268": {
    "context": "001268",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "夏彦は彼女に問いかける。"
  },
  "001279": {
    "context": "001279",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「君はこの事を知っていて、ぼくをこの部屋に入れたのか！？」"
  },
  "001308": {
    "context": "001308",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「わたしがひこりんを……？　違うよ、ひこりんが自分でこの部屋を選んだんだよ」"
  },
  "001319": {
    "context": "001319",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「……？　どういう意味だ？」"
  },
  "001348": {
    "context": "001348",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「９年前も、この部屋に来たでしょ？　火と煙に追われて……」"
  },
  "001359": {
    "context": "001359",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「……！」"
  },
  "001370": {
    "context": "001370",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「あの時、カードは見つけられなかったね。だけど今回は、必ず見つかる」"
  },
  "001381": {
    "context": "001381",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「どうしてわかる！？」"
  },
  "001392": {
    "context": "001392",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「見落としているだけなんだよ。ひこりんはすでに、カードのありかを知ってるはずだよ」"
  },
  "001397": {
    "context": "001397",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "悠里は夏彦を見つめて言う。"
  },
  "001426": {
    "context": "001426",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「それを探して……ましろんと一緒に逃げて」"
  },
  "001437": {
    "context": "001437",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「ましろと、って……悠里はどうするんだ？」"
  },
  "001466": {
    "context": "001466",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「わたしは一緒に行けないの」"
  },
  "001477": {
    "context": "001477",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「どうして！？」"
  },
  "001506": {
    "context": "001506",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「ひこりんもそろそろ、気付いているんじゃない？」"
  },
  "001511": {
    "context": "001511",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "悠里は寂しげに微笑んだ。"
  },
  "001517": {
    "context": "001517",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "（……どういう事だ）"
  },
  "001523": {
    "context": "001523",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "（知っているって言うのか……ぼくがその理由を？）"
  },
  "001528": {
    "context": "001528",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "だが徐々に、夏彦は気づき始めていた。"
  },
  "001533": {
    "context": "001533",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "それまで見てきたものが、聞いてきた言葉が、感じてきた思いが――"
  },
  "001538": {
    "context": "001538",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "全ての記憶が少しずつ、真実に近づきつつある事を。"
  },
  "001543": {
    "context": "001543",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "回想と現実の、時間は重なりつつある事を。"
  },
  "001548": {
    "context": "001548",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "夏彦を急かすように、時計がまた動き出す。"
  },
  "001553": {
    "context": "001553",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "――日付表示は『９月１６日』。"
  },
  "001558": {
    "context": "001558",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "今日の『３時１９分』を指していた。"
  },
  "001587": {
    "context": "001587",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「……ねぇひこりん、今日見聞きした事を思い出して」"
  },
  "001598": {
    "context": "001598",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「そこに真実はあるから……」"
  },
  "001603": {
    "context": "001603",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "そして悠里のささやきと共に、夏彦の意識は――"
  },
  "001608": {
    "context": "001608",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "４時間分、過去に遡った。"
  }
}