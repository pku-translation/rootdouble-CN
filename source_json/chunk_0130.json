{
  "000366": {
    "context": "000366",
    "code": "0x55",
    "string": "√Before Chapter0"
  },
  "000374": {
    "context": "000374",
    "code": "0x86",
    "string": "You have taken my friends from me."
  },
  "000375": {
    "context": "000375",
    "code": "0x86",
    "string": "You have made me an abomination to them."
  },
  "000376": {
    "context": "000376",
    "code": "0x86",
    "string": "Ⅰ am confined, and Ⅰ can't escape."
  },
  "000377": {
    "context": "000377",
    "code": "0x86",
    "string": "(The Old Testament / Psalms 88:8)"
  },
  "000379": {
    "context": "000379",
    "code": "0x45",
    "developer_comment": "",
    "string": "――その轟音は、終末を報せる鐘のように――"
  },
  "000384": {
    "context": "000384",
    "code": "0x45",
    "developer_comment": "",
    "string": "夜明けの研究所に響き渡った。"
  },
  "000504": {
    "context": "000504",
    "code": "0x45",
    "developer_comment": "少年",
    "string": "「なッ！？」"
  },
  "000509": {
    "context": "000509",
    "code": "0x45",
    "developer_comment": "",
    "string": "少年が驚愕に息を呑む。"
  },
  "000602": {
    "context": "000602",
    "code": "0x45",
    "developer_comment": "少女１",
    "string": "「きゃあッ！」"
  },
  "000607": {
    "context": "000607",
    "code": "0x45",
    "developer_comment": "",
    "string": "傍らの少女たちが悲鳴を上げる。"
  },
  "000651": {
    "context": "000651",
    "code": "0x45",
    "developer_comment": "少女２",
    "string": "「くッ……！」"
  },
  "000656": {
    "context": "000656",
    "code": "0x45",
    "developer_comment": "",
    "string": "床から振動が伝わるのを感じた。"
  },
  "000845": {
    "context": "000845",
    "code": "0x45",
    "developer_comment": "少女２",
    "string": "「……爆弾、それも複数……！？」"
  },
  "000856": {
    "context": "000856",
    "code": "0x45",
    "developer_comment": "少女１",
    "string": "「な、夏彦、これって……！」"
  },
  "000933": {
    "context": "000933",
    "code": "0x45",
    "developer_comment": "",
    "string": "傍らの少女たちの言葉に、『夏彦』と呼ばれた少年が歯噛みする。"
  },
  "000962": {
    "context": "000962",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「あぁ、わかってる……」"
  },
  "000980": {
    "context": "000980",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「……ぼくらは、間に合わなかったんだ」"
  },
  "000985": {
    "context": "000985",
    "code": "0x45",
    "developer_comment": "",
    "string": "だがその言葉は、けたたましく響く警報にかき消された。"
  },
  "002220": {
    "context": "002220",
    "code": "0x45",
    "developer_comment": "",
    "string": "自動放送が冷徹に、警告を叫ぶ。"
  },
  "002228": {
    "context": "002228",
    "code": "0x45",
    "developer_comment": "自動放送",
    "string": "「施設内のセキュリティを、『避難モード』に移行。Ｎエリアを除く全ゲートを開放します」"
  },
  "002236": {
    "context": "002236",
    "code": "0x45",
    "developer_comment": "自動放送",
    "string": "「職員の皆様は、落ち着いて地上へ避難して下さい」"
  },
  "002357": {
    "context": "002357",
    "code": "0x45",
    "developer_comment": "",
    "string": "断片的な情報に、誰もが戸惑いの声を上げる。"
  },
  "002365": {
    "context": "002365",
    "code": "0x45",
    "developer_comment": "研究員１",
    "string": "「な、何が起きたんだ！」"
  },
  "002373": {
    "context": "002373",
    "code": "0x45",
    "developer_comment": "研究員２",
    "string": "「わかりません！　爆発のようですが……？」"
  },
  "002381": {
    "context": "002381",
    "code": "0x45",
    "developer_comment": "研究員３",
    "string": "「爆発だと！？　この施設でか！？」"
  },
  "002386": {
    "context": "002386",
    "code": "0x45",
    "developer_comment": "",
    "string": "その問いに答えるように、放送が続く。"
  },
  "002486": {
    "context": "002486",
    "code": "0x45",
    "developer_comment": "男職員",
    "string": "「き、緊急放送です！」"
  },
  "002497": {
    "context": "002497",
    "code": "0x45",
    "developer_comment": "男職員",
    "string": "「ただいま施設内で、原因不明の爆発が発生しました！」"
  },
  "002508": {
    "context": "002508",
    "code": "0x45",
    "developer_comment": "男職員",
    "string": "「現状判明している限り、施設内の１０数か所で同時に爆発が起き、それに伴い火災も発生しています！」"
  },
  "002519": {
    "context": "002519",
    "code": "0x45",
    "developer_comment": "男職員",
    "string": "「江里口研究員、柏木研究員！　至急第２エリアに来てください！」"
  },
  "002612": {
    "context": "002612",
    "code": "0x45",
    "developer_comment": "職員１",
    "string": "「１０数か所で爆発だと……！？」"
  },
  "002620": {
    "context": "002620",
    "code": "0x45",
    "developer_comment": "職員２",
    "string": "「原因不明！？　どういう事だ！？」"
  },
  "002625": {
    "context": "002625",
    "code": "0x45",
    "developer_comment": "",
    "string": "戦慄する男たちの横で、女科学者が呟いた。"
  },
  "002714": {
    "context": "002714",
    "code": "0x45",
    "developer_comment": "女科学者",
    "string": "「大丈夫よ……！　この施設には二重三重の防災システムが配備されているんだから」"
  },
  "002725": {
    "context": "002725",
    "code": "0x45",
    "developer_comment": "女科学者",
    "string": "「何が起こっても、大規模な災害になったりはしないはず」"
  },
  "002754": {
    "context": "002754",
    "code": "0x45",
    "developer_comment": "女科学者",
    "string": "「安全は確保されているの……安心して」"
  },
  "002759": {
    "context": "002759",
    "code": "0x45",
    "developer_comment": "",
    "string": "その声はあくまで穏やかだった。怯える者を勇気づけるように。"
  },
  "002877": {
    "context": "002877",
    "code": "0x45",
    "developer_comment": "",
    "string": "――だが、その希望は踏みにじられる。"
  },
  "002966": {
    "context": "002966",
    "code": "0x45",
    "developer_comment": "警備員１",
    "string": "「消火システムが作動しない！？」"
  },
  "002974": {
    "context": "002974",
    "code": "0x45",
    "developer_comment": "警備員２",
    "string": "「なぜだ……くそッ！　所員の避難を急がせろ！」"
  },
  "002979": {
    "context": "002979",
    "code": "0x45",
    "developer_comment": "",
    "string": "頼みの綱のシステムは、まるで役に立たなかった。"
  },
  "003062": {
    "context": "003062",
    "code": "0x45",
    "developer_comment": "職員１",
    "string": "「ファクトリーでも出火を確認！　開発中の機材が炎上してます！」"
  },
  "003070": {
    "context": "003070",
    "code": "0x45",
    "developer_comment": "職員２",
    "string": "「ふざけるな！　開発にいくらかかってると思ってる！」"
  },
  "003078": {
    "context": "003078",
    "code": "0x45",
    "developer_comment": "職員２",
    "string": "「燃えましたで済ませられるか、人力で消火しろ！」"
  },
  "003086": {
    "context": "003086",
    "code": "0x45",
    "developer_comment": "職員１",
    "string": "「４箇所で同時出火してるんですよ！？　誰が消火できるっていうんですか！」"
  },
  "003202": {
    "context": "003202",
    "code": "0x45",
    "developer_comment": "",
    "string": "混乱と狂騒が、施設内を支配する。"
  },
  "003207": {
    "context": "003207",
    "code": "0x45",
    "developer_comment": "",
    "string": "不安が膨らみ、張り詰めていく。"
  },
  "003212": {
    "context": "003212",
    "code": "0x45",
    "developer_comment": "",
    "string": "それにとどめを刺すかのように、新たな宣告がなされた。"
  },
  "003311": {
    "context": "003311",
    "code": "0x45",
    "developer_comment": "男職員",
    "string": "「Ｎエリアの数値増幅を確認……！　緊急停止システムを始動しました！」"
  },
  "003322": {
    "context": "003322",
    "code": "0x45",
    "developer_comment": "男職員",
    "string": "「Ｎエリア及び制御室の職員も、全員ただちに地上へ避難してください！」"
  },
  "003400": {
    "context": "003400",
    "code": "0x45",
    "developer_comment": "",
    "string": "――放送が終わった瞬間、静寂はパニックに変わった。"
  },
  "003408": {
    "context": "003408",
    "code": "0x45",
    "developer_comment": "研究員１",
    "string": "「す……数値増幅！？」"
  },
  "003416": {
    "context": "003416",
    "code": "0x45",
    "developer_comment": "研究員３",
    "string": "「と、とにかく地上へ！　遠隔制御室へ！」"
  },
  "003424": {
    "context": "003424",
    "code": "0x45",
    "developer_comment": "研究員２",
    "string": "「エレベーターは駄目です！　階段ッ！　階段です！」"
  },
  "003429": {
    "context": "003429",
    "code": "0x45",
    "developer_comment": "",
    "string": "誰もが口々に喚き散らす。"
  },
  "003510": {
    "context": "003510",
    "code": "0x45",
    "developer_comment": "研究員１",
    "string": "「ＡＤを投与しろ！　備蓄してあるはずだろう！」"
  },
  "003518": {
    "context": "003518",
    "code": "0x45",
    "developer_comment": "研究員２",
    "string": "「まさかこれを使う事になるとは……！」"
  },
  "003526": {
    "context": "003526",
    "code": "0x45",
    "developer_comment": "研究員１",
    "string": "「迷うな！　マニュアル通りに行動しろ！」"
  },
  "003531": {
    "context": "003531",
    "code": "0x45",
    "developer_comment": "",
    "string": "可能な限り、しかるべき対応をしようとする。"
  },
  "003536": {
    "context": "003536",
    "code": "0x45",
    "developer_comment": "",
    "string": "だがその時には、全てが遅かった。"
  },
  "003617": {
    "context": "003617",
    "code": "0x45",
    "developer_comment": "研究員１",
    "string": "「非常階段が崩れ落ちただと！？」"
  },
  "003625": {
    "context": "003625",
    "code": "0x45",
    "developer_comment": "研究員２",
    "string": "「エレベーターも全て停止しています！　避難の術がありません！」"
  },
  "003633": {
    "context": "003633",
    "code": "0x45",
    "developer_comment": "研究員３",
    "string": "「バカな……なんでそんな事に！」"
  },
  "003679": {
    "context": "003679",
    "code": "0x45",
    "developer_comment": "",
    "string": "脱出口は絶たれていた。"
  },
  "003684": {
    "context": "003684",
    "code": "0x45",
    "developer_comment": "",
    "string": "なぜそんな事になったのか、知る由はない。"
  },
  "003689": {
    "context": "003689",
    "code": "0x45",
    "developer_comment": "",
    "string": "ここで何が起きているのか、理解している者はいない。"
  },
  "003700": {
    "context": "003700",
    "code": "0x45",
    "developer_comment": "",
    "string": "――ごく一部の者を除いては。"
  },
  "003799": {
    "context": "003799",
    "code": "0x45",
    "developer_comment": "",
    "string": "崩れ落ちた階段を睨み、夏彦たちは声を上げた。"
  },
  "003810": {
    "context": "003810",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「――ちくしょう！　全部、奴らの仕業だ！」"
  },
  "003821": {
    "context": "003821",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「まさかここまでの状況になるなんて……」"
  },
  "003941": {
    "context": "003941",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「どうするの、夏彦！？」"
  },
  "003946": {
    "context": "003946",
    "code": "0x45",
    "developer_comment": "",
    "string": "傍らの少女が悲痛な声を上げる。"
  },
  "003969": {
    "context": "003969",
    "code": "0x45",
    "developer_comment": "",
    "string": "自分たちに残された時間は、どのくらいあるのだろうか。"
  },
  "003974": {
    "context": "003974",
    "code": "0x45",
    "developer_comment": "",
    "string": "そう思って腕時計を見たが――"
  },
  "004210": {
    "context": "004210",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「あ……」"
  },
  "004215": {
    "context": "004215",
    "code": "0x45",
    "developer_comment": "",
    "string": "先ほどの爆発が起きた時に、時計は壊れてしまっていた。"
  },
  "004220": {
    "context": "004220",
    "code": "0x45",
    "developer_comment": "",
    "string": "日付表示は『９月１６日』、針は『６時１９分』で止まっている。"
  },
  "004225": {
    "context": "004225",
    "code": "0x45",
    "developer_comment": "",
    "string": "それは生涯忘れられない時刻になりそうだった。"
  },
  "004294": {
    "context": "004294",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "（あとほんの少し、早く来ていれば……！）"
  },
  "004317": {
    "context": "004317",
    "code": "0x45",
    "developer_comment": "",
    "string": "だがそれでも、ここに来た事は意味がある。"
  },
  "004430": {
    "context": "004430",
    "code": "0x45",
    "developer_comment": "",
    "string": "夏彦が歩き出そうとすると、少女が手を掴んだ。"
  },
  "004441": {
    "context": "004441",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「待ってよ夏彦！　そんな状態でどこに行くつもり！？」"
  },
  "004452": {
    "context": "004452",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「奴らを、探しに……！」"
  },
  "004481": {
    "context": "004481",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「――って、まさか夏彦！？」"
  },
  "004492": {
    "context": "004492",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「……奴らの計画を阻止するんだ。今からでも、ぼくにできる事を――」"
  },
  "004536": {
    "context": "004536",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「それはダメ！　夏彦、頭を冷やして！」"
  },
  "004547": {
    "context": "004547",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「どうして！？　ましろだってぼくがここに来ることには、同意してくれたじゃないか！」"
  },
  "004576": {
    "context": "004576",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「さっきまでとは状況が違うじゃない！　もう遅いんだよ！」"
  },
  "004587": {
    "context": "004587",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「遅くない！　きっとまだ間に合う！」"
  },
  "004616": {
    "context": "004616",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「それにどこに逃げても同じだ！　だったら――」"
  },
  "004735": {
    "context": "004735",
    "code": "0x45",
    "developer_comment": "",
    "string": "そう叫んだ瞬間、頬に痛みが走った。"
  },
  "004740": {
    "context": "004740",
    "code": "0x45",
    "developer_comment": "",
    "string": "目の前の少女が、夏彦の頬を張ったのだ。"
  },
  "004769": {
    "context": "004769",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「……！」"
  },
  "004780": {
    "context": "004780",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「また、失いたいの……！？」"
  },
  "004803": {
    "context": "004803",
    "code": "0x45",
    "developer_comment": "",
    "string": "その言葉が、彼の胸をえぐる。"
  },
  "004907": {
    "context": "004907",
    "code": "0x45",
    "developer_comment": "",
    "string": "――その時、またも轟音が響いた。"
  },
  "004936": {
    "context": "004936",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「ッ！？」"
  },
  "004965": {
    "context": "004965",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「爆発……今度はどこで……」"
  },
  "004994": {
    "context": "004994",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「お願い夏彦、早く逃げよう！」"
  },
  "005017": {
    "context": "005017",
    "code": "0x45",
    "developer_comment": "",
    "string": "懇願する少女の声が、夏彦の心を揺らす。"
  },
  "005023": {
    "context": "005023",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「…………」"
  },
  "005047": {
    "context": "005047",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「………」"
  },
  "005053": {
    "context": "005053",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「……」"
  },
  "005082": {
    "context": "005082",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「……わかった。まずは出口を探そう……！」"
  },
  "005282": {
    "context": "005282",
    "code": "0x45",
    "developer_comment": "",
    "string": "――そして彼らは、出口を目指して駆け出した。"
  },
  "005287": {
    "context": "005287",
    "code": "0x45",
    "developer_comment": "",
    "string": "その先に、何が待ち受けているかも知らずに。"
  },
  "005292": {
    "context": "005292",
    "code": "0x45",
    "developer_comment": "",
    "string": "後に幾度となく振り返る、彼らの長い１日は――"
  },
  "005297": {
    "context": "005297",
    "code": "0x45",
    "developer_comment": "",
    "string": "こうして幕を開けた。"
  },
  "005425": {
    "context": "005425",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「別の階段を探そう！　どこかに無事な階段があるはずだ！」"
  },
  "005454": {
    "context": "005454",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「施設内の構造はわかる！？」"
  },
  "005483": {
    "context": "005483",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「先に調べるべき。闇雲に逃げるのは危険」"
  },
  "005871": {
    "context": "005871",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「サリュ、何をしてるんだ！？」"
  },
  "005967": {
    "context": "005967",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「今、この施設内の地図を検索する」"
  },
  "006185": {
    "context": "006185",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「先ほど崩れたのは、外周棟の非常階段だと思う」"
  },
  "006239": {
    "context": "006239",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「隣の『内周棟』にも、非常階段があるみたいだね……」"
  },
  "006377": {
    "context": "006377",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「よし、じゃあまずはそこに行こう！」"
  },
  "006704": {
    "context": "006704",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「火！？　なんでこんなに早く火災が！？」"
  },
  "006715": {
    "context": "006715",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「まさか、施設内の人たちを逃がさないようにしているの……？」"
  },
  "006744": {
    "context": "006744",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「くっ……ここは危険だ！　別の道を探そう！」"
  },
  "006879": {
    "context": "006879",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「ぼくは隣の『第２エリア』に続く道が無事か、確認してくる！」"
  },
  "006908": {
    "context": "006908",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「了解、気をつけて！」"
  },
  "007043": {
    "context": "007043",
    "code": "0x45",
    "developer_comment": "",
    "string": "――彼らは勇気を奮い、走った。"
  },
  "007112": {
    "context": "007112",
    "code": "0x45",
    "developer_comment": "",
    "string": "この施設を脱出し、生き延びるために。"
  },
  "007187": {
    "context": "007187",
    "code": "0x45",
    "developer_comment": "",
    "string": "そして、その道のさなかで――"
  },
  "007279": {
    "context": "007279",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「えっ……！」"
  },
  "007363": {
    "context": "007363",
    "code": "0x45",
    "developer_comment": "",
    "string": "――彼は、彼女に遭遇した。"
  },
  "007587": {
    "context": "007587",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「――悠里！？」"
  },
  "007592": {
    "context": "007592",
    "code": "0x45",
    "developer_comment": "",
    "string": "そこにいたのは、『悠里』だった。"
  },
  "007695": {
    "context": "007695",
    "code": "0x45",
    "developer_comment": "",
    "string": "家で帰りを待っているはずの、夏彦の同居人。"
  },
  "007790": {
    "context": "007790",
    "code": "0x45",
    "developer_comment": "",
    "string": "ここにいるはずのない彼女――"
  },
  "007875": {
    "context": "007875",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "（どうして、悠里が……！？）"
  },
  "007880": {
    "context": "007880",
    "code": "0x45",
    "developer_comment": "",
    "string": "驚愕とともに夏彦は、彼女の姿を見据える。"
  },
  "007964": {
    "context": "007964",
    "code": "0x45",
    "developer_comment": "",
    "string": "悠里は――"
  },
  "007993": {
    "context": "007993",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「どうして来てしまったの……！？」"
  },
  "007998": {
    "context": "007998",
    "code": "0x45",
    "developer_comment": "",
    "string": "悲しげに、そう呟いた。"
  },
  "008088": {
    "context": "008088",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「悠里こそ、どうしてここに！？」"
  },
  "008099": {
    "context": "008099",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「家から出られたのか！？」"
  },
  "008104": {
    "context": "008104",
    "code": "0x45",
    "developer_comment": "",
    "string": "そう問いを返しながらも、夏彦は思い返していた。"
  },
  "008127": {
    "context": "008127",
    "code": "0x45",
    "developer_comment": "",
    "string": "ここに至るまでの道程を。"
  },
  "008211": {
    "context": "008211",
    "code": "0x45",
    "developer_comment": "",
    "string": "やがて悠里が、寂しげに言う。"
  },
  "008240": {
    "context": "008240",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「……そういう事だったの……」"
  },
  "008245": {
    "context": "008245",
    "code": "0x45",
    "developer_comment": "",
    "string": "吹っ切れたような微笑を浮かべて。"
  },
  "008250": {
    "context": "008250",
    "code": "0x45",
    "developer_comment": "",
    "string": "――その時。"
  },
  "008329": {
    "context": "008329",
    "code": "0x45",
    "developer_comment": "",
    "string": "２人のすぐ傍で、閃光が瞬いた。"
  },
  "008454": {
    "context": "008454",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「ッ！　悠里！」"
  },
  "008750": {
    "context": "008750",
    "code": "0x45",
    "developer_comment": "",
    "string": "とっさに夏彦は悠里に飛びつく。同時に爆風が２人を襲った。"
  },
  "008761": {
    "context": "008761",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「あ！！」"
  },
  "008766": {
    "context": "008766",
    "code": "0x45",
    "developer_comment": "",
    "string": "夏彦は悠里と共に、吹き飛ばされた。"
  },
  "008812": {
    "context": "008812",
    "code": "0x45",
    "developer_comment": "",
    "string": "地面に叩きつけられるまでのほんの刹那――"
  },
  "008817": {
    "context": "008817",
    "code": "0x45",
    "developer_comment": "",
    "string": "夏彦の眼前に、今日まで見聞きしたものがよぎった。"
  },
  "008892": {
    "context": "008892",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "（どうしてこんな事に――）"
  },
  "009296": {
    "context": "009296",
    "code": "0x45",
    "developer_comment": "まだ何もかもが平穏だったあの日から――",
    "string": "――やがて、茫漠とした意識の奥で――"
  },
  "009365": {
    "context": "009365",
    "code": "0x45",
    "developer_comment": "",
    "string": "目覚めよ、と呼ぶ声が聞こえた。"
  },
  "009370": {
    "context": "009370",
    "code": "0x45",
    "developer_comment": "",
    "string": "あるいはそんな記憶があった。"
  },
  "009375": {
    "context": "009375",
    "code": "0x45",
    "developer_comment": "",
    "string": "白く塗られた墓の下で、彼はその声を聞き……"
  }
}