{
  "000093": {
    "context": "000093",
    "code": "0x55",
    "developer_comment": "",
    "string": "√Before Chapter2"
  },
  "000107": {
    "context": "000107",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "「さて、みんな着いたわよ～」"
  },
  "000118": {
    "context": "000118",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "恵那が、生徒たちを前に声を上げる。"
  },
  "000147": {
    "context": "000147",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "「ここが鹿鳴市におけるＢＣ研究のメッカ、『超心理学研究所』よ」"
  },
  "000467": {
    "context": "000467",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "「それじゃさっそく、中に入ってみましょー！」"
  },
  "000500": {
    "context": "000500",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "妙にテンション高めの恵那に引き連れられ、夏彦たちも施設の中に入っていった。"
  },
  "000617": {
    "context": "000617",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "中では見学者を案内する役の職員が、ＢＣに関する様々な解説をしてくれた。"
  },
  "000625": {
    "context": "000625",
    "code": "0x45",
    "developer_comment": "超心研の職員",
    "string": "「えー、ここはオフィスとなっておりまして。各種実験データの分析などを行っております」"
  },
  "000633": {
    "context": "000633",
    "code": "0x45",
    "developer_comment": "超心研の職員",
    "string": "「主に生物学的見地から、ＢＣ使用時の脳の働きと、その関連性を――」"
  },
  "000685": {
    "context": "000685",
    "code": "0x45",
    "developer_comment": "超心研の職員",
    "string": "ましろや他の生徒たちは興味深そうにしていたが、夏彦はまるっきり上の空だ。"
  },
  "000691": {
    "context": "000691",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "（最先端の研究をしているのに、案外フツーのオフィスなんだなぁ……）"
  },
  "000696": {
    "context": "000696",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "そんなどうでもいいような事ばかり考えてしまう。"
  },
  "000712": {
    "context": "000712",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "そして夏彦と逆に、誰より興味津々な様子を見せていたのは――"
  },
  "000923": {
    "context": "000923",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "「なるほど～、勉強になりますっ！」"
  },
  "000928": {
    "context": "000928",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "引率役の恵那だった。"
  },
  "001005": {
    "context": "001005",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "「ＢＣ粒子関連の研究なんかも、ここで行っているんですか？」"
  },
  "001013": {
    "context": "001013",
    "code": "0x45",
    "developer_comment": "超心研の職員",
    "string": "「そういった研究は、別の研究室で行っております。機密レベルが高いのでお見せできませんが」"
  },
  "001039": {
    "context": "001039",
    "code": "0x45",
    "developer_comment": "超心研の職員",
    "string": "「例えばある研究室では、脳内におけるマインド粒子の作用及び、ワンダラー粒子との相互作用について研究しております」"
  },
  "001068": {
    "context": "001068",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "「ＢＣ使用時に、脳内で発生するエネルギー量の測定・分析などは？」"
  },
  "001076": {
    "context": "001076",
    "code": "0x45",
    "developer_comment": "超心研の職員",
    "string": "「えぇ、そう言った事ももちろんやっておりますよ」"
  },
  "001102": {
    "context": "001102",
    "code": "0x45",
    "developer_comment": "超心研の職員",
    "string": "「例えばレベル４のコミュニケーターの方の場合、最大１０マイクロ・スカラー・ヴァリュー程度のエネルギー量が――」"
  },
  "001171": {
    "context": "001171",
    "code": "0x45",
    "developer_comment": "超心研の職員",
    "string": "そんな風に、恵那と職員が生徒そっちのけで話し込む一幕もあったが……"
  },
  "001176": {
    "context": "001176",
    "code": "0x45",
    "developer_comment": "超心研の職員",
    "string": "やがて滞りなく、見学は終了した。"
  },
  "001281": {
    "context": "001281",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "「はいっ！　どうでしたか皆さん、面白かったわねー？」"
  },
  "001286": {
    "context": "001286",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "施設を出るなり、恵那は居並ぶ生徒たちに言った。"
  },
  "001359": {
    "context": "001359",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「一番面白がってたのは、先生じゃないですかー？」"
  },
  "001370": {
    "context": "001370",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「途中から専門用語だらけで、置いてかれちゃいましたよ」"
  },
  "001375": {
    "context": "001375",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "ましろのツッコミに、生徒たちの間から笑い声が上がる。"
  },
  "001430": {
    "context": "001430",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "「そ、そうね、ごめんなさい」"
  },
  "001459": {
    "context": "001459",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "「他の先生とああいう話はできないから、ちょっと盛り上がってしまって……恥ずかしいわ」"
  },
  "001488": {
    "context": "001488",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "「ともかくこれで、今日の授業は終わりです。学校に帰るバスもあるけど、現地解散もＯＫよ」"
  },
  "001496": {
    "context": "001496",
    "code": "0x45",
    "developer_comment": "生徒たち",
    "string": "「は～い」"
  },
  "001525": {
    "context": "001525",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "「みなさん帰り道に気をつけてね。家に着くまでが課外授業ですよ～」"
  },
  "001530": {
    "context": "001530",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "その言葉をもって、課外授業は終了となった。"
  },
  "001570": {
    "context": "001570",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "生徒たちが思い思いの方向に歩き出す。"
  },
  "001575": {
    "context": "001575",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "部活に入っておらず、学校に用のない夏彦たちは、そのまま帰宅する事になる。"
  },
  "001604": {
    "context": "001604",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「うーし、そんじゃ夏彦、帰ろっか」"
  },
  "001615": {
    "context": "001615",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「ん、そうだな」"
  },
  "001620": {
    "context": "001620",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "夏彦は気のない返事を返した。"
  },
  "001626": {
    "context": "001626",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "（あーぁ……なんか疲れたな）"
  },
  "001631": {
    "context": "001631",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "そう思ってあくびをすると、そばにいた恵那が、目ざとく声をかけてきた。"
  },
  "001667": {
    "context": "001667",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "「お疲れのご様子ね、天川くん？」"
  },
  "001696": {
    "context": "001696",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「あ……先生」"
  },
  "001764": {
    "context": "001764",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "ぎくりとする夏彦に、恵那が苦笑して言う。"
  },
  "001775": {
    "context": "001775",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "「だめよ、課外授業中もぼーっとしていたでしょう」"
  },
  "001786": {
    "context": "001786",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「す、すみません」"
  },
  "001815": {
    "context": "001815",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "「うーん……天川くんは、超心理学があまり好きじゃないのかしら？　担当教師としては悲しいんだけど」"
  },
  "001826": {
    "context": "001826",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「違うんです先生。ただ……」"
  },
  "001855": {
    "context": "001855",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "「ただ？」"
  },
  "001866": {
    "context": "001866",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「実はぼく、ＢＣってもの自体が苦手なんですよ。理由は一口じゃ説明できないんですが……」"
  },
  "001871": {
    "context": "001871",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "夏彦は思った通りに答えた。"
  },
  "001876": {
    "context": "001876",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "すると横にいたましろが、口を挟む。"
  },
  "001905": {
    "context": "001905",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「夏彦も昔は、こんなにＢＣ嫌いじゃなかったんですけど……年々、苦手意識が強くなっちゃったみたいで」"
  },
  "001934": {
    "context": "001934",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "「なるほどね……でもそういう人は、昔から多いわ」"
  },
  "001939": {
    "context": "001939",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "恵那はどこか寂しげに笑って言った。"
  },
  "001950": {
    "context": "001950",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "「私はそんな人々の意識を、できれば変えたいと思ってるのよ」"
  },
  "001994": {
    "context": "001994",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "「そのために、超心理学の教師になったようなものなの」"
  },
  "002023": {
    "context": "002023",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "「私自身は、コミュニケーターでもないのにね……ちょっと変かしら？」"
  },
  "002034": {
    "context": "002034",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「いや、ちっとも変じゃないです」"
  },
  "002039": {
    "context": "002039",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "夏彦は慌ててフォローした。"
  },
  "002090": {
    "context": "002090",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "恵那が言う通り、彼女は超心理学の教師ではあるが、ＢＣは一切使えない。"
  },
  "002095": {
    "context": "002095",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "だが恵那の世代の者としては、それはむしろ当然の事だ。"
  },
  "002100": {
    "context": "002100",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "コミュニケーターが世に大量発生し始めたのは、約１６年前の事である。"
  },
  "002105": {
    "context": "002105",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "それ以前に生まれた人々の大半は、ＢＣが使えないのだ。"
  },
  "002110": {
    "context": "002110",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "むろん鹿鳴学園の教師も例外ではない。しかし――"
  },
  "002115": {
    "context": "002115",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "夏彦はこの年齢の近い女教師に、ほのかな好意を抱いていた。"
  },
  "002120": {
    "context": "002120",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "だから彼にしては珍しく、意に反した事を言った。"
  },
  "002142": {
    "context": "002142",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「その……立派だと思います。超心理学の教師になりたがる人は、少ないと聞きますから」"
  },
  "002171": {
    "context": "002171",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "「ふふ、ありがと」"
  },
  "002182": {
    "context": "002182",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「でも先生は、どうしてそんな志を持ったんですか？」"
  },
  "002211": {
    "context": "002211",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「先生の世代の方が、ＢＣ嫌いの人が多いって聞きますけど」"
  },
  "002240": {
    "context": "002240",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "「それはね、鳥羽さん。そうする事が今後の社会にとって、とても大切な事だと思っているからよ」"
  },
  "002263": {
    "context": "002263",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "そこで恵那は、不意に真面目な顔になって言った。"
  },
  "002274": {
    "context": "002274",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "「これから人と人との関わり方は大きく変わってくる。きっと社会のあり方もね」"
  },
  "002318": {
    "context": "002318",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "「いま各国では、ＢＣの研究とコミュニケーターの保護に力を入れているし、誰もがその可能性に期待しているわ」"
  },
  "002329": {
    "context": "002329",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「可能性って言いますと……？」"
  },
  "002358": {
    "context": "002358",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "「例えばテレパシーでは、『感情』を対象者に直接伝える事ができるじゃない？」"
  },
  "002369": {
    "context": "002369",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "「これはいずれ、精神治療の分野に応用できるだろうと考えられているのよ」"
  },
  "002398": {
    "context": "002398",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "「例えば思い切り『明るい感情』を対象者に送り込めば、精神疾患の原因となる感情を駆逐できるかもしれないってね」"
  },
  "002409": {
    "context": "002409",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「なるほど、それっていい使い方ですね」"
  },
  "002438": {
    "context": "002438",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「ひょっとしたらうちの学校からも、そういうニュータイプなセラピストが出てくるかもしれないね？」"
  },
  "002467": {
    "context": "002467",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "「えぇ。でもその一方で、扱い方を間違えたら危険なものでもある……」"
  },
  "002511": {
    "context": "002511",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "「ＢＣの歴史はまだ浅いもの。これが社会にとって『良い技術』になるのか『悪い技術』になるのか、まだ誰にもわからない」"
  },
  "002522": {
    "context": "002522",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "「でもせっかくなら『良い技術』になるよう、若いコミュニケーターたちを導いてあげたい」"
  },
  "002551": {
    "context": "002551",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "「そう思ったから、私はこの仕事を志したのよ」"
  },
  "002556": {
    "context": "002556",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "いつもおちゃらけている恵那も、この時は真剣な口調で言った。"
  },
  "002585": {
    "context": "002585",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「先生……かっこいいです！　尊敬しちゃいます！」"
  },
  "002614": {
    "context": "002614",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "「ふふ、そう言ってもらえると教師冥利につきるわ」"
  },
  "002637": {
    "context": "002637",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "恵那はそう言ってから、夏彦をちらりと見た。"
  },
  "002666": {
    "context": "002666",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "「だからこそね、天川くん。私は生徒のみんなにはＢＣに対し、正しい理解をしてほしいのよ」"
  },
  "002677": {
    "context": "002677",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「は、はい」"
  },
  "002688": {
    "context": "002688",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "「今日の課外授業も、いつもの実習も、全てはそのためのカリキュラムなの」"
  },
  "002717": {
    "context": "002717",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "「その対象を知ろうともしないまま、嫌っちゃだめよ。ね？」"
  },
  "002728": {
    "context": "002728",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「なるほど……わかりました、なるべく努力します」"
  },
  "002757": {
    "context": "002757",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "「うん、今はそれで充分よ」"
  },
  "002762": {
    "context": "002762",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "恵那はそう言って、踵を返した。"
  },
  "002791": {
    "context": "002791",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "「それじゃ先生、職員の人にもう一度挨拶してくるわ。また明日学校でね」"
  },
  "002802": {
    "context": "002802",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「はい、また明日」"
  },
  "002831": {
    "context": "002831",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「先生さようならー」"
  },
  "002857": {
    "context": "002857",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "そう挨拶を交わし、夏彦たちは恵那と別れた。"
  },
  "002902": {
    "context": "002902",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "恵那が立ち去った後、ましろが意地悪く笑って言う。"
  },
  "002913": {
    "context": "002913",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「やれやれ、夏彦は恵那先生の前では素直なんだからなぁ」"
  },
  "002924": {
    "context": "002924",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「う……そう見えたか？」"
  },
  "002935": {
    "context": "002935",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「そりゃあね。『努力します』とか言っちゃって。そんなこと大して思ってないくせに～」"
  },
  "002987": {
    "context": "002987",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "図星だった。"
  },
  "002992": {
    "context": "002992",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "いつからかさえ自分でも忘れてしまったが、夏彦のＢＣ嫌いは筋金入りである。"
  },
  "002997": {
    "context": "002997",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "クラスメートたちがどんどんテレパシーなどをマスターしていくのに、夏彦だけはいつまでたっても、能力レベル０のままだ。"
  },
  "003002": {
    "context": "003002",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "今では超心理学の授業にも、すっかりやる気を失っていた。担当教師が恵那であってもだ。"
  },
  "003024": {
    "context": "003024",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「……いやでもな、正直に答えたら、先生が傷つくだけだろ」"
  },
  "003053": {
    "context": "003053",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「まあねぇ。先生、本当にＢＣが好きみたいだからね」"
  },
  "003064": {
    "context": "003064",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「いや、多分それだけじゃないぞ。『扱い方を間違えたら危険』と言ってたし」"
  },
  "003075": {
    "context": "003075",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「過去にＢＣ絡みで、何か嫌な目にあったのかもな……だからきっと、あんなに真剣に話してたんだよ」"
  },
  "003104": {
    "context": "003104",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「ふーん、さすがよく見てる……」"
  },
  "003109": {
    "context": "003109",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "そうましろが言った時――不意に、背後で声が聞こえた。"
  },
  "003121": {
    "context": "003121",
    "code": "0x45",
    "developer_comment": "？？",
    "string": "「あなたたちも、そう思う？」"
  },
  "003150": {
    "context": "003150",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「わっ！？」"
  },
  "003196": {
    "context": "003196",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "はっとして振り返ると、後ろにサリュが立っていた。"
  },
  "003241": {
    "context": "003241",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「さ、サリュ？　どうしたのヤブから棒に」"
  },
  "003252": {
    "context": "003252",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「あなたたちもそう思うって、何が？」"
  },
  "003263": {
    "context": "003263",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「『ＢＣは扱い方を間違えたら危険』だって思ってる？」"
  },
  "003268": {
    "context": "003268",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "彼女から話を振ってくるのは初めての事だ。"
  },
  "003273": {
    "context": "003273",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "少し驚きつつ、夏彦は答える。"
  },
  "003284": {
    "context": "003284",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「いや……まぁ、昔からそういう風に言われているよ」"
  },
  "003313": {
    "context": "003313",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「フランスでは違うの？」"
  },
  "003342": {
    "context": "003342",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「さぁ……」"
  },
  "003353": {
    "context": "003353",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「さぁって……えっと、何が言いたいんだ？」"
  },
  "003382": {
    "context": "003382",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「別に、意見を聞きたかっただけ」"
  },
  "003411": {
    "context": "003411",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「用は済んだ。それじゃ」"
  },
  "003416": {
    "context": "003416",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "サリュはそう言って、さっさと歩き出そうとした。"
  },
  "003427": {
    "context": "003427",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「え？　お、おい……」"
  },
  "003456": {
    "context": "003456",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「ちょ、ちょっと待ってサリュ！」"
  },
  "003485": {
    "context": "003485",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「なに？」"
  },
  "003514": {
    "context": "003514",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「せっかくだから一緒に帰らない？　もうすぐ、住宅街方面のバスが出るから――」"
  },
  "003543": {
    "context": "003543",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「断る。行くところがあるから」"
  },
  "003601": {
    "context": "003601",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "サリュはましろの声も突っぱね、歩き去って行ってしまった。"
  },
  "003606": {
    "context": "003606",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "後には夏彦たちだけが取り残された。"
  },
  "003633": {
    "context": "003633",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「……なんだあれ？」"
  },
  "003662": {
    "context": "003662",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「う、うーん……不思議ちゃんだねぇ」"
  },
  "003667": {
    "context": "003667",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "少々戸惑いつつ、どちらともなくバス停に向かった。"
  }
}