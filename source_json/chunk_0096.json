{
  "000014": {
    "context": "000014",
    "code": "0x55",
    "developer_comment": "",
    "string": "√After Chapter6"
  },
  "000106": {
    "context": "000106",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「！？」"
  },
  "000111": {
    "context": "000111",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "はっとして振り返ると、そこには宇喜多が立っていた。"
  },
  "000140": {
    "context": "000140",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "「おやおや、これはこれは隊長さんに副隊長さん」"
  },
  "000169": {
    "context": "000169",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "「物音がするから来てみれば、君らは何をやっているのかね？」"
  },
  "000175": {
    "context": "000175",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "（な、なんだ……？）"
  },
  "000180": {
    "context": "000180",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "宇喜多の眼の光は、不安定にぶれている。"
  },
  "000185": {
    "context": "000185",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "その視線が、ベッドの上の２人に止まった。"
  },
  "000214": {
    "context": "000214",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "「夏彦くん！？　それに三ノ宮くんも！？」"
  },
  "000225": {
    "context": "000225",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "「ずいぶんひどい怪我じゃないか！　いったいなぜ？　そして今までどこに……！？」"
  },
  "000230": {
    "context": "000230",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "渡瀬は慎重に、言葉を選びながら言った。"
  },
  "000241": {
    "context": "000241",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「実は……火災を消火しようとＮエリアに入って、そこで彼らを保護したんだ」"
  },
  "000270": {
    "context": "000270",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "「Ｎエリアの消火……だと？」"
  },
  "000299": {
    "context": "000299",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "「なるほど……それで全て理解できたよ……！」"
  },
  "000310": {
    "context": "000310",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "「君がやはり、裏切り者だったと言う事をね！」"
  },
  "000321": {
    "context": "000321",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「お、おっさん……？」"
  },
  "000326": {
    "context": "000326",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "宇喜多が怒った理由がわからなかった。"
  },
  "000331": {
    "context": "000331",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "戸惑う渡瀬に、宇喜多が押し殺した声で続ける。"
  },
  "000360": {
    "context": "000360",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "「隊長さん……僕はこれでも、君を信頼していたんだ」"
  },
  "000371": {
    "context": "000371",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "「君は正義の人だと、本当にそう思っていたんだよ……」"
  },
  "000400": {
    "context": "000400",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "「だからこそ彼女らを救うため、僕も僕なりに命を賭けた。なのに……！」"
  },
  "000429": {
    "context": "000429",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "「許せん……！」"
  },
  "000434": {
    "context": "000434",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "唸るように言い、宇喜多が懐に手を入れる。"
  },
  "000439": {
    "context": "000439",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "そこから出てきたのは――拳銃だった。"
  },
  "000625": {
    "context": "000625",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「っ！？」"
  },
  "000630": {
    "context": "000630",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "宇喜多が構えた拳銃は、もともと恵那が持っていたものだった。"
  },
  "000635": {
    "context": "000635",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "それは宇喜多・洵・恵那の３人でバラバラに保管していたはずなのだが――"
  },
  "000646": {
    "context": "000646",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「おっさん、ハッタリのつもりか！？　その銃は、弾倉も入ってないハリボテだろ！？」"
  },
  "000657": {
    "context": "000657",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "「ハッタリ？　君は知らなかったようだね」"
  },
  "000668": {
    "context": "000668",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "「弾倉がなくても弾丸さえあれば、銃を撃つ事はできるのだよ。もっとも１発ずつだがね」"
  },
  "000679": {
    "context": "000679",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「弾丸って、そんなもんどこに……！？」"
  },
  "000690": {
    "context": "000690",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "「さてね」"
  },
  "000695": {
    "context": "000695",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "宇喜多の様子を見るに、ハッタリではなさそうだった。"
  },
  "000706": {
    "context": "000706",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "「……脆弱な正義と笑うがいいさ。だがこれが僕の選択だ」"
  },
  "000717": {
    "context": "000717",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "「君を殺してこの状況が終わるなら、そうしよう」"
  },
  "000728": {
    "context": "000728",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "「一生に一度くらいは、罪に手を染めてでも……！」"
  },
  "000733": {
    "context": "000733",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "――言葉の意味がわからない。表情には明らかに狂気が滲んでいる。"
  },
  "000744": {
    "context": "000744",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「う、宇喜多さん……！」"
  },
  "000755": {
    "context": "000755",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「待ておっさん、そいつを撃つ前に聞いてくれ！」"
  },
  "000766": {
    "context": "000766",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「あんたは知らされてなかったのかもしれないが、Ｎエリアに原子炉は存在しなかったんだ！」"
  },
  "000777": {
    "context": "000777",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「だからオレたちが放射線を恐れる必要なんて、初めから無かったんだよ！」"
  },
  "000782": {
    "context": "000782",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "宇喜多が被曝への恐怖から常軌を逸してしまったのであれば、これで正気に戻るはずだった。"
  },
  "000787": {
    "context": "000787",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "だが――"
  },
  "000837": {
    "context": "000837",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "「君は……」"
  },
  "000848": {
    "context": "000848",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "「この期に及んで、そんなたわごとを……？」"
  },
  "000859": {
    "context": "000859",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「信じろ！　本当なんだ！」"
  },
  "000870": {
    "context": "000870",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "「まぁどちらでもいいさ。どうせ――」"
  },
  "000920": {
    "context": "000920",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "「――君の『任務』は、ここで終わりだ！」"
  },
  "000931": {
    "context": "000931",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「っ！？」"
  },
  "000942": {
    "context": "000942",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "「死ね、裏切り者！」"
  },
  "000947": {
    "context": "000947",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "宇喜多が引き鉄に指をかける。渡瀬の身が強張る。"
  },
  "000952": {
    "context": "000952",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "――その刹那。"
  },
  "001035": {
    "context": "001035",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「隊長！　危ない！」"
  },
  "001152": {
    "context": "001152",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "宇喜多が放った銃弾が、渡瀬の背後の壁に穴を開けた。"
  },
  "001241": {
    "context": "001241",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「おっさんっ！」"
  },
  "001246": {
    "context": "001246",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "風見ともつれ合いながら、渡瀬も懐から拳銃を抜いた。"
  },
  "001275": {
    "context": "001275",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "「なっ！？」"
  },
  "001329": {
    "context": "001329",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "渡瀬が差し向けた銃口を見て、宇喜多が慌てて身をひるがえす。"
  },
  "001334": {
    "context": "001334",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "宇喜多の背中を撃つチャンスはあったが――渡瀬は引き鉄を引かなかった。"
  },
  "001339": {
    "context": "001339",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "大体この拳銃に、弾が残っているかどうかもわからない。"
  },
  "001344": {
    "context": "001344",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "ドアの向こうに宇喜多が消える。"
  },
  "001349": {
    "context": "001349",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "そして、その向こうから声だけが――"
  },
  "001478": {
    "context": "001478",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "「とうとう本性を表したな、隊長さん！」"
  },
  "001489": {
    "context": "001489",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "「だが僕は必ず、君を裁いてやる！　その事を忘れるな！」"
  },
  "001530": {
    "context": "001530",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "宇喜多の足音が第４エリア側へ去って行く。"
  },
  "001579": {
    "context": "001579",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「くそっ！　おっさんの奴、どうしちまったんだ！？」"
  },
  "001584": {
    "context": "001584",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "渡瀬に疑わしい点があるにせよ、宇喜多の行動はあまりに極端過ぎだった。"
  },
  "001589": {
    "context": "001589",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "ふと気づくと、風見が渡瀬の拳銃を見つめている。"
  },
  "001654": {
    "context": "001654",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「隊長……その拳銃は……？」"
  },
  "001665": {
    "context": "001665",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「あぁ、そう言えばさっき話してなかったな」"
  },
  "001676": {
    "context": "001676",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「これはＮエリアで拾ったものだ。夏彦の傍に落ちていたんだ」"
  },
  "001705": {
    "context": "001705",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「そ、そうだったんですか……」"
  },
  "001710": {
    "context": "001710",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "風見は拳銃に怯えているようだ。"
  },
  "001715": {
    "context": "001715",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "渡瀬は慌ててそれを懐にしまい、話を逸らす。"
  },
  "001726": {
    "context": "001726",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「とにかく、すぐにこの場所から移動しよう」"
  },
  "001737": {
    "context": "001737",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「おっさんに知られてしまった以上、いつまでもここに留まっているのは危険だからな」"
  },
  "001742": {
    "context": "001742",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "夏彦の輸血はまだ終っておらず、サリュも目覚める気配がない。"
  },
  "001747": {
    "context": "001747",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "さっきのような襲撃があれば、彼らにも危害が加えられる危険性があった。"
  },
  "001758": {
    "context": "001758",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「……それに、先生も危ない」"
  },
  "001787": {
    "context": "001787",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「椿山さんですか？　そう言えば彼女はどこに？」"
  },
  "001798": {
    "context": "001798",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「第２エリアの小部屋に置いてきた。おっさんはあの様子じゃ、先生も襲いかねない」"
  },
  "001809": {
    "context": "001809",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「……わかりました。では天川くんの輸血を続けたまま、椿山さんと合流しましょう」"
  },
  "001814": {
    "context": "001814",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "血液パックと輸血キットを、夏彦の体にくくりつける。"
  },
  "001819": {
    "context": "001819",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "そして渡瀬が夏彦を、風見がサリュをそれぞれ背負い、医務室を出た。"
  }
}