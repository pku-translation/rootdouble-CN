{
  "000089": {
    "context": "000089",
    "code": "0x55",
    "string": "√Double Chapter7"
  },
  "000125": {
    "context": "000125",
    "code": "0x85",
    "string": "　%N　%N　%N　%N　%N"
  },
  "000210": {
    "context": "000210",
    "code": "0x45",
    "developer_comment": "【――橘風見との記憶――】",
    "string": "――刑務所の裏門を潜る渡瀬に、看守がお決まりのセリフを言う。"
  },
  "000218": {
    "context": "000218",
    "code": "0x45",
    "developer_comment": "看守",
    "string": "「――もうこんなところ、来るんじゃないぞ」"
  },
  "000229": {
    "context": "000229",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「もちろんです」"
  },
  "000240": {
    "context": "000240",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「……色々お世話になりました」"
  },
  "000245": {
    "context": "000245",
    "code": "0x45",
    "developer_comment": "",
    "string": "渡瀬はそう言って、深々と頭を下げ……"
  },
  "000250": {
    "context": "000250",
    "code": "0x45",
    "developer_comment": "",
    "string": "そして踵を返し、約１年ぶりに外の世界に出た。"
  },
  "000255": {
    "context": "000255",
    "code": "0x45",
    "developer_comment": "",
    "string": "渡瀬は大きく深呼吸する。"
  },
  "000266": {
    "context": "000266",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「ふぅうう～……やっぱシャバの空気は美味いぜ」"
  },
  "000271": {
    "context": "000271",
    "code": "0x45",
    "developer_comment": "",
    "string": "これもお決まりのセリフだが、半分以上は本気だった。"
  },
  "000282": {
    "context": "000282",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「さて、これからどうするか……」"
  },
  "000287": {
    "context": "000287",
    "code": "0x45",
    "developer_comment": "",
    "string": "出られたのはいいが、今夜寝泊りする場所もない。"
  },
  "000320": {
    "context": "000320",
    "code": "0x45",
    "developer_comment": "",
    "string": "予定では恵那が迎えにきて、当面の仮住まいに案内してくれるはずなのだが――"
  },
  "000325": {
    "context": "000325",
    "code": "0x45",
    "developer_comment": "",
    "string": "彼女の姿が見当たらない。"
  },
  "000365": {
    "context": "000365",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "（なんか用事でも入ったのか……？）"
  },
  "000370": {
    "context": "000370",
    "code": "0x45",
    "developer_comment": "",
    "string": "そう思った時、背後で声がした。"
  },
  "000383": {
    "context": "000383",
    "code": "0x45",
    "developer_comment": "？？",
    "string": "「渡瀬」"
  },
  "000409": {
    "context": "000409",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「っ」"
  },
  "000468": {
    "context": "000468",
    "code": "0x45",
    "developer_comment": "",
    "string": "はっとして振り返ると、そこには――"
  },
  "000473": {
    "context": "000473",
    "code": "0x45",
    "developer_comment": "",
    "string": "風見が、立っていた。"
  },
  "000506": {
    "context": "000506",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「風見……お前、どうして……？」"
  },
  "000517": {
    "context": "000517",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「椿山さんが教えてくれたの。今日、あなたが出てくるって」"
  },
  "000528": {
    "context": "000528",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「なに？」"
  },
  "000539": {
    "context": "000539",
    "code": "0x45",
    "developer_comment": "",
    "string": "恵那が余計な気を利かせたのだ。"
  },
  "000544": {
    "context": "000544",
    "code": "0x45",
    "developer_comment": "",
    "string": "渡瀬はそれを望んでいないというのに――"
  },
  "000555": {
    "context": "000555",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「いや……それよりお前、こんな所に来て大丈夫なのか！？」"
  },
  "000566": {
    "context": "000566",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「大丈夫って？」"
  },
  "000577": {
    "context": "000577",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「お前、マスコミとか各監査機関とかに目をつけられてんだろ？」"
  },
  "000582": {
    "context": "000582",
    "code": "0x45",
    "developer_comment": "",
    "string": "風見は現在日本に数名しかいない、能力レベル６コミュニケーターのうちの１人なのだ。"
  },
  "000587": {
    "context": "000587",
    "code": "0x45",
    "developer_comment": "",
    "string": "その事は世間には伏せられているとはいえ、同時に例のラボの事件の被害者でもある。"
  },
  "000592": {
    "context": "000592",
    "code": "0x45",
    "developer_comment": "",
    "string": "現在は風見も夏彦たちと同様、隠遁生活を送っているのだと、恵那から聞いていた。"
  },
  "000597": {
    "context": "000597",
    "code": "0x45",
    "developer_comment": "",
    "string": "だから彼女は渡瀬への面会にも来なかったし、渡瀬も会おうと思ってはいなかったが……。"
  },
  "000608": {
    "context": "000608",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「出所したばかりの囚人と会ってたら、どんな記事書かれるかわからないぞ？」"
  },
  "000619": {
    "context": "000619",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「構わないわ」"
  },
  "000630": {
    "context": "000630",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「そういうわけにいくかよ。超科局の監視だって――」"
  },
  "000682": {
    "context": "000682",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「構わないって言ってるでしょ」"
  },
  "000687": {
    "context": "000687",
    "code": "0x45",
    "developer_comment": "",
    "string": "風見は遮るように、強く言った。"
  },
  "000692": {
    "context": "000692",
    "code": "0x45",
    "developer_comment": "",
    "string": "言葉を呑む渡瀬を、彼女が見つめる。"
  },
  "000744": {
    "context": "000744",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「そんな事、どうだっていいじゃない……！」"
  },
  "000755": {
    "context": "000755",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「この日が来るのを……ずっと待ってたんだから」"
  },
  "000760": {
    "context": "000760",
    "code": "0x45",
    "developer_comment": "",
    "string": "その目にはいつの間にか、涙が滲んでいた。"
  },
  "000771": {
    "context": "000771",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「……風見」"
  },
  "000782": {
    "context": "000782",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「私がどれだけあなたに会いたかったか、わかってないのね？」"
  },
  "000793": {
    "context": "000793",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「２年前いきなり別れ話されて、１年待ってようやくあなたの真意がわかって……」"
  },
  "000804": {
    "context": "000804",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「もう１度やりなおせると思ったら、また離れ離れって……」"
  },
  "000858": {
    "context": "000858",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「勝手するのもいい加減にしなさい！」"
  },
  "000869": {
    "context": "000869",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「っ！」"
  },
  "000880": {
    "context": "000880",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「なによ１年ぶりに会ったのに、優しい言葉の一つもかけず、迷惑そうな顔して！」"
  },
  "000891": {
    "context": "000891",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「超科局がどうとかマスコミがどうとか！　そんなのホントどうでもいいわよ！」"
  },
  "000902": {
    "context": "000902",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「椿山さんにはしょっちゅう会ってるくせに、私には手紙１つよこさないし！」"
  },
  "000913": {
    "context": "000913",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「あ、いや……それはすまん」"
  },
  "000918": {
    "context": "000918",
    "code": "0x45",
    "developer_comment": "",
    "string": "風見は１年分のうっぷんが溜まっているのか、一気にまくし立てた。"
  },
  "000923": {
    "context": "000923",
    "code": "0x45",
    "developer_comment": "",
    "string": "今までに見た事のない彼女の表情に、渡瀬は戸惑う。"
  },
  "000964": {
    "context": "000964",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「だいたいあなたね、ちゃんと約束覚えてるの！？」"
  },
  "000975": {
    "context": "000975",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「エンジンルームで私にキスした時、言ったでしょう！」"
  },
  "000986": {
    "context": "000986",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「『ここを出たら、また一緒に公園を歩こう』って！」"
  },
  "000997": {
    "context": "000997",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「お、覚えてるよ！　覚えてるけど――」"
  },
  "001002": {
    "context": "001002",
    "code": "0x45",
    "developer_comment": "",
    "string": "渡瀬は風見をなだめつつ、真顔で言った。"
  },
  "001013": {
    "context": "001013",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「……風見、わかってるのか？　俺はムショ帰りなんだぞ」"
  },
  "001024": {
    "context": "001024",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「しかもラボを爆破した元テロリストであり、Ｑから見れば裏切り者だ」"
  },
  "001035": {
    "context": "001035",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「鹿鳴市にもＱにも、俺を憎む奴や恨む奴、命を狙う奴だっているだろう」"
  },
  "001046": {
    "context": "001046",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「俺なんかと一緒にいたら、不幸になるだけだ」"
  },
  "001096": {
    "context": "001096",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「馬鹿ね。そんなちんけな脅しで、私があっさり身を引くと思ってるの？」"
  },
  "001107": {
    "context": "001107",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「ちんけってお前……」"
  },
  "001118": {
    "context": "001118",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「私ね、強くなったのよ」"
  },
  "001129": {
    "context": "001129",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「凪沙をまた失って、３ヶ月ほど泣いて暮らしたけど……」"
  },
  "001140": {
    "context": "001140",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「あなたがいなくても、乗り越える事ができたの」"
  },
  "001190": {
    "context": "001190",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「私はもう、あなたをただ待ち続けるだけの女じゃないわ」"
  },
  "001201": {
    "context": "001201",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「あなたが嫌だって言っても、ついていくからね」"
  },
  "001211": {
    "context": "001211",
    "code": "0x45",
    "developer_comment": "",
    "string": "風見がじっと渡瀬を見つめる。"
  },
  "001216": {
    "context": "001216",
    "code": "0x45",
    "developer_comment": "",
    "string": "その目には確かに、強い光があったが――"
  },
  "001222": {
    "context": "001222",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "（……無理してる部分も、あるだろうな）"
  },
  "001227": {
    "context": "001227",
    "code": "0x45",
    "developer_comment": "",
    "string": "渡瀬はそう感じた。"
  },
  "001237": {
    "context": "001237",
    "code": "0x45",
    "developer_comment": "",
    "string": "風見は今、日本中のＢＣ関係者からの監視を受けているだろう。"
  },
  "001242": {
    "context": "001242",
    "code": "0x45",
    "developer_comment": "",
    "string": "夏彦たちがそうであるように。"
  },
  "001247": {
    "context": "001247",
    "code": "0x45",
    "developer_comment": "",
    "string": "そのプレッシャーの中、独りで耐えられる女などそうはいないはずだ。"
  },
  "001252": {
    "context": "001252",
    "code": "0x45",
    "developer_comment": "",
    "string": "なのにそれを押し込め、渡瀬に会いに来て、そして初めてこんな感情をさらけ出した。"
  },
  "001257": {
    "context": "001257",
    "code": "0x45",
    "developer_comment": "",
    "string": "その彼女の覚悟に――渡瀬は心から、敬意を抱いた。"
  },
  "001270": {
    "context": "001270",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「……悪かったよ、風見」"
  },
  "001281": {
    "context": "001281",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「１年も待たせてごめんな。約束は守るよ」"
  },
  "001292": {
    "context": "001292",
    "code": "0x45",
    "developer_comment": "",
    "string": "渡瀬はそう言って、風見を見つめる。"
  },
  "001297": {
    "context": "001297",
    "code": "0x45",
    "developer_comment": "",
    "string": "――彼女は普通の人間ではない。"
  },
  "001302": {
    "context": "001302",
    "code": "0x45",
    "developer_comment": "",
    "string": "かつて渡瀬が憎悪と殺意を抱いた相手と同じ、レベル６のコミュニケーターだ。"
  },
  "001307": {
    "context": "001307",
    "code": "0x45",
    "developer_comment": "",
    "string": "いつＮ化するかわからない。制御不能の怪物になるかもしれない。"
  },
  "001312": {
    "context": "001312",
    "code": "0x45",
    "developer_comment": "",
    "string": "だが、それでも渡瀬は――"
  },
  "001317": {
    "context": "001317",
    "code": "0x45",
    "developer_comment": "",
    "string": "彼女の想いに、応えようと思った。"
  },
  "001328": {
    "context": "001328",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「……ただ、また恋人に戻ろうとは言えないな」"
  },
  "001378": {
    "context": "001378",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「えっ……！」"
  },
  "001389": {
    "context": "001389",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「恋人じゃ嫌なんだよ」"
  },
  "001395": {
    "context": "001395",
    "code": "0x45",
    "developer_comment": "",
    "string": "渡瀬はそう言って、風見に歩み寄り――"
  },
  "001421": {
    "context": "001421",
    "code": "0x45",
    "developer_comment": "",
    "string": "彼女を抱き締めた。"
  },
  "001432": {
    "context": "001432",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「結婚しよう、風見」"
  },
  "001485": {
    "context": "001485",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「っ……」"
  },
  "001490": {
    "context": "001490",
    "code": "0x45",
    "developer_comment": "",
    "string": "腕の中の彼女が、息を呑む。"
  },
  "001536": {
    "context": "001536",
    "code": "0x45",
    "developer_comment": "",
    "string": "そしてやがて安堵したように、微笑んで答えた。"
  },
  "001547": {
    "context": "001547",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「――はい」"
  }
}