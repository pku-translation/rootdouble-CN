{
  "000008": {
    "context": "000008",
    "code": "0x55",
    "developer_comment": "",
    "string": "√After Chapter3"
  },
  "000117": {
    "context": "000117",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「ッ！！」"
  },
  "000128": {
    "context": "000128",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "瞬間、渡瀬の背筋に電流が走った。"
  },
  "000133": {
    "context": "000133",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "頭上からパラパラと何かが落ちてきている。"
  },
  "000138": {
    "context": "000138",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "はっとして頭上を見ると――"
  },
  "000163": {
    "context": "000163",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "――天井が崩れてくる！？"
  },
  "000186": {
    "context": "000186",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「みんな、ここから離れろ！」"
  },
  "000191": {
    "context": "000191",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "そう叫ぶと同時に、天井の崩落が始まった。"
  },
  "000279": {
    "context": "000279",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "「きゃああ！」"
  },
  "000290": {
    "context": "000290",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "「ひっ、ひぃぃっ！」"
  },
  "000295": {
    "context": "000295",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "恵那たちの悲鳴が上がった。"
  },
  "000300": {
    "context": "000300",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "次々に落下する瓦礫・砂礫・鉄片・鉄骨・鉄板。"
  },
  "000427": {
    "context": "000427",
    "code": "0x45",
    "developer_comment": "ユウリ",
    "string": "「あっ！」"
  },
  "000432": {
    "context": "000432",
    "code": "0x45",
    "developer_comment": "ユウリ",
    "string": "ユウリが足を取られ、尻餅をつく。"
  },
  "000437": {
    "context": "000437",
    "code": "0x45",
    "developer_comment": "ユウリ",
    "string": "そこに、一際巨大な――５ｍ四方もある天井板が、ユウリ目掛けて落下して――"
  },
  "000520": {
    "context": "000520",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「ユウリっ！」"
  },
  "000525": {
    "context": "000525",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "渡瀬はとっさにユウリに覆いかぶさった。"
  },
  "000536": {
    "context": "000536",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "一瞬の後、渡瀬の背中に衝撃が走る。"
  },
  "000547": {
    "context": "000547",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「ぐっ！」"
  },
  "000552": {
    "context": "000552",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "落下してきた板を、背中で受け止めてしまった。"
  },
  "000557": {
    "context": "000557",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "板自体はそう重いものではなかった。２０ｋｇ程度だろうか。"
  },
  "000562": {
    "context": "000562",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "ユウリを体の下に敷いたまま、四つんばいになって、板を背中で押し上げる。"
  },
  "000568": {
    "context": "000568",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "（早く抜け出さなきゃ！）"
  },
  "000573": {
    "context": "000573",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "だが、そこにさらに瓦礫が落ちて来て――"
  },
  "000585": {
    "context": "000585",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "板越しに、背中を衝撃が襲った。"
  },
  "000596": {
    "context": "000596",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「がああっ！！」"
  },
  "000601": {
    "context": "000601",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "同時に重量が、激烈な勢いで増してくる。"
  },
  "000606": {
    "context": "000606",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "抜け出そうと思ったが、身動きが取れない。"
  },
  "000612": {
    "context": "000612",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "（く、くそ！！）"
  },
  "000617": {
    "context": "000617",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "背中の上に積み重ねられた瓦礫。その重量はどのくらいか概算もできない。"
  },
  "000622": {
    "context": "000622",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "１５０ｋｇ？　２００ｋｇ？　あまりの重量に、四つんばいになった手足が折れそうになる。"
  },
  "000653": {
    "context": "000653",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "――だが、その時、ユウリと眼が合った。"
  },
  "000658": {
    "context": "000658",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "恐怖と絶望に彩られた瞳の中に、渡瀬の顔が映っている。"
  },
  "000664": {
    "context": "000664",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "（オレが踏ん張らなきゃ――ユウリまで潰れちまう！）"
  },
  "000678": {
    "context": "000678",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "渡瀬は手足に力を込め、瓦礫の重みに耐えた。"
  },
  "000683": {
    "context": "000683",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "ユウリを護る事、ただそれだけを考えて。"
  },
  "000695": {
    "context": "000695",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "やがて天井の崩落が止んだ頃――"
  },
  "000763": {
    "context": "000763",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "渡瀬とユウリは、瓦礫の山の中にいた。"
  },
  "000779": {
    "context": "000779",
    "code": "0x45",
    "developer_comment": "ユウリ",
    "string": "「……た、隊長さん……！」"
  },
  "000784": {
    "context": "000784",
    "code": "0x45",
    "developer_comment": "ユウリ",
    "string": "ユウリが両目を見開いて見上げてくる。"
  },
  "000789": {
    "context": "000789",
    "code": "0x45",
    "developer_comment": "ユウリ",
    "string": "そのとき埃に塗れたユウリの頬に、ポタポタと何かが落ちた。"
  },
  "000833": {
    "context": "000833",
    "code": "0x45",
    "developer_comment": "ユウリ",
    "string": "――それは、渡瀬の血だった。"
  },
  "000839": {
    "context": "000839",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "（後頭部が、切れちまったか……）"
  },
  "000844": {
    "context": "000844",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "痛みを堪え、渡瀬は言う。"
  },
  "000855": {
    "context": "000855",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「ユウリ、安心しろ……絶対に、お前を助けてやるからな」"
  },
  "000866": {
    "context": "000866",
    "code": "0x45",
    "developer_comment": "ユウリ",
    "string": "「は、はい……でも……」"
  },
  "000877": {
    "context": "000877",
    "code": "0x45",
    "developer_comment": "ユウリ",
    "string": "「それより、隊長さんが……！」"
  },
  "000888": {
    "context": "000888",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「なに、こんなのかすり傷だ」"
  },
  "000893": {
    "context": "000893",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "そう言って渡瀬は、ぎこちない笑顔を見せる。"
  },
  "000943": {
    "context": "000943",
    "code": "0x45",
    "developer_comment": "ユウリ",
    "string": "「でも、血が……！」"
  },
  "000948": {
    "context": "000948",
    "code": "0x45",
    "developer_comment": "ユウリ",
    "string": "ユウリが泣きそうな表情を浮かべる。"
  },
  "000953": {
    "context": "000953",
    "code": "0x45",
    "developer_comment": "ユウリ",
    "string": "そこへ崩落を逃れた４人の声が聞こえて来た。"
  },
  "000964": {
    "context": "000964",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「隊長！？　ユウリさん！？」"
  },
  "000975": {
    "context": "000975",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "「２人ともどこなの！？　返事をして！」"
  },
  "000986": {
    "context": "000986",
    "code": "0x45",
    "developer_comment": "洵",
    "string": "「いや――まさか、この瓦礫の下に！？」"
  },
  "000997": {
    "context": "000997",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "「い、生きてるのかね！？」"
  },
  "001002": {
    "context": "001002",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "するとユウリが瓦礫の外へ向かって叫ぶ。"
  },
  "001052": {
    "context": "001052",
    "code": "0x45",
    "developer_comment": "ユウリ",
    "string": "「皆さん、わたしたちはここです！」"
  },
  "001063": {
    "context": "001063",
    "code": "0x45",
    "developer_comment": "ユウリ",
    "string": "「早く瓦礫を退けて下さい！　じゃないと隊長さんが！」"
  },
  "001074": {
    "context": "001074",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "「で、でも――どうやって！？」"
  },
  "001125": {
    "context": "001125",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "渡瀬は瞬時に考えた。"
  },
  "001130": {
    "context": "001130",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "――この瓦礫を、人の手で撤去するのは難しいだろう。"
  },
  "001135": {
    "context": "001135",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "しかも筋力の消耗を考えれば、手をこまねいている時間は無い。"
  },
  "001141": {
    "context": "001141",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "（か……考えろ！　オレなら何か考えつくはずだ！）"
  },
  "001147": {
    "context": "001147",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "（ユウリを救うための、最善の方法は――！？）"
  },
  "001160": {
    "context": "001160",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "渡瀬は己の勘が命じるまま、瓦礫の外へ指示を飛ばした。"
  },
  "001214": {
    "context": "001214",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "一瞬何か思いついた気がしたが、それで助かるという自信はなかった。"
  },
  "001219": {
    "context": "001219",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "瓦礫が背中に圧しかかる。もはや一刻の猶予もない。"
  },
  "001232": {
    "context": "001232",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "渡瀬はついに悲鳴を上げた。"
  },
  "001243": {
    "context": "001243",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「橘！　守部！　頼むから何とかしてくれ！」"
  },
  "001253": {
    "context": "001253",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "渡瀬は己の勘が命じるまま、瓦礫の外へ指示を飛ばした。"
  },
  "001264": {
    "context": "001264",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「守部！　すぐに簡易基地に行ってエアジャッキを取ってくるんだ！」"
  },
  "001275": {
    "context": "001275",
    "code": "0x45",
    "developer_comment": "洵",
    "string": "「ここからじゃ遠い――」"
  },
  "001286": {
    "context": "001286",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「いや！　第１エリアを経由すれば、それほど時間はかからないはずだ！」"
  },
  "001297": {
    "context": "001297",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「橘と先生とおっさんは、オレの上の瓦礫をどけられるだけどけてくれ！」"
  },
  "001308": {
    "context": "001308",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「わ、わかりました！」"
  },
  "001319": {
    "context": "001319",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「時間が無い！　みんな頼む！」"
  },
  "001330": {
    "context": "001330",
    "code": "0x45",
    "developer_comment": "洵",
    "string": "「了解！　隊長、２分以内に戻るから頑張って！」"
  },
  "001347": {
    "context": "001347",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「では宇喜多さん椿山さん、私たちも！」"
  },
  "001358": {
    "context": "001358",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "「あ、あぁ！」"
  },
  "001369": {
    "context": "001369",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "「どうすればいいの！？」"
  },
  "001380": {
    "context": "001380",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「手始めにこの瓦礫から撤去します！」"
  },
  "001406": {
    "context": "001406",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "洵がファクトリーを去り、風見と恵那と宇喜多が瓦礫の撤去を開始する。"
  },
  "001411": {
    "context": "001411",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "しかし１分もすると――"
  },
  "001416": {
    "context": "001416",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "渡瀬の腕は、早くも震え始めていた。"
  },
  "001427": {
    "context": "001427",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「くっ……！」"
  },
  "001432": {
    "context": "001432",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "背中にのしかかる重みは、容赦なく渡瀬の身をさいなむ。"
  },
  "001437": {
    "context": "001437",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "延ばした肘がギシギシと軋み、今にもへし折れそうだった。"
  },
  "001443": {
    "context": "001443",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "（守部……頼むから、急いでくれ……！）"
  },
  "001448": {
    "context": "001448",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "渡瀬の心情を見抜いてか、ユウリが辛そうに言う。"
  },
  "001499": {
    "context": "001499",
    "code": "0x45",
    "developer_comment": "ユウリ",
    "string": "「ごめんなさい、隊長さん……わたしのせいで、隊長さんまで巻き添えに……」"
  },
  "001510": {
    "context": "001510",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「ば、ばかやろう……いらん事、言うんじゃねぇ……」"
  },
  "001521": {
    "context": "001521",
    "code": "0x45",
    "developer_comment": "ユウリ",
    "string": "「は、はい……」"
  },
  "001532": {
    "context": "001532",
    "code": "0x45",
    "developer_comment": "ユウリ",
    "string": "「……でも、どうして隊長さんが……」"
  },
  "001543": {
    "context": "001543",
    "code": "0x45",
    "developer_comment": "ユウリ",
    "string": "「どうしてそうまでして、わたしを救おうとしたりするんですか？」"
  },
  "001548": {
    "context": "001548",
    "code": "0x45",
    "developer_comment": "ユウリ",
    "string": "それは自分でもわからない。幻聴のせいだとも言えるし、そうでない気もする。"
  },
  "001553": {
    "context": "001553",
    "code": "0x45",
    "developer_comment": "ユウリ",
    "string": "ただ渡瀬は、自分に与えられた役割を答えた。"
  },
  "001564": {
    "context": "001564",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「あ、当たり前だろ……オレは、レスキュー隊の、隊長なん、だぜ」"
  },
  "001575": {
    "context": "001575",
    "code": "0x45",
    "developer_comment": "ユウリ",
    "string": "「そんな……」"
  },
  "001586": {
    "context": "001586",
    "code": "0x45",
    "developer_comment": "ユウリ",
    "string": "「それじゃあ、どうしてあの時……」"
  },
  "001591": {
    "context": "001591",
    "code": "0x45",
    "developer_comment": "ユウリ",
    "string": "そうしてユウリが何かを口にしかけた時――"
  },
  "001596": {
    "context": "001596",
    "code": "0x45",
    "developer_comment": "ユウリ",
    "string": "洵の声が聞こえた。"
  },
  "001613": {
    "context": "001613",
    "code": "0x45",
    "developer_comment": "洵",
    "string": "「お待たせ！　そちらの状況は！？」"
  },
  "001624": {
    "context": "001624",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「正、直、きつい……い、急いでくれ！」"
  },
  "001635": {
    "context": "001635",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「わかりました！　守部、すぐにエアジャッキの用意を！」"
  },
  "001646": {
    "context": "001646",
    "code": "0x45",
    "developer_comment": "洵",
    "string": "「スタンバッてるよ！　指示どうぞ！」"
  },
  "001657": {
    "context": "001657",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「まずここからここまでのラインを、エンジンカッターで切断します！」"
  },
  "001668": {
    "context": "001668",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「その後、開口部にエアジャッキを！　瓦礫を持ち上げ、２人を助け出します！」"
  },
  "001679": {
    "context": "001679",
    "code": "0x45",
    "developer_comment": "洵",
    "string": "「了解！　椿山さんと宇喜多さんは、いったん下がって！」"
  },
  "001690": {
    "context": "001690",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「では切断開始！！」"
  },
  "001711": {
    "context": "001711",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "レスキュー隊員２人による、撤去作業が始まったが――"
  },
  "001717": {
    "context": "001717",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "（ぐ……ち、ちくしょう……！）"
  },
  "001722": {
    "context": "001722",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "渡瀬の腕の筋力は、すでに失われつつあった。"
  },
  "001727": {
    "context": "001727",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "もう、いつ瓦礫の重さに負けてもおかしくない。"
  },
  "001732": {
    "context": "001732",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "渡瀬はユウリを見据えて言う。"
  },
  "001749": {
    "context": "001749",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「な、なぁユウリ……１つだけ、オレの願いを、聞いてくれるか？」"
  },
  "001760": {
    "context": "001760",
    "code": "0x45",
    "developer_comment": "ユウリ",
    "string": "「願い……？」"
  },
  "001771": {
    "context": "001771",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「オレに……『頑張れ』と、言ってくれ」"
  },
  "001821": {
    "context": "001821",
    "code": "0x45",
    "developer_comment": "ユウリ",
    "string": "「えっ……」"
  },
  "001832": {
    "context": "001832",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「頼、む……！」"
  },
  "001843": {
    "context": "001843",
    "code": "0x45",
    "developer_comment": "ユウリ",
    "string": "「隊長さん……！」"
  },
  "001893": {
    "context": "001893",
    "code": "0x45",
    "developer_comment": "ユウリ",
    "string": "「……頑張って、下さい」"
  },
  "001898": {
    "context": "001898",
    "code": "0x45",
    "developer_comment": "ユウリ",
    "string": "その言葉を聞いた途端、渡瀬の全身に再び力がみなぎった。"
  },
  "001909": {
    "context": "001909",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「おおおっしゃああ！」"
  },
  "001914": {
    "context": "001914",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "渾身の力を込めて、瓦礫を押し上げる。"
  },
  "001966": {
    "context": "001966",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "同時に瓦礫の一部が取り除かれ、ユウリの顔に光が差した。"
  },
  "001971": {
    "context": "001971",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "そして洵の手によって、瓦礫の隙間にエアジャッキが差し込まれ――"
  },
  "001982": {
    "context": "001982",
    "code": "0x45",
    "developer_comment": "洵",
    "string": "「エアジャッキセット完了！」"
  },
  "001993": {
    "context": "001993",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「皆さん、ジャッキアップ時に瓦礫が崩れないよう手を貸して下さい！」"
  },
  "002004": {
    "context": "002004",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "「任せて！」"
  },
  "002015": {
    "context": "002015",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "「よし、こっちは僕が支えよう！」"
  },
  "002026": {
    "context": "002026",
    "code": "0x45",
    "developer_comment": "洵",
    "string": "「空気注入、行くよぉっ！」"
  },
  "002031": {
    "context": "002031",
    "code": "0x45",
    "developer_comment": "洵",
    "string": "すると瓦礫が大きく持ち上がる。"
  },
  "002104": {
    "context": "002104",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「隊長！　ユウリさん！　早く！」"
  },
  "002109": {
    "context": "002109",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "渡瀬とユウリはその声に従い――"
  },
  "002114": {
    "context": "002114",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "瓦礫の下から抜け出した。"
  }
