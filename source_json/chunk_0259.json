  "000550": {
    "context": "000550",
    "code": "0x86",
    "developer_comment": "夏彦",
    "string": "？？"
  },
  "000551": {
    "context": "000551",
    "code": "0x86",
    "developer_comment": "夏彦",
    "string": "「件の事件の前からも、彼女はその能力ゆえに人々に疎まれ続けてきた」"
