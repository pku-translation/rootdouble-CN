{
  "000109": {
    "context": "000109",
    "code": "0x55",
    "string": "√Current Chapter2"
  },
  "000126": {
    "context": "000126",
    "code": "0x45",
    "developer_comment": "",
    "string": "――渡瀬が皆に、ユウリの死を告げると――皆は驚愕して目を見開いた。"
  },
  "000185": {
    "context": "000185",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "「バカな！　彼女が死んでるだと！？」"
  },
  "000214": {
    "context": "000214",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "「事故！？　他殺！？」"
  },
  "000225": {
    "context": "000225",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「わ、わからん……だが本当だ……あの娘の死体を、見つけたんだ……」"
  },
  "000254": {
    "context": "000254",
    "code": "0x45",
    "developer_comment": "洵",
    "string": "「ど、どこで！？　どこで見つけたの！」"
  },
  "000265": {
    "context": "000265",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「来てくれ……こっちだ……」"
  },
  "000308": {
    "context": "000308",
    "code": "0x45",
    "developer_comment": "",
    "string": "――破綻はそこから始まった。"
  },
  "000403": {
    "context": "000403",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「……し……死体が、なくなってる……！」"
  },
  "000432": {
    "context": "000432",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "「やれやれ……幻覚でも見たのではないのかね？」"
  },
  "000443": {
    "context": "000443",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「ほ、本当なんだ！　本当にここに死体が！」"
  },
  "000454": {
    "context": "000454",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「頭から血も流して、どう見ても死んでいる彼女が……」"
  },
  "000483": {
    "context": "000483",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "「血？　どこにも血痕なんかないわよ……？」"
  },
  "000494": {
    "context": "000494",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「……？？？」"
  },
  "000500": {
    "context": "000500",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "（しまった……！　こうなることは、当然予想しておくべきだったのに……！）"
  },
  "000543": {
    "context": "000543",
    "code": "0x45",
    "developer_comment": "",
    "string": "そして夏彦が手をこまねいている内に、さらに新たな問題が勃発し――"
  },
  "000603": {
    "context": "000603",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "（っ！？　ＡＤがなくなってる！？）"
  },
  "000614": {
    "context": "000614",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「そんな……！？」"
  },
  "000625": {
    "context": "000625",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「くそ、どうしてこんな事に！」"
  },
  "000720": {
    "context": "000720",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "「どうしてだって？　決まってるさ、ＡＤを持ち去った人がいるんだ！」"
  },
  "000749": {
    "context": "000749",
    "code": "0x45",
    "developer_comment": "洵",
    "string": "「そうだよ、そして……犯人はきっと、あたしたちの中にいる」"
  },
  "000760": {
    "context": "000760",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「……いや、そんなことはありえない。オレたちの中に犯人なんていやしない」"
  },
  "000803": {
    "context": "000803",
    "code": "0x45",
    "developer_comment": "",
    "string": "しかし皆の疑心暗鬼を晴らすために向かったその場所で、渡瀬が直面したものは――"
  },
  "000879": {
    "context": "000879",
    "code": "0x45",
    "developer_comment": "",
    "string": "――夏彦が渡瀬に与えた『幻想』を否定する、映像記録という『現実』だった。"
  },
  "000890": {
    "context": "000890",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「……え！？」"
  },
  "000936": {
    "context": "000936",
    "code": "0x45",
    "developer_comment": "洵",
    "string": "「死体、見つけてない……！？」"
  },
  "001025": {
    "context": "001025",
    "code": "0x45",
    "developer_comment": "",
    "string": "そして噴き上がる渡瀬への疑惑。"
  },
  "001054": {
    "context": "001054",
    "code": "0x45",
    "developer_comment": "洵",
    "string": "「隊長は……あたしたちを、騙してたの……？」"
  },
  "001065": {
    "context": "001065",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「ち、違う……」"
  },
  "001154": {
    "context": "001154",
    "code": "0x45",
    "developer_comment": "",
    "string": "さらに向けられる厳しい追及。"
  },
  "001183": {
    "context": "001183",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "「君は要救助者を助けるため、地下に降りてきたという。だがそれもまた嘘で――」"
  },
  "001194": {
    "context": "001194",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "「本当は別の目的のために、降りてきたんじゃないのかね」"
  },
  "001205": {
    "context": "001205",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「べ、別の目的って……」"
  },
  "001234": {
    "context": "001234",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "「決まってるさ。殺人のためだ」"
  },
  "001323": {
    "context": "001323",
    "code": "0x45",
    "developer_comment": "",
    "string": "その果てに渡瀬が突き付けられたのは――"
  },
  "001352": {
    "context": "001352",
    "code": "0x45",
    "developer_comment": "洵",
    "string": "「ねぇ、教えてよ……隊長は……人の命を救うために、ここに来たんじゃないの……？」"
  },
  "001357": {
    "context": "001357",
    "code": "0x45",
    "developer_comment": "",
    "string": "――殺気と憤怒に満ちた、２人の視線だった。"
  },
  "001363": {
    "context": "001363",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "（ま、まずい……！）"
  },
  "001369": {
    "context": "001369",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "（この状況、前に渡瀬が疑われた時よりも……ッ！）"
  },
  "001374": {
    "context": "001374",
    "code": "0x45",
    "developer_comment": "",
    "string": "だが洵たちの疑念が的を射ている事は、夏彦が一番よくわかっていた。"
  },
  "001379": {
    "context": "001379",
    "code": "0x45",
    "developer_comment": "",
    "string": "なぜなら実際に――"
  },
  "001484": {
    "context": "001484",
    "code": "0x45",
    "developer_comment": "",
    "string": "――夏彦もましろも、渡瀬に銃撃されたのだから。"
  },
  "001620": {
    "context": "001620",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "（ッ……！！！）"
  },
  "001625": {
    "context": "001625",
    "code": "0x45",
    "developer_comment": "",
    "string": "ましろの泣き声が、サリュの傷ついた姿が、夏彦の意識に甦る。"
  },
  "001631": {
    "context": "001631",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "（でも……今のぼくは、渡瀬とは運命共同体なんだ）"
  },
  "001637": {
    "context": "001637",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "（それに悔しいけど、今の渡瀬はレスキュー隊員として頼りになる……）"
  },
  "001643": {
    "context": "001643",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "（肉体のないぼくが、サリュやユウリを救うためには、どうしたって渡瀬の力が必要なんだ……！）"
  },
  "001649": {
    "context": "001649",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "（だから今は、なんとかこいつの疑惑を晴らさなきゃ……！）"
  },
  "001654": {
    "context": "001654",
    "code": "0x45",
    "developer_comment": "",
    "string": "そう思ったが、方法が思いつかない。"
  },
  "001659": {
    "context": "001659",
    "code": "0x45",
    "developer_comment": "",
    "string": "しかし、その直後――意外な人物が、渡瀬を庇った。"
  },
  "001743": {
    "context": "001743",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "「待ちなさい、２人とも。あなたたち、少しおかしいわよ」"
  },
  "001804": {
    "context": "001804",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "「なんなのよ、ＡＤがなくなったとたん手のひら返したように。それがまともな人間のする事？」"
  },
  "001810": {
    "context": "001810",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "（ッ！　ダメだよ先生、こんな時に渡瀬を庇ったら――！）"
  },
  "001815": {
    "context": "001815",
    "code": "0x45",
    "developer_comment": "",
    "string": "すると洵と宇喜多が、ぎろりと恵那を睨みつける。"
  },
  "001844": {
    "context": "001844",
    "code": "0x45",
    "developer_comment": "洵",
    "string": "「今の話聞いてどうして、椿山さんは隊長の味方するの？　よく知りもしない他人でしょ？」"
  },
  "001873": {
    "context": "001873",
    "code": "0x45",
    "developer_comment": "洵",
    "string": "「まさか……２人はグルだったとか？」"
  },
  "001902": {
    "context": "001902",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "「そうか、なるほど、道理で行動がおかしいと思ったら……！」"
  },
  "001931": {
    "context": "001931",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "「ようやくわかったぞ！　君たちは２人だけで、助かろうというつもりなんだな！」"
  },
  "001942": {
    "context": "001942",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "「ＡＤを隠したのは君たちか！？　２人でＡＤを独占しているんだ！」"
  },
  "002060": {
    "context": "002060",
    "code": "0x45",
    "developer_comment": "",
    "string": "洵と宇喜多が、渡瀬と恵那ににじり寄ってくる。とっさに夏彦は、渡瀬に告げた。"
  },
  "002130": {
    "context": "002130",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "《危険だ……先生を連れて逃げろ！》"
  },
  "002213": {
    "context": "002213",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「っ！」"
  },
  "002218": {
    "context": "002218",
    "code": "0x45",
    "developer_comment": "",
    "string": "夏彦の声を受け、渡瀬が恵那の手を引く。"
  },
  "002314": {
    "context": "002314",
    "code": "0x45",
    "developer_comment": "洵",
    "string": "「逃げるつもり！？」"
  },
  "002325": {
    "context": "002325",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "「いかん！　取り押さえろ！」"
  },
  "002373": {
    "context": "002373",
    "code": "0x45",
    "developer_comment": "",
    "string": "その声を背に聞きながら、渡瀬と恵那は逃げ出した。"
  }
}