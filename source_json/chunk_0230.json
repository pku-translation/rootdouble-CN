{
  "000046": {
    "context": "000046",
    "code": "0x55",
    "string": "√Before Chapter7"
  },
  "000060": {
    "context": "000060",
    "code": "0x45",
    "developer_comment": "",
    "string": "――――――"
  },
  "000065": {
    "context": "000065",
    "code": "0x45",
    "developer_comment": "",
    "string": "――――"
  },
  "000070": {
    "context": "000070",
    "code": "0x45",
    "developer_comment": "",
    "string": "――"
  },
  "000075": {
    "context": "000075",
    "code": "0x45",
    "developer_comment": "",
    "string": "――そうして、記憶の中の映像は消え――"
  },
  "000146": {
    "context": "000146",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「――……！」"
  },
  "000151": {
    "context": "000151",
    "code": "0x45",
    "developer_comment": "",
    "string": "夢から覚めたように、夏彦は現実に戻ってきた。"
  },
  "000156": {
    "context": "000156",
    "code": "0x45",
    "developer_comment": "",
    "string": "目の前には壁に押し付けられ、泣いているましろがいる。"
  },
  "000161": {
    "context": "000161",
    "code": "0x45",
    "developer_comment": "",
    "string": "どうやら現実では、ほんの一瞬しか経っていないらしい。"
  },
  "000166": {
    "context": "000166",
    "code": "0x45",
    "developer_comment": "",
    "string": "だが夏彦は、彼女の９年間に触れ――とめどなく涙を流していた。"
  },
  "000177": {
    "context": "000177",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「な……夏彦？」"
  },
  "000182": {
    "context": "000182",
    "code": "0x45",
    "developer_comment": "",
    "string": "戸惑うように言うましろ。"
  },
  "000187": {
    "context": "000187",
    "code": "0x45",
    "developer_comment": "",
    "string": "その声を聞いた瞬間、"
  },
  "000198": {
    "context": "000198",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「……！」"
  },
  "000203": {
    "context": "000203",
    "code": "0x45",
    "developer_comment": "",
    "string": "夏彦は無言で、彼女を抱きしめていた。"
  },
  "000330": {
    "context": "000330",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「っ！？」"
  },
  "000335": {
    "context": "000335",
    "code": "0x45",
    "developer_comment": "",
    "string": "ましろが身を強張らせるのがわかる。"
  },
  "000340": {
    "context": "000340",
    "code": "0x45",
    "developer_comment": "",
    "string": "夏彦は彼女を強く抱いて言った。"
  },
  "000351": {
    "context": "000351",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「ましろ……！」"
  },
  "000362": {
    "context": "000362",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「ごめん……！　ごめんな……！」"
  },
  "000373": {
    "context": "000373",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「え……？」"
  },
  "000384": {
    "context": "000384",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「お前、ずっとぼくを支えてくれてたんだな……」"
  },
  "000395": {
    "context": "000395",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「ぼくのために、ずっと一緒にいてくれてたんだな……！」"
  },
  "000400": {
    "context": "000400",
    "code": "0x45",
    "developer_comment": "",
    "string": "彼女はその言葉に息を呑む。"
  },
  "000411": {
    "context": "000411",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「ど、どうしてそのこと……！？」"
  },
  "000422": {
    "context": "000422",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「勝手にやって申し訳なかったけど……ＢＣで、お前の記憶を読んだんだ……！」"
  },
  "000433": {
    "context": "000433",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「え……」"
  },
  "000444": {
    "context": "000444",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「記憶って、そんな事が……！？」"
  },
  "000455": {
    "context": "000455",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「できたんだよ。恐らく、ＢＣの新しい能力だと思うんだけど」"
  },
  "000466": {
    "context": "000466",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「ただお前の気持ちが知りたくて、心底そう願ったら――」"
  },
  "000477": {
    "context": "000477",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「９年前から今日までのお前の記憶が、ぼくにも見えたんだ」"
  },
  "000528": {
    "context": "000528",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「……！」"
  },
  "000539": {
    "context": "000539",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「それで全部知った。ぼくが悠里を失った時、心が壊れてしまったことも」"
  },
  "000550": {
    "context": "000550",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「そしてお前が、ずっと見守ってくれてた事も……」"
  },
  "000561": {
    "context": "000561",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「そんなことまで……！」"
  },
  "000572": {
    "context": "000572",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「あぁ、だから……」"
  },
  "000577": {
    "context": "000577",
    "code": "0x45",
    "developer_comment": "",
    "string": "そこまで言って、夏彦の声は詰まった。"
  },
  "000582": {
    "context": "000582",
    "code": "0x45",
    "developer_comment": "",
    "string": "込み上げる感情を必死で抑え、言葉を搾り出す。"
  },
  "000593": {
    "context": "000593",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「……ありがとう」"
  },
  "000604": {
    "context": "000604",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「……ありがとう、ましろ……」"
  },
  "000609": {
    "context": "000609",
    "code": "0x45",
    "developer_comment": "",
    "string": "涙がまたも溢れ出す。"
  },
  "000614": {
    "context": "000614",
    "code": "0x45",
    "developer_comment": "",
    "string": "この６日間、幻の悠里はずっと自分に囁きかけていた。"
  },
  "000705": {
    "context": "000705",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「だから周りの人を疑わないで」"
  },
  "000716": {
    "context": "000716",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「……疑うなら、わたしだけでいい」"
  },
  "000727": {
    "context": "000727",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「何が本当で何が嘘でも、ひこりんは善意に囲まれているはずだよ？」"
  },
  "000797": {
    "context": "000797",
    "code": "0x45",
    "developer_comment": "",
    "string": "――悠里の言っていたことは本当だった。"
  },
  "000802": {
    "context": "000802",
    "code": "0x45",
    "developer_comment": "",
    "string": "夏彦は、周りの人間に護られ続けていたのだ。"
  },
  "000807": {
    "context": "000807",
    "code": "0x45",
    "developer_comment": "",
    "string": "９年もの間、ずっと。"
  },
  "000812": {
    "context": "000812",
    "code": "0x45",
    "developer_comment": "",
    "string": "そしてましろもまた――"
  },
  "000863": {
    "context": "000863",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「いいんだよ……」"
  },
  "000868": {
    "context": "000868",
    "code": "0x45",
    "developer_comment": "",
    "string": "夏彦にそう言ってくれた。"
  },
  "000879": {
    "context": "000879",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「私ね、ずっと心配だったんだ……」"
  },
  "000890": {
    "context": "000890",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「……心配？」"
  },
  "000901": {
    "context": "000901",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「うん……夏彦がこの事に気付いたら、また夏彦が９年前と同じになっちゃうんじゃないかって」"
  },
  "000907": {
    "context": "000907",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「……」"
  },
  "000918": {
    "context": "000918",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「……でも、夏彦はそうはならなかった」"
  },
  "000929": {
    "context": "000929",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「ちゃんと悲しみを受け止めてくれた」"
  },
  "000940": {
    "context": "000940",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「それだけで私、嬉しいんだよ……」"
  },
  "000951": {
    "context": "000951",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「ねぇ夏彦……よく、耐えたね……」"
  },
  "000962": {
    "context": "000962",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「……頑張ったね……」"
  },
  "000968": {
    "context": "000968",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「……」"
  },
  "000973": {
    "context": "000973",
    "code": "0x45",
    "developer_comment": "",
    "string": "夏彦はその言葉に、首を振って言う。"
  },
  "000984": {
    "context": "000984",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「ましろ……ぼくが耐えられたのは、お前のおかげなんだ」"
  },
  "000995": {
    "context": "000995",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「お前がずっと……そばにいてくれたから……」"
  },
  "001006": {
    "context": "001006",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「……そばにいたのは、悠里だよ？」"
  },
  "001017": {
    "context": "001017",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「違う。お前のおかげでぼくは……――」"
  },
  "001022": {
    "context": "001022",
    "code": "0x45",
    "developer_comment": "",
    "string": "それ以上は言葉にならなかった。"
  },
  "001027": {
    "context": "001027",
    "code": "0x45",
    "developer_comment": "",
    "string": "夏彦はただ、ましろをもう一度強く抱きしめた。"
  },
  "001032": {
    "context": "001032",
    "code": "0x45",
    "developer_comment": "",
    "string": "９年分の思いを込めて。"
  },
  "001083": {
    "context": "001083",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「夏彦……」"
  },
  "001094": {
    "context": "001094",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「……私、夏彦のそばにいられてよかったよ」"
  },
  "001099": {
    "context": "001099",
    "code": "0x45",
    "developer_comment": "",
    "string": "ましろが夏彦を抱き返す。"
  },
  "001142": {
    "context": "001142",
    "code": "0x45",
    "developer_comment": "",
    "string": "そのまま２人は、しばらく抱き合った。"
  },
  "001147": {
    "context": "001147",
    "code": "0x45",
    "developer_comment": "",
    "string": "言葉もなく、ＢＣも使わず――。"
  },
  "001207": {
    "context": "001207",
    "code": "0x45",
    "developer_comment": "",
    "string": "――それからどれくらい時が経ったのだろう。"
  },
  "001266": {
    "context": "001266",
    "code": "0x45",
    "developer_comment": "",
    "string": "やがて夏彦は、そっとましろを離して言った。"
  },
  "001277": {
    "context": "001277",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「……行こうか、ましろ」"
  },
  "001288": {
    "context": "001288",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「ん……」"
  },
  "001299": {
    "context": "001299",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「お前をここから脱出させるのが、ぼくの役目だ……それはちゃんと果たさなきゃな」"
  },
  "001328": {
    "context": "001328",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「でも、カードは……？」"
  },
  "001339": {
    "context": "001339",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「……この棟にあるはずだ。なんとなくだけど、わかるんだよ」"
  },
  "001344": {
    "context": "001344",
    "code": "0x45",
    "developer_comment": "",
    "string": "――幻影の悠里は、『夏彦が見落としているだけ』と言っていた。"
  },
  "001349": {
    "context": "001349",
    "code": "0x45",
    "developer_comment": "",
    "string": "彼女の言葉を信じてみようと思った。"
  },
  "001355": {
    "context": "001355",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "（思い出せ……ぼくは見ているはずだ）"
  },
  "001361": {
    "context": "001361",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "（すでに行った場所の、どこにカードがあった！？）"
  },
  "001366": {
    "context": "001366",
    "code": "0x45",
    "developer_comment": "",
    "string": "そう思った瞬間、フラッシュバックのように――"
  },
  "001426": {
    "context": "001426",
    "code": "0x45",
    "developer_comment": "",
    "string": "カードのありかが明滅した。"
  },
  "001507": {
    "context": "001507",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「――隣の階段室だ！　探そうましろ！」"
  },
  "001536": {
    "context": "001536",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「う、うん！」"
  },
  "001590": {
    "context": "001590",
    "code": "0x45",
    "developer_comment": "",
    "string": "夏彦たちはその部屋を飛び出し、階段室に駆け込んだ。"
  },
  "001640": {
    "context": "001640",
    "code": "0x45",
    "developer_comment": "",
    "string": "電灯が壊れて、真っ暗な階段室。"
  },
  "001700": {
    "context": "001700",
    "code": "0x45",
    "developer_comment": "",
    "string": "そこを目を凝らして、ましろと一緒にカードを探す。"
  },
  "001746": {
    "context": "001746",
    "code": "0x45",
    "developer_comment": "",
    "string": "するとやがて、瓦礫の陰に――四角いものが落ちているのを見つけた。"
  },
  "001757": {
    "context": "001757",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「ッ！　あったぞ！」"
  },
  "001786": {
    "context": "001786",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「え！？」"
  },
  "001791": {
    "context": "001791",
    "code": "0x45",
    "developer_comment": "",
    "string": "夏彦が拾い上げたカードを、ましろも覗き込む。"
  },
  "001877": {
    "context": "001877",
    "code": "0x45",
    "developer_comment": "",
    "string": "そこには『天川美夜子』という名と、『Ｌｖ．Ｎ』という文字が書かれていた。"
  },
  "001888": {
    "context": "001888",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「母さんのカード……？　どうしてここに？」"
  },
  "001919": {
    "context": "001919",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「避難するときに、落としていったのかな」"
  },
  "001924": {
    "context": "001924",
    "code": "0x45",
    "developer_comment": "",
    "string": "詳しい理由はわからない。しかし――"
  },
  "001929": {
    "context": "001929",
    "code": "0x45",
    "developer_comment": "",
    "string": "それはあたかも悠里が、夏彦たちに遺してくれたカードに思えた。"
  },
  "001940": {
    "context": "001940",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「とにかくこれなら、きっとあのセキュリティゲートも開けられるぞ！」"
  },
  "001969": {
    "context": "001969",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「だね！　さっそく試してみよう！」"
  },
  "001980": {
    "context": "001980",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「あぁ！」"
  },
  "001985": {
    "context": "001985",
    "code": "0x45",
    "developer_comment": "",
    "string": "夏彦たちは連れ立って、階段室を飛び出した。"
  },
  "002073": {
    "context": "002073",
    "code": "0x45",
    "developer_comment": "",
    "string": "そして先ほど行く手を阻まれた、あのゲートの前に着く。"
  },
  "002078": {
    "context": "002078",
    "code": "0x45",
    "developer_comment": "",
    "string": "夏彦は先ほど手に入れたカードを、ゲートの脇のカードスロットに通した。"
  },
  "002089": {
    "context": "002089",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「頼む……」"
  },
  "002118": {
    "context": "002118",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「……お願い」"
  },
  "002123": {
    "context": "002123",
    "code": "0x45",
    "developer_comment": "",
    "string": "夏彦とましろは、寄り添うように祈る。"
  },
  "002128": {
    "context": "002128",
    "code": "0x45",
    "developer_comment": "",
    "string": "……"
  },
  "002133": {
    "context": "002133",
    "code": "0x45",
    "developer_comment": "",
    "string": "………"
  },
  "002138": {
    "context": "002138",
    "code": "0x45",
    "developer_comment": "",
    "string": "…………"
  },
  "002143": {
    "context": "002143",
    "code": "0x45",
    "developer_comment": "",
    "string": "…………その祈りが、天に届いたのか――"
  },
  "002232": {
    "context": "002232",
    "code": "0x45",
    "developer_comment": "",
    "string": "ゆっくりと、目の前のゲートが開いた。"
  },
  "002243": {
    "context": "002243",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「……！」"
  },
  "002248": {
    "context": "002248",
    "code": "0x45",
    "developer_comment": "",
    "string": "ましろが目を輝かせ、夏彦を見る。"
  },
  "002259": {
    "context": "002259",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「よし……！　この先の第６エリアに、きっと脱出口がある」"
  },
  "002288": {
    "context": "002288",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「わかるの？」"
  },
  "002299": {
    "context": "002299",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「９年前もさ、第６エリアから脱出したんだよ」"
  },
  "002304": {
    "context": "002304",
    "code": "0x45",
    "developer_comment": "",
    "string": "夏彦の言葉に、ましろが複雑な笑みを浮かべる。"
  },
  "002352": {
    "context": "002352",
    "code": "0x45",
    "developer_comment": "",
    "string": "夏彦自身も寂しさを感じ、来た道を振り返った。"
  },
  "002357": {
    "context": "002357",
    "code": "0x45",
    "developer_comment": "",
    "string": "――あの日、悠里と共に閉じ込められた第５エリア。"
  },
  "002362": {
    "context": "002362",
    "code": "0x45",
    "developer_comment": "",
    "string": "そして悠里の幻影が消失した場所。"
  },
  "002415": {
    "context": "002415",
    "code": "0x45",
    "developer_comment": "",
    "string": "だがそこには何もない。"
  },
  "002420": {
    "context": "002420",
    "code": "0x45",
    "developer_comment": "",
    "string": "悠里はもう、どこにもいない。"
  },
  "002425": {
    "context": "002425",
    "code": "0x45",
    "developer_comment": "",
    "string": "夏彦はようやく、その事実を受け入れ――"
  },
  "002431": {
    "context": "002431",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "（さよなら、悠里……）"
  },
  "002436": {
    "context": "002436",
    "code": "0x45",
    "developer_comment": "",
    "string": "彼女に、別れを告げた。"
  },
  "002494": {
    "context": "002494",
    "code": "0x45",
    "developer_comment": "",
    "string": "もはや彼女がいるのは、記憶の中だけだ。"
  },
  "002499": {
    "context": "002499",
    "code": "0x45",
    "developer_comment": "",
    "string": "夏彦の脳裏に、懐かしい想い出がよぎる。"
  },
  "002504": {
    "context": "002504",
    "code": "0x45",
    "developer_comment": "",
    "string": "――それは彼女の、生前の記憶――"
  },
  "002555": {
    "context": "002555",
    "code": "0x55",
    "string": "√Before Chapter7"
  },
  "002601": {
    "context": "002601",
    "code": "0x45",
    "developer_comment": "",
    "string": "――あの時の約束を、夏彦は結局果たせなかった。"
  },
  "002606": {
    "context": "002606",
    "code": "0x45",
    "developer_comment": "",
    "string": "ずっと護られていたのは、自分だった。"
  },
  "002611": {
    "context": "002611",
    "code": "0x45",
    "developer_comment": "",
    "string": "押し寄せる後悔、喪失感、そして無力感。"
  },
  "002616": {
    "context": "002616",
    "code": "0x45",
    "developer_comment": "",
    "string": "しかし、だからこそ――やらなければならない事がある。"
  },
  "002622": {
    "context": "002622",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "（……ぼくにできる贖罪は、ひとつだけだ）"
  },
  "002628": {
    "context": "002628",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "（ずっと一緒にいてくれた、もう１人の女の子を……！）"
  },
  "002633": {
    "context": "002633",
    "code": "0x45",
    "developer_comment": "",
    "string": "必ず、護ってみせる。"
  },
  "002638": {
    "context": "002638",
    "code": "0x45",
    "developer_comment": "",
    "string": "追憶が彼に、使命感を与えた。"
  },
  "002643": {
    "context": "002643",
    "code": "0x45",
    "developer_comment": "",
    "string": "悠里への感謝の気持ちと共に、夏彦は再び誓いを立てた。"
  },
  "002724": {
    "context": "002724",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「……」"
  },
  "002729": {
    "context": "002729",
    "code": "0x45",
    "developer_comment": "",
    "string": "ましろが無言で夏彦の手を握る。彼はその手を握り返す。"
  },
  "002892": {
    "context": "002892",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「行こう、ましろ！」"
  },
  "002903": {
    "context": "002903",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「……うん！」"
  },
  "002948": {
    "context": "002948",
    "code": "0x45",
    "developer_comment": "",
    "string": "２人はそう言葉を交わし、第５エリアを出た。"
  },
  "002953": {
    "context": "002953",
    "code": "0x45",
    "developer_comment": "",
    "string": "過去と別離し、そして――"
  },
  "002958": {
    "context": "002958",
    "code": "0x45",
    "developer_comment": "",
    "string": "未来に向けて。"
  }
}