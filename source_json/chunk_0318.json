  "002858": {
    "context": "002858",
    "code": "0x45",
    "developer_comment": "",
    "string": "すると渡瀬は破顔した。"
  },
  "002915": {
    "context": "002915",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「はっ！　そうかそうか、俺が心配だったか！」"
  },
  "002926": {
    "context": "002926",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "「そ、そうとも」"
  },
  "002955": {
    "context": "002955",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「ならいいけどな。疑われてたらどうしようと思ってたんだ」"
  },
  "002984": {
    "context": "002984",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「だって堂島みたいなゴツイ男を素手で殺せるのは、俺くらいだもんなぁ？」"
  },
  "003010": {
    "context": "003010",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "「うっ……」"
  },
  "003039": {
    "context": "003039",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「でもな、それはありえねぇよ。俺は何しろ記憶喪失だし、あいつを殺す動機なんかない」"
  },
  "003050": {
    "context": "003050",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「違うか、おっさん？」"
  },
  "003061": {
    "context": "003061",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "「あ、ああ……」"
  },
  "003090": {
    "context": "003090",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「おう、それでいい。それじゃさっさと消火システムを起動するぞ」"
  },
  "003101": {
    "context": "003101",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「さ、案内してくれ」"
  },
