  "000898": {
    "context": "000898",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "眠い"
  },
  "000904": {
    "context": "000904",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "（待てもしかして、これって――）"
  },
  "000909": {
    "context": "000909",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "渡瀬の中に滞留していた"
