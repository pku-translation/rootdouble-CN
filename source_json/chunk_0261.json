  "000946": {
    "context": "000946",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "（う、嘘だろ……！）"
  },
  "000951": {
    "context": "000951",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "夏彦は薄れいく意識の中に、慌てて気を張り巡らせた。"
