  },
  "000925": {
    "context": "000925",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "（ぼくが消えようとしているのか！？）"
  },
  "000930": {
    "context": "000930",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "――消える？"
