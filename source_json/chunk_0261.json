  "001164": {
    "context": "001164",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "２人の会話の内容が、理解できるようになってきた。"
  },
  "001175": {
    "context": "001175",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「なっ……まさか、１人で行くつもりか？」"
