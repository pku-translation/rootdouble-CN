  "000887": {
    "context": "000887",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "だが何を見せているのか、理解できない。"
  },
  "000893": {
    "context": "000893",
    "code": "0x45",
    "developer_comment": "夏彦",
