  "001426": {
    "context": "001426",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "「え？」"
  },
  "001437": {
    "context": "001437",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「あいつらは銃の一件で、先生に対してもかなり疑心を抱いている」"
