{
  "000013": {
    "context": "000013",
    "code": "0x55",
    "developer_comment": "",
    "string": "√Before Chapter1"
  },
  "000021": {
    "context": "000021",
    "code": "0x45",
    "developer_comment": "",
    "string": "――やがて食事も終わり。"
  },
  "000026": {
    "context": "000026",
    "code": "0x45",
    "developer_comment": "",
    "string": "しばらく食後の談笑をした後、ましろの帰る時間が訪れた。"
  },
  "000115": {
    "context": "000115",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「それじゃ夏彦に悠里、また明日ね」"
  },
  "000120": {
    "context": "000120",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "いつものように言うましろに、夏彦たちは声をかける。"
  },
  "000149": {
    "context": "000149",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「ましろん、今日もありがと」"
  },
  "000160": {
    "context": "000160",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「たまには送っていこうか？」"
  },
  "000189": {
    "context": "000189",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「いいのいいの。どうせすぐ近くだし」"
  },
  "000218": {
    "context": "000218",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「それじゃおやすみ、おふたりさ～ん」"
  },
  "000257": {
    "context": "000257",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "夏彦たちの声にましろは笑みを返し、家を出て行った。"
  },
  "000280": {
    "context": "000280",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "――ましろが帰れば、あとはもうする事はない。"
  },
  "000285": {
    "context": "000285",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "寝るまでの時間を、のんびり過ごすだけだ。"
  },
  "000296": {
    "context": "000296",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「さて……シャワーでも浴びるかな」"
  },
  "000325": {
    "context": "000325",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「はぁい、どうぞ～」"
  },
  "000330": {
    "context": "000330",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "悠里はいつも、夏彦に気を遣って、昼間に風呂を済ませているらしい。"
  },
  "000341": {
    "context": "000341",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「それじゃちょっと失礼するよ」"
  },
  "000346": {
    "context": "000346",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "夏彦はそう言って、バスルームに向かった。"
  },
  "000465": {
    "context": "000465",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "簡単にシャワーを済ませ――"
  },
  "000477": {
    "context": "000477",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "ついでに歯磨きを終えて、リビングに戻ると……"
  },
  "000599": {
    "context": "000599",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "悠里は本を読みながら、夏彦の戻りを待っていた。"
  },
  "000628": {
    "context": "000628",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「あ、おかえりひこりん～」"
  },
  "000639": {
    "context": "000639",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「ただいま」"
  },
  "000668": {
    "context": "000668",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「ひこりん、もう寝る？　少しお喋りしない？」"
  },
  "000679": {
    "context": "000679",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「いいね、それじゃホットミルクでも入れようか」"
  },
  "000708": {
    "context": "000708",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「あ、飲む飲む～」"
  },
  "000719": {
    "context": "000719",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「了解」"
  },
  "000770": {
    "context": "000770",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "夏彦はそう答え、キッチンに向かった。"
  },
  "000775": {
    "context": "000775",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "ホットミルクは、夏彦と悠里の共通の好物なのだ。"
  },
  "000780": {
    "context": "000780",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "２人分のミルクを温め、リビングに戻ってくる。"
  },
  "000858": {
    "context": "000858",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「はい、どうぞ」"
  },
  "000887": {
    "context": "000887",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「ありがと～」"
  },
  "000908": {
    "context": "000908",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "夏彦たちはテーブルを囲み、ソファに腰掛けた。"
  },
  "000919": {
    "context": "000919",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「悠里、今日はどういう風に過ごしたんだい？」"
  },
  "000948": {
    "context": "000948",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「えっとね、少し編み物をしたよ」"
  },
  "000953": {
    "context": "000953",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "そう言えば悠里はこの夏から、編み物を始めていた。"
  },
  "000958": {
    "context": "000958",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "夏彦とましろに、プレゼントするためだという。"
  },
  "000969": {
    "context": "000969",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「あぁ、進みはどうだい？」"
  },
  "000998": {
    "context": "000998",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「どうかなぁ……冬までにできればいいけど」"
  },
  "001009": {
    "context": "001009",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「はは、それじゃ楽しみにしておこうかな」"
  },
  "001038": {
    "context": "001038",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「うぅ……期待に添えるように頑張るよ」"
  },
  "001043": {
    "context": "001043",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "夏彦の答えを聞いて、悠里は微笑む。"
  },
  "001048": {
    "context": "001048",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "こうしてましろが帰った後、悠里と穏やかな会話を交わすのを、夏彦は日々の楽しみにしていた。"
  },
  "001077": {
    "context": "001077",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「それとねぇ、今日は２冊本を読んで、さっき３冊目に突入したところだよ」"
  },
  "001088": {
    "context": "001088",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「へぇ、どんな本？」"
  },
  "001117": {
    "context": "001117",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「１冊目はね、南米の作家さんの本で……ある一族の隆盛から滅亡までを描いた、幻想的な物語」"
  },
  "001128": {
    "context": "001128",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「２冊目はね、夢をテーマにした連作だよ。例えば死んでしまった女の人を、１００年間ずっと待ち続ける男の人の話とか」"
  },
  "001139": {
    "context": "001139",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「なるほどね、どっちも『夢』とか『幻想』ってのがキーワードか」"
  },
  "001168": {
    "context": "001168",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「それと『孤独』と『約束』かな？」"
  },
  "001173": {
    "context": "001173",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "悠里はいわゆる文学少女で、かなりの濫読家だ。毎日ジャンルを選ばず、少なくとも１冊は読む。"
  },
  "001178": {
    "context": "001178",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "そして夏彦に、その感想を語ってくれるのだ。"
  },
  "001207": {
    "context": "001207",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「ちなみに、こんな素敵なセリフがあったよ」"
  },
  "001236": {
    "context": "001236",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「『天から落ちた星のかけらを、私のお墓に置いてくれますか』」"
  },
  "001247": {
    "context": "001247",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「『そうして１００年、お墓のそばで待っててくれますか。きっと逢いにきますから』」"
  },
  "001258": {
    "context": "001258",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「む、ムチャクチャ言うなぁ」"
  },
  "001333": {
    "context": "001333",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「でもそう言った女性にね、男の人は『待っている』と即答するの。そして本当にたった１人で、１００年間待っちゃうの」"
  },
  "001344": {
    "context": "001344",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「え？　そりゃすごいな……２人は恋人同士だったのか？」"
  },
  "001373": {
    "context": "001373",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「違うんだよ～。ただ約束をしたってだけの相手」"
  },
  "001402": {
    "context": "001402",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「なのに女の人もちゃんと約束を守って、男の人に逢いに来るの」"
  },
  "001413": {
    "context": "001413",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「１００年前と同じ星が瞬く夜、花に姿を変えてね～」"
  },
  "001432": {
    "context": "001432",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「へぇ……綺麗な話だな」"
  },
  "001461": {
    "context": "001461",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「でしょ？　ロマンチックでしょ？」"
  },
  "001490": {
    "context": "001490",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「ね、ひこりんならどう？　１００年間も１つの約束を、守り続ける事ができる？」"
  },
  "001501": {
    "context": "001501",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「うーん、さすがに『待っている』って即答するのは難しいだろうけど……」"
  },
  "001512": {
    "context": "001512",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「それでも約束したのなら、きっと守ると思うよ」"
  },
  "001541": {
    "context": "001541",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「だよね。ひこりん約束破らないもんね？」"
  },
  "001552": {
    "context": "001552",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「あぁ、それだけが取り柄だからさ」"
  },
  "001566": {
    "context": "001566",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「へぇ……なんていうか、壮絶な話だな」"
  },
  "001595": {
    "context": "001595",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「でも、綺麗な話でしょう？」"
  },
  "001606": {
    "context": "001606",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「うん、確かに」"
  },
  "001611": {
    "context": "001611",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "そう言って夏彦は、悠里と笑みを交わした。"
  },
  "001662": {
    "context": "001662",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "――本を読んで編み物をして、日々を気ままに過ごす。それが悠里の日常だった。"
  },
  "001667": {
    "context": "001667",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "学校に行く事もなく、働く事もなく……この家から一歩も出る事なく。"
  },
  "001672": {
    "context": "001672",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "そしてそれを見守るのが、夏彦の務めだった。"
  },
  "001678": {
    "context": "001678",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "――悠里が家から出られなくなったのには、事情がある。"
  },
  "001683": {
    "context": "001683",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "それは９年前に起きた、ある出来事が関わっていた。"
  },
  "001823": {
    "context": "001823",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "……あの頃の悠里は、のんびり屋ではあるけれど、今よりはずっと行動的だった。"
  },
  "001922": {
    "context": "001922",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "そして夏彦は、悠里たちを引っ張りまわして、色んなところを探検していた。"
  },
  "002019": {
    "context": "002019",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "しかしある日、夏彦は――"
  },
  "002024": {
    "context": "002024",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "自分の行動が原因で、悠里を死の危険にさらさせてしまったのだ。"
  },
  "002438": {
    "context": "002438",
    "code": "0x45",
    "developer_comment": "泣きじゃくる悠里の声――",
    "string": "思い出すだけで身震いが起きる。"
  },
  "002443": {
    "context": "002443",
    "code": "0x45",
    "developer_comment": "泣きじゃくる悠里の声――",
    "string": "この９年間、何度も夏彦は後悔した。"
  },
  "002449": {
    "context": "002449",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "（あんな事、もうあっちゃいけない……）"
  },
  "002454": {
    "context": "002454",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "それは夏彦の心の傷にもなっていた。"
  },
  "002503": {
    "context": "002503",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "だが悠里の傷は、夏彦のそれよりはるかに深かっただろう。"
  },
  "002604": {
    "context": "002604",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "折悪しく、悠里の両親が交通事故で他界し……彼女は天涯孤独の身になった。"
  },
  "002609": {
    "context": "002609",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "それを不憫に思った夏彦の母親が、悠里を引き取ったのだが――"
  },
  "002614": {
    "context": "002614",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "その時にはすでに、度重なる悲劇が、幼い彼女の心を引き裂いていた。"
  },
  "002720": {
    "context": "002720",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "あの日から悠里は、外に出る事を極度に恐れ、家にこもって暮らすようになったのだ。"
  },
  "002725": {
    "context": "002725",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "無理に外に出そうとする事は、誰にもできなかった。"
  },
  "002815": {
    "context": "002815",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「な、なぁ悠里……そとにでてみない？」"
  },
  "002844": {
    "context": "002844",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「い……いやっ……！」"
  },
  "002849": {
    "context": "002849",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "天川家に住むようになってから３日目の事。"
  },
  "002854": {
    "context": "002854",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "家から出ようとしない彼女が心配になり、夏彦は意を決して、そう声をかけた。"
  },
  "002865": {
    "context": "002865",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「いやって、そういうわけにもいかないだろ？」"
  },
  "002894": {
    "context": "002894",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「やなの……こわいの……！」"
  },
  "002905": {
    "context": "002905",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「このまま学校にもいかないで、ここでずっと暮らすのか？」"
  },
  "002916": {
    "context": "002916",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「ほら、ぼくがいっしょにいってあげるから――」"
  },
  "002999": {
    "context": "002999",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "夏彦が手を引こうとした瞬間、"
  },
  "003028": {
    "context": "003028",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「いやああああッ！　やっ！　やああああ！」"
  },
  "003033": {
    "context": "003033",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "悠里は火がついたように泣き出した。"
  },
  "003062": {
    "context": "003062",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「お外はいや！　出たくない！　出たくないッ！！」"
  },
  "003073": {
    "context": "003073",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「わたしをここから出さないで！　ここにいさせてぇえッ！！」"
  },
  "003113": {
    "context": "003113",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "悠里は泣きながらうずくまった。"
  },
  "003124": {
    "context": "003124",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「うぅ～……うぅ～……うぅ～……！」"
  },
  "003135": {
    "context": "003135",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「ゆ、悠里……！」"
  },
  "003181": {
    "context": "003181",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「っ！」"
  },
  "003186": {
    "context": "003186",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "手を差し伸べようとしたら、悠里はびくんと震えた。"
  },
  "003191": {
    "context": "003191",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "そして石になったように動かなくなり――あとには苦しげな息遣いだけが響く。"
  },
  "003196": {
    "context": "003196",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "夏彦はどうしていいかわからず、ただそこに佇んでいた……。"
  },
  "003354": {
    "context": "003354",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "――別の日も、別の日も、また別の日も同じだった。"
  },
  "003366": {
    "context": "003366",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "泣き出したり、頭痛にさいなまれたり、呼吸困難に陥ったり――彼女は様々な症状を訴え、外出を拒む。"
  },
  "003417": {
    "context": "003417",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "そのまま１年が過ぎた頃、夏彦は悠里を外に出す事を諦めた。"
  },
  "003422": {
    "context": "003422",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "怖かったのだ。悠里の心が壊れてしまいそうで。"
  },
  "003427": {
    "context": "003427",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "そして悠里は、２度と外に出る事なく……"
  },
  "003562": {
    "context": "003562",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "今日に至る。"
  },
  "003569": {
    "context": "003569",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "（ＰＴＳＤか……）"
  },
  "003621": {
    "context": "003621",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "心的外傷後ストレス障害――死に瀕した体験などが原因で発症する、心理的疾患。"
  },
  "003626": {
    "context": "003626",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "今でも悠里は、それから脱していない。"
  },
  "003631": {
    "context": "003631",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "症状は、過去の体験のフラッシュバックや、悪夢の多発。"
  },
  "003636": {
    "context": "003636",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "トラウマの原因になった事物に対しての、回避傾向。"
  },
  "003641": {
    "context": "003641",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "そしてそれに伴うパニック障害――全て彼女に当てはまっていた。"
  },
  "003647": {
    "context": "003647",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "（ぼくが彼女を、変えてしまったんだ……）"
  },
  "003652": {
    "context": "003652",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "外に出られず、この家の中で、ただ静かに生きていく悠里。"
  },
  "003657": {
    "context": "003657",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "彼女について考えると、夏彦は胸が潰れそうになる。"
  },
  "003662": {
    "context": "003662",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "だが彼がそれを憂いているのを悟ると、悠里はいつも切なげな顔をした。"
  },
  "003723": {
    "context": "003723",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「……どうしたのひこりん、急に黙って……」"
  },
  "003752": {
    "context": "003752",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「また、昔のこと考えてるの？」"
  },
  "003763": {
    "context": "003763",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「え……？」"
  },
  "003792": {
    "context": "003792",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「ひこりんは何も気にしなくてもいいんだよ」"
  },
  "003803": {
    "context": "003803",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「この家にも住まわせてもらってるし……ひこりんにも、すごくよくしてもらってるしね」"
  },
  "003832": {
    "context": "003832",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「ひこりんがいて、ましろんがいて、今は毎日楽しいから。だから本当に気にしないで」"
  },
  "003837": {
    "context": "003837",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "そう言って悠里は穏やかに笑う。"
  },
  "003842": {
    "context": "003842",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "それはどこか儚くて、とても透き通った笑顔だった。"
  },
  "003893": {
    "context": "003893",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "――そう。"
  },
  "003898": {
    "context": "003898",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "脆く壊れやすい心を抱えていても、彼女は夏彦がそばにいる事を許してくれる。"
  },
  "003903": {
    "context": "003903",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "だから夏彦は、その笑顔に応え続けようと誓ったのだ。"
  },
  "003909": {
    "context": "003909",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "（彼女のそばにいて、見守り続ける……それが、ぼくの役目だ）"
  },
  "003914": {
    "context": "003914",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "クラスメートとの友人関係など、悠里に比べればまるで重要じゃない。"
  },
  "003919": {
    "context": "003919",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "ましろも通ってきてくれるし、それで充分満ち足りる。"
  },
  "003935": {
    "context": "003935",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "そう思いつつ、夏彦は笑みを返した。"
  },
  "003946": {
    "context": "003946",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「……ありがとう、悠里」"
  },
  "003957": {
    "context": "003957",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「こんなこと言ったら、怒られるかもしれないけど……ぼくも悠里といられて嬉しいと思っているよ」"
  },
  "003962": {
    "context": "003962",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "夏彦がそう言うと、悠里は少し寂しげな表情を浮かべた。"
  },
  "003991": {
    "context": "003991",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「あ、でもね……ひとつだけ気になる事があるの」"
  },
  "004002": {
    "context": "004002",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「な、なに？」"
  },
  "004077": {
    "context": "004077",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「ひこりんとましろんの仲が、ちっとも進展しないことだよ」"
  },
  "004106": {
    "context": "004106",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「わたし、その……邪魔してないかな？」"
  },
  "004117": {
    "context": "004117",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「なっ……なんでそこでましろが出てくるんだよ！？」"
  },
  "004146": {
    "context": "004146",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「だってましろん、あんなに素敵な女の子で、お料理も上手だし、ひこりんに優しいのに……」"
  },
  "004165": {
    "context": "004165",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「確かになぁ。あいつ学校でも、結構モテるみたいだしな」"
  },
  "004194": {
    "context": "004194",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「やっぱり？」"
  },
  "004205": {
    "context": "004205",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「あぁ。けっこうあちこちの男から、告白とかされてるみたいだよ」"
  },
  "004234": {
    "context": "004234",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「こ、告白！？　それでそれで？」"
  },
  "004245": {
    "context": "004245",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「それがましろの奴、全部断ってるらしい。なんでって聞くと――」"
  },
  "004274": {
    "context": "004274",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「『今は男子とか恋とか興味ないよ。そういうのは漫画とアニメで充分』だってさ」"
  },
  "004303": {
    "context": "004303",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「でもでも、ひこりんとは仲良しじゃない？」"
  },
  "004314": {
    "context": "004314",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「ましろいわく、『夏彦は付き合い長すぎて異性として見れない。恋愛対象外』との事だよ」"
  },
  "004325": {
    "context": "004325",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「まぁぼくも同感だ」"
  },
  "004330": {
    "context": "004330",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "ましろが魅力的な少女なのだという事は、正直夏彦もそう思う。"
  },
  "004335": {
    "context": "004335",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "だがこうも付き合いが長いと、親友を通り越して家族のような感覚になり、恋愛感情を抱くのも難しいのだ。"
  },
  "004346": {
    "context": "004346",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「というわけで、ぼくらはそういう関係にはなりえないって」"
  },
  "004375": {
    "context": "004375",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「んー、本当に……？」"
  },
  "004386": {
    "context": "004386",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「ほんとほんと」"
  },
  "004415": {
    "context": "004415",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「ふぅん……？」"
  },
  "004426": {
    "context": "004426",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「……ほんとだってば」"
  },
  "004437": {
    "context": "004437",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「怪しいな……？」"
  },
  "004448": {
    "context": "004448",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「し、信じてくれないのか……？」"
  },
  "004453": {
    "context": "004453",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "悠里はしばらく、意味深げなまなざしを送っていたが――"
  },
  "004468": {
    "context": "004468",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「なーに言ってんだよ？　あいつが優しいのは、ぼくに対してだけじゃないって」"
  },
  "004473": {
    "context": "004473",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "悠里の誤解を避けるため、夏彦は慌てて弁解した。"
  },
  "004484": {
    "context": "004484",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「『困った人を見過ごせない』ってのが、あいつのモットーなんだよ。親切心以上の感情は持ってないよ」"
  },
  "004513": {
    "context": "004513",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「ほんとかな……」"
  },
  "004524": {
    "context": "004524",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「ほんとほんと。それにあいつ、全然男に興味ないって言ってたよ？」"
  },
  "004553": {
    "context": "004553",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「『どっちかっていうと、女の子の方に興味があるねェ』だってさ。それもどうかとは思うけど……」"
  },
  "004558": {
    "context": "004558",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "そう言っても、悠里は疑いのまなざしを送っていたが――"
  },
  "004587": {
    "context": "004587",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「……なんてね、冗談だよ」"
  },
  "004592": {
    "context": "004592",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "やがてにこっと笑って言った。"
  },
  "004603": {
    "context": "004603",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「わ、悪い冗談だぞ悠里」"
  },
  "004632": {
    "context": "004632",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「素敵な恋の話を読んだから、たまにはこんな話もいいかと思ってね～」"
  },
  "004661": {
    "context": "004661",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「しどろもどろになるひこりんが面白くて、ちょっと意地悪しちゃった」"
  },
  "004672": {
    "context": "004672",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「っ……適わないな」"
  },
  "004677": {
    "context": "004677",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "夏彦たちは笑みを交わした。"
  },
  "004722": {
    "context": "004722",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "そうして穏やかな時間が、ゆっくりと過ぎていく。"
  },
  "004727": {
    "context": "004727",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "恋とも友情とも違う、絶対的な安心がその空間を満たしていた。"
  },
  "004732": {
    "context": "004732",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "――それから少し、３冊目の本の話をして。"
  },
  "004737": {
    "context": "004737",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "気づけばすっかり、夜中になっていた。"
  },
  "004821": {
    "context": "004821",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「ん……もうこんな時間か」"
  },
  "004844": {
    "context": "004844",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "見れば悠里も、眠そうな目をしている。"
  },
  "004855": {
    "context": "004855",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「そろそろ寝ようか、悠里」"
  },
  "004884": {
    "context": "004884",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「ふぁ～あぁ……ん、そうだね」"
  },
  "004925": {
    "context": "004925",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "悠里がそう言って立ち上がる。"
  },
  "004954": {
    "context": "004954",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「……今日は色々な事があったね……」"
  },
  "004965": {
    "context": "004965",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「あぁ、明日は何事もない１日であればいいな」"
  },
  "005024": {
    "context": "005024",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "夏彦はそう言って、キッチンにコップを運び、悠里の分のミルクを捨てた。"
  },
  "005114": {
    "context": "005114",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「それじゃおやすみ、悠里」"
  },
  "005125": {
    "context": "005125",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「ん……」"
  },
  "005226": {
    "context": "005226",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "そう言葉を交わし、夏彦がリビングの電気を消した時――"
  },
  "005252": {
    "context": "005252",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "不意に背後で、静かな声が聞こえた。"
  },
  "005275": {
    "context": "005275",
    "code": "0x85",
    "developer_comment": "悠里",
    "string": "「……ねぇ、ひこりん」"
  },
  "005284": {
    "context": "005284",
    "code": "0x86",
    "developer_comment": "悠里",
    "string": "悠里"
  },
  "005285": {
    "context": "005285",
    "code": "0x86",
    "developer_comment": "悠里",
    "string": "「……ねぇ、ひこりん」"
  },
  "005295": {
    "context": "005295",
    "code": "0x85",
    "developer_comment": "悠里",
    "string": "「ひこりんは、怖いと思ってるよね？」"
  },
  "005304": {
    "context": "005304",
    "code": "0x86",
    "developer_comment": "悠里",
    "string": "悠里"
  },
  "005305": {
    "context": "005305",
    "code": "0x86",
    "developer_comment": "悠里",
    "string": "「ひこりんは、怖いと思ってるよね？」"
  },
  "005325": {
    "context": "005325",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「え？」"
  },
  "005397": {
    "context": "005397",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "夏彦ははっとして、悠里を見た。"
  },
  "005408": {
    "context": "005408",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「怖いって……何が？」"
  },
  "005419": {
    "context": "005419",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「今の穏やかな生活を、失うこと」"
  },
  "005430": {
    "context": "005430",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「繰り返しの日々が、繰り返しじゃなくなることだよ」"
  },
  "005435": {
    "context": "005435",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "闇の中に、悠里の影が浮かび上がる。"
  },
  "005446": {
    "context": "005446",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「……穏やかに暮らしているのは、何か行動することで、予想もしない事が起きるのが怖いんでしょ」"
  },
  "005457": {
    "context": "005457",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「自分だけなら大丈夫でも、他人を巻き込むのが怖くて……」"
  },
  "005462": {
    "context": "005462",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "悠里の口調は、いつもと違っていた。"
  },
  "005474": {
    "context": "005474",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "（な、なんだ……！？）"
  },
  "005479": {
    "context": "005479",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "――悠里がパニック障害などを起こす事は、過去にも幾度かあった。"
  },
  "005484": {
    "context": "005484",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "それはいつだって、彼女を外に連れ出そうとした時に限られていた。"
  },
  "005490": {
    "context": "005490",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "（でも、これは……違う？）"
  },
  "005495": {
    "context": "005495",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "彼女を外に出そうとしたわけでもないし、パニック障害が起きているようにも見えない。"
  },
  "005500": {
    "context": "005500",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "戸惑う夏彦に、悠里は続ける。"
  },
  "005517": {
    "context": "005517",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「でもひこりんはもう、あの頃のような子供じゃないんだよ？」"
  },
  "005528": {
    "context": "005528",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「悲しみを乗り越えられる強さを、手に入れてるのかもしれないのに……」"
  },
  "005545": {
    "context": "005545",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「ゆ、悠里……何を言ってるんだ？」"
  },
  "005594": {
    "context": "005594",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "窓から差し込む月光が、悠里の姿を照らす。"
  },
  "005599": {
    "context": "005599",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "光は頼りなく、彼女の表情は読めない。"
  },
  "005610": {
    "context": "005610",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「……これからひこりんの身には、色んな出来事が起こると思う」"
  },
  "005621": {
    "context": "005621",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「楽しい事も、辛い事も……勇気なくしては、乗り越えられない事も」"
  },
  "005632": {
    "context": "005632",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「だけどきっと、そのどれもが、避けちゃいけない事なんだよ……」"
  },
  "005637": {
    "context": "005637",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "言う事がだんだん予言じみてくる。"
  },
  "005642": {
    "context": "005642",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "夏彦は不穏なものを感じた。"
  },
  "005653": {
    "context": "005653",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「ゆ、悠里……？　もしかして、もう半分寝てるのか？　寝ぼけてるんだろ？」"
  },
  "005664": {
    "context": "005664",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「……そんな当たり前の答えだと思う？」"
  },
  "005675": {
    "context": "005675",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「だっていつもなら、もう寝てる時間……」"
  },
  "005686": {
    "context": "005686",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「何時？」"
  },
  "005691": {
    "context": "005691",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "そう言われて、夏彦は時計を見て――"
  },
  "005909": {
    "context": "005909",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "そして目を見開いた。"
  },
  "005920": {
    "context": "005920",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「なっ……！！」"
  },
  "005925": {
    "context": "005925",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "時計が、いつの間にか壊れていた。"
  },
  "005930": {
    "context": "005930",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "今朝と同じように、針が止まっている。"
  },
  "005941": {
    "context": "005941",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「そんな……どうして……？」"
  },
  "005946": {
    "context": "005946",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "夏彦は闇の中、目を凝らして文字盤を見た。"
  }
}