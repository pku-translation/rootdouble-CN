{
  "000041": {
    "context": "000041",
    "code": "0x55",
    "developer_comment": "",
    "string": "√Before Chapter5"
  },
  "000067": {
    "context": "000067",
    "code": "0x45",
    "developer_comment": "",
    "string": "――バスに乗っている間、ましろは固い声で呟いた。"
  },
  "000078": {
    "context": "000078",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「学校、大丈夫かな……」"
  },
  "000089": {
    "context": "000089",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「大丈夫だってば、さすがに粉々になったりはしてないと思うぞ」"
  },
  "000100": {
    "context": "000100",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「ならいいけど……」"
  },
  "000106": {
    "context": "000106",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "そんな事を言い合いつつ、学校の前でバスを降りたのだが――"
  },
  "000184": {
    "context": "000184",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「……あれ？」"
  },
  "000189": {
    "context": "000189",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "学校は静けさに満ちていた。"
  },
  "000194": {
    "context": "000194",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "騒ぎはもう沈静化したのだろうか。"
  },
  "000238": {
    "context": "000238",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「ホントに危険物、仕掛けられてたの？」"
  },
  "000267": {
    "context": "000267",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「少なくともそんな放送はあった。私たちも詳細は――」"
  },
  "000272": {
    "context": "000272",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "サリュがそう言いかけた時、どこかから怒声が飛んできた。"
  },
  "000283": {
    "context": "000283",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "「こら！　あなたたち！」"
  },
  "000294": {
    "context": "000294",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「あっ、先生！」"
  },
  "000299": {
    "context": "000299",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "見れば恵那が、こちらに駆け寄ってくる所だった。"
  },
  "000393": {
    "context": "000393",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "「もうっ、勝手なことして。先生心配したのよ」"
  },
  "000404": {
    "context": "000404",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「す、すいません先生！　私のせいなんです……」"
  },
  "000433": {
    "context": "000433",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "「あら鳥羽さん。風邪かなにかじゃなかったの？」"
  },
  "000444": {
    "context": "000444",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「あの、そういうワケじゃないんですが……ちょっとヤボ用で」"
  },
  "000473": {
    "context": "000473",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "「ヤボ用で学校を休むんじゃありません」"
  },
  "000484": {
    "context": "000484",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「は、はい」"
  },
  "000489": {
    "context": "000489",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "ましろが深々と頭を下げる。"
  },
  "000500": {
    "context": "000500",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「ところで先生、例の危険物騒ぎはどうなったんですか？」"
  },
  "000529": {
    "context": "000529",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "「あぁ、あれはデマだったみたいなのよ」"
  },
  "000540": {
    "context": "000540",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「デマ？」"
  },
  "000569": {
    "context": "000569",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "「トイレに時限爆弾のようなものが仕掛けられていたんだけど……それっぽく見えるオモチャだったわ」"
  },
  "000598": {
    "context": "000598",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「オモチャ？　どういうこと？」"
  },
  "000627": {
    "context": "000627",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "「わからないわね……誰がやったのか知らないけど、たちの悪いイタズラかしら」"
  },
  "000656": {
    "context": "000656",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "「近頃こういう事件が多いようだけど……困るわ……」"
  },
  "000661": {
    "context": "000661",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "どうやら恵那にも、本当に事情が掴めていないらしい。"
  },
  "000690": {
    "context": "000690",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「なるほど……それじゃここに長居する意味はないね」"
  },
  "000719": {
    "context": "000719",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「行こうみんな」"
  },
  "000745": {
    "context": "000745",
    "code": "0x45",
    "developer_comment": "夏彦・サリュ",
    "string": "「え？」"
  },
  "000839": {
    "context": "000839",
    "code": "0x45",
    "developer_comment": "夏彦・サリュ",
    "string": "ましろは夏彦たちの腕を引き、駆け出した。"
  },
  "000844": {
    "context": "000844",
    "code": "0x45",
    "developer_comment": "夏彦・サリュ",
    "string": "背後で恵那が、驚いたように叫ぶ。"
  },
  "000855": {
    "context": "000855",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "「ちょ、ちょっとあなたたちどこに行くの！？　授業再開してるから、出なさいってば！」"
  },
  "000866": {
    "context": "000866",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「すみません、ヤボ用です！」"
  },
  "000877": {
    "context": "000877",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "「だからヤボ用で授業を休むなっていってるでしょ！　こらー！」"
  },
  "000882": {
    "context": "000882",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "その声もやがて小さくなっていく。"
  },
  "000893": {
    "context": "000893",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「お、おいましろ！　なにやってんだよ！？」"
  },
  "000904": {
    "context": "000904",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「あの、ましろ、腕が痛い」"
  },
  "000909": {
    "context": "000909",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "夏彦たちがたまりかねて声を上げると、ましろは足を止めて答えた。"
  },
  "001000": {
    "context": "001000",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「構うことないよ、どうせ明日は日曜日だもん。週明けにはほとぼりも冷めてるでしょ」"
  },
  "001029": {
    "context": "001029",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「恵那先生、夏彦には甘いしね～」"
  },
  "001040": {
    "context": "001040",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「お前なぁ……！」"
  },
  "001045": {
    "context": "001045",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "だが今から学校に戻っても、せいぜい６時限目くらいしか出られないだろう。"
  },
  "001050": {
    "context": "001050",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "恵那の心証が少しだけ気になったが――夏彦は、諦めて尋ねた。"
  },
  "001061": {
    "context": "001061",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「まぁいいよ……で？　ぼくらを引っ張っていって、どうするつもりだ？」"
  },
  "001090": {
    "context": "001090",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「……いやそのね、もしよかったら……」"
  },
  "001119": {
    "context": "001119",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「テロリストの捜索を、手伝って欲しいの」"
  },
  "001130": {
    "context": "001130",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「えっ！？」"
  },
  "001159": {
    "context": "001159",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「夏彦はわかってくれないみたいだけど……」"
  },
  "001188": {
    "context": "001188",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「私は自分の住む街に危機が迫ってるかもしれないのに、何もしないなんて嫌なんだよ」"
  },
  "001217": {
    "context": "001217",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「でも、テロリストが実在するとしても、私たちの力で阻止するのは難しいのでは？」"
  },
  "001246": {
    "context": "001246",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「そうだね。だけど『それは無理っぽい』と思ったからって、何もしなくていいって事にはならないでしょ？」"
  },
  "001275": {
    "context": "001275",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「……っ」"
  },
  "001280": {
    "context": "001280",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "サリュは息を呑んだ。先ほどの話と似ていると思ったのだろうか。"
  },
  "001285": {
    "context": "001285",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "夏彦もましろの言葉を聞き、少しだけ考えを改めた。"
  },
  "001291": {
    "context": "001291",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "（やっぱり、言葉では説得できなそうだな……）"
  },
  "001296": {
    "context": "001296",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "このままましろを放っておけば、１人で行動し続けるだろう。"
  },
  "001301": {
    "context": "001301",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "テロリストが実在するしないに関わらず、目を離したままでいると、心配になるのも確かだった。"
  },
  "001307": {
    "context": "001307",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "（仕方ない……ましろの気が済むまで、付き合ってやるか）"
  },
  "001318": {
    "context": "001318",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「わかったよ、ましろ……そんなに言うなら、手伝ってやる」"
  },
  "001347": {
    "context": "001347",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「え！？　ほ、本当に！？」"
  },
  "001358": {
    "context": "001358",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「ただしとりあえず今日だけな。それからの事は後で考えよう」"
  },
  "001387": {
    "context": "001387",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「それでも充分だよ、夏彦！」"
  },
  "001392": {
    "context": "001392",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "ましろはようやく笑顔を見せた。"
  },
  "001403": {
    "context": "001403",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「サリュはどうする？」"
  },
  "001432": {
    "context": "001432",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「……私としては、テロリストの捜索などという危険な事、推奨はできない」"
  },
  "001461": {
    "context": "001461",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「だけど夏彦が言うなら、仕方ない……私も同行する」"
  },
  "001490": {
    "context": "001490",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「サリュも！？　やった、嬉しいよ！」"
  },
  "001519": {
    "context": "001519",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「２人ともありがとう！　本当は１人で心細かったんだ！」"
  },
  "001524": {
    "context": "001524",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "現金なもんだ――そう思って夏彦は苦笑した。"
  },
  "001535": {
    "context": "001535",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「それじゃ次はどこに行くつもりだ？」"
  },
  "001564": {
    "context": "001564",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「うん、どうしようかな……」"
  },
  "001593": {
    "context": "001593",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「まず気になるのは、学校の爆弾騒ぎがデマだったって事。これって妙じゃない？」"
  },
  "001622": {
    "context": "001622",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「そうなると、他の事件の実情も気になってくるよ」"
  },
  "001633": {
    "context": "001633",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「他の事件？」"
  },
  "001662": {
    "context": "001662",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「ほら、近頃街を騒がしている火事とか怪文書騒ぎとか」"
  },
  "001667": {
    "context": "001667",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "夏彦は数日前に聞いたニュースについて思い出した。"
  },
  "001782": {
    "context": "001782",
    "code": "0x45",
    "developer_comment": "キャスター",
    "string": "「今朝未明、鹿鳴市の『ＢＣトレーニングセンター』に、過激派団体による怪文書が届きました」"
  },
  "001790": {
    "context": "001790",
    "code": "0x45",
    "developer_comment": "キャスター",
    "string": "「昨夜、鹿鳴市内の研究施設『ＢＣ開発センター』で火災があり、研究棟６棟が全焼しました」"
  },
  "001887": {
    "context": "001887",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「あぁ、あれか……」"
  },
  "001916": {
    "context": "001916",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「そう言えば恵那もそんなことを言っていた。類似の事件が多発してるの？」"
  },
  "001945": {
    "context": "001945",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「だったら当事者に話を聞いてみるのがいいかもしれないね。怪文書が届いたってのは、どこだっけ？」"
  },
  "001956": {
    "context": "001956",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「確かＢＣトレーニングセンターだよ」"
  },
  "001985": {
    "context": "001985",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「よし！　さっそく行ってみようよ！」"
  },
  "001996": {
    "context": "001996",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「えぇ……？　ちょっと遠いぞ」"
  },
  "002025": {
    "context": "002025",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「なぁに、大した距離じゃないよ！　ほら行こう！」"
  },
  "002136": {
    "context": "002136",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "ましろに半ば引きずられるようにして、夏彦たちはまた駆け出した。"
  },
  "002141": {
    "context": "002141",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "街の中心部にある、ＢＣトレーニングセンターに向かって。"
  }
}