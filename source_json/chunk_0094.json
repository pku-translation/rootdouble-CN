  "001857": {
    "context": "001857",
    "code": "0x45",
    "developer_comment": "",
    "string": "風見は夏彦を見てうなずく。"
  },
  "001886": {
    "context": "001886",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「出血がひどいようですね……！　輸血が必要かと思われます」"
