  "001548": {
    "context": "001548",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「この子たちに何が……？」"
  },
  "001559": {
    "context": "001559",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「夏彦はＮエリアに倒れていたんだ」"
  },
  "001570": {
    "context": "001570",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「サリュは恐らく、Ｎエリアに後から入ってきたんだと思う……」"
  },
  "001599": {
    "context": "001599",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「『Ｎエリアに倒れていた』って……Ｎエリアには、炎上する原子炉があるんじゃ！？」"
  },
  "001610": {
    "context": "001610",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「いや……オレにもわけがわからないんだが……」"
  },
  "001621": {
    "context": "001621",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「Ｎエリアには、そんなものはなかったんだ」"
  },
  "001650": {
    "context": "001650",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「――えっ！？」"
  },
  "001655": {
    "context": "001655",
    "code": "0x45",
    "developer_comment": "",
    "string": "風見はその言葉に、言葉を失った。"
  },
  "001660": {
    "context": "001660",
    "code": "0x45",
    "developer_comment": "",
    "string": "だが言葉だけでは信じてもらえないだろう。渡瀬は風見に『レベルＮ』のカードを渡す。"
  },
  "001671": {
    "context": "001671",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「橘、聞くより見るのが早い。その眼で確認してきてくれ」"
  },
  "001700": {
    "context": "001700",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「……っ」"
  },
  "001720": {
    "context": "001720",
    "code": "0x45",
    "developer_comment": "",
    "string": "風見は訝しげに頷くと、カードを受け取ってＮエリアに走った。"
  },
  "001726": {
    "context": "001726",
    "code": "0x45",
    "developer_comment": "",
    "string": "――そして、３０秒もすると――"
  },
  "001737": {
    "context": "001737",
    "code": "0x45",
    "developer_comment": "",
    "string": "すぐに戻ってきて、声を上げた。"
  },
  "001774": {
    "context": "001774",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「信じられません……まさかこんな事が……！」"
  },
  "001780": {
    "context": "001780",
    "code": "0x45",
    "developer_comment": "",
    "string": "風見からレベルＮのカードを返してもらいつつ、渡瀬はうなずく。"
  },
  "001791": {
    "context": "001791",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「あぁ……オレたちはどうも根本的なところで、誰かに騙されていたようだ」"
  },
  "001820": {
    "context": "001820",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「し、しかもあの機械……恐らく原子炉とは、別の何かだと……」"
