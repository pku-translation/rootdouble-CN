{
  "000051": {
    "context": "000051",
    "code": "0x55",
    "developer_comment": "",
    "string": "√Double Chapter6"
  },
  "000096": {
    "context": "000096",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「な……なんの冗談だ、渡瀬！？」"
  },
  "000101": {
    "context": "000101",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "夏彦は目を見開き、叫んだ。"
  },
  "000106": {
    "context": "000106",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "渡瀬が銃を、自分たちに向けている。"
  },
  "000111": {
    "context": "000111",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "それが信じられなかった。"
  },
  "000122": {
    "context": "000122",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「せっかく脱出できたっていうのに、何をしてるんだ！？」"
  },
  "000160": {
    "context": "000160",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「……すまないな、夏彦。これが俺の使命なんだよ」"
  },
  "000171": {
    "context": "000171",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「悠里をＱに連れて帰るという事が」"
  },
  "000209": {
    "context": "000209",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「そ……そんな……！」"
  },
  "000214": {
    "context": "000214",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "悠里がかすれた声を上げた。"
  },
  "000219": {
    "context": "000219",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "夏彦は怒りのあまり、わめきちらす。"
  },
  "000296": {
    "context": "000296",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「渡瀬ッ！！　お前、どうしてそこまでＱにこだわる！」"
  },
  "000307": {
    "context": "000307",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「お前の本性は、テロリストなんかじゃないだろう！」"
  },
  "000318": {
    "context": "000318",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「人を助けるために、お前は生きてきたんじゃないか！？」"
  },
  "000329": {
    "context": "000329",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「なのに、どうして……どうしてそんな事を言うんだ！」"
  },
  "000367": {
    "context": "000367",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「……だからこそだよ」"
  },
  "000378": {
    "context": "000378",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「この９時間の間に、俺は皆の過去に触れてきた」"
  },
  "000389": {
    "context": "000389",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「そして少しずつ、皆の事を理解していった」"
  },
  "000400": {
    "context": "000400",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「だけど……全員の気持ちに立とうとすればするほど、湧き上がってきた考えがあるんだ」"
  },
  "000438": {
    "context": "000438",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「考えって……」"
  },
  "000476": {
    "context": "000476",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「この市がどうしようもなく狂っているっていう事だよ」"
  },
  "000487": {
    "context": "000487",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「お前だってそう思うだろ？」"
  },
  "000498": {
    "context": "000498",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「……！」"
  },
  "000558": {
    "context": "000558",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「俺もお前たちも、他の皆も、鹿鳴市の連中に人生を狂わされてきたんだ」"
  },
  "000616": {
    "context": "000616",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「何年にも渡って、色々なものを奪われてきたんだよ」"
  },
  "000661": {
    "context": "000661",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「……ラボが壊れたところで、市の連中にはさほど痛くもないだろう」"
  },
  "000672": {
    "context": "000672",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「また似たような施設が再建され、システムは再構築され、狂った世界が続いていく」"
  },
  "000683": {
    "context": "000683",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「それを止められるのは、恐らくＱだけだ……」"
  },
  "000721": {
    "context": "000721",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「そんな！？　先生のいる超科局とか――」"
  },
  "000759": {
    "context": "000759",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「今まで超科局が何もできなかったから、悲劇はずっと続いてきたんじゃないか」"
  },
  "000797": {
    "context": "000797",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「ッ……」"
  },
  "000874": {
    "context": "000874",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「だが、Ｑに賛同するコミュニケーターがいれば、状況は一変する」"
  },
  "000885": {
    "context": "000885",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「鹿鳴市の連中も、ラボの上層部も、きっと裁く事ができるはずだ」"
  },
  "000896": {
    "context": "000896",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「だから、悠里――」"
  },
  "000907": {
    "context": "000907",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「俺たちの戦いに、力を貸してくれ」"
  },
  "000945": {
    "context": "000945",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「何を……そんな事、できると思ってるのか！」"
  },
  "000956": {
    "context": "000956",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「お前について行ったりしたら、悠里もテロリストとして、追われる立場になるだろ！？」"
  },
  "000967": {
    "context": "000967",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「危険な目にもたくさん遭うし、この街に帰って来る事もできなくなる！」"
  },
  "000978": {
    "context": "000978",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「約束を破る事になっても、それだけは認められない！」"
  },
  "000983": {
    "context": "000983",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "その言葉を聞き、渡瀬は諦めたように笑う。"
  },
  "001021": {
    "context": "001021",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「……お前がそう言うのは、わかっていたよ」"
  },
  "001032": {
    "context": "001032",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「でもな、夏彦。今回ばかりは引けない」"
  },
  "001043": {
    "context": "001043",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「誰かがやらなきゃダメなんだ。行動しなきゃ、何も変えられないんだよ」"
  },
  "001062": {
    "context": "001062",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "そう言う彼の表情には、信念が滲んでいた。"
  },
  "001067": {
    "context": "001067",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "Ｑはテロ組織ではあるが、その活動には確かな大義がある。"
  },
  "001072": {
    "context": "001072",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "夏彦を敵に回してでも、その意思を貫こうとしているのだ。"
  },
  "001077": {
    "context": "001077",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "だけどそれが夏彦には、途方もなく悲しかった。"
  },
  "001082": {
    "context": "001082",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "ようやく渡瀬と仲間になれたと思ったのに、芯の部分では結局分かり合えないという事が。"
  },
  "001120": {
    "context": "001120",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「でも、渡瀬……今回ばかりは従えないよ。たとえ銃を突きつけられても……」"
  },
  "001158": {
    "context": "001158",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「……そうか……そうだろうさ……」"
  },
  "001169": {
    "context": "001169",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「だったらもう、仕方ないよな……お前がどうしても、この取引に応じたくないというのなら……！」"
  },
  "001174": {
    "context": "001174",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "渡瀬はそう言って、苦い笑みを浮かべた。"
  },
  "001179": {
    "context": "001179",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "撃たれる！？　夏彦がそう思った時――"
  },
  "001184": {
    "context": "001184",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "渡瀬は、思いもよらない事を口にした。"
  },
  "001195": {
    "context": "001195",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「夏彦、俺の記憶を消せ」"
  },
  "001200": {
    "context": "001200",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "夏彦はその言葉を聞き、耳を疑った。"
  },
  "001238": {
    "context": "001238",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「な……何を言い出すんだ？」"
  },
  "001276": {
    "context": "001276",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「俺の全ての記憶を消せと言ったんだ。かつてお前がましろを護るために、俺にそうしたように」"
  },
  "001314": {
    "context": "001314",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「そんな……馬鹿な事言うな渡瀬！」"
  },
  "001325": {
    "context": "001325",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「お前は既に、ぼくに何度も記憶を消去されている！　脳にもダメージが蓄積してるはずだ！」"
  },
  "001336": {
    "context": "001336",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「もう一度、お前の全ての記憶を消してしまったら――今度こそお前は、死んでしまうかもしれないんだぞ！？」"
  },
  "001347": {
    "context": "001347",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「なのにどうして、そんな事を……！」"
  },
  "001385": {
    "context": "001385",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「わからないか、夏彦……俺とお前の意思を、同時に採る事はできないって事だ」"
  },
  "001396": {
    "context": "001396",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「悠里を連れて行けないなら、俺は使命を果たせない。だが俺の意思は、俺が俺である限り変わらない」"
  },
  "001407": {
    "context": "001407",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「それがお前の意思と相反するというのなら、お前の手で俺の意思をかき消してくれ」"
  },
  "001418": {
    "context": "001418",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「そうすればお前たちも、自由になれる」"
  },
  "001456": {
    "context": "001456",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「っ……！」"
  },
  "001461": {
    "context": "001461",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "その言葉を聞いた時、夏彦は渡瀬の心情を理解した。"
  },
  "001514": {
    "context": "001514",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "人命救助に命を賭けてきた渡瀬は、自分の起こしたテロで、多くの犠牲者を出してしまった。"
  },
  "001566": {
    "context": "001566",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "長年一緒にいた同僚や、椿山先生の上司や、ラボの幹部研究員……"
  },
  "001618": {
    "context": "001618",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "そして凪沙も救えなかった。"
  },
  "001623": {
    "context": "001623",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "もう彼は引き下がれないのだ。"
  },
  "001687": {
    "context": "001687",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "一度テロという手段を取ってしまった以上、その手で真の悪を裁くしか、償う道はないと思っているのだろう。"
  },
  "001692": {
    "context": "001692",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "だが――"
  },
  "001730": {
    "context": "001730",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「渡瀬……他に、道はないのか……！？」"
  },
  "001768": {
    "context": "001768",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「お前にはあるかもしれないが、しかし……少なくともこれが、俺の選んだ道だ」"
  },
  "001779": {
    "context": "001779",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「たとえ大切な者たちに背を向けてでも、両手を罪に染めてでも、己の使命を果たすために」"
  },
  "001784": {
    "context": "001784",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "彼の悲痛な声を聞き、夏彦の胸がずきりと疼く。"
  },
  "001789": {
    "context": "001789",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "渡瀬が握っている銃は、彼自身に向けられているように思えた。"
  },
  "001800": {
    "context": "001800",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「……だから夏彦、お前も選べ」"
  },
  "001811": {
    "context": "001811",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「これが最後の選択だ。お前と俺の、どちらの主張を取るのか」"
  },
  "001868": {
    "context": "001868",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「お前と俺の、どちらが正しいのか……！」"
  },
  "001905": {
    "context": "001905",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「っ……！」"
  },
  "002041": {
    "context": "002041",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "選択権は、夏彦にあった。"
  },
  "002069": {
    "context": "002069",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "選ぶチャンスは、１度だけ。"
  },
  "002098": {
    "context": "002098",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "（……この選択が、未来を決めるんだ）"
  },
  "002127": {
    "context": "002127",
    "code": "0x45",
