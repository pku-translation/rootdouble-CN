{
  "000050": {
    "context": "000050",
    "code": "0x55",
    "string": "√Double Chapter7"
  },
  "000086": {
    "context": "000086",
    "code": "0x85",
    "string": "　%N　%N　%N　%N　%N"
  },
  "000170": {
    "context": "000170",
    "code": "0x45",
    "developer_comment": "【――鳥羽ましろとの記憶――】",
    "string": "――あの事件の後、夏彦たちは――"
  },
  "000175": {
    "context": "000175",
    "code": "0x45",
    "developer_comment": "",
    "string": "恵那の手引きで、北海道の某所にある、超科局の宿舎に寝泊りしていた。"
  },
  "000180": {
    "context": "000180",
    "code": "0x45",
    "developer_comment": "",
    "string": "生まれ育った街を離れ、マスコミや鹿鳴市の関係者から、隠れて暮らす日々。"
  },
  "000185": {
    "context": "000185",
    "code": "0x45",
    "developer_comment": "",
    "string": "そんな生活も、半年ほど過ぎたある日の事。"
  },
  "000190": {
    "context": "000190",
    "code": "0x45",
    "developer_comment": "",
    "string": "夏彦はましろと、宿舎の廊下で話していた……。"
  },
  "000300": {
    "context": "000300",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「――いやぁ、なんかすっかりここの生活にも慣れちゃったねぇ」"
  },
  "000311": {
    "context": "000311",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「最初はどうなる事かと思ったけどな」"
  },
  "000316": {
    "context": "000316",
    "code": "0x45",
    "developer_comment": "",
    "string": "夏彦はミルクコーヒーを飲みながら言う。"
  },
  "000327": {
    "context": "000327",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「生活に不便はないし、超科局の人たちもいい人だしさ」"
  },
  "000372": {
    "context": "000372",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「時々銃声が聞こえるのが怖いけどね」"
  },
  "000405": {
    "context": "000405",
    "code": "0x45",
    "developer_comment": "",
    "string": "――この宿舎は本来、超科局の局員の訓練施設なのだ。"
  },
  "000410": {
    "context": "000410",
    "code": "0x45",
    "developer_comment": "",
    "string": "宿舎には射撃場や格技場なども隣接している。"
  },
  "000415": {
    "context": "000415",
    "code": "0x45",
    "developer_comment": "",
    "string": "恵那も入局当時は、ここで射撃の腕を磨いたらしい。"
  },
  "000426": {
    "context": "000426",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「それにしても、ここに来てもう半年か……」"
  },
  "000437": {
    "context": "000437",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「本来だったら、もう２年生になってる頃だね」"
  },
  "000448": {
    "context": "000448",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「ぼくら、留年って事になるのかな？」"
  },
  "000459": {
    "context": "000459",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「大丈夫だって。恵那先生が何らかのワザを使って、ちゃんと２年生にしてくれるってさ」"
  },
  "000470": {
    "context": "000470",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「勉強も遅れてないし、大丈夫だよ」"
  },
  "000481": {
    "context": "000481",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「そうか……」"
  },
  "000486": {
    "context": "000486",
    "code": "0x45",
    "developer_comment": "",
    "string": "文科省の役人である恵那が言うのだから、なんとかなるのだろう。"
  },
  "000491": {
    "context": "000491",
    "code": "0x45",
    "developer_comment": "",
    "string": "しかし夏彦は少しだけ不安になる。"
  },
  "000496": {
    "context": "000496",
    "code": "0x45",
    "developer_comment": "",
    "string": "この隠遁生活が終わって、鹿鳴市に帰った後、元の生活に戻れるのかと。"
  },
  "000507": {
    "context": "000507",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「……なぁ、ましろ」"
  },
  "000518": {
    "context": "000518",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「お前、今後の事って何か考えてるか？」"
  },
  "000568": {
    "context": "000568",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「へ？　なに、ヤブから棒に」"
  },
  "000579": {
    "context": "000579",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「いや、この先いつか鹿鳴市に戻る時が来たとしても――」"
  },
  "000590": {
    "context": "000590",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「これだけ社会の裏を知ってしまった以上、平然と暮らしていけるかって事だよ」"
  },
  "000601": {
    "context": "000601",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「ぼくは色々迷いが出ちゃってね。今後どうしようかなって……」"
  },
  "000612": {
    "context": "000612",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「うーん、そうだねぇ……」"
  },
  "000617": {
    "context": "000617",
    "code": "0x45",
    "developer_comment": "",
    "string": "ましろは少し考えてから、ためらうように答えた。"
  },
  "000667": {
    "context": "000667",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「……まだずっと先の事だけどさ」"
  },
  "000678": {
    "context": "000678",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「実は私ね、恵那先生に誘われてるんだ」"
  },
  "000689": {
    "context": "000689",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「高校卒業したら、超科局に入らないかって」"
  },
  "000700": {
    "context": "000700",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「えっ……？」"
  },
  "000711": {
    "context": "000711",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「いま超科局には、エンパシーが使える管理官が、ほとんどいないんだってさ」"
  },
  "000722": {
    "context": "000722",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「私が管理官になれば、超科局の仕事もずっとやりやすくなるよ」"
  },
  "000733": {
    "context": "000733",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「９月１６日の事件とか、悠里たちが拉致されたみたいな事も、未然に防げるかもしれないじゃない？」"
  },
  "000744": {
    "context": "000744",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「それはそうだけど……どうするんだ？」"
  },
  "000755": {
    "context": "000755",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「まだわかんないけど……やってみようかなって、ちょっと思ってる」"
  },
  "000766": {
    "context": "000766",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「……！」"
  },
  "000771": {
    "context": "000771",
    "code": "0x45",
    "developer_comment": "",
    "string": "夏彦は息を呑む。彼にはそんな誘いはかかっていなかった。"
  },
  "000776": {
    "context": "000776",
    "code": "0x45",
    "developer_comment": "",
    "string": "もっとも恵那が夏彦に声をかけなかったのは、彼がセンシズシンパシーの使い手だからだろう。"
  },
  "000781": {
    "context": "000781",
    "code": "0x45",
    "developer_comment": "",
    "string": "センシズシンパシーを使えるコミュニケーターは、常にＮ化の危険を孕んでいる。"
  },
  "000786": {
    "context": "000786",
    "code": "0x45",
    "developer_comment": "",
    "string": "仕事でＢＣを使うなど、その危険性を倍加させるだけだ。"
  },
  "000791": {
    "context": "000791",
    "code": "0x45",
    "developer_comment": "",
    "string": "だからましろにだけ声がかかったのだという事は、夏彦にも推察できた。"
  },
  "000802": {
    "context": "000802",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「……管理官って、どんな仕事なんだろう」"
  },
  "000813": {
    "context": "000813",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「詳しい事はわからないけど、たぶん恵那先生と一緒に、ＢＣ絡みの事件を捜査するんじゃないかな？」"
  },
  "000824": {
    "context": "000824",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「あと全国の政令指定機密都市を監査したりとか、教師のフリしたりとか」"
  },
  "000835": {
    "context": "000835",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「じゃあ、ぼくらとは離れて暮らす事になるのか……」"
  },
  "000846": {
    "context": "000846",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「……そうかもね。でもずっと会えないわけじゃないよ」"
  },
  "000857": {
    "context": "000857",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「お前に向いてるかなぁ」"
  },
  "000868": {
    "context": "000868",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「たぶん向いてるよ、私そういうの大好きだし」"
  },
  "000879": {
    "context": "000879",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「社会を裏から護るため、全国飛び回る美少女管理官ってかっこいいじゃん？」"
  },
  "000890": {
    "context": "000890",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「まぁ『私に正義の味方が務まるのかどうか分からないけど』――」"
  },
  "000902": {
    "context": "000902",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「『困っている人がいるなら、助けてあげたいな』って？」"
  },
  "000913": {
    "context": "000913",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「そうそう、それよ」"
  },
  "000918": {
    "context": "000918",
    "code": "0x45",
    "developer_comment": "",
    "string": "それはましろの最も愛する作品のセリフだった。"
  },
  "000923": {
    "context": "000923",
    "code": "0x45",
    "developer_comment": "",
    "string": "何回も聞かされてるうちに、とうとう夏彦も覚えてしまった。"
  },
  "000934": {
    "context": "000934",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「まぁようやく夏彦が私の趣味を理解してくれたのに、お別れってのもちょっと寂しいけどね。まーだまだ先の話だから」"
  },
  "000940": {
    "context": "000940",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「……」"
  },
  "000945": {
    "context": "000945",
    "code": "0x45",
    "developer_comment": "",
    "string": "そう言うましろの声には、やはり不安が滲んでいる気がした。"
  },
  "000951": {
    "context": "000951",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "（まだ先の話とは言え……そんな危険な仕事、不安にならないわけがないよな）"
  },
  "000963": {
    "context": "000963",
    "code": "0x45",
    "developer_comment": "",
    "string": "そう思った夏彦は、ましろを見据えて言った。"
  },
  "000974": {
    "context": "000974",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「わかった、ましろ」"
  },
  "000985": {
    "context": "000985",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「もしお前が管理官になるんなら、その時はぼくらも一緒に行く」"
  },
  "001043": {
    "context": "001043",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「えっ？」"
  },
  "001054": {
    "context": "001054",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「ぼくもそういうの好きなんだよ。それに――」"
  },
  "001065": {
    "context": "001065",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「ずっと昔にお前と話した、『正義の旅』の続きができるじゃないか？」"
  },
  "001117": {
    "context": "001117",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「……！」"
  },
  "001122": {
    "context": "001122",
    "code": "0x45",
    "developer_comment": "",
    "string": "その言葉に、ましろが目を見開く。"
  },
  "001127": {
    "context": "001127",
    "code": "0x45",
    "developer_comment": "",
    "string": "夏彦の脳裏に、かつて彼女と交わした会話がよぎった。"
  },
  "001178": {
    "context": "001178",
    "code": "0x45",
    "developer_comment": "",
    "string": "――遥か遠い、夏の夜。"
  },
  "001242": {
    "context": "001242",
    "code": "0x45",
    "developer_comment": "",
    "string": "犬のシリウスを救出した後、２人で逃げ込んだ秘密基地で、彼は彼女に言ったのだ。"
  },
  "001253": {
    "context": "001253",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「……だったらこのまま、２人で『せいぎのたび』にでようか？」"
  },
  "001300": {
    "context": "001300",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「あっ、なんかわかんないけどそれいい！　やりたいやりたい！」"
  },
  "001311": {
    "context": "001311",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「だろー？」"
  },
  "001322": {
    "context": "001322",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「わるいやついっぱいやっつけてさ。『ほけんじょ』もこらしめてさ」"
  },
  "001351": {
    "context": "001351",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「でもそうなると、てきもおおいよね……」"
  },
  "001380": {
    "context": "001380",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「そのためには、なかまがもっといっぱいひつようかなぁ？」"
  },
  "001391": {
    "context": "001391",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「ははっ、２人じゃさすがにむりか」"
  },
  "001456": {
    "context": "001456",
    "code": "0x45",
    "developer_comment": "",
    "string": "――あの笑顔を見て、夏彦は思ったのだ。"
  },
  "001461": {
    "context": "001461",
    "code": "0x45",
    "developer_comment": "",
    "string": "家に着いたら終わってしまうこの旅が、いつまでも続けばいいと。"
  },
  "001466": {
    "context": "001466",
    "code": "0x45",
    "developer_comment": "",
    "string": "ましろとならどこまでも、こうして歩いていけそうだと――。"
  },
  "001522": {
    "context": "001522",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「……あの頃のぼくらは、ただの子供だった」"
  },
  "001533": {
    "context": "001533",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「だけど今は違う。仲間だってたくさんできた。悠里にサリュに先生に……」"
  },
  "001544": {
    "context": "001544",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「だから今度こそ、正義の旅ができるはずだよ」"
  },
  "001555": {
    "context": "001555",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「お前となら、みんな一緒なら、どこまでも歩いていける……」"
  },
  "001566": {
    "context": "001566",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「そうだろ、ましろ？」"
  },
  "001610": {
    "context": "001610",
    "code": "0x45",
    "developer_comment": "",
    "string": "その言葉に、ましろは一瞬複雑そうな表情を浮かべ――"
  },
  "001654": {
    "context": "001654",
    "code": "0x45",
    "developer_comment": "",
    "string": "そして満面の笑みでうなずいた。"
  },
  "001665": {
    "context": "001665",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「――うん！」"
  }
}