{
  "000064": {
    "context": "000064",
    "code": "0x45",
    "developer_comment": "",
    "string": "――２０２１年、４月７日。"
  },
  "000069": {
    "context": "000069",
    "code": "0x45",
    "developer_comment": "",
    "string": "仕事からの帰り道を、風見は足早に歩いていた。"
  },
  "000105": {
    "context": "000105",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "（遅くなってしまったわ。急がないと）"
  },
  "000110": {
    "context": "000110",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "明日は凪沙の、小学校の入学式なのだ。"
  },
  "000115": {
    "context": "000115",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "前日くらいは一緒にゆっくり過ごし、明日の準備などもしてやりたい。"
  },
  "000121": {
    "context": "000121",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "（明日は休みを取ったし、入学をお祝いしたいわね）"
  },
  "000127": {
    "context": "000127",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "（そうだわ、せっかくだから洵ちゃんも呼んで……）"
  },
  "000132": {
    "context": "000132",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "そう思った時――"
  },
  "000146": {
    "context": "000146",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "消防車がサイレンを鳴らして、風見を追い越して行った。"
  },
  "000154": {
    "context": "000154",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "（……？）"
  },
  "000160": {
    "context": "000160",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "（何か、あったのかしら……）"
  },
  "000165": {
    "context": "000165",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "不意に胸騒ぎがした。"
  },
  "000170": {
    "context": "000170",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "風見は行く手の空に目をやり、そして気付く。"
  },
  "000175": {
    "context": "000175",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "――風見と凪沙が住む団地の方角の空に、煙が一筋たなびいているのを。"
  },
  "000184": {
    "context": "000184",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「え……！？」"
  },
  "000195": {
    "context": "000195",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "風見は思わず駆け出した。"
  },
  "000200": {
    "context": "000200",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "凪沙が助けを求める声が聞こえた気がしたのだ。"
  },
  "000246": {
    "context": "000246",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "そして、そこで彼女が見たのは――"
  },
  "000325": {
    "context": "000325",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "――団地から煙が上がり、崩壊しかけている光景だった。"
  },
  "000333": {
    "context": "000333",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「あ、あ、あああ……」"
  },
  "000341": {
    "context": "000341",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「嘘よ、嫌、なにこれ……！！」"
  },
  "000346": {
    "context": "000346",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "目の前にあるものが信じられなかった。"
  },
  "000351": {
    "context": "000351",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "消防隊が駆け回り、野次馬が辺りに殺到し、ガヤガヤと騒いでいる。"
  },
  "000359": {
    "context": "000359",
    "code": "0x45",
    "developer_comment": "野次馬１",
    "string": "「えれぇこったな、何が起きたんだこりゃ！？」"
  },
  "000367": {
    "context": "000367",
    "code": "0x45",
    "developer_comment": "野次馬２",
    "string": "「ガス爆発だってよ！　あーあ、こりゃ中にいた人ヤバいぜ」"
  },
  "000372": {
    "context": "000372",
    "code": "0x45",
    "developer_comment": "野次馬２",
    "string": "彼らが言う通り、団地は橘家のある３階部分が崩れかけ――"
  },
  "000380": {
    "context": "000380",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「あ、あぁ……凪沙、凪沙……！」"
  },
  "000385": {
    "context": "000385",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "風見はよろよろと、団地に向かって歩き出した。"
  },
  "000448": {
    "context": "000448",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "歩き慣れた廊下が傾いている。"
  },
  "000455": {
    "context": "000455",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "一歩歩くごとに、天井からパラパラと何かが落ちてくる。"
  },
  "000460": {
    "context": "000460",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "それでも３階を目指して、階段を登ろうとした時、"
  },
  "000468": {
    "context": "000468",
    "code": "0x45",
    "developer_comment": "？？",
    "string": "「君、何をしてるんだ！？」"
  },
  "000473": {
    "context": "000473",
    "code": "0x45",
    "developer_comment": "？？",
    "string": "階上から声が聞こえた。"
  },
  "000481": {
    "context": "000481",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「！？」"
  },
  "000504": {
    "context": "000504",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "視線を移すと、オレンジ色の服を着た男が立っていた。"
  },
  "000512": {
    "context": "000512",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「あ、あなたは……」"
  },
  "000541": {
    "context": "000541",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「レスキュー隊です。団地内の人命救助に当たってます。中に家族の方でも？」"
  },
  "000549": {
    "context": "000549",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「い、妹がいるんです！　助けてあげて下さい！　３０３号室に――！」"
  },
  "000560": {
    "context": "000560",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「ちょうどその部屋に向かうところです」"
  },
  "000617": {
    "context": "000617",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「大丈夫です、妹さんは必ず我々が助けます！　あなたは危険ですから、外にいて下さい」"
  },
  "000625": {
    "context": "000625",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「は……はい！」"
  },
  "000630": {
    "context": "000630",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "男の冷静な口調に、頼れそうな感触があった。"
  },
  "000635": {
    "context": "000635",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "風見は迷いつつもうなずき、そして団地の外に出た。"
  },
  "000732": {
    "context": "000732",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "――そして男は、風見に約束した通り、凪沙を救助してくれた。"
  },
  "000737": {
    "context": "000737",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "一緒に居合わせて、爆発に巻き込まれた洵も一緒に。"
  },
  "000742": {
    "context": "000742",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "凪沙と洵は救急車で運ばれ、風見もそれに着いていった。"
  },
  "000809": {
    "context": "000809",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "だが、事態はそれで終わらなかった。"
  },
  "000814": {
    "context": "000814",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "病院で検査した結果、凪沙は爆発の際に頭を打っており、脳内出血を起こしていると診断されたのだ。"
  },
  "000819": {
    "context": "000819",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "それから丸一日に渡り、集中治療が行われている間、風見は一睡もせずに凪沙の生還を待った。"
  },
  "000830": {
    "context": "000830",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "そして、４月８日の夜中――"
  },
  "000835": {
    "context": "000835",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "凪沙の治療を行っていた医者が、治療室から出てきた。"
  },
  "000843": {
    "context": "000843",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「お医者様！　凪沙は、妹は！？」"
  },
  "000848": {
    "context": "000848",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "そうわめく風見に、医者が沈痛な面持ちで言う。"
  },
  "000856": {
    "context": "000856",
    "code": "0x45",
    "developer_comment": "医者",
    "string": "「お気の毒ですが……！」"
  },
  "000870": {
    "context": "000870",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「――……っ」"
  },
  "000875": {
    "context": "000875",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "風見はその瞬間、世界が崩れる音を聞いた気がした。"
  },
  "000920": {
    "context": "000920",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "――その後の事を、風見はよく覚えていない。"
  },
  "000925": {
    "context": "000925",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "彼女は凪沙を失った事を、受け入れられず……"
  },
  "000930": {
    "context": "000930",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "しばらくの間、精神を失調してしまったのだ。"
  },
  "001023": {
    "context": "001023",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "風見は街を歩きながら呟く。"
  },
  "001064": {
    "context": "001064",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「凪沙……ねぇ、どこにいるの……？」"
  },
  "001103": {
    "context": "001103",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「そろそろごはんの時間よ……一緒に帰りましょう」"
  },
  "001114": {
    "context": "001114",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「私たちの住む家、鹿鳴市営団地に……」"
  },
  "001119": {
    "context": "001119",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "もういなくなってしまった妹と、もう無くなってしまった家に帰ろうと、風見は街をさまよう。"
  },
  "001124": {
    "context": "001124",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "それは夢の中の出来事のように、現実感がない。時間の感覚も曖昧だ。"
  },
  "001169": {
    "context": "001169",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "その時期、風見は団地の管理組合が与えてくれた仮住居に、寝泊りしていた。"
  },
  "001174": {
    "context": "001174",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "そこには誰かが何度か訪ねてきたようだが、それさえもよくわからない。"
  },
  "001179": {
    "context": "001179",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "恐らく管理組合の誰かが謝罪しに来たんだろう。"
  },
  "001184": {
    "context": "001184",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "何度も謝るような声が聞こえた気がした。"
  },
  "001189": {
    "context": "001189",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "だがそれもこれも、全てどうでもいい。客は全て追い返す。"
  },
  "001194": {
    "context": "001194",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "そして毎日街を歩き回っては、その仮住居に帰るだけの生活。"
  },
  "001199": {
    "context": "001199",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "仕事もせず、食事もろくにとらず、ただ失われたものを探し続けるだけの日々――"
  },
  "001285": {
    "context": "001285",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "――そこから彼女を現実に引き戻したのは、洵だった。"
  },
  "001290": {
    "context": "001290",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "ある日、久しぶりに訪れた公園で、風見は洵に会った。"
  },
  "001323": {
    "context": "001323",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「あら、洵ちゃん……凪沙、見なかった？」"
  },
  "001328": {
    "context": "001328",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "そう尋ねると、彼女は――涙を浮かべて言ったのだ。"
  },
  "001357": {
    "context": "001357",
    "code": "0x45",
    "developer_comment": "洵",
    "string": "「もういい！　もういいんだよ風見ちゃん！」"
  },
  "001368": {
    "context": "001368",
    "code": "0x45",
    "developer_comment": "洵",
    "string": "「探しても無駄なんだよ……！　どんなに探したって、凪沙はもういないの！」"
  },
  "001376": {
    "context": "001376",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「な……何を言ってるの……？」"
  },
  "001387": {
    "context": "001387",
    "code": "0x45",
    "developer_comment": "洵",
    "string": "「死んじゃったんだよ、凪沙は！　あのときの爆発事故で、頭を打って！」"
  },
  "001392": {
    "context": "001392",
    "code": "0x45",
    "developer_comment": "洵",
    "string": "―― 一瞬、何を言われたかわからなかった。"
  },
  "001397": {
    "context": "001397",
    "code": "0x45",
    "developer_comment": "洵",
    "string": "だがその言葉が風見の心に突き刺さり――"
  },
  "001402": {
    "context": "001402",
    "code": "0x45",
    "developer_comment": "洵",
    "string": "頭にかかった靄を、晴らした。"
  },
  "001408": {
    "context": "001408",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "（ああ……）"
  },
  "001460": {
    "context": "001460",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "剥落した現実が、徐々に脳裏に蘇る。"
  },
  "001465": {
    "context": "001465",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "曖昧な感覚の中に埋没していた記憶が、思い返される。"
  },
  "001470": {
    "context": "001470",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "――街外れの小さな葬儀場。"
  },
  "001475": {
    "context": "001475",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "読経の声。"
  },
  "001480": {
    "context": "001480",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "参列者のほとんどいない中、それでも来てくれた洵。"
  },
  "001485": {
    "context": "001485",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "申し訳なさそうに風見を見つめる、凪沙を救助したレスキュー隊員。"
  },
  "001490": {
    "context": "001490",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "そして、棺の中で眠る凪沙――。"
  },
  "001535": {
    "context": "001535",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "……あれから何日経つのだろう。"
  },
  "001540": {
    "context": "001540",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "少しずつあの日の事が、心に染み入ってきた。"
  },
  "001615": {
    "context": "001615",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「そう……」"
  },
  "001623": {
    "context": "001623",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「あの子は……死んだのね……」"
  },
  "001628": {
    "context": "001628",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "風見もようやく、現実を認めた。"
  },
  "001633": {
    "context": "001633",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "そして再び気付かされた。自分が失ったものの大きさに。"
  },
  "001638": {
    "context": "001638",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "――とうとう、１人になってしまった。"
  },
  "001643": {
    "context": "001643",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "ずっと２人きりで暮らしてきた、たった１人の妹を失ってしまったのだ。"
  },
  "001648": {
    "context": "001648",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "凪沙のいないこれからの日々を、自分はどう生きればいいのだろう？"
  },
  "001653": {
    "context": "001653",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "風見がそう思った時――洵が意を決したように言う。"
  },
  "001682": {
    "context": "001682",
    "code": "0x45",
    "developer_comment": "洵",
    "string": "「――風見ちゃん」"
  },
  "001693": {
    "context": "001693",
    "code": "0x45",
    "developer_comment": "洵",
    "string": "「風見ちゃんを１人にはさせないよ」"
  },
  "001704": {
    "context": "001704",
    "code": "0x45",
    "developer_comment": "洵",
    "string": "「あたしが凪沙の代わりに――風見ちゃんの、妹になるから」"
  },
  "001712": {
    "context": "001712",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「え……？」"
  },
  "001741": {
    "context": "001741",
    "code": "0x45",
    "developer_comment": "洵",
    "string": "「凪沙とは全然違う、かわいくない子だけど……あたし、お姉ちゃんの妹になるから……」"
  },
  "001770": {
    "context": "001770",
    "code": "0x45",
    "developer_comment": "洵",
    "string": "「だから……」"
  },
  "001778": {
    "context": "001778",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「洵ちゃん……あなた……」"
  },
  "001783": {
    "context": "001783",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "その言葉を聞き、風見は悟った。"
  },
  "001788": {
    "context": "001788",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "悲しんでいるのは、自分だけではないと。"
  },
  "001793": {
    "context": "001793",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "風見以外にも、凪沙の死を悼んでくれる子がいたと。"
  },
  "001798": {
    "context": "001798",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "そして風見は、そのたった１人の女の子に――悲しい想いをさせてしまったのだ。"
  },
  "001805": {
    "context": "001805",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "（……私は、１人じゃない……）"
  },
  "001811": {
    "context": "001811",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "（同じ悲しみを分かち合えるこの子が、いてくれたんだから……）"
  },
  "001816": {
    "context": "001816",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "赤の他人である洵が、年端も行かない彼女が、自分にかけてくれた優しさ――"
  },
  "001821": {
    "context": "001821",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "それが凪沙の優しさと重なった。"
  },
  "001845": {
    "context": "001845",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "風見は洵を、そっと抱きしめて言う。"
  },
  "001853": {
    "context": "001853",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「……ありがとう」"
  },
  "001981": {
    "context": "001981",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "――そして洵と別れた後、風見は仮住宅に帰った。"
  },
  "001986": {
    "context": "001986",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "恐らく室内は荒れ果てているだろう。"
  },
  "001991": {
    "context": "001991",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "それさえもわからないほど、日々をぼんやりと過ごしていたのだ。"
  },
  "001997": {
    "context": "001997",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "（帰ったら、まず片づけを……）"
  },
  "002002": {
    "context": "002002",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "そう思いつつ、家のドアに歩み寄り――"
  },
  "002007": {
    "context": "002007",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "そこで風見は立ち止まった。"
  },
  "002015": {
    "context": "002015",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「……？」"
  },
  "002020": {
    "context": "002020",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "ポストに、封筒が差さっているのに気付いたのだ。"
  },
  "002025": {
    "context": "002025",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "抜き出してみると、差出人は『鹿鳴市消防局』となっている。"
  },
  "002033": {
    "context": "002033",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「なにかしら……？」"
  },
  "002038": {
    "context": "002038",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "風見はその封筒を、そっと開けた。"
  },
  "002043": {
    "context": "002043",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "中にぐしゃぐしゃに潰れた小さな箱と、手紙が入っている。"
  },
  "002048": {
    "context": "002048",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "手紙には硬い字で、こう書かれていた。"
  },
  "002087": {
    "context": "002087",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "――橘風見様"
  },
  "002091": {
    "context": "002091",
    "code": "0x85",
    "developer_comment": "風見",
    "string": "　"
  },
  "002094": {
    "context": "002094",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "先日は失礼致しました。"
  },
  "002098": {
    "context": "002098",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "妹御・凪沙様が身罷られた事、心中謹んでお察し申し上げます。"
  },
  "002102": {
    "context": "002102",
    "code": "0x85",
    "developer_comment": "風見",
    "string": "　"
  },
  "002105": {
    "context": "002105",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "たびたびの日参、大変ご迷惑をおかけ致しました。"
  },
  "002109": {
    "context": "002109",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "風見様の心労を鑑み、もう御宅を訪ねる事は致しません。"
  },
  "002113": {
    "context": "002113",
    "code": "0x85",
    "developer_comment": "風見",
    "string": "　"
  },
  "002116": {
    "context": "002116",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "しかし、非礼は重々承知ですが……"
  },
  "002120": {
    "context": "002120",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "凪沙様が救助された時、手に握り締めていたものを、"
  },
  "002124": {
    "context": "002124",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "お送りさせて頂きます。"
  },
  "002128": {
    "context": "002128",
    "code": "0x85",
    "developer_comment": "風見",
    "string": "　%N"
  },
  "002131": {
    "context": "002131",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "救急車にお乗り頂く前に、"
  },
  "002135": {
    "context": "002135",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "私たちが凪沙様から預かっていたものです。"
  },
  "002139": {
    "context": "002139",
    "code": "0x85",
    "developer_comment": "風見",
    "string": "　"
  },
  "002142": {
    "context": "002142",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "中身は見ておりません。"
  },
  "002146": {
    "context": "002146",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "風見様にお渡しするようにのみ、頼まれておりました。"
  },
  "002150": {
    "context": "002150",
    "code": "0x85",
    "developer_comment": "風見",
    "string": "　"
  },
  "002153": {
    "context": "002153",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "どうかお気を落とさず、今後の日々を、"
  },
  "002156": {
    "context": "002156",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "心安らかにお過ごし願えればと思います。"
  },
  "002160": {
    "context": "002160",
    "code": "0x85",
    "developer_comment": "風見",
    "string": "　%N"
  },
  "002163": {
    "context": "002163",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "最後に、何度も申し上げましたが――"
  },
  "002167": {
    "context": "002167",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "大切な妹様を救えず、まことに申し訳ございませんでした。"
  },
  "002171": {
    "context": "002171",
    "code": "0x85",
    "developer_comment": "風見",
    "string": "　"
  },
  "002174": {
    "context": "002174",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "ですが私たちは、これからも、この仕事を続けていきます。"
  },
  "002178": {
    "context": "002178",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "それが凪沙様のために、私たちができる唯一の事だからです。"
  },
  "002182": {
    "context": "002182",
    "code": "0x85",
    "developer_comment": "風見",
    "string": "　"
  },
  "002185": {
    "context": "002185",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "――鹿鳴市消防局　特別高度救助隊　隊員　笠鷺渡瀬・堂島誠二"
  },
  "002214": {
    "context": "002214",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「え？　え……？」"
  },
  "002219": {
    "context": "002219",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "風見はその名前に、見覚えがなかった。"
  },
  "002227": {
    "context": "002227",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「何度も訪ねてきたって……？　どういう事なの？」"
  },
  "002232": {
    "context": "002232",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "そう言えば曖昧な日々を過ごしていた間、誰かが何度も家を訪ね、謝っていた気がした。"
  },
  "002238": {
    "context": "002238",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "（あれは……この消防士さんたちだったの？）"
  },
  "002244": {
    "context": "002244",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "（私、この人たちを、追い返してしまったの……！？）"
  },
  "002249": {
    "context": "002249",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "その時の記憶が飛んでいる。"
  },
  "002255": {
    "context": "002255",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "（『凪沙が救助された時、手に握り締めていたもの』って……！）"
  },
  "002260": {
    "context": "002260",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "混乱しながらも風見は、封筒の中に入っていた箱を見た。"
  },
  "002265": {
    "context": "002265",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "リボンのかけられた小さな箱。"
  },
  "002270": {
    "context": "002270",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "贈り物を入れる箱のように見えた。"
  },
  "002281": {
    "context": "002281",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "そっとリボンを取り、箱を開けると――"
  },
  "002398": {
    "context": "002398",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "中には、小さな首飾りが入っていた。"
  },
  "002406": {
    "context": "002406",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「……！？」"
  },
  "002417": {
    "context": "002417",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "首飾りは、中に写真を入れられる、ロケットのようだった。"
  },
  "002422": {
    "context": "002422",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "それにも小さな手紙が添えてある。"
  },
  "002427": {
    "context": "002427",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "手紙には、凪沙の字でこう書いてあった。"
  },
  "002471": {
    "context": "002471",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "おねえちゃんへ"
  },
  "002475": {
    "context": "002475",
    "code": "0x85",
    "developer_comment": "風見",
    "string": "　"
  },
  "002478": {
    "context": "002478",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "いつもおいしいおりょうりをつくってくれて、ありがとう。"
  },
  "002482": {
    "context": "002482",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "おれいに、わたしとじゅんちゃんからプレゼントです。"
  },
  "002486": {
    "context": "002486",
    "code": "0x85",
    "developer_comment": "風見",
    "string": "　"
  },
  "002489": {
    "context": "002489",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "これからもいつもいっしょだよ。"
  },
  "002493": {
    "context": "002493",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "だいすきだよ、おねえちゃん。"
  },
  "002497": {
    "context": "002497",
    "code": "0x85",
    "developer_comment": "風見",
    "string": "　"
  },
  "002500": {
    "context": "002500",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "なぎさ"
  },
  "002529": {
    "context": "002529",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「…………あ」"
  },
  "002537": {
    "context": "002537",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「あ、あ、ああああ……」"
  },
  "002542": {
    "context": "002542",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "風見はその手紙を見た瞬間、嗚咽を漏らした。"
  },
  "002547": {
    "context": "002547",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "凪沙の死から、ずっと流れなかった涙が、そのとき不意に溢れ出した。"
  },
  "002560": {
    "context": "002560",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「あああああっ！　わぁああああっ！」"
  },
  "002565": {
    "context": "002565",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "風見は人目もはばからず、大声で泣いた。"
  },
  "002573": {
    "context": "002573",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「凪沙……凪沙ぁあああ！」"
  },
  "002578": {
    "context": "002578",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "声を枯らして叫ぶにつれ、凪沙の言葉が心に染み入ってくる。"
  },
  "002583": {
    "context": "002583",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "妹と過ごした日々が、脳裏に浮かんでは消える。"
  },
  "002588": {
    "context": "002588",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "風見は凪沙の贈り物を握り締め、いつまでもそこで泣き続けた。"
  },
  "002632": {
    "context": "002632",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "やがて、その涙も枯れた頃――"
  },
  "002637": {
    "context": "002637",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "風見は、決意した。"
  },
  "002643": {
    "context": "002643",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "（……生きよう……凪沙の分まで……）"
  },
  "002648": {
    "context": "002648",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "それが彼女のためにできる、ただ一つの事だ。"
  },
  "002653": {
    "context": "002653",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "その決意は風見の心に、希望をもたらした。"
  },
  "002714": {
    "context": "002714",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "――だが同時に、風見は後悔に縛られていた。"
  },
  "002719": {
    "context": "002719",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "もしあのとき、自分がもっと早く家に帰っていたら、凪沙を救えたかもしれない。"
  },
  "002724": {
    "context": "002724",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "いつしか風見は、そのことばかりを考えるようになっていた。"
  },
  "002730": {
    "context": "002730",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "（……ただ生きるだけでは、ダメだわ……）"
  },
  "002736": {
    "context": "002736",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "（この命を使って、何かできる事を）"
  },
  "002742": {
    "context": "002742",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "（救えなかった凪沙の代わりに、誰かを……！）"
  },
  "002747": {
    "context": "002747",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "やがて風見は考えた末に、己の生きる道を決めた。"
  },
  "002838": {
    "context": "002838",
    "code": "0x45",
    "developer_comment": "洵",
    "string": "「――お姉ちゃんが、消防士に！？」"
  },
  "002843": {
    "context": "002843",
    "code": "0x45",
    "developer_comment": "洵",
    "string": "洵と会った時、風見は彼女にその意思を告げてみた。"
  },
  "002872": {
    "context": "002872",
    "code": "0x45",
    "developer_comment": "洵",
    "string": "「なんで急に、そんな事……」"
  },
  "002880": {
    "context": "002880",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「急にじゃないわ。考え抜いた結果よ」"
  },
  "002909": {
    "context": "002909",
    "code": "0x45",
    "developer_comment": "洵",
    "string": "「花屋の仕事は？」"
  },
  "002917": {
    "context": "002917",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「あの仕事も好きだったけど……辞めて、消防学校に入るわ」"
  },
  "002922": {
    "context": "002922",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "団地の管理組合から、多額の賠償金が支払われたため、当座の生活費については問題なかった。"
  },
  "002927": {
    "context": "002927",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "だが洵はわずかな沈黙の後、風見をじっと見て言う。"
  },
  "002956": {
    "context": "002956",
    "code": "0x45",
    "developer_comment": "洵",
    "string": "「ねぇ、お姉ちゃん……」"
  },
  "002967": {
    "context": "002967",
    "code": "0x45",
    "developer_comment": "洵",
    "string": "「もしかして、それって――凪沙のことが関係してる？」"
  },
  "002975": {
    "context": "002975",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「それは……」"
  },
  "003004": {
    "context": "003004",
    "code": "0x45",
    "developer_comment": "洵",
    "string": "「もしそうだとしたら、あたしはお姉ちゃんが消防士になるのは反対だな」"
  },
  "003012": {
    "context": "003012",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「どうして？」"
  },
  "003041": {
    "context": "003041",
    "code": "0x45",
    "developer_comment": "洵",
    "string": "「だってお姉ちゃん、とんでもなく無茶しそうなんだもん」"
  },
  "003049": {
    "context": "003049",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「……！」"
  },
  "003054": {
    "context": "003054",
    "code": "0x45",
    "developer_comment": "風見",
