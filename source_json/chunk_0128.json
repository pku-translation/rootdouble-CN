{
  "000098": {
    "context": "000098",
    "code": "0x55",
    "developer_comment": "",
    "string": "√After Chapter7"
  },
  "000103": {
    "context": "000103",
    "code": "0x45",
    "developer_comment": "",
    "string": "薄れ行く意識の中、渡瀬は思った。"
  },
  "000121": {
    "context": "000121",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "（どうして、こんな事に、なっちまったんだ……）"
  },
  "000139": {
    "context": "000139",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "（オレは……何か間違っていたのか……？）"
  },
  "000156": {
    "context": "000156",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "考えてもわからない。全くわかりそうもなかった。"
  },
  "000205": {
    "context": "000205",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "――この６時間の間、あまりにもおかしな事が起きすぎた。"
  },
  "000210": {
    "context": "000210",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "閉じられた瞼の裏に、今まで見た光景と謎が明滅する。"
  },
  "000262": {
    "context": "000262",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "渡瀬が記憶を失った事。"
  },
  "000310": {
    "context": "000310",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "この９人が施設に閉じ込められた事。"
  },
  "000360": {
    "context": "000360",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "施設内に他殺体があった事。"
  },
  "000410": {
    "context": "000410",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "要救助者に幾度となく逃げられた事。"
  },
  "000460": {
    "context": "000460",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "搬出用リフトが破壊され、そこに見知らぬ少女の死体があった事。"
  },
  "000508": {
    "context": "000508",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "保管しておいたＡＤが消えた事。"
  },
  "000603": {
    "context": "000603",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "皆の言動が、明らかにおかしくなった事。"
  },
  "000653": {
    "context": "000653",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "Ｎエリアに火災も原子炉もなく、代わりに夏彦が倒れていた事。"
  },
  "000703": {
    "context": "000703",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "そして――ユウリの死体を見つけ、その死体が消えた事。"
  },
  "000750": {
    "context": "000750",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "（全ては、本当に……現実だったのか……？）"
  },
  "000755": {
    "context": "000755",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "今ではそれすらもわからなくなっていた。"
  },
  "000761": {
    "context": "000761",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "（だが、現実だとしたら……ここでは何が起きていたんだ……）"
  },
  "000767": {
    "context": "000767",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "（ユウリの死体は、どこに消えたんだ……？）"
  },
  "000772": {
    "context": "000772",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "風見、洵、恵那、宇喜多――そしてサリュ。"
  },
  "000777": {
    "context": "000777",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "渡瀬がこれまでに出会った者の顔が、脳裏に浮かんだ。"
  },
  "000783": {
    "context": "000783",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "（……いや、それだけじゃない）"
  },
  "000789": {
    "context": "000789",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "（まだ見つかってない９人目の要救助者……）"
  },
  "000795": {
    "context": "000795",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "（『鳥羽ましろ』が……？）"
  },
  "000842": {
    "context": "000842",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "誰も疑いたくはなかった。"
  },
  "000853": {
    "context": "000853",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "しかし誰もが疑わしかった。"
  },
  "000865": {
    "context": "000865",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "（誰かがこうなるように仕向けたと、思えて仕方がない……！）"
  },
  "000876": {
    "context": "000876",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "では誰が黒幕だったのか。"
  },
  "000887": {
    "context": "000887",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "確かな事は、自分ではないという事。"
  },
  "000898": {
    "context": "000898",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "そして――"
  },
  "000910": {
    "context": "000910",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "（夏彦ではない……彼はここで死にかけている……）"
  },
  "000922": {
    "context": "000922",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "（せめて、彼だけは……ここから逃がしてやりたかった……！）"
  },
  "000933": {
    "context": "000933",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "だが意識をもうつなぎとめられてはいられない。"
  },
  "000944": {
    "context": "000944",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "渡瀬が気を失おうとした、"
  },
  "000954": {
    "context": "000954",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "――その時。"
  },
  "000989": {
    "context": "000989",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "心臓の鼓動が聞こえた。"
  },
  "001021": {
    "context": "001021",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "（っ！？）"
  },
  "001052": {
    "context": "001052",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "一瞬、自分の心音かと思った。"
  },
  "001100": {
    "context": "001100",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "だが――違う。"
  },
  "001105": {
    "context": "001105",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "その音は、すぐ近くから聞こえてきている。"
  },
  "001169": {
    "context": "001169",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "渡瀬の傍らで、何かが動く気配がした。"
  },
  "001175": {
    "context": "001175",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "（ば……バカな……！）"
  },
  "001180": {
    "context": "001180",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "渡瀬は瞼をこじ開け、そして見た。"
  },
  "001245": {
    "context": "001245",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "そこに彼が、立っているのを。"
  },
  "001332": {
    "context": "001332",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "（な……なぜ……！？）"
  },
  "001381": {
    "context": "001381",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "瀕死のはずの彼が立ち上がり、ぎろりと目を見開き、渡瀬を睨みつけている。"
  },
  "001386": {
    "context": "001386",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "なぜ急に目覚めたのか。なぜ渡瀬を睨んでいるのか。"
  },
  "001435": {
    "context": "001435",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "何一つ理解できない渡瀬に――"
  },
  "001440": {
    "context": "001440",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "彼は言った。"
  },
  "001496": {
    "context": "001496",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「――立て」"
  },
  "001507": {
    "context": "001507",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「まだ、何も終わってない……！」"
  },
  "001555": {
    "context": "001555",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "――だが、その声を聞くと同時に――"
  },
  "001560": {
    "context": "001560",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "渡瀬の意識は、闇に落ちた。"
  }
}