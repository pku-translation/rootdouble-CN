{
  "000080": {
    "context": "000080",
    "code": "0x55",
    "string": "√After Chapter7"
  },
  "000095": {
    "context": "000095",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「ぐっ！」"
  },
  "000160": {
    "context": "000160",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「なっ……！」"
  },
  "000165": {
    "context": "000165",
    "code": "0x45",
    "developer_comment": "",
    "string": "眼の前の光景に、渡瀬は言葉を失った。"
  },
  "000170": {
    "context": "000170",
    "code": "0x45",
    "developer_comment": "",
    "string": "サリュの細い腕が、風見の首に食い込んでいる――！"
  },
  "000181": {
    "context": "000181",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「か、はっ……」"
  },
  "000186": {
    "context": "000186",
    "code": "0x45",
    "developer_comment": "",
    "string": "苦しそうに喘ぐ風見の背後から、サリュが鋭く渡瀬を睨む。"
  },
  "000197": {
    "context": "000197",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「夏彦を解放して。でないとこの女の息の根を止める」"
  },
  "000208": {
    "context": "000208",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「やめろ、サリュ！」"
  },
  "000219": {
    "context": "000219",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「オレたちは君たちを救いたいだけなんだ！　どうしてそれをわかろうとしない！」"
  },
  "000230": {
    "context": "000230",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「だから、見え透いた嘘はやめろと言っている」"
  },
  "000241": {
    "context": "000241",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「あなたが夏彦を、撃ったくせに」"
  },
  "000258": {
    "context": "000258",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「――ッ！？」"
  },
  "000263": {
    "context": "000263",
    "code": "0x45",
    "developer_comment": "",
    "string": "その言葉が、渡瀬の胸に突き刺さった。"
  },
  "000314": {
    "context": "000314",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「な……今、なにを……！」"
  },
  "000325": {
    "context": "000325",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「女、あなたは知らなかったの？」"
  },
  "000336": {
    "context": "000336",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「嘘、ですよね……？　隊長が、そんな事するはずないですよね……？」"
  },
  "000347": {
    "context": "000347",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「…………っ」"
  },
  "000352": {
    "context": "000352",
    "code": "0x45",
    "developer_comment": "",
    "string": "渡瀬には、夏彦を撃ったという記憶は無い。"
  },
  "000357": {
    "context": "000357",
    "code": "0x45",
    "developer_comment": "",
    "string": "だが――"
  },
  "000559": {
    "context": "000559",
    "code": "0x45",
    "developer_comment": "",
    "string": "――説明不可能の記憶があった。"
  },
  "000564": {
    "context": "000564",
    "code": "0x45",
    "developer_comment": "",
    "string": "何も答えられない渡瀬に代わって、サリュが風見の耳元で囁く。"
  },
  "000575": {
    "context": "000575",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「知らないなら教えてあげる」"
  },
  "000586": {
    "context": "000586",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「この男は、テロリスト」"
  },
  "000597": {
    "context": "000597",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「私たちは昨夜も今朝も、この男の襲撃を受けた」"
  },
  "000608": {
    "context": "000608",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「ここで起こっていることは、全部あの男のせい」"
  },
  "000619": {
    "context": "000619",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「あなたにだって、思い当たることはいくつもあるはず」"
  },
  "000630": {
    "context": "000630",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「やめ……なさい……！　これ以上、そんなこと、聞きたくない……！」"
  },
  "000641": {
    "context": "000641",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「私は隊長を信じて……」"
  },
  "000652": {
    "context": "000652",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「ならよく見て。あの男の懐にあるものを」"
  },
  "000749": {
    "context": "000749",
    "code": "0x45",
    "developer_comment": "",
    "string": "渡瀬の懐にあるもの――"
  },
  "000754": {
    "context": "000754",
    "code": "0x45",
    "developer_comment": "",
    "string": "それはＮエリアで手に入れた拳銃だった。"
  },
  "000781": {
    "context": "000781",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「あれは恐らく、この男の持ち物」"
  },
  "000792": {
    "context": "000792",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「夏彦はあの銃で撃たれて――」"
  },
  "000797": {
    "context": "000797",
    "code": "0x45",
    "developer_comment": "",
    "string": "サリュがそう言いかけた時、"
  },
  "000859": {
    "context": "000859",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「――もう、やめて！」"
  },
  "000864": {
    "context": "000864",
    "code": "0x45",
    "developer_comment": "",
    "string": "風見が絶叫し、乱暴にサリュを振り解いた。"
  },
  "000945": {
    "context": "000945",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「！」"
  },
  "000950": {
    "context": "000950",
    "code": "0x45",
    "developer_comment": "",
    "string": "その拍子にサリュの体が、後方へと飛ばされた。"
  },
  "000955": {
    "context": "000955",
    "code": "0x45",
    "developer_comment": "",
    "string": "そして飛ばされた先には、階段が――"
  },
  "000966": {
    "context": "000966",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「あ……！」"
  },
  "000971": {
    "context": "000971",
    "code": "0x45",
    "developer_comment": "",
    "string": "渡瀬はその一部始終を、見ていることしかできなかった。"
  },
  "000983": {
    "context": "000983",
    "code": "0x45",
    "developer_comment": "",
    "string": "サリュはそのまま階段を転がり落ち――"
  },
  "000990": {
    "context": "000990",
    "code": "0x45",
    "developer_comment": "",
    "string": "地下１階の踊り場で、ぐったりと動かなくなった。"
  },
  "001001": {
    "context": "001001",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「――サリュ！」"
  },
  "001111": {
    "context": "001111",
    "code": "0x45",
    "developer_comment": "",
    "string": "渡瀬は慌ててサリュに駆け寄り、容態を確認した。"
  },
  "001116": {
    "context": "001116",
    "code": "0x45",
    "developer_comment": "",
    "string": "だがどうやら骨折などの目立った外傷は見当たらず、脈も呼吸もしっかりしている。"
  },
  "001121": {
    "context": "001121",
    "code": "0x45",
    "developer_comment": "",
    "string": "渡瀬が安堵の息をついた時――風見がふらふらと、階段を降りてきた。"
  },
  "001150": {
    "context": "001150",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「隊長……三ノ宮さんは……？」"
  },
  "001155": {
    "context": "001155",
    "code": "0x45",
    "developer_comment": "",
    "string": "その顔は青ざめ、ひどく動揺しているようだった。"
  },
  "001166": {
    "context": "001166",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「大丈夫、気を失っているだけだ」"
  },
  "001195": {
    "context": "001195",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「そ、そうですか……」"
  },
  "001200": {
    "context": "001200",
    "code": "0x45",
    "developer_comment": "",
    "string": "風見の身体が震えている。"
  },
  "001205": {
    "context": "001205",
    "code": "0x45",
    "developer_comment": "",
    "string": "震えながら、風見は渡瀬を見ていた。"
  },
  "001210": {
    "context": "001210",
    "code": "0x45",
    "developer_comment": "",
    "string": "――怯えたような眼で。"
  },
  "001221": {
    "context": "001221",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「橘……？」"
  },
  "001226": {
    "context": "001226",
    "code": "0x45",
    "developer_comment": "",
    "string": "風見の様子がおかしい。"
  },
  "001231": {
    "context": "001231",
    "code": "0x45",
    "developer_comment": "",
    "string": "彼女はまるで自分を抱き止めるように、自分で自分の身体を抱き――"
  },
  "001260": {
    "context": "001260",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「隊長、私は…………」"
  },
  "001265": {
    "context": "001265",
    "code": "0x45",
    "developer_comment": "",
    "string": "そう呟いたきり、黙った。"
  },
  "001276": {
    "context": "001276",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「……どうした？」"
  },
  "001282": {
    "context": "001282",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「……」"
  },
  "001287": {
    "context": "001287",
    "code": "0x45",
    "developer_comment": "",
    "string": "風見は答えなかった。"
  },
  "001292": {
    "context": "001292",
    "code": "0x45",
    "developer_comment": "",
    "string": "ただ渡瀬を見据えている。"
  },
  "001297": {
    "context": "001297",
    "code": "0x45",
    "developer_comment": "",
    "string": "渡瀬の胸中に不安が湧き出した。"
  },
  "001308": {
    "context": "001308",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「……橘、どうして……そんな眼で、オレを見るんだ」"
  },
  "001319": {
    "context": "001319",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「まさか、サリュの言った事を信じてるのか？」"
  },
  "001330": {
    "context": "001330",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「バカな！　オレがテロリストのわけがないだろ？　記憶喪失だろうとなんだろうと――」"
  },
  "001341": {
    "context": "001341",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「オレが以前からずっとレスキュー隊員だったって事は、お前たちが一番よく知っているじゃないか！」"
  },
  "001347": {
    "context": "001347",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「……」"
  },
  "001352": {
    "context": "001352",
    "code": "0x45",
    "developer_comment": "",
    "string": "風見は答えない。不安が大きくなっていく。"
  },
  "001357": {
    "context": "001357",
    "code": "0x45",
    "developer_comment": "",
    "string": "やがて風見は――怯えを振り切るように、呟いた。"
  },
  "001386": {
    "context": "001386",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「……隊長。もう１度だけ質問しても、よろしいでしょうか」"
  },
  "001397": {
    "context": "001397",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「……なんだ」"
  },
  "001408": {
    "context": "001408",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「解けていなかった疑問があるんです……」"
  },
  "001437": {
    "context": "001437",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「隊長は、この事故が起きた直後の、６時４２分頃。どうして地下に降りてきたんですか……？」"
  },
  "001448": {
    "context": "001448",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「それは――だから、堂島と桧山に無線で呼ばれて……」"
  },
  "001459": {
    "context": "001459",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「だったら無線は、通じていたということですよね？」"
  },
  "001488": {
    "context": "001488",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「だけど……だけど、隊長」"
  },
  "001499": {
    "context": "001499",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「私はね、その少し前に、隊長に無線通信を試みていたんですよ……！」"
  },
  "001510": {
    "context": "001510",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「……っ！」"
  },
  "001515": {
    "context": "001515",
    "code": "0x45",
    "developer_comment": "",
    "string": "渡瀬の表情が凍りついた。"
  },
  "001520": {
    "context": "001520",
    "code": "0x45",
    "developer_comment": "",
    "string": "そう言えば風見は、ずっと前に言っていた。"
  },
  "001525": {
    "context": "001525",
    "code": "0x45",
    "developer_comment": "",
    "string": "『６時３５分頃、渡瀬との無線通信が断絶した』と。"
  },
  "001530": {
    "context": "001530",
    "code": "0x45",
    "developer_comment": "",
    "string": "――その言葉との辻褄が合わない。"
  },
  "001535": {
    "context": "001535",
    "code": "0x45",
    "developer_comment": "",
    "string": "だとすると――"
  },
  "001564": {
    "context": "001564",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「堂島たちの無線は通じたのに、どうして私からの無線には出なかったんですか……？」"
  },
  "001575": {
    "context": "001575",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「何か理由があって……意図的に、出なかったんじゃないですか……？」"
  },
  "001604": {
    "context": "001604",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「救命任務の最中に、それを抜け、何か別の事を行うために」"
  },
  "001615": {
    "context": "001615",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「お、お前――いつからそんなこと考えていたんだ？」"
  },
  "001626": {
    "context": "001626",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「隊長が『記憶が少し戻った』と言った時から……地下に降りた理由を聞いた時からです」"
  },
  "001637": {
    "context": "001637",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「だけど、言えなかった……あなたを信じたかったから……」"
  },
  "001666": {
    "context": "001666",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「時刻の不整合については、きっとあなたの記憶間違いだって思って……！」"
  },
  "001671": {
    "context": "001671",
    "code": "0x45",
    "developer_comment": "",
    "string": "風見は目を伏せ、震える声で続けた。"
  },
  "001682": {
    "context": "001682",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「私……必死に隊長を信じようとしていたんです」"
  },
  "001693": {
    "context": "001693",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「ずっとずっと昔から、あなたを信じ続けていたんです」"
  },
  "001724": {
    "context": "001724",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「あなたがどれほど変わろうとも……記憶喪失になろうとも」"
  },
  "001753": {
    "context": "001753",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「だから先ほど抱きしめてくれた事は、本当に嬉しかった……あなたが昔のあなたに戻ってくれた気がして」"
  },
  "001782": {
    "context": "001782",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「だけど……やっぱり、過去は過去なんです」"
  },
  "001793": {
    "context": "001793",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「いくら自分に言い聞かせようと、もうあなたを信じる事は……」"
  },
  "001823": {
    "context": "001823",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「だけど……それも、もうできません」"
  },
  "001835": {
    "context": "001835",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「た、橘……？」"
  },
  "001840": {
    "context": "001840",
    "code": "0x45",
    "developer_comment": "",
    "string": "そう問いかけた瞬間――風見の表情と、口調が変わった。"
  },
  "001869": {
    "context": "001869",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「……できないのよ、渡瀬」"
  },
  "001880": {
    "context": "001880",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「さっきの三ノ宮さんの話が本当だとしたら、何もかも辻褄が合ってしまうの」"
  },
  "001891": {
    "context": "001891",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「この施設の正体、以前のあなた……それを考えると、答えは１つなのよ」"
  },
  "001902": {
    "context": "001902",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「橘、何を言っている！？」"
  },
  "001931": {
    "context": "001931",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「……あなたが殺したのなら……」"
  },
  "001942": {
    "context": "001942",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「せめて、あなたの名誉のために……！」"
  },
  "001977": {
    "context": "001977",
    "code": "0x45",
    "developer_comment": "",
    "string": "風見は非常階段に備え付けられた消火器に手を伸ばした。"
  },
  "001988": {
    "context": "001988",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「たっ……橘、そんなもので何をするつもりだ……？」"
  },
  "001993": {
    "context": "001993",
    "code": "0x45",
    "developer_comment": "",
    "string": "風見は答えない。"
  },
  "001998": {
    "context": "001998",
    "code": "0x45",
    "developer_comment": "",
    "string": "代わりに悲しげに呟く。"
  },
  "002027": {
    "context": "002027",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「渡瀬、あなたは道を違えてしまった……」"
  },
  "002038": {
    "context": "002038",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「……その報いを、受ける時が来たの」"
  },
  "002043": {
    "context": "002043",
    "code": "0x45",
    "developer_comment": "",
    "string": "そして次の瞬間、"
  },
  "002055": {
    "context": "002055",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「っ！？」"
  },
  "002168": {
    "context": "002168",
    "code": "0x45",
    "developer_comment": "",
    "string": "風見が消火器を、横薙ぎに振るった。"
  },
  "002200": {
    "context": "002200",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「ぐあっ！！」"
  },
  "002229": {
    "context": "002229",
    "code": "0x45",
    "developer_comment": "",
    "string": "渡瀬はとっさに、それを腕でガードする。"
  },
  "002234": {
    "context": "002234",
    "code": "0x45",
    "developer_comment": "",
    "string": "全く手加減の無い一撃に、腕の骨が軋んだ。"
  },
  "002306": {
    "context": "002306",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「よ、よせ橘！　なぜこんな事を！？」"
  },
  "002335": {
    "context": "002335",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「渡瀬……ごめんなさい、渡瀬……」"
  },
  "002364": {
    "context": "002364",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「私にはもう、こうするしか……できる事がないの……」"
  },
  "002431": {
    "context": "002431",
    "code": "0x45",
    "developer_comment": "",
    "string": "そう謝る風見の眼には、殺意の色が――！"
  },
  "002471": {
    "context": "002471",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "（どうして橘まで！？）"
  },
  "002476": {
    "context": "002476",
    "code": "0x45",
    "developer_comment": "",
    "string": "渡瀬は戦慄し、とっさに風見を突き飛ばした。"
  },
  "002570": {
    "context": "002570",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「きゃっ！」"
  },
  "002575": {
    "context": "002575",
    "code": "0x45",
    "developer_comment": "",
    "string": "消火器を持ったまま、風見が階段に倒れ込む。"
  },
  "002629": {
    "context": "002629",
    "code": "0x45",
    "developer_comment": "",
    "string": "渡瀬はその隙に、地下２階まで駆け降り、階段室から飛び出した。"
  },
  "002679": {
    "context": "002679",
    "code": "0x45",
    "developer_comment": "",
    "string": "夏彦を背負ったまま、渡瀬は走った。"
  },
  "002684": {
    "context": "002684",
    "code": "0x45",
    "developer_comment": "",
    "string": "階段室に置いてきた、サリュのことも気になる。"
  },
  "002689": {
    "context": "002689",
    "code": "0x45",
    "developer_comment": "",
    "string": "だが渡瀬には、夏彦を安全な場所へ連れて行くので精一杯だった。"
  },
  "002700": {
    "context": "002700",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「くそっ！　どうなってやがるんだ！」"
  },
  "002705": {
    "context": "002705",
    "code": "0x45",
    "developer_comment": "",
    "string": "ついさっきまで、風見とは心が通っていたはずなのに。"
  },
  "002710": {
    "context": "002710",
    "code": "0x45",
    "developer_comment": "",
    "string": "確かに信頼し合っていたはずなのに――！"
  },
  "002716": {
    "context": "002716",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "（全部がオレの錯覚だったっていうのか！？）"
  },
  "002721": {
    "context": "002721",
    "code": "0x45",
    "developer_comment": "",
    "string": "だが風見に殴られた腕の痛みは、紛れもない現実だった。"
  },
  "002726": {
    "context": "002726",
    "code": "0x45",
    "developer_comment": "",
    "string": "地を蹴る振動が伝わる度に、酷く痛む。"
  },
  "002731": {
    "context": "002731",
    "code": "0x45",
    "developer_comment": "",
    "string": "辛い現実を思い知らせるように。"
  },
  "002780": {
    "context": "002780",
    "code": "0x45",
    "developer_comment": "",
    "string": "闇雲に逃げるうち、連絡通路に出た。"
  },
  "002785": {
    "context": "002785",
    "code": "0x45",
    "developer_comment": "",
    "string": "だが通路の向こう側から、足音が聞こえてくる。"
  },
  "002791": {
    "context": "002791",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "（誰だ！？）"
  },
  "002796": {
    "context": "002796",
    "code": "0x45",
    "developer_comment": "",
    "string": "そう思うと同時に、通路の向こうから洵が姿を現した。"
  },
  "002873": {
    "context": "002873",
    "code": "0x45",
    "developer_comment": "洵",
    "string": "「隊長……！」"
  },
  "002878": {
    "context": "002878",
    "code": "0x45",
    "developer_comment": "",
    "string": "その手には今も、エンジンカッターが握られていて――"
  },
  "002939": {
    "context": "002939",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「ッ！！」"
  },
  "002944": {
    "context": "002944",
    "code": "0x45",
    "developer_comment": "",
    "string": "渡瀬は踵を返し、逃げ出した。"
  },
  "002994": {
    "context": "002994",
    "code": "0x45",
    "developer_comment": "",
    "string": "どこかわからないエリアを、夏彦を背負って逃げ惑う。"
  },
  "002999": {
    "context": "002999",
    "code": "0x45",
    "developer_comment": "",
    "string": "息が上がる。全身が痛む。"
  },
  "003004": {
    "context": "003004",
    "code": "0x45",
    "developer_comment": "",
    "string": "もう走れない――そう思った時。"
  },
  "003019": {
    "context": "003019",
    "code": "0x45",
    "developer_comment": "",
    "string": "乾いた音が響いた。"
  },
  "003061": {
    "context": "003061",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「がっ！」"
  },
  "003066": {
    "context": "003066",
    "code": "0x45",
    "developer_comment": "",
    "string": "渡瀬はその場に転倒した。"
  },
  "003071": {
    "context": "003071",
    "code": "0x45",
    "developer_comment": "",
    "string": "腹が熱い。その熱がすぐに激痛に変わる。"
  },
  "003076": {
    "context": "003076",
    "code": "0x45",
    "developer_comment": "",
    "string": "脇腹を撃たれたのだ。"
  },
  "003087": {
    "context": "003087",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「……！」"
  },
  "003092": {
    "context": "003092",
    "code": "0x45",
    "developer_comment": "",
    "string": "腹を押さえながら周囲を伺う。"
  },
  "003097": {
    "context": "003097",
    "code": "0x45",
    "developer_comment": "",
    "string": "姿は見えないが、どこからか声が聞こえてきた。"
  },
  "003128": {
    "context": "003128",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "「今度は当たったな、隊長さん」"
  },
  "003139": {
    "context": "003139",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「お……おっさん……！？」"
  },
  "003150": {
    "context": "003150",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "「苦しそうだね。では最後の情けだ、すぐ楽にしてやろう」"
  },
  "003155": {
    "context": "003155",
    "code": "0x45",
    "developer_comment": "",
    "string": "その声を聞き終える前に、渡瀬は立ち上がった。"
  },
  "003209": {
    "context": "003209",
    "code": "0x45",
    "developer_comment": "",
    "string": "夏彦を抱え、また走り出す。"
  },
  "003214": {
    "context": "003214",
    "code": "0x45",
    "developer_comment": "",
    "string": "脇腹から血を流しながら。"
  },
  "003219": {
    "context": "003219",
    "code": "0x45",
    "developer_comment": "",
    "string": "幸い銃弾は、内臓を避けてくれたようだ。"
  },
  "003224": {
    "context": "003224",
    "code": "0x45",
    "developer_comment": "",
    "string": "しかし撃たれた傷はずきずきと痛み、血も止まらない。"
  },
  "003235": {
    "context": "003235",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「畜生……畜生……！」"
  },
  "003240": {
    "context": "003240",
    "code": "0x45",
    "developer_comment": "",
    "string": "安全な場所を探し、渡瀬は走った。"
  },
  "003245": {
    "context": "003245",
    "code": "0x45",
    "developer_comment": "",
    "string": "やがて彼の眼に――それが見えてきた。"
  },
  "003290": {
    "context": "003290",
    "code": "0x45",
    "developer_comment": "",
    "string": "Ｎエリアへ続く連絡通路。"
  },
  "003295": {
    "context": "003295",
    "code": "0x45",
    "developer_comment": "",
    "string": "そこに続く扉を開けられるのは、渡瀬の持っている『Ｌｖ．Ｎ』のカードしかない！"
  },
  "003316": {
    "context": "003316",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「う……うぉおおお！」"
  },
  "003321": {
    "context": "003321",
    "code": "0x45",
    "developer_comment": "",
    "string": "渡瀬は叫び、自分を奮い立たせる。"
  },
  "003326": {
    "context": "003326",
    "code": "0x45",
    "developer_comment": "",
    "string": "そしてＮエリアに向けて駆け出した。"
  },
  "003414": {
    "context": "003414",
    "code": "0x45",
    "developer_comment": "",
    "string": "ゲートを開け、その向こうに転がり込む。"
  },
  "003488": {
    "context": "003488",
    "code": "0x45",
    "developer_comment": "",
    "string": "例のＷＸ粒子とやらの影響か、奇怪な感覚が走り抜け、指先が痙攣する。"
  },
  "003493": {
    "context": "003493",
    "code": "0x45",
    "developer_comment": "",
    "string": "だが、渡瀬はそれに耐えた。"
  },
  "003499": {
    "context": "003499",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "（レベルＮのカードを持っているのは、オレだけだ……）"
  },
  "003505": {
    "context": "003505",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "（このゲートが閉まってしまえば、あいつらの追跡を阻止できる！）"
  },
  "003510": {
    "context": "003510",
    "code": "0x45",
    "developer_comment": "",
    "string": "そう思った時、"
  },
  "003515": {
    "context": "003515",
    "code": "0x45",
    "developer_comment": "",
    "string": "まだ開いたままのゲートの向こうに、"
  },
  "003676": {
    "context": "003676",
    "code": "0x45",
    "developer_comment": "",
    "string": "消火器を持った、風見が現れた。"
  },
  "003682": {
    "context": "003682",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "（！！）"
  },
  "003687": {
    "context": "003687",
    "code": "0x45",
    "developer_comment": "",
    "string": "彼女の視線が、渡瀬を射抜く。"
  },
  "003692": {
    "context": "003692",
    "code": "0x45",
    "developer_comment": "",
    "string": "そして風見は――"
  },
  "003728": {
    "context": "003728",
    "code": "0x45",
    "developer_comment": "",
    "string": "渡瀬めがけ、猛然と駆け寄ってきた！"
  },
  "003788": {
    "context": "003788",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「閉まれ！　早く！」"
  },
  "003793": {
    "context": "003793",
    "code": "0x45",
    "developer_comment": "",
    "string": "ゆっくりとゲートが閉まり始める。だがそれが間に合わない。"
  },
  "003800": {
    "context": "003800",
    "code": "0x45",
    "developer_comment": "",
    "string": "風見はゲートを潜り抜け、"
  },
  "003859": {
    "context": "003859",
    "code": "0x45",
    "developer_comment": "",
    "string": "そして消火器を、渡瀬の頭に振り下ろした。"
  },
  "003891": {
    "context": "003891",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「がっ！！」"
  },
  "003896": {
    "context": "003896",
    "code": "0x45",
    "developer_comment": "",
    "string": "渡瀬の眼の中で火花が散った。"
  },
  "003920": {
    "context": "003920",
    "code": "0x45",
    "developer_comment": "",
    "string": "平衡感覚を失い、身体が横にぐらりと傾ぐ。"
  },
  "003966": {
    "context": "003966",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "（こ、このままじゃ殺される……！）"
  },
  "003972": {
    "context": "003972",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "（いまオレがここで殺されたら……夏彦もやばい！）"
  },
  "003977": {
    "context": "003977",
    "code": "0x45",
    "developer_comment": "",
    "string": "その想いが、渡瀬の断裂しかけた意識を繋ぎ止める。"
  },
  "003982": {
    "context": "003982",
    "code": "0x45",
    "developer_comment": "",
    "string": "そして渡瀬は、最後の力を振り絞り――"
  },
  "004002": {
    "context": "004002",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「ああああぁっ！」"
  },
  "004028": {
    "context": "004028",
    "code": "0x45",
    "developer_comment": "",
    "string": "風見を、Ｎエリアの外に突き飛ばした。"
  },
  "004082": {
    "context": "004082",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「ぎっ！」"
  },
  "004093": {
    "context": "004093",
    "code": "0x45",
    "developer_comment": "",
    "string": "風見のうめきが聞こえるのと、ゲートが閉まり切るのは、ほぼ同時だった。"
  },
  "004141": {
    "context": "004141",
    "code": "0x45",
    "developer_comment": "",
    "string": "ゲートの向こうから声が聞こえる。"
  },
  "004152": {
    "context": "004152",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「渡瀬ッ！　渡瀬ぇええッ！」"
  },
  "004163": {
    "context": "004163",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「開けなさい渡瀬ぇえええッ！！」"
  },
  "004173": {
    "context": "004173",
    "code": "0x45",
    "developer_comment": "",
    "string": "その声も遠のいていく。"
  },
  "004178": {
    "context": "004178",
    "code": "0x45",
    "developer_comment": "",
    "string": "頭痛と体の痙攣が、急速に激しさを増す。"
  },
  "004184": {
    "context": "004184",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "（――ＡＤが切れたのか！？）"
  },
  "004189": {
    "context": "004189",
    "code": "0x45",
    "developer_comment": "",
    "string": "そう思った瞬間、全身の力が抜けた。"
  },
  "004269": {
    "context": "004269",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「っ！」"
  },
  "004274": {
    "context": "004274",
    "code": "0x45",
    "developer_comment": "",
    "string": "急速な失墜感。"
  },
  "004279": {
    "context": "004279",
    "code": "0x45",
    "developer_comment": "",
    "string": "渡瀬はそれに抗おうとしたが――"
  },
  "004330": {
    "context": "004330",
    "code": "0x45",
    "developer_comment": "",
    "string": "――耐えられない。"
  },
  "004335": {
    "context": "004335",
    "code": "0x45",
    "developer_comment": "",
    "string": "床が迫る。"
  },
  "004340": {
    "context": "004340",
    "code": "0x45",
    "developer_comment": "",
    "string": "ダメだ――倒れる！"
  },
  "004364": {
    "context": "004364",
    "code": "0x45",
    "developer_comment": "",
    "string": "倒れると同時に、渡瀬の背中から、夏彦の重さが消えた。"
  },
  "004369": {
    "context": "004369",
    "code": "0x45",
    "developer_comment": "",
    "string": "どうやら横に投げ出してしまったらしい。"
  },
  "004374": {
    "context": "004374",
    "code": "0x45",
    "developer_comment": "",
    "string": "だが渡瀬には、もうどうすることもできなかった。"
  }
}