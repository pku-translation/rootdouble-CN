{
  "000055": {
    "context": "000055",
    "code": "0x55",
    "string": "√Before Chapter4"
  },
  "000088": {
    "context": "000088",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「あ、みんなおかえり～」"
  },
  "000093": {
    "context": "000093",
    "code": "0x45",
    "developer_comment": "",
    "string": "家に帰ると、悠里が３人を迎え入れてくれた。"
  },
  "000104": {
    "context": "000104",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「ただいま悠里」"
  },
  "000191": {
    "context": "000191",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「いや～、雨に降られて大変だったよー」"
  },
  "000196": {
    "context": "000196",
    "code": "0x45",
    "developer_comment": "",
    "string": "ましろはもう、すっかり元気そうだった。キッチンに向かいながら言う。"
  },
  "000225": {
    "context": "000225",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「それじゃご飯つくるね。今日も腕によりをかけるから、ちょっと待ってて」"
  },
  "000320": {
    "context": "000320",
    "code": "0x45",
    "developer_comment": "",
    "string": "――ましろはそう言って、凝ったソースのスパゲッティを作った。"
  },
  "000325": {
    "context": "000325",
    "code": "0x45",
    "developer_comment": "",
    "string": "和気あいあいと、その日の夕食は終わったのだが――"
  },
  "000378": {
    "context": "000378",
    "code": "0x45",
    "developer_comment": "",
    "string": "食後、夏彦が食卓の片づけをしていると、ましろが神妙な顔で囁いた。"
  },
  "000407": {
    "context": "000407",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「……ねぇ夏彦」"
  },
  "000418": {
    "context": "000418",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「ん、どうした？」"
  },
  "000447": {
    "context": "000447",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「ちょっと話したい事があるんだけど、いいかな？」"
  },
  "000458": {
    "context": "000458",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「え？　なに？」"
  },
  "000487": {
    "context": "000487",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「ごめん、ここじゃちょっと……」"
  },
  "000492": {
    "context": "000492",
    "code": "0x45",
    "developer_comment": "",
    "string": "ましろはちらりと、ソファの方を見た。"
  },
  "000592": {
    "context": "000592",
    "code": "0x45",
    "developer_comment": "",
    "string": "そこでは悠里とサリュが、くつろいで本を読んでいる。"
  },
  "000657": {
    "context": "000657",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「……２人に聞かれると、まずい話か？」"
  },
  "000686": {
    "context": "000686",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「まずくないけど、心配かけたくないんだ……」"
  },
  "000697": {
    "context": "000697",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「……？」"
  },
  "000702": {
    "context": "000702",
    "code": "0x45",
    "developer_comment": "",
    "string": "夏彦はましろが何を言おうとしているのか、見当がつかなかった。"
  },
  "000713": {
    "context": "000713",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「わかったよ。ぼくの部屋に行こう」"
  },
  "000718": {
    "context": "000718",
    "code": "0x45",
    "developer_comment": "",
    "string": "そう言って夏彦は、ましろを自室に連れて行った。"
  },
  "000852": {
    "context": "000852",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「どうぞ、ましろ」"
  },
  "000897": {
    "context": "000897",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「あ……お邪魔します」"
  },
  "000902": {
    "context": "000902",
    "code": "0x45",
    "developer_comment": "",
    "string": "ましろを部屋に迎え入れると、彼女は遠慮がちに中に入ってきた。"
  },
  "000913": {
    "context": "000913",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「で、なんだ話って？」"
  },
  "000942": {
    "context": "000942",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「いや……言おうかどうしようか、すごく迷ったんだけど……」"
  },
  "000971": {
    "context": "000971",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「今から話す内容がどんなものでも、信じてくれる？」"
  },
  "000982": {
    "context": "000982",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「あぁ、もちろんだ」"
  },
  "001011": {
    "context": "001011",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「ありがとう」"
  },
  "001016": {
    "context": "001016",
    "code": "0x45",
    "developer_comment": "",
    "string": "ましろはそう答え、それから神妙に囁いた。"
  },
  "001045": {
    "context": "001045",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「――それじゃ夏彦、よく聞いて」"
  },
  "001074": {
    "context": "001074",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「近いうちに、この街で誰かが……」"
  },
  "001085": {
    "context": "001085",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「テロを起こそうとしてるみたいなんだよ……！」"
  },
  "001097": {
    "context": "001097",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「…………は？」"
  },
  "001102": {
    "context": "001102",
    "code": "0x45",
    "developer_comment": "",
    "string": "夏彦は耳を疑った。"
  },
  "001107": {
    "context": "001107",
    "code": "0x45",
    "developer_comment": "",
    "string": "まさかそんな言葉が出てくるとは思わなかったからだ。"
  },
  "001118": {
    "context": "001118",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「テロって、ましろ……いくらなんでもそれは……」"
  },
  "001153": {
    "context": "001153",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「冗談じゃないよ！　本気だよ！」"
  },
  "001164": {
    "context": "001164",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「！！」"
  },
  "001175": {
    "context": "001175",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「ご、ごめん……信じるって言ったのにな」"
  },
  "001186": {
    "context": "001186",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「でも、なんで急にそんな事、言い出したんだ？」"
  },
  "001215": {
    "context": "001215",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「……今日、バスに乗って帰る途中、急に私が黙り込んだのを覚えてる？」"
  },
  "001226": {
    "context": "001226",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「え？　あぁ……」"
  },
  "001346": {
    "context": "001346",
    "code": "0x45",
    "developer_comment": "",
    "string": "そう言えば確かに、そんな事があった。"
  },
  "001351": {
    "context": "001351",
    "code": "0x45",
    "developer_comment": "",
    "string": "誰かが『うるさいなぁ』と言った声を、ましろが聞いたと言っていた。"
  },
  "001411": {
    "context": "001411",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「……覚えてるけど、それがどうしたんだ？」"
  },
  "001440": {
    "context": "001440",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「あの時、私が聞いたのは、『うるさい』なんて声じゃなくて……」"
  },
  "001469": {
    "context": "001469",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「もっと恐ろしい、独り言だったんだ」"
  },
  "001498": {
    "context": "001498",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「乗客の誰かが、ぶつぶつ言っていたの。声が小さくてよく聞こえなかったんだけど……」"
  },
  "001509": {
    "context": "001509",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「なんて言ってたんだ？」"
  },
  "001520": {
    "context": "001520",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「『コミュニケーターは忌むべき存在だ』『こんな街の歴史は終わらせてやる』『決行の日は近い』って……」"
  },
  "001531": {
    "context": "001531",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「……！」"
  },
  "001536": {
    "context": "001536",
    "code": "0x45",
    "developer_comment": "",
    "string": "それは確かに、テロの決意のような言葉だった。"
  },
  "001565": {
    "context": "001565",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「私、ぎょっとしちゃって……慌てて耳を澄ましてみたんだけど、すると声の主も黙っちゃって」"
  },
  "001594": {
    "context": "001594",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「聞き間違えだと思う事にしたんだけど、やっぱり気になってさ」"
  },
  "001658": {
    "context": "001658",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「なんだかだんだん、怖くなってきちゃってさ……！」"
  },
  "001663": {
    "context": "001663",
    "code": "0x45",
    "developer_comment": "",
    "string": "ましろの声には、怯えが滲んでいた。"
  },
  "001668": {
    "context": "001668",
    "code": "0x45",
    "developer_comment": "",
    "string": "顔色は青ざめ、肩も小刻みに震えている。"
  },
  "001673": {
    "context": "001673",
    "code": "0x45",
    "developer_comment": "",
    "string": "どこか様子がおかしかった。"
  },
  "001703": {
    "context": "001703",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「お、おいましろ、大丈夫か？」"
  },
  "001732": {
    "context": "001732",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「大丈夫……ちょっと思い出して、おっかなくなっただけ」"
  },
  "001761": {
    "context": "001761",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「それより私、思うんだけど……」"
  },
  "001790": {
    "context": "001790",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「あの人、ニュースでやってた『コミュニケーター排斥を目論む過激派』……あれじゃないかって気がするの」"
  },
  "001795": {
    "context": "001795",
    "code": "0x45",
    "developer_comment": "",
    "string": "その話は夏彦も聞いた事があった。"
  },
  "001800": {
    "context": "001800",
    "code": "0x45",
    "developer_comment": "",
    "string": "独り言をぶつぶつ言っていたというのも、不気味なだけに本当っぽく感じられる。"
  },
  "001805": {
    "context": "001805",
    "code": "0x45",
    "developer_comment": "",
    "string": "宇喜多の話に出てきた『コミュニケーターを疎ましく思う者の増加』とも、関連があるのかもしれない。"
  },
  "001816": {
    "context": "001816",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「……声の主の顔、見たのか？」"
  },
  "001845": {
    "context": "001845",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「え、えっと……ちゃんと見たわけじゃないけど……」"
  },
  "001874": {
    "context": "001874",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「それでももう一度会えば、きっとわかる……！」"
  },
  "001879": {
    "context": "001879",
    "code": "0x45",
    "developer_comment": "",
    "string": "ましろはそう言って、夏彦を見据えた。"
  },
  "001908": {
    "context": "001908",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「ね、ねぇ……きっとこの事に気付いてるのは、私たちだけだよ」"
  },
  "001937": {
    "context": "001937",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「だったらなんとしても、テロを阻止しなきゃ！」"
  },
  "001948": {
    "context": "001948",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「おいおい、落ち着けよましろ」"
  },
  "001959": {
    "context": "001959",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「そんな独り言を言っていたからって、テロリストとは限らないだろ？」"
  },
  "001988": {
    "context": "001988",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「え……？」"
  },
  "001999": {
    "context": "001999",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「例えば『誰かを殺したい』って独り言を言っていたとしても、本当に殺人が起きるとは限らないしさ」"
  },
  "002028": {
    "context": "002028",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「そうじゃない！　あれは本物のテロリストだよ！」"
  },
  "002039": {
    "context": "002039",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「なんでそう言いきれる？」"
  },
  "002068": {
    "context": "002068",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「だってあの人──……」"
  },
  "002073": {
    "context": "002073",
    "code": "0x45",
    "developer_comment": "",
    "string": "ましろはそこで、言葉を呑んだ。後に続く言葉が出てこないらしい。"
  },
  "002084": {
    "context": "002084",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「ほら、根拠なんてないだろ？」"
  },
  "002113": {
    "context": "002113",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「……っ」"
  },
  "002124": {
    "context": "002124",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「だいたい仮にそいつがテロリストだとしてもさ、ぼくらにその阻止なんてできるワケないじゃないか」"
  },
  "002153": {
    "context": "002153",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「……できるわけないって、放っておくって事？」"
  },
  "002164": {
    "context": "002164",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「そうじゃないけど、ぼくらはただの高校生だぞ。わざわざぼくらが止める必要はないだろ？」"
  },
  "002193": {
    "context": "002193",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「夏彦、本気で言ってるの？」"
  },
  "002204": {
    "context": "002204",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「あぁ」"
  },
  "002210": {
    "context": "002210",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "（そりゃそうだ……そんな話、急に聞かされて、『じゃあ止めようか』とはならない）"
  },
  "002215": {
    "context": "002215",
    "code": "0x45",
    "developer_comment": "",
    "string": "夏彦は内心では、ましろの話を信じていなかった。"
  },
  "002220": {
    "context": "002220",
    "code": "0x45",
    "developer_comment": "",
    "string": "だが、ましろを傷つけないように、夏彦は優しく言う。"
  },
  "002231": {
    "context": "002231",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「どうしても気になるなら、警察や市の保安課にでも通報するべきだよ」"
  },
  "002242": {
    "context": "002242",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「それがぼくらにできる精一杯の事さ。そこから先は関係のない――」"
  },
  "002271": {
    "context": "002271",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「関係なくないよ！　私たちの街が狙われてるんだよ！？」"
  },
  "002282": {
    "context": "002282",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「通報以外にも、できる事があるかもしれないじゃん！」"
  },
  "002311": {
    "context": "002311",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「『大切なものは誰かが護ってくれるのを待つんじゃなくて、自分たちの手で護るもの』。そうでしょ、夏彦！？」"
  },
  "002317": {
    "context": "002317",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「…………」"
  },
  "002323": {
    "context": "002323",
    "code": "0x45",
    "developer_comment": "",
    "string": "夏彦は少々呆れてきた。"
  },
  "002328": {
    "context": "002328",
    "code": "0x45",
    "developer_comment": "",
    "string": "ましろの言葉が、妙に芝居がかって聞こえたのだ。"
  },
  "002339": {
    "context": "002339",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「何だよそれ……またなんかの引用か？」"
  },
  "002368": {
    "context": "002368",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「え……！？」"
  },
  "002379": {
    "context": "002379",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「前にも言ったろ？　良かれと思って行動するだけじゃダメなんだって」"
  },
  "002390": {
    "context": "002390",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「正義感も過ぎると、危険を招くだけだよ」"
  },
  "002395": {
    "context": "002395",
    "code": "0x45",
    "developer_comment": "",
    "string": "だがましろはそれを聞いて――寂しそうに笑った。"
  },
  "002424": {
    "context": "002424",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「……夏彦は、変わっちゃったね」"
  },
  "002435": {
    "context": "002435",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「変わってなんかないよ。ぼくは昔からこうさ」"
  },
  "002464": {
    "context": "002464",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「いや、変わったよ。あの時は私が止めたのに……」"
  },
  "002475": {
    "context": "002475",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「あの時……？」"
  },
  "002480": {
    "context": "002480",
    "code": "0x45",
    "developer_comment": "",
    "string": "なんの話なのか、まるで身に覚えがなかった。"
  },
  "002485": {
    "context": "002485",
    "code": "0x45",
    "developer_comment": "",
    "string": "あるとしたら、きっとはるか昔の――"
  },
  "002496": {
    "context": "002496",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「あ……」"
  },
  "002590": {
    "context": "002590",
    "code": "0x45",
    "developer_comment": "",
    "string": "やがて記憶の扉が開いた。"
  },
  "002596": {
    "context": "002596",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "（そうだ……９年前のあの日）"
  },
  "002659": {
    "context": "002659",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "（悠里と一緒に、出かけようとしたぼくは……）"
  },
  "002673": {
    "context": "002673",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "（……ましろに止められたんだ）"
  },
  "002733": {
    "context": "002733",
    "code": "0x45",
    "developer_comment": "",
    "string": "遠い昔、交わした会話が、胸の内に蘇る。"
  },
  "002762": {
    "context": "002762",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「やめなよ夏彦、あぶないよ！　あんなところ、いっちゃダメだって！」"
  },
  "002773": {
    "context": "002773",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「え、ましろビビッてるのか？　じゃあいいや、悠里さそおっかな～」"
  },
  "002802": {
    "context": "002802",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「……なにさそれ！　ちかごろいっつも悠里悠里って……！」"
  },
  "002813": {
    "context": "002813",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「なんだよ、まさかヤキモチ？　おとこおんなのくせにー」"
  },
  "002842": {
    "context": "002842",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「……！」"
  },
  "002871": {
    "context": "002871",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「夏彦なんか、もうしらない！　悠里とどこにでもいっちゃえ！」"
  },
  "002917": {
    "context": "002917",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "（そうしてましろの制止を振り切って、ぼくらはあの場所に行って――）"
  },
  "002928": {
    "context": "002928",
    "code": "0x45",
    "developer_comment": "",
    "string": "――その結果、悠里は……！"
  },
  "003024": {
    "context": "003024",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「……！」"
  },
  "003029": {
    "context": "003029",
    "code": "0x45",
    "developer_comment": "",
    "string": "動悸が速くなり、呼吸が苦しくなった。"
  },
  "003034": {
    "context": "003034",
    "code": "0x45",
    "developer_comment": "",
    "string": "苦い後悔が、夏彦の心を満たす。"
  },
  "003091": {
    "context": "003091",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「……そうだな、そんな事もあったな」"
  },
  "003120": {
    "context": "003120",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「あの時は夏彦の方が、わがまま通したくせに……」"
  },
  "003125": {
    "context": "003125",
    "code": "0x45",
    "developer_comment": "",
    "string": "その言葉が胸をえぐった。"
  },
  "003130": {
    "context": "003130",
    "code": "0x45",
    "developer_comment": "",
    "string": "９年前、夏彦と悠里はましろの制止を振り切って出かけ、事故に遭遇した。"
  },
  "003135": {
    "context": "003135",
    "code": "0x45",
    "developer_comment": "",
    "string": "その事について、ましろも『止められなかった』という責任を感じているかもしれない。"
  },
  "003148": {
    "context": "003148",
    "code": "0x45",
    "developer_comment": "",
    "string": "だが夏彦の責任の方がはるかに大きい。だからこそ――"
  },
  "003159": {
    "context": "003159",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「でも、だからこそ……お前には危険なマネをさせたくないんだ」"
  },
  "003188": {
    "context": "003188",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「……！」"
  },
  "003199": {
    "context": "003199",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「頼むよましろ。警察に通報して、それで終わりにしよう」"
  },
  "003210": {
    "context": "003210",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「どの道、ぼくらにできる事なんてそれくらいしかないんだから」"
  },
  "003218": {
    "context": "003218",
    "code": "0x45",
    "developer_comment": "",
    "string": "だが、それでも――言うべき事は言わねばならないと思った。"
  },
  "003229": {
    "context": "003229",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「……昔とは違うんだよ。あの頃はただ、向こう見ずだったんだ」"
  },
  "003258": {
    "context": "003258",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「……！」"
  },
  "003269": {
    "context": "003269",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「ましろ、警察に通報しよう。ぼくらにできる事はそれだけだ」"
  },
  "003298": {
    "context": "003298",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「……そっか。ホントに変わっちゃったんだね、夏彦……」"
  },
  "003339": {
    "context": "003339",
    "code": "0x45",
    "developer_comment": "",
    "string": "ましろは諦めたように微笑み、"
  },
  "003344": {
    "context": "003344",
    "code": "0x45",
    "developer_comment": "",
    "string": "一瞬後、怒りの表情を浮かべた。"
  },
  "003355": {
    "context": "003355",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「そんなに安全が好きなら、ずっと家にいれば！」"
  },
  "003366": {
    "context": "003366",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「え！？」"
  },
  "003395": {
    "context": "003395",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「もう夏彦には頼らないよ！　じゃあね！」"
  },
  "003424": {
    "context": "003424",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「お、おい、ちょっと待てよましろ！」"
  },
  "003429": {
    "context": "003429",
    "code": "0x45",
    "developer_comment": "",
    "string": "夏彦の制止を振り切るように、ましろは部屋を飛び出していった。"
  },
  "003514": {
    "context": "003514",
    "code": "0x45",
    "developer_comment": "",
    "string": "夏彦はわけもわからず追いかけたが――"
  },
  "003572": {
    "context": "003572",
    "code": "0x45",
    "developer_comment": "",
    "string": "家を出ると、すでにそこにましろの姿はなかった。"
  },
  "003577": {
    "context": "003577",
    "code": "0x45",
    "developer_comment": "",
    "string": "夕方降っていた雨はひとまず止んだらしく、わずかに星空が見えている。"
  },
  "003588": {
    "context": "003588",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「……なんだよ、あいつ」"
  },
  "003593": {
    "context": "003593",
    "code": "0x45",
    "developer_comment": "",
    "string": "苦い感情を吐き出すように、夏彦は呟いた。"
  },
  "003598": {
    "context": "003598",
    "code": "0x45",
    "developer_comment": "",
    "string": "なぜましろがあんなに怒ったのか、まるでわからない。"
  },
  "003604": {
    "context": "003604",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "（ぼくは間違ったことは言ってないはずだろ……？）"
  },
  "003609": {
    "context": "003609",
    "code": "0x45",
    "developer_comment": "",
    "string": "夏彦はそう思い、ふと夜空を見上げた。"
  },
  "003664": {
    "context": "003664",
    "code": "0x45",
    "developer_comment": "",
    "string": "そこには満天の星が輝いていた。"
  },
  "003669": {
    "context": "003669",
    "code": "0x45",
    "developer_comment": "",
    "string": "星々の間は離れていて、決して重なる事はない。"
  },
  "003674": {
    "context": "003674",
    "code": "0x45",
    "developer_comment": "",
    "string": "人と人の心が、重なりはしないように。"
  },
  "003679": {
    "context": "003679",
    "code": "0x45",
    "developer_comment": "",
    "string": "互いの間には、絶望的なまでの距離がある。"
  },
  "003685": {
    "context": "003685",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "（……長年一緒にいても、理解できない事はいくつもあるんだな……）"
  },
  "003699": {
    "context": "003699",
    "code": "0x55",
    "string": "√Before Chapter4"
  },
  "003706": {
    "context": "003706",
    "code": "0x45",
    "developer_comment": "",
    "string": "そう思いつつ踵を返し、家に戻った。"
  },
  "003760": {
    "context": "003760",
    "code": "0x45",
    "developer_comment": "",
    "string": "――リビングに行かず、そのまま自分の部屋に向かう。"
  },
  "003818": {
    "context": "003818",
    "code": "0x45",
    "developer_comment": "",
    "string": "そこでベッドに腰掛けながら、ましろの件について考えていると――"
  },
  "003823": {
    "context": "003823",
    "code": "0x45",
    "developer_comment": "",
    "string": "やがてノックの音がした。"
  },
  "003842": {
    "context": "003842",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「あの～、ひこりん？」"
  },
  "003853": {
    "context": "003853",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「ん、悠里か」"
  },
  "003864": {
    "context": "003864",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「開いてるよ、どうぞ」"
  },
  "003915": {
    "context": "003915",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「おじゃましま～す……」"
  },
  "003926": {
    "context": "003926",
    "code": "0x45",
    "developer_comment": "",
    "string": "悠里はおずおずと、夏彦の部屋に入ってきた。"
  },
  "003937": {
    "context": "003937",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「どうしたんだい悠里、部屋を訪ねてくるとは珍しいな」"
  },
  "003970": {
    "context": "003970",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「んー、なんかましろんの、大きな声が聞こえたから……」"
  },
  "003981": {
    "context": "003981",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「あ……聞こえてたのか、さっきの話？」"
  },
  "004010": {
    "context": "004010",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「少しだけ」"
  },
  "004015": {
    "context": "004015",
    "code": "0x45",
    "developer_comment": "",
    "string": "どうやら大声で話していたため、リビングまで筒抜けだったらしい。"
  },
  "004020": {
    "context": "004020",
    "code": "0x45",
    "developer_comment": "",
    "string": "バツの悪い思いをする夏彦に、悠里が尋ねてくる。"
  },
  "004049": {
    "context": "004049",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「ましろん怒っていたみたいだけど……なにかあったの？」"
  },
  "004060": {
    "context": "004060",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「いや、ぼくにも何がなんだかわからないんだけどさ……」"
  },
  "004065": {
    "context": "004065",
    "code": "0x45",
    "developer_comment": "",
    "string": "夏彦はかいつまんで、ましろとの会話について話した。"
  }
}