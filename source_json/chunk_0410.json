{
  "000042": {
    "context": "000042",
    "code": "0x55",
    "developer_comment": "",
    "string": "√Double Chapter6"
  },
  "000053": {
    "context": "000053",
    "code": "0x45",
    "developer_comment": "",
    "string": "夏彦は俯き、思考を巡らせた。"
  },
  "000059": {
    "context": "000059",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "（選べるのか、ぼくに……ッ！）"
  },
  "000105": {
    "context": "000105",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "できることならば、渡瀬の想いだって叶えてやりたい。"
  },
  "000110": {
    "context": "000110",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "一度は心を通わせた相棒なのだ。"
  },
  "000116": {
    "context": "000116",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "（……でも、そんなの許されるわけないじゃないか！）"
  },
  "000178": {
    "context": "000178",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "（ぼくを助けるために、悠里は９年の歳月を無駄にしたんだぞ！？）"
  },
  "000184": {
    "context": "000184",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "（その悠里をＱに引き渡すことなんて、絶対に――……）"
  },
  "000242": {
    "context": "000242",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "――そう考えているうち、不意に――"
  },
  "000295": {
    "context": "000295",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "（……――ッ！）"
  },
  "000300": {
    "context": "000300",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "夏彦の頭に、電流のような閃きが走った。"
  },
  "000306": {
    "context": "000306",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "（そ、そうか……この方法が……！）"
  },
  "000311": {
    "context": "000311",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "１つだけあったのだ。"
  },
  "000316": {
    "context": "000316",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "渡瀬の要求を叶え、悠里も救う方法が。"
  },
  "000322": {
    "context": "000322",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "（そうだ、これしかない……！）"
  },
  "000328": {
    "context": "000328",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "（悠里を引き渡せないなら、いっその事！）"
  },
  "000434": {
    "context": "000434",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "――夏彦は静かに覚悟を決めた。"
  },
  "000439": {
    "context": "000439",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "渡瀬を見据えて言う。"
  },
  "000450": {
    "context": "000450",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「なぁ、渡瀬……」"
  },
  "000489": {
    "context": "000489",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「……なんだ」"
  },
  "000528": {
    "context": "000528",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「お前に同行するコミュニケーターは……Ｑに協力するコミュニケーターは――」"
  },
  "000539": {
    "context": "000539",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「――悠里じゃなくても、いいんだよな？」"
  },
  "000619": {
    "context": "000619",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「っ！？」"
  },
  "000624": {
    "context": "000624",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "瞬間、悠里の目が見開かれた。"
  },
  "000629": {
    "context": "000629",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "夏彦の言葉の意味を、理解したのだろう。"
  },
  "000658": {
    "context": "000658",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「ひこりん、まさか……！？」"
  },
  "000669": {
    "context": "000669",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「……そうだよ、悠里」"
  },
  "000680": {
    "context": "000680",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「『ぼくが君の代わりに、渡瀬と一緒に行く』」"
  },
  "000747": {
    "context": "000747",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「なぁ渡瀬、それでいいだろう？」"
  },
  "000752": {
    "context": "000752",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "夏彦が問うと、渡瀬はその言葉を待ちかねていたように――"
  },
  "000791": {
    "context": "000791",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「……それに勝る答えはない」"
  },
  "000796": {
    "context": "000796",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "そう答えた。"
  },
  "000877": {
    "context": "000877",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「……夏彦、苦労をかけるな」"
  },
  "000888": {
    "context": "000888",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「……お前のために、この道を選んだわけじゃないさ」"
  },
  "000899": {
    "context": "000899",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「ただぼくは、大切な人たちのために――」"
  },
  "000983": {
    "context": "000983",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「ダメだよひこりん！　隊長さんと一緒に行っちゃダメ！」"
  },
  "000988": {
    "context": "000988",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "夏彦の言葉を遮るように、悠里が声を上げた。"
  },
  "001022": {
    "context": "001022",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「ひこりんはＱの仲間になりたいわけじゃないでしょ！？　なのにどうして……！」"
  },
  "001033": {
    "context": "001033",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「……ぼくはテロリストにはならないよ」"
  },
  "001044": {
    "context": "001044",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「だけど渡瀬の言う通り、確かにこの街は異常だ」"
  },
  "001055": {
    "context": "001055",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「誰かがそれを正さなきゃならないなら……ぼくもそのために行動したいんだ」"
  },
  "001066": {
    "context": "001066",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「今まで一緒にいてくれた、全ての大切な人たちのためにも……」"
  },
  "001096": {
    "context": "001096",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「だけどこの事件で、きっとＱは今まで以上に、あらゆる機関から睨まれるんだよ！？」"
  },
  "001107": {
    "context": "001107",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「隊長さんについて行ったら、もう普通の生活なんかできなくなる！　みんなとも２度と会えなくなる！」"
  },
  "001137": {
    "context": "001137",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「そんなのいやだよひこりん！　行かないで！」"
  },
  "001142": {
    "context": "001142",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "しかし夏彦は首を振り、彼女の手を握る。"
  },
  "001153": {
    "context": "001153",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「……悠里、そう言ってくれて嬉しいよ」"
  },
  "001164": {
    "context": "001164",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「でもぼくは、君に９年間救われ続けてきた……」"
  },
  "001175": {
    "context": "001175",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「今度はぼくが、君を救う番だ」"
  },
  "001205": {
    "context": "001205",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「じゃ、じゃあ……」"
  },
  "001235": {
    "context": "001235",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「ひこりんが行くんだったら、わたしも行く！」"
  },
  "001246": {
    "context": "001246",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「駄目だよ悠里。それは危険だ」"
  },
  "001257": {
    "context": "001257",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「ましろやサリュや母さんにも、もう会えなくなるんだぞ？」"
  },
  "001268": {
    "context": "001268",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「だから君は連れて行けない。行くのはぼく１人だ」"
  },
  "001298": {
    "context": "001298",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「嫌だよひこりん！　そんなの嫌！」"
  },
  "001303": {
    "context": "001303",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "悠里は大粒の涙を瞳に溜めて叫んだ。"
  },
  "001308": {
    "context": "001308",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "まるで子供がダダをこねるように。"
  },
  "001319": {
    "context": "001319",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「わたしは……今度こそ一緒にいられるって……」"
  },
  "001330": {
    "context": "001330",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「これからはずっと一緒にいたいって……えぐっ……思って……」"
  },
  "001341": {
    "context": "001341",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「うっ……ぐすっ……」"
  },
  "001352": {
    "context": "001352",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「悠里……ごめんな」"
  },
  "001363": {
    "context": "001363",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「だけどぼくは、きっと戻ってくる……それまで少しの間、お別れだ」"
  },
  "001374": {
    "context": "001374",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「さよなら、悠里」"
  },
  "001379": {
    "context": "001379",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "夏彦は彼女に、そう囁いて――"
  },
  "001437": {
    "context": "001437",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "最後のセンシズシンパシーを使った。"
  },
  "001442": {
    "context": "001442",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "悠里の『現在の記憶』を消し、気を失わせるために。"
  },
  "001555": {
    "context": "001555",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "――瞬間、目の前が眩しい光に満ちる。"
  },
  "001560": {
    "context": "001560",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "彼女の心の奥から、温かいものが伝わってきた。"
  },
  "001619": {
    "context": "001619",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "《……ごめんね、最後まで泣き虫で……》"
  },
  "001635": {
    "context": "001635",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "《それがひこりんの選択だったら、わたしに止める事はできないよね……》"
  },
  "001651": {
    "context": "001651",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "《だけどひこりん、ひとつだけお願いだよ。絶対絶対、帰ってきてね！》"
  },
  "001730": {
    "context": "001730",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "《わたし、待ってるから……ましろんとサリュちゃんと博士と、ひこりんの帰りを待ってるから……！》"
  },
  "001735": {
    "context": "001735",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "彼女のテレパシーと共に、夏彦を案じる気持ちが伝わってくる。"
  },
  "001740": {
    "context": "001740",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "その想いに包まれながら、夏彦は――"
  },
  "001811": {
    "context": "001811",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "悠里の記憶を、消し飛ばした。"
  }
}