  "002902": {
    "context": "002902",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「えぇ、まさか堂島が……」"
  },
  "002907": {
    "context": "002907",
    "code": "0x45",
    "developer_comment": "",
