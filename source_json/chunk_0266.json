  "001037": {
    "context": "001037",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "（ッ！？）"
  },
  "001043": {
    "context": "001043",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "（洵さん――どうして頭から血を！？　それになんで、エンジンカッターなんかッ！？）"
  },
  "001099": {
    "context": "001099",
    "code": "0x45",
    "developer_comment": "",
    "string": "――洵から逃れて通路を走りながら、風見が呆然と呟いた。"
  },
  "001186": {
    "context": "001186",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「……どうして、こんな事に……！」"
  },
  "001197": {
    "context": "001197",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「助けるべき人と、仲間に追われて……逃げ場もなくて……」"
  },
  "001202": {
    "context": "001202",
    "code": "0x45",
    "developer_comment": "",
    "string": "それを聞いて、夏彦も思う。"
  },
  "001208": {
    "context": "001208",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "（……おかしい……）"
  },
  "001214": {
    "context": "001214",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "（宇喜多さんもそうだったけど……いくら渡瀬が疑わしいからって、洵さんまであんな風になるなんて……）"
  },
  "001220": {
    "context": "001220",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "（あの２人……あれじゃまるで、あの時の渡瀬と同じじゃないか！）"
  },
  "001226": {
    "context": "001226",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "（この状況は何なんだ！？　こんなめちゃくちゃな状況で、どうやって生き残れって言うんだよ！？）"
  },
  "001231": {
    "context": "001231",
    "code": "0x45",
    "developer_comment": "",
    "string": "だが、そのとき渡瀬が言った。"
  },
  "001264": {
    "context": "001264",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「……諦めるな」"
