  },
  "001845": {
    "context": "001845",
    "code": "0x45",
    "developer_comment": "",
    "string": "だが――"
  },
  "001992": {
    "context": "001992",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「――ぷはぁ！」"
  },
  "002060": {
