{
  "000072": {
    "context": "000072",
    "code": "0x55",
    "developer_comment": "",
    "string": "√Before Chapter2"
  },
  "000080": {
    "context": "000080",
    "code": "0x45",
    "developer_comment": "",
    "string": "――それはもう、１０年も前の話だ。"
  },
  "000085": {
    "context": "000085",
    "code": "0x45",
    "developer_comment": "",
    "string": "その日、夏彦とましろは公園でケンカしていた。"
  },
  "000175": {
    "context": "000175",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「なにさ、夏彦のチョウチンフグ！」"
  },
  "000186": {
    "context": "000186",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「なんだよ、ましろのスベスベマンジュウガニ！」"
  },
  "000191": {
    "context": "000191",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "きっかけは夏彦が好きなヒーローもののテレビ番組を、ましろがつまらないと言ったとか、そんな些細な事だった。"
  },
  "000242": {
    "context": "000242",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "かたわらで悠里が、２人を見ておろおろしている。"
  },
  "000280": {
    "context": "000280",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「夏彦くん、ましろちゃん、ケンカはやめてよぅ」"
  },
  "000315": {
    "context": "000315",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「悠里はだまってろ！」"
  },
  "000359": {
    "context": "000359",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「『だまってろ』って！？」"
  },
  "000388": {
    "context": "000388",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「なにさ夏彦、悠里にむかってえらそうに！　このやばん人！」"
  },
  "000423": {
    "context": "000423",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「なにをー！？」"
  },
  "000446": {
    "context": "000446",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "２人は悠里に構わず、にらみ合う。"
  },
  "000451": {
    "context": "000451",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "この頃には夏彦もましろも、悠里とはすでに仲良くなっていた。"
  },
  "000456": {
    "context": "000456",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "だが夏彦とましろの間は、まだひどく仲が悪かったのだ。"
  },
  "000485": {
    "context": "000485",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「だいたいなに、なんとかマンジュウガニって！　いみわかんないよ！」"
  },
  "000496": {
    "context": "000496",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「おまえのほうがわかんねーよ、ましろのバーカバーカ！」"
  },
  "000525": {
    "context": "000525",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「グルルル！」"
  },
  "000536": {
    "context": "000536",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「ガルルル！」"
  },
  "000541": {
    "context": "000541",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "そんな風に、当時の夏彦とましろは、何かといえばケンカしていた。"
  },
  "000546": {
    "context": "000546",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "誰とでも仲良くできる夏彦だったが、ましろとはなんだかソリが合わなかったのだ。"
  },
  "000575": {
    "context": "000575",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「……フン、つきあってらんない！」"
  },
  "000615": {
    "context": "000615",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "やがてましろが捨てゼリフを残し、どこへともなく歩き去る。"
  },
  "000642": {
    "context": "000642",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「そりゃこっちのセリフだ！」"
  },
  "000647": {
    "context": "000647",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "夏彦もその背に、憎まれ口を返す。"
  },
  "000652": {
    "context": "000652",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "それがいつもの２人の日常だった。"
  },
  "000731": {
    "context": "000731",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "悠里もましろの背を見ながら、困ったように言う。"
  },
  "000742": {
    "context": "000742",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「もう……どうして夏彦くんって、ましろちゃんとはいつもケンカばっかりなの？」"
  },
  "000769": {
    "context": "000769",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「だってあいつ、女なのにナマイキなんだもん」"
  },
  "000798": {
    "context": "000798",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「むかしっからなにかっていうと、ぼくにかみついてきてさー」"
  },
  "000803": {
    "context": "000803",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "夏彦とましろは家が近所のため、物心ついた時には、すでに顔見知りだった。"
  },
  "000808": {
    "context": "000808",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "幼稚園も同じで、ゆえにケンカした回数も、数え切れないほどあった。"
  },
  "000837": {
    "context": "000837",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「でも夏彦くん、『せいぎのヒーロー』にあこがれてるんでしょ？」"
  },
  "000848": {
    "context": "000848",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「だったら女の子とケンカしちゃだめじゃない？」"
  },
  "000859": {
    "context": "000859",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「うっ」"
  },
  "000864": {
    "context": "000864",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "そこを突かれると、夏彦としては痛い。"
  },
  "000869": {
    "context": "000869",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "夏彦は正義感が強く、体の大きないじめっ子などにも果敢に挑む少年だった。"
  },
  "000874": {
    "context": "000874",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "だからテレビや漫画に出てくる正義のヒーローに憧れていたし、悠里の意見ももっともだったのだが――"
  },
  "000885": {
    "context": "000885",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「でもましろは口はわるいし、パンチもいたいし……」"
  },
  "000896": {
    "context": "000896",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「あいつはきっと、悪のてさきなんだ」"
  },
  "000925": {
    "context": "000925",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「そんなことないよ、ましろちゃんはいい子だよ～」"
  },
  "000954": {
    "context": "000954",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「女の子でいちばんさいしょに、わたしのともだちになってくれたしね～」"
  },
  "000959": {
    "context": "000959",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "悠里がほわほわした口調で、ましろを庇う。"
  },
  "001207": {
    "context": "001207",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "２つ年上の悠里は、夏彦にとってはケンカ相手ではない。"
  },
  "001212": {
    "context": "001212",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "彼が女の子に望むものとは、まさに悠里のような優しさとおとなしさである。"
  },
  "001319": {
    "context": "001319",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "しかしましろはその真逆だ。"
  },
  "001324": {
    "context": "001324",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "年も同じで、お互い気が強く、だから何かと衝突したのだろう。"
  },
  "001450": {
    "context": "001450",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「うーん……もっと２人がなかよくなってくれれば、わたしはうれしいんだけどなぁ」"
  },
  "001461": {
    "context": "001461",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「へん。そんな日はきっとこないね」"
  },
  "001466": {
    "context": "001466",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "夏彦はそううそぶいた。"
  }
}