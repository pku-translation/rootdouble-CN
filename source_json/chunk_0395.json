  },
  "000483": {
    "context": "000483",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "（そう……僕は、彼女を救う事ができなかったのだ……）"
  },
  "000489": {
    "context": "000489",
    "code": "0x45",
    "developer_comment": "宇喜多",
