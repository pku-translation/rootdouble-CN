{
  "000102": {
    "context": "000102",
    "code": "0x55",
    "string": "√Current Chapter2"
  },
  "000114": {
    "context": "000114",
    "code": "0x45",
    "developer_comment": "",
    "string": "――"
  },
  "000119": {
    "context": "000119",
    "code": "0x45",
    "developer_comment": "",
    "string": "――――"
  },
  "000124": {
    "context": "000124",
    "code": "0x45",
    "developer_comment": "",
    "string": "――――――５回目の覚醒は、ゆるやかに始まった。"
  },
  "000288": {
    "context": "000288",
    "code": "0x45",
    "developer_comment": "？？",
    "string": "「――……テロリストが危険――……」"
  },
  "000299": {
    "context": "000299",
    "code": "0x45",
    "developer_comment": "？？",
    "string": "「――……９人の中に、テロリストが――……」"
  },
  "000310": {
    "context": "000310",
    "code": "0x45",
    "developer_comment": "？？",
    "string": "「――……そんな事、誰も言ってないでしょう！」"
  },
  "000330": {
    "context": "000330",
    "code": "0x45",
    "developer_comment": "？？",
    "string": "「ではこの６人の中にテロリストがいると？　それはますますありえません」"
  },
  "000417": {
    "context": "000417",
    "code": "0x45",
    "developer_comment": "",
    "string": "男の周囲で複数の人間が、口々に何かを言い争っている。"
  },
  "000423": {
    "context": "000423",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "（……今度はどういう状況だ……？）"
  },
  "000429": {
    "context": "000429",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "（テロリストがどうのって……まさか男の正体がばれたのか！？）"
  },
  "000435": {
    "context": "000435",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "（でも、ちゃんと記憶を消したはずなのに――！？）"
  },
  "000456": {
    "context": "000456",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「……わかった！　ちょっと落ち着こうみんな！」"
  },
  "000539": {
    "context": "000539",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "「なによ隊長さん！　あなただけ、何も意見を言ってないじゃない！」"
  },
  "000544": {
    "context": "000544",
    "code": "0x45",
    "developer_comment": "",
    "string": "戻ってきた夏彦の視覚に、まず恵那の顔が映った。"
  },
  "000598": {
    "context": "000598",
    "code": "0x45",
    "developer_comment": "",
    "string": "その周囲に、洵と副隊長と、あの少女と――"
  },
  "000697": {
    "context": "000697",
    "code": "0x45",
    "developer_comment": "",
    "string": "そして宇喜多がいる。"
  },
  "000703": {
    "context": "000703",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "（ッ！　なんで、おじさんまでこのラボに！？）"
  },
  "000709": {
    "context": "000709",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "（だっておじさんは、あの授業の時――）"
  },
  "000840": {
    "context": "000840",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "「万が一の事態が発生した際には、自動的に放射能漏洩防止システムが作動」"
  },
  "000909": {
    "context": "000909",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "「無数に設置された『隔壁』や『ゲート』が閉鎖され、放射性物質の拡散を完全にシャットアウトします」"
  },
  "001001": {
    "context": "001001",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "（メルトダウンが起きたら地下に閉じ込められるって事は、おじさんも知ってたはずなのに……！）"
  },
  "001007": {
    "context": "001007",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "（そのおじさんが、どうして逃げずに残ってるんだ？）"
  },
  "001013": {
    "context": "001013",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "（やっぱり、おじさんは内通者だったのか……！？）"
  },
  "001018": {
    "context": "001018",
    "code": "0x45",
    "developer_comment": "",
    "string": "一瞬、宇喜多への疑惑が深くなったが――"
  },
  "001024": {
    "context": "001024",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "（……いや、待てよ？　よく考えたら、それって変じゃないか？）"
  },
  "001030": {
    "context": "001030",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "（おじさんがテロリストの仲間なら、メルトダウンを起こした時点で、さっさと逃げてるはずだ……）"
  },
  "001036": {
    "context": "001036",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "（……でもどのみち、おじさんが地下に残ってる理由がわからないな）"
  },
  "001136": {
    "context": "001136",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "（それに先生がいるのだっておかしい……高校教師が、なんでこんなところにいるんだ？）"
  },
  "001141": {
    "context": "001141",
    "code": "0x45",
    "developer_comment": "",
    "string": "疑問はそれだけにとどまらない。"
  },
  "001195": {
    "context": "001195",
    "code": "0x45",
    "developer_comment": "",
    "string": "悠里そっくりの少女が何者で、なぜここにいるのかも、夏彦には謎のままなのだ。"
  },
  "001201": {
    "context": "001201",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "（ダメだ！　わからないことが多すぎる！）"
  },
  "001207": {
    "context": "001207",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "（仕方ない、こうなったら――！）"
  },
  "001212": {
    "context": "001212",
    "code": "0x45",
    "developer_comment": "",
    "string": "夏彦はましろの記憶に潜った時の事を思い出しながら、"
  },
  "001217": {
    "context": "001217",
    "code": "0x45",
    "developer_comment": "",
    "string": "『知りたい！』"
  },
  "001222": {
    "context": "001222",
    "code": "0x45",
    "developer_comment": "",
    "string": "全身全霊を集中させ、強くそう願った。"
  },
  "001606": {
    "context": "001606",
    "code": "0x45",
    "developer_comment": "",
    "string": "――その瞬間、男が目覚めてからの記憶が、次々と眼前に浮かんだ。"
  },
  "001611": {
    "context": "001611",
    "code": "0x45",
    "developer_comment": "",
    "string": "それは絶望的な状況下での、壮絶な戦いの記憶だった。"
  },
  "001952": {
    "context": "001952",
    "code": "0x45",
    "developer_comment": "",
    "string": "ケースＮの発令にともなう、隔壁の完全閉鎖。"
  },
  "001957": {
    "context": "001957",
    "code": "0x45",
    "developer_comment": "",
    "string": "取り残されていた恵那や、宇喜多との遭遇。"
  },
  "002204": {
    "context": "002204",
    "code": "0x45",
    "developer_comment": "",
    "string": "迫りくる炎。広がり続ける放射線汚染。不足するＡＤ。難航する要救助者の捜索。"
  },
  "002497": {
    "context": "002497",
    "code": "0x45",
    "developer_comment": "",
    "string": "そして脱出不可能なラボ内で起きていた、凄惨な殺人事件――！"
  },
  "002790": {
    "context": "002790",
    "code": "0x45",
    "developer_comment": "",
    "string": "無数の苦難を乗り越えるため、男は自ら危険に身を投じ、傷つき、それでも前に進み続けた。"
  },
  "002945": {
    "context": "002945",
    "code": "0x45",
    "developer_comment": "",
    "string": "やがてたどりついた脱出口。第６エリアの、資材搬出用リフト。"
  },
  "003101": {
    "context": "003101",
    "code": "0x45",
    "developer_comment": "",
    "string": "そこで彼らを待ち受けていたのは、夏彦が破壊したコントロールパネルと――"
  },
  "003256": {
    "context": "003256",
    "code": "0x45",
    "developer_comment": "",
    "string": "――見知らぬ少女の死体だった。"
  },
  "003405": {
    "context": "003405",
    "code": "0x45",
    "developer_comment": "",
    "string": "――――――"
  },
  "003410": {
    "context": "003410",
    "code": "0x45",
    "developer_comment": "",
    "string": "――……"
  },
  "003415": {
    "context": "003415",
    "code": "0x45",
    "developer_comment": "",
    "string": "……男の記憶に潜っていた夏彦の感覚が、再び現実とリンクする。"
  },
  "003420": {
    "context": "003420",
    "code": "0x45",
    "developer_comment": "",
    "string": "気づけば皆が、男の顔を見つめていた。"
  },
  "003437": {
    "context": "003437",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "「な、なによ急に黙って」"
  },
  "003494": {
    "context": "003494",
    "code": "0x85",
    "string": "《……なんだ、今のは……！？》"
  },
  "003495": {
    "context": "003495",
    "code": "0x86",
    "string": "渡瀬"
  },
  "003496": {
    "context": "003496",
    "code": "0x86",
    "string": "《……なんだ、今のは……！？》"
  },
  "003524": {
    "context": "003524",
    "code": "0x45",
    "developer_comment": "",
    "string": "男自身も、強い戸惑いを感じているようだった。"
  },
  "003646": {
    "context": "003646",
    "code": "0x45",
    "developer_comment": "",
    "string": "その様子に、夏彦ははっとした。"
  },
  "003652": {
    "context": "003652",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "（もしかして……こいつもぼくと一緒に、記憶を思い出していたのか？）"
  },
  "003657": {
    "context": "003657",
    "code": "0x45",
    "developer_comment": "",
    "string": "だとすると今の数秒間で、男も約３時間の記憶を追体験したことになる。"
  },
  "003662": {
    "context": "003662",
    "code": "0x45",
    "developer_comment": "",
    "string": "テロリストなどではなく、『笠鷺渡瀬』という名の、レスキュー隊員としての記憶を。"
  },
  "003667": {
    "context": "003667",
    "code": "0x45",
    "developer_comment": "",
    "string": "現在の『渡瀬』は、あの殺人鬼のような男とは、まるで別人だった。"
  },
  "003673": {
    "context": "003673",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "（人は記憶を失っただけで、こうも変わるものなのか？）"
  },
  "003679": {
    "context": "003679",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "（記憶を失う前のあの男が本物だとしたら、今の『渡瀬』の人格はいったいどこから……？）"
  },
  "003684": {
    "context": "003684",
    "code": "0x45",
    "developer_comment": "",
    "string": "だが夏彦には、他にも考えなければならないことがあった。"
  },
  "003690": {
    "context": "003690",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "（それにしても――殺人事件なんて、このラボで何が起こってるんだ！？）"
  },
  "003696": {
    "context": "003696",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "（死んでいた２人のレスキュー隊員は、たぶん、あの時のテロリストだと思うけど――）"
  },
  "003702": {
    "context": "003702",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "（ラボの研究員やあんな少女まで殺すなんて、誰が何の目的で……？）"
  },
  "003708": {
    "context": "003708",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "（もし全ての事件が、隔壁が降りた後に起こったんだとしたら、もしかして犯人はこの中に……？）"
  },
  "003713": {
    "context": "003713",
    "code": "0x45",
    "developer_comment": "",
    "string": "その疑問に呼応するかのように、渡瀬が皆を見回した。"
  },
  "003743": {
    "context": "003743",
    "code": "0x45",
    "developer_comment": "",
    "string": "――まず、宇喜多。"
  },
  "003749": {
    "context": "003749",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "（おじさんは内通者なのか？　それとも単に巻き込まれただけなのか……？）"
  },
  "003755": {
    "context": "003755",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "（もしおじさんが内通者で、何かの理由があってテロリストたちを裏切ったんだとしたら――）"
  },
  "003761": {
    "context": "003761",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "（少なくともテロリストの２人を殺した可能性が出てくるぞ……！）"
  },
  "003799": {
    "context": "003799",
    "code": "0x45",
    "developer_comment": "",
    "string": "――次に恵那。"
  },
  "003805": {
    "context": "003805",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "（先生が人殺しだなんて考えたくないけど……でも本当に、どうしてこんな所にいるんだろう？）"
  },
  "003811": {
    "context": "003811",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "（ぼくらが助けを求めに行ったあの時は、何も協力してくれなかったのに……）"
  },
  "003817": {
    "context": "003817",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "（みんなには『ここの職員に呼ばれて来た』って説明してたみたいだけど、それもよくわからないし……）"
  },
  "003879": {
    "context": "003879",
    "code": "0x45",
    "developer_comment": "",
    "string": "――さらにレスキュー隊員の２人。"
  },
  "003885": {
    "context": "003885",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "（洵さんと副隊長さんは、こいつと同じレスキュー隊員だけど……）"
  },
  "003891": {
    "context": "003891",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "（今までの行動を見る限り、信用してもよさそうだよな。特に洵さんなんて、ぜったい人殺しには見えないし）"
  },
  "003995": {
    "context": "003995",
    "code": "0x45",
    "developer_comment": "",
    "string": "そして渡瀬が最後に、『ユウリ』と名乗るあの少女を見ようとした時――"
  },
  "004002": {
    "context": "004002",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "（っ！？）"
  },
  "004019": {
    "context": "004019",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「――ユウリが、いない！」"
  },
  "004093": {
    "context": "004093",
    "code": "0x45",
    "developer_comment": "洵",
    "string": "「ほ、ホントだ！　どうして！？」"
  },
  "004136": {
    "context": "004136",
    "code": "0x45",
    "developer_comment": "",
    "string": "渡瀬が慌てて周囲を見回すと、彼女の背中がちらりと見えた。"
  },
  "004234": {
    "context": "004234",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「いた！　第５エリアに行った！」"
  },
  "004428": {
    "context": "004428",
    "code": "0x45",
    "developer_comment": "",
    "string": "――第５エリアに逃げて行ったユウリを、渡瀬たちは散り散りになって捜索した。"
  },
  "004485": {
    "context": "004485",
    "code": "0x85",
    "string": "《ユウリは内周棟が汚染区域だと言う事を知らない！》"
  },
  "004486": {
    "context": "004486",
    "code": "0x86",
    "string": "渡瀬"
  },
  "004487": {
    "context": "004487",
    "code": "0x86",
    "string": "《ユウリは内周棟が汚染区域だと言う事を知らない！》"
  },
  "004502": {
    "context": "004502",
    "code": "0x85",
    "string": "《もし、そこに逃げていたとしたら――！》"
  },
  "004503": {
    "context": "004503",
    "code": "0x86",
    "string": "渡瀬"
  },
  "004504": {
    "context": "004504",
    "code": "0x86",
    "string": "《もし、そこに逃げていたとしたら――！》"
  },
  "004532": {
    "context": "004532",
    "code": "0x45",
    "developer_comment": "",
    "string": "そこには『人を救いたい』という、当たり前の正義があった。"
  },
  "004543": {
    "context": "004543",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "《行け！　彼女を助けろ！》"
  },
  "004548": {
    "context": "004548",
    "code": "0x45",
    "developer_comment": "",
    "string": "夏彦は彼の意思に同調し、共に汚染区域へと駆け込んだ。"
  }
}