{
  "000074": {
    "context": "000074",
    "code": "0x55",
    "string": "√Before Chapter6"
  },
  "000090": {
    "context": "000090",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「サリュもさぞ腹が減ってるだろ」"
  },
  "000134": {
    "context": "000134",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「だね。サリュはずーっと頑張りっぱなしだったからね」"
  },
  "000145": {
    "context": "000145",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「ありがとうサリュ、お礼になでなでしてあげる」"
  },
  "000231": {
    "context": "000231",
    "code": "0x45",
    "developer_comment": "",
    "string": "そう言ってましろがサリュの頭を撫でる。"
  },
  "000236": {
    "context": "000236",
    "code": "0x45",
    "developer_comment": "",
    "string": "くすぐったそうにしているが、おとなしく撫でられている以上、悪い気はしてないようだ。"
  },
  "000247": {
    "context": "000247",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「……２人を護るためにした事。褒められるほどの事ではない」"
  },
  "000312": {
    "context": "000312",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「またまた～。ツンデレちゃって、このカワイコちゃんめ～」"
  },
  "000341": {
    "context": "000341",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「照れているわけではない。というより、むしろ……」"
  },
  "000352": {
    "context": "000352",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「むしろ？」"
  },
  "000376": {
    "context": "000376",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「……」"
  },
  "000405": {
    "context": "000405",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「こんな事を言ったら変に思われるかもしれないけど――」"
  },
  "000434": {
    "context": "000434",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「今日はちょっと、楽しかった」"
  },
  "000463": {
    "context": "000463",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「へっ？　楽しかったって？」"
  },
  "000468": {
    "context": "000468",
    "code": "0x45",
    "developer_comment": "",
    "string": "唐突な言葉に、夏彦たちはきょとんとする。"
  },
  "000473": {
    "context": "000473",
    "code": "0x45",
    "developer_comment": "",
    "string": "サリュはうなずいて答えた。"
  },
  "000502": {
    "context": "000502",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「２日前、バスに乗ったときの事を覚えてる？」"
  },
  "000513": {
    "context": "000513",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「あの時、街を回る約束をした事を」"
  },
  "000524": {
    "context": "000524",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「あ、あぁ」"
  },
  "000553": {
    "context": "000553",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「それは思いもしない形で叶えられた」"
  },
  "000564": {
    "context": "000564",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「追いかけたり、追いかけられたり、街の中を冒険することもできた」"
  },
  "000593": {
    "context": "000593",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「まるで夏彦たちの想い出の中に、入り込んだみたいだった」"
  },
  "000604": {
    "context": "000604",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「想い出？」"
  },
  "000633": {
    "context": "000633",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「そう、想い出」"
  },
  "000638": {
    "context": "000638",
    "code": "0x45",
    "developer_comment": "",
    "string": "サリュは微笑んで、夏彦たちを見据えた。"
  },
  "000649": {
    "context": "000649",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「……私には想い出が、何もなかった。故郷には何もなかったから」"
  },
  "000678": {
    "context": "000678",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「暗い灰色に塗られたような、本当に味気ない日々……」"
  },
  "000707": {
    "context": "000707",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「だけど今日の事は、きっといい想い出になる」"
  },
  "000718": {
    "context": "000718",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「この街に来てよかった」"
  },
  "000755": {
    "context": "000755",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「……っ」"
  },
  "000760": {
    "context": "000760",
    "code": "0x45",
    "developer_comment": "",
    "string": "夏彦は思わず、サリュから目を逸らす。"
  },
  "000765": {
    "context": "000765",
    "code": "0x45",
    "developer_comment": "",
    "string": "サリュのいじらしさに、不意を打たれたのだ。"
  },
  "000771": {
    "context": "000771",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "（素直すぎる……なんだこの子、なんかずるいぞ！）"
  },
  "000817": {
    "context": "000817",
    "code": "0x45",
    "developer_comment": "",
    "string": "ましろに目をやると、彼女は――"
  },
  "000822": {
    "context": "000822",
    "code": "0x45",
    "developer_comment": "",
    "string": "目頭を押さえ、震えていた。"
  },
  "000868": {
    "context": "000868",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「サリュ、あんたって子は……！」"
  },
  "000879": {
    "context": "000879",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「私を萌えさせるために存在してるの！？」"
  },
  "000890": {
    "context": "000890",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「お前、今どき『萌え』って……死語だぞ」"
  },
  "000919": {
    "context": "000919",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「燃えさせる？」"
  },
  "000948": {
    "context": "000948",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「たぶん字ぃ違ってるけど、それでも可っ」"
  },
  "000977": {
    "context": "000977",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「……意味がつかめない」"
  },
  "000988": {
    "context": "000988",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「気にしなくていいよ。要するに可愛いって言ってるんだ」"
  },
  "001017": {
    "context": "001017",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「そ、そう」"
  },
  "001022": {
    "context": "001022",
    "code": "0x45",
    "developer_comment": "",
    "string": "サリュは戸惑うように、視線を泳がせる。"
  },
  "001051": {
    "context": "001051",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「ともかく……私の言いたい事は、今日は楽しかったという事」"
  },
  "001062": {
    "context": "001062",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「買い物はできなかったけど、それはまた今度にしたい」"
  },
  "001091": {
    "context": "001091",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「３人で行きたい……いい？」"
  },
  "001120": {
    "context": "001120",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「もちろん！　今度の日曜日は、絶対買い物に行こうね！」"
  },
  "001131": {
    "context": "001131",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「ぼくも賛成だよ」"
  },
  "001160": {
    "context": "001160",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「よかった。２人がそう言ってくれて」"
  },
  "001171": {
    "context": "001171",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「本当は、新しい服も欲しい」"
  },
  "001200": {
    "context": "001200",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「ましろが選んでくれた服、実はとても気に入っていた。また選んでもらいたい」"
  },
  "001265": {
    "context": "001265",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「選ぶ選ぶ！　も～いっくらでも選んであげちゃう！」"
  },
  "001276": {
    "context": "001276",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「他には欲しいものはない！？　ない！？」"
  },
  "001305": {
    "context": "001305",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「欲しい物……ぬいぐるみ」"
  },
  "001316": {
    "context": "001316",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「ん？　アリスじゃだめなのか？」"
  },
  "001345": {
    "context": "001345",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「だめじゃない。ただいつも持ち歩くアリスの他に、部屋に置くぬいぐるみが欲しい」"
  },
  "001374": {
    "context": "001374",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「アリスはましろに名づけてもらったから、今度は夏彦に名づけてもらいたい」"
  },
  "001385": {
    "context": "001385",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「そ、そうか……！」"
  },
  "001396": {
    "context": "001396",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「う～ん、ネーミングセンスには自信ないんだけど……」"
  },
  "001425": {
    "context": "001425",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「夏彦、まさかここで断るようなヘタレじゃないよね？」"
  },
  "001436": {
    "context": "001436",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「と、当然さ。ぼくでよければつけてあげるよ」"
  },
  "001465": {
    "context": "001465",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「そう。だったら嬉しい。きっとそのぬいぐるみも私の宝物になる」"
  },
  "001530": {
    "context": "001530",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「う～！　もう本だろうがぬいぐるみだろうが地球破壊爆弾だろうが、私がなんでも買ってあげようじゃない！」"
  },
  "001559": {
    "context": "001559",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「まとめて面倒みてあげるよ！　お姉さんにまかせとけ！」"
  },
  "001564": {
    "context": "001564",
    "code": "0x45",
    "developer_comment": "",
    "string": "そう言って、ましろが自分の胸をドンと叩く。"
  },
  "001569": {
    "context": "001569",
    "code": "0x45",
    "developer_comment": "",
    "string": "二人のやり取りを見ながら、夏彦は一瞬、未来を夢想した。"
  },
  "001574": {
    "context": "001574",
    "code": "0x45",
    "developer_comment": "",
    "string": "――気になるぬいぐるみを見て回るサリュ。"
  },
  "001579": {
    "context": "001579",
    "code": "0x45",
    "developer_comment": "",
    "string": "そんなサリュにオススメを紹介しながら、ちゃっかり自分も楽しむましろ。"
  },
  "001584": {
    "context": "001584",
    "code": "0x45",
    "developer_comment": "",
    "string": "それを遠巻きに眺める夏彦。"
  },
  "001590": {
    "context": "001590",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "（きっと楽しい一日になるだろうな……）"
  },
  "001595": {
    "context": "001595",
    "code": "0x45",
    "developer_comment": "",
    "string": "夏彦はそう思い、密かに微笑む。"
  },
  "001601": {
    "context": "001601",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "（それくらいのご褒美は、与えてしかるべきだ）"
  },
  "001607": {
    "context": "001607",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "（それだけの事をやったんだから……）"
  },
  "001613": {
    "context": "001613",
    "code": "0x45",
    "developer_comment": "",
    "string": "それは確かだが――一方で悔やまれる事もある。"
  },
  "001630": {
    "context": "001630",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「……でも結局、奴らの狙いだった超心研の爆破を阻止する事はできなかったな……」"
  },
  "001683": {
    "context": "001683",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「うん……死傷者が出なかったってのは、救いだったけど」"
  }
}
