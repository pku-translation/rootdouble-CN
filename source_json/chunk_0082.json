  "001155": {
    "context": "001155",
    "code": "0x45",
    "developer_comment": "洵",
    "string": "「回復したら、脳科学実験室に戻るって言ってたよ」"
  },
  "001166": {
    "context": "001166",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「ならちょうどいいな。第６エリアなら、脳科学実験室のすぐ近くだし」"
  },
  "001195": {
    "context": "001195",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "「では捜索を続けるとするかね」"
