{
  "000048": {
    "context": "000048",
    "code": "0x55",
    "string": "√Double Chapter6"
  },
  "000059": {
    "context": "000059",
    "code": "0x45",
    "developer_comment": "",
    "string": "夏彦は俯き、一瞬で思考を巡らせた。"
  },
  "000065": {
    "context": "000065",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "（選べるのか、ぼくに……ッ！）"
  },
  "000160": {
    "context": "000160",
    "code": "0x45",
    "developer_comment": "",
    "string": "できることならば、渡瀬の想いだって叶えてやりたい。"
  },
  "000165": {
    "context": "000165",
    "code": "0x45",
    "developer_comment": "",
    "string": "一度は心を通わせた相棒なのだ。"
  },
  "000171": {
    "context": "000171",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "（……でも、そんなの許されるわけないじゃないか！）"
  },
  "000226": {
    "context": "000226",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "（ぼくを助けるために、悠里は９年の歳月を無駄にしたんだぞ！？）"
  },
  "000232": {
    "context": "000232",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "（その悠里をＱに引き渡すことなんて、絶対に――……）"
  },
  "000237": {
    "context": "000237",
    "code": "0x45",
    "developer_comment": "",
    "string": "――そう考えているうち、不意に――"
  },
  "000295": {
    "context": "000295",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "（……――ッ！）"
  },
  "000300": {
    "context": "000300",
    "code": "0x45",
    "developer_comment": "",
    "string": "夏彦の頭に、電流のような閃きが走った。"
  },
  "000306": {
    "context": "000306",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "（そ、そうか……この方法が……！）"
  },
  "000311": {
    "context": "000311",
    "code": "0x45",
    "developer_comment": "",
    "string": "１つだけあったのだ。"
  },
  "000316": {
    "context": "000316",
    "code": "0x45",
    "developer_comment": "",
    "string": "渡瀬の要求を叶え、悠里も救う方法が。"
  },
  "000322": {
    "context": "000322",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "（そうだ、これしかない……！）"
  },
  "000328": {
    "context": "000328",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "（悠里を引き渡せないなら、いっその事！）"
  },
  "000378": {
    "context": "000378",
    "code": "0x45",
    "developer_comment": "",
    "string": "――夏彦は静かに覚悟を決めた。"
  },
  "000383": {
    "context": "000383",
    "code": "0x45",
    "developer_comment": "",
    "string": "渡瀬を見据えて言う。"
  },
  "000420": {
    "context": "000420",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「なぁ、渡瀬……」"
  },
  "000458": {
    "context": "000458",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「……なんだ」"
  },
  "000496": {
    "context": "000496",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「お前に同行するコミュニケーターは……Ｑに協力するコミュニケーターは――」"
  },
  "000507": {
    "context": "000507",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「――悠里じゃなくても、いいんだよな？」"
  },
  "000545": {
    "context": "000545",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「っ！？」"
  },
  "000550": {
    "context": "000550",
    "code": "0x45",
    "developer_comment": "",
    "string": "瞬間、悠里の目が見開かれた。"
  },
  "000555": {
    "context": "000555",
    "code": "0x45",
    "developer_comment": "",
    "string": "夏彦の言葉の意味を、理解したのだろう。"
  },
  "000566": {
    "context": "000566",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「ひこりん、まさか……！？」"
  },
  "000604": {
    "context": "000604",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「……そうだよ、悠里」"
  },
  "000615": {
    "context": "000615",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「『ぼくが君の代わりに、渡瀬と一緒に行く』」"
  },
  "000626": {
    "context": "000626",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「なぁ渡瀬、それでいいだろう？」"
  },
  "000631": {
    "context": "000631",
    "code": "0x45",
    "developer_comment": "",
    "string": "夏彦が問うと、渡瀬はその言葉を待ちかねていたように――"
  },
  "000669": {
    "context": "000669",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「……それに勝る答えはない」"
  },
  "000674": {
    "context": "000674",
    "code": "0x45",
    "developer_comment": "",
    "string": "そう答えた。"
  },
  "000685": {
    "context": "000685",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「……夏彦、苦労をかけるな」"
  },
  "000723": {
    "context": "000723",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「……うるさい。お前のためじゃない」"
  },
  "000734": {
    "context": "000734",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「ただぼくは、悠里のために――」"
  },
  "000806": {
    "context": "000806",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「ダメだよひこりん！　隊長さんと一緒に行っちゃダメ！」"
  },
  "000811": {
    "context": "000811",
    "code": "0x45",
    "developer_comment": "",
    "string": "夏彦の言葉を遮るように、悠里が声を上げた。"
  },
  "000841": {
    "context": "000841",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「ひこりんはＱの仲間になりたいわけじゃないでしょ！？」"
  },
  "000852": {
    "context": "000852",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「この事件でＱは、今まで以上にあらゆる機関から睨まれる！」"
  },
  "000863": {
    "context": "000863",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「隊長さんについて行ったら、もう普通の生活なんかできないんだよ！？　みんなとも２度と会えなくなる！」"
  },
  "000893": {
    "context": "000893",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「そんなのいやだよひこりん！　行かないで！」"
  },
  "000898": {
    "context": "000898",
    "code": "0x45",
    "developer_comment": "",
    "string": "しかし夏彦は首を振り、彼女の手を握る。"
  },
  "000909": {
    "context": "000909",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「……悠里、そう言ってくれて嬉しいよ」"
  },
  "000920": {
    "context": "000920",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「でもぼくは、君に９年間救われ続けてきた……」"
  },
  "000931": {
    "context": "000931",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「今度はぼくが、君を救う番だ」"
  },
  "000961": {
    "context": "000961",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「じゃ、じゃあ……」"
  },
  "000991": {
    "context": "000991",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「ひこりんが行くんだったら、わたしも行く！」"
  },
  "001002": {
    "context": "001002",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「駄目だよ悠里。それは危険だ」"
  },
  "001013": {
    "context": "001013",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「ましろやサリュや母さんにも、もう会えなくなるんだぞ？」"
  },
  "001024": {
    "context": "001024",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「だから君は連れて行けない。行くのはぼく１人だ」"
  },
  "001054": {
    "context": "001054",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「嫌だよひこりん！　そんなの嫌！」"
  },
  "001059": {
    "context": "001059",
    "code": "0x45",
    "developer_comment": "",
    "string": "悠里は大粒の涙を瞳に溜めて叫んだ。"
  },
  "001064": {
    "context": "001064",
    "code": "0x45",
    "developer_comment": "",
    "string": "まるで子供がダダをこねるように。"
  },
  "001069": {
    "context": "001069",
    "code": "0x45",
    "developer_comment": "",
    "string": "その声に、彼女の記憶の中で見た情景が蘇る。"
  },
  "001123": {
    "context": "001123",
    "code": "0x45",
    "developer_comment": "",
    "string": "――それは遠い日の出来事。"
  },
  "001128": {
    "context": "001128",
    "code": "0x45",
    "developer_comment": "",
    "string": "ラボの実験室で、彼女は夏彦の記憶を操作した。"
  },
  "001133": {
    "context": "001133",
    "code": "0x45",
    "developer_comment": "",
    "string": "自失状態に陥った夏彦を救うため……。"
  },
  "001139": {
    "context": "001139",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "（……あの時の恩返しをするんだ）"
  },
  "001203": {
    "context": "001203",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「わたしは……今度こそ一緒にいられるって……」"
  },
  "001214": {
    "context": "001214",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「これからはずっと一緒にいたいって……えぐっ……思って……」"
  },
  "001244": {
    "context": "001244",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「うっ……ぐすっ……」"
  },
  "001255": {
    "context": "001255",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「悠里……」"
  },
  "001260": {
    "context": "001260",
    "code": "0x45",
    "developer_comment": "",
    "string": "夏彦は、悠里の肩に手をかけ――優しく微笑んだ。"
  },
  "001271": {
    "context": "001271",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「……大丈夫。悲しむことはないよ」"
  },
  "001301": {
    "context": "001301",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「……え？」"
  },
  "001312": {
    "context": "001312",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「ぼくは君と、ずっと一緒だ。いつも君のそばにいる」"
  },
  "001323": {
    "context": "001323",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「君の中に、ぼくは居続けるから」"
  },
  "001353": {
    "context": "001353",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「い……居続けるって……」"
  },
  "001358": {
    "context": "001358",
    "code": "0x45",
    "developer_comment": "",
    "string": "悠里の声が、不意に強張る。"
  },
  "001363": {
    "context": "001363",
    "code": "0x45",
    "developer_comment": "",
    "string": "夏彦がやろうとしている事を悟ったのだろう。"
  },
  "001393": {
    "context": "001393",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「ひこりん、まさか……！？」"
  },
  "001404": {
    "context": "001404",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「今までありがとう、悠里」"
  },
  "001409": {
    "context": "001409",
    "code": "0x45",
    "developer_comment": "",
    "string": "夏彦は彼女に、そう囁いて――"
  },
  "001467": {
    "context": "001467",
    "code": "0x45",
    "developer_comment": "",
    "string": "最後のセンシズシンパシーを使った。"
  },
  "001580": {
    "context": "001580",
    "code": "0x45",
    "developer_comment": "",
    "string": "――瞬間、目の前が眩しい光に満ちる。"
  },
  "001585": {
    "context": "001585",
    "code": "0x45",
    "developer_comment": "",
    "string": "彼女の心の奥から、温かいものが伝わってきた。"
  },
  "001644": {
    "context": "001644",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "《ひこりん！　わたし、忘れないよ！》"
  },
  "001652": {
    "context": "001652",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "《ひこりんがわたしを救ってくれた事、絶対忘れない！》"
  },
  "001660": {
    "context": "001660",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "《だから、わたし――》"
  },
  "001673": {
    "context": "001673",
    "code": "0x45",
    "developer_comment": "",
    "string": "そう思う悠里の心が、静かに崩れていく。"
  },
  "001686": {
    "context": "001686",
    "code": "0x45",
    "developer_comment": "",
    "string": "消えゆく記憶の断片から、１つの言葉が浮かんだ。"
  },
  "001765": {
    "context": "001765",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "《必ずひこりんに、会いに行くから……！》"
  },
  "001834": {
    "context": "001834",
    "code": "0x45",
    "developer_comment": "",
    "string": "だがその言葉も、光の中に消えていき――……"
  }
}