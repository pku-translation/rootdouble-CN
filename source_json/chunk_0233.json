  "000772": {
    "context": "000772",
    "code": "0x45",
    "developer_comment": "",
    "string": "ましろは夏彦に詰め寄ってわめいた。"
  },
  "000783": {
    "context": "000783",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「そんなわけないじゃない！　サリュはいつだって、私たちのために頑張ってくれたでしょ！」"
