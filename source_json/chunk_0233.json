  },
  "000304": {
    "context": "000304",
    "code": "0x45",
    "developer_comment": "",
    "string": "だがやるべき事はわかる。"
  },
  "000389": {
    "context": "000389",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「ゆ、悠里、ましろ……逃げよう！」"
