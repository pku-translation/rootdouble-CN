  "000794": {
    "context": "000794",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「２度目に超心研に行った時も！　廃工場に追い詰められた時も！」"
  },
  "000805": {
    "context": "000805",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「私たちのために戦ってくれたサリュを、どうして疑うのよ！」"
