  "000665": {
    "context": "000665",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「それもあの子を置いて――」"
  },
  "000676": {
    "context": "000676",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「だって仕方がないだろう！」"
