  "000716": {
    "context": "000716",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「なっ……それ、どういう意味！？」"
  },
  "000727": {
    "context": "000727",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「サリュはぼくらの、味方のフリをしてたって事だろ！？」"
