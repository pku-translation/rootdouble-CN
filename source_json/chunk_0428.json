{
  "000053": {
    "context": "000053",
    "code": "0x45",
    "developer_comment": "",
    "string": "そして、運命の日がやってきた。"
  },
  "000105": {
    "context": "000105",
    "code": "0x45",
    "developer_comment": "",
    "string": "９月１６日、５時３０分――"
  },
  "000110": {
    "context": "000110",
    "code": "0x45",
    "developer_comment": "",
    "string": "通路に人気がないのを見計らって、宇喜多は行動を開始した。"
  },
  "000195": {
    "context": "000195",
    "code": "0x45",
    "developer_comment": "",
    "string": "扉を開けると、監視ルームに待機していた警備員たちは――"
  },
  "000203": {
    "context": "000203",
    "code": "0x45",
    "developer_comment": "警備員１",
    "string": "「んがー、んごー、んがー、んごー」"
  },
  "000211": {
    "context": "000211",
    "code": "0x45",
    "developer_comment": "警備員２",
    "string": "「がーぷひゅるるる、がーぷしゅるるる」"
  },
  "000216": {
    "context": "000216",
    "code": "0x45",
    "developer_comment": "警備員２",
    "string": "――全員が深い眠りに落ちていた。"
  },
  "000221": {
    "context": "000221",
    "code": "0x45",
    "developer_comment": "警備員２",
    "string": "３０分前に差し入れたコーヒーに、強力な睡眠薬を混ぜておいたのだ。"
  },
  "000227": {
    "context": "000227",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "（どうやら、チェスで負け続けた甲斐はあったようだな……）"
  },
  "000232": {
    "context": "000232",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "宇喜多はそうほくそ笑むと、消火システムを停止させるため、エンジンルームへと向かった。"
  },
  "000282": {
    "context": "000282",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "そして、６時１９分――"
  },
  "000336": {
    "context": "000336",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "エンジンルームから戻った宇喜多は、すぐに監視モニターで、第１エリア内周棟に人がいない事を確認した。"
  },
  "000347": {
    "context": "000347",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "「……よし、それでは始めよう」"
  },
  "000358": {
    "context": "000358",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "自分に言い聞かせるように呟き、第１エリア内周棟の館内放送をＯＮにする。"
  },
  "000363": {
    "context": "000363",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "そしてＰＤＡを取り出すと、宇喜多は即時起爆用の音楽データを再生した。"
  },
  "000368": {
    "context": "000368",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "すると超音波が、館内放送を伝い――"
  },
  "000394": {
    "context": "000394",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "第１エリア内周棟で起こった爆発と振動が、監視ルームにもかすかに届く。"
  },
  "000400": {
    "context": "000400",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "（さらに第２、第３エリアも……）"
  },
  "000406": {
    "context": "000406",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "（周りに人がいないことを確認して――今だ！）"
  },
  "000447": {
    "context": "000447",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "そうして次々に爆破を起こしていると、ラボ内に自動放送が鳴り響いた。"
  },
  "000461": {
    "context": "000461",
    "code": "0x45",
    "developer_comment": "自動放送",
    "string": "「――ただいま地下１階第２エリアで、火災が発生しました」"
  },
  "000490": {
    "context": "000490",
    "code": "0x45",
    "developer_comment": "自動放送",
    "string": "「――ただいま地下２階第３エリアで、火災が発生しました」"
  },
  "000519": {
    "context": "000519",
    "code": "0x45",
    "developer_comment": "自動放送",
    "string": "「――ただいま地下１階第５エリアで、火災が発生しました」"
  },
  "000548": {
    "context": "000548",
    "code": "0x45",
    "developer_comment": "自動放送",
    "string": "「施設内のセキュリティを、『避難モード』に移行。Ｎエリアを除く全ゲートを開放します」"
  },
  "000577": {
    "context": "000577",
    "code": "0x45",
    "developer_comment": "自動放送",
    "string": "「職員の皆様は、落ち着いて地上へ避難して下さい」"
  },
  "000604": {
    "context": "000604",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "（いいぞ！　警報装置が作動した！　これでシリウスにも出動要請がかかったはずだ！）"
  },
  "000610": {
    "context": "000610",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "（――彼らが到着するまでに、ラボ内の人間を外に出さなければ！）"
  },
  "000615": {
    "context": "000615",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "宇喜多は必死を装いつつ、館内放送で呼びかけた。"
  },
  "000626": {
    "context": "000626",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "「き、緊急放送です！」"
  },
  "000637": {
    "context": "000637",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "「ただいま施設内で、原因不明の爆発が発生しました！」"
  },
  "000648": {
    "context": "000648",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "「現状判明している限り、施設内の１０数か所で同時に爆発が起き、それに伴い火災も発生しています！」"
  },
  "000659": {
    "context": "000659",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "「江里口研究員、柏木研究員！　至急第２エリアに来てください！」"
  },
  "000670": {
    "context": "000670",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "「機密資料の持ち出しについて、ただちに相談したい事があります！」"
  },
  "000675": {
    "context": "000675",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "――そうして計画の布石を打ちつつ、ひとまず放送を停止する。"
  },
  "000680": {
    "context": "000680",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "その頃には監視ルームの外でも、逃げ惑う人々の喧騒が始まっていた。"
  },
  "000738": {
    "context": "000738",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "宇喜多は監視ルームの扉を開けると、周辺にいた人々を呼び止めた。"
  },
  "000749": {
    "context": "000749",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "「君たち頼む！　中で警備員たちが倒れているんだ！　外に連れ出してやってくれ！」"
  },
  "000757": {
    "context": "000757",
    "code": "0x45",
    "developer_comment": "職員１",
    "string": "「わ、わかった。でもあんたはどうするんだ？」"
  },
  "000768": {
    "context": "000768",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "「僕は残って避難誘導をする！　君たちは先に逃げろ！」"
  },
  "000773": {
    "context": "000773",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "宇喜多はそう告げて、制御室に走った。"
  },
  "000830": {
    "context": "000830",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "そこにはまだ多数の研究員が残っていた。"
  },
  "000835": {
    "context": "000835",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "宇喜多が調べたところによると、今朝はＷＸ粒子増幅器を使って、何らかの実験を行っていたという。"
  },
  "000840": {
    "context": "000840",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "律儀に持ち場を護る彼らに、宇喜多は叫んだ。"
  },
  "000851": {
    "context": "000851",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "「君たち、何をやっているんだ！　早く逃げないか！」"
  },
  "000859": {
    "context": "000859",
    "code": "0x45",
    "developer_comment": "研究員１",
    "string": "「しかし、増幅器が稼働中で――」"
  },
  "000870": {
    "context": "000870",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "「放置しておけば、ＢｒａｉｎＣｅｌｌが自動停止処理を行うだろう！」"
  },
  "000881": {
    "context": "000881",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "「万が一それが上手くいかなくても、地上の遠隔制御室からも停止処理はできる！」"
  },
  "000892": {
    "context": "000892",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "「いいから早く逃げろ！　死んだら元も子もない！」"
  },
  "000900": {
    "context": "000900",
    "code": "0x45",
    "developer_comment": "研究員２",
    "string": "「――あ、ああ！」"
  },
  "000906": {
    "context": "000906",
    "code": "0x45",
    "developer_comment": "研究員２",
    "string": "誰かがそう叫んだのを皮切りに、研究員たちは我先にと部屋を出て行った。"
  },
  "000912": {
    "context": "000912",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "（よし、今のうちに――！）"
  },
  "000918": {
    "context": "000918",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "誰もいなくなった制御室で、宇喜多はＢｒａｉｎＣｅｌｌのコンパネに歩み寄った。"
  },
  "000923": {
    "context": "000923",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "モニターを確認すると、現在ＷＸ粒子増幅器は、『５００ｍＳｖ』の出力を保っているようだ。"
  },
  "000928": {
    "context": "000928",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "このまま放置していたら、自動的にこの数値は減衰していく。"
  },
  "000939": {
    "context": "000939",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "だが宇喜多はあえてコンパネを操作し、その出力を最大にまで上げた。"
  },
  "000945": {
    "context": "000945",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "――すると、徐々にその数値が上昇しはじめる。"
  },
  "000950": {
    "context": "000950",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "『５０１ｍＳｖ』、『５０２ｍＳｖ』、『５０４ｍＳｖ』……！"
  },
  "000956": {
    "context": "000956",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "（……よし、これでじきに増幅器は制御不能になる！）"
  },
  "000961": {
    "context": "000961",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "もともと運用に細心の注意が必要な装置だ。出力をわざと最大にすれば、容易に暴走状態は起こる。"
  },
  "000967": {
    "context": "000967",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "（ここからが正念場だ！）"
  },
  "000972": {
    "context": "000972",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "宇喜多は制御室を飛び出した。"
  },
  "001025": {
    "context": "001025",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "そして監視ルームに戻ると、警備員たちの姿は消えていた。"
  },
  "001030": {
    "context": "001030",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "誰かが落としていったらしいセキュリティカードが、１枚残されているだけだった。"
  },
  "001035": {
    "context": "001035",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "それを拾い、再び爆破活動と放送を続ける。"
  },
  "001087": {
    "context": "001087",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "やがて喧騒が遠ざかり、監視モニターに映る人影も見当たらなくなった頃。"
  },
  "001098": {
    "context": "001098",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "鳴り響いた内線電話の受話器を、宇喜多はすぐに取った。"
  },
  "001115": {
    "context": "001115",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "「はい。こちら監視ルーム」"
  },
  "001123": {
    "context": "001123",
    "code": "0x45",
    "developer_comment": "警備員",
    "string": "「あ……まだ残っていた者がいたのか！」"
  },
  "001131": {
    "context": "001131",
    "code": "0x45",
    "developer_comment": "警備員",
    "string": "「こちらは地上フロアの監視ルームだ。さっそくで悪いが、君は誰だ？」"
  },
  "001139": {
    "context": "001139",
    "code": "0x45",
    "developer_comment": "警備員",
    "string": "「こちらで行った点呼では、まだ地下に残っている職員は江里口、柏木、宇喜多の３名となっているが――」"
  },
  "001150": {
    "context": "001150",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "「了解！　すぐにその２人を探し、脱出する！」"
  },
  "001155": {
    "context": "001155",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "宇喜多はそう叫び、受話器を置いた。"
  },
  "001166": {
    "context": "001166",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "『江里口』と『柏木』とは、さきほど宇喜多が放送で呼び出した２名の研究員。"
  },
  "001171": {
    "context": "001171",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "事前にリストアップしておいた、Ｚｅｒｏエリアに入る権限を持つ者の名前だった。"
  },
  "001176": {
    "context": "001176",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "恐らくすでに到着しているはずの渡瀬たちが、彼らを上手く捕らえたのだろう。"
  },
  "001181": {
    "context": "001181",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "宇喜多はそう判断し、館内放送のマイクに向かって言った。"
  },
  "001192": {
    "context": "001192",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "「地上フロアで行った点呼により、施設内の全職員の避難が確認できました！」"
  },
  "001203": {
    "context": "001203",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "「万が一職員以外の方が残っていたら、ただちに第５・第６エリア方面から脱出してください！」"
  },
  "001208": {
    "context": "001208",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "それは念のための放送であり、渡瀬たちへの合図だった。"
  },
  "001256": {
    "context": "001256",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "だが渡瀬たちを待つ間も、時間は無駄にしない。"
  },
  "001313": {
    "context": "001313",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "いったん制御室に行き、増幅器の稼動状態を確認する。"
  },
  "001318": {
    "context": "001318",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "Ｎエリアの数値は『２３１５ｍＳｖ』。順調に上昇していた。"
  },
  "001323": {
    "context": "001323",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "ＢｒａｉｎＣｅｌｌはすでに増幅器の緊急停止処理を始めていたが、もう遅い。"
  },
  "001328": {
    "context": "001328",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "停止処理と減衰が終了するのは、『１８：１６』と表示されていた。"
  },
  "001334": {
    "context": "001334",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "（今から約１１時間後か！　よし、ここまでは完璧だ！）"
  },
  "001339": {
    "context": "001339",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "そしてまた監視ルームに戻ると――"
  },
  "001399": {
    "context": "001399",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "すぐに渡瀬らレスキュー隊員の３人が、気絶した幹部研究員２名を抱えて現れた。"
  },
  "001435": {
    "context": "001435",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「じゃあ俺たちはＺｅｒｏエリアに降り、被験体を救出してくる」"
  },
  "001446": {
    "context": "001446",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "「僕はここに留まり、不測の事態が起きないかどうか見張っておこう」"
  },
  "001451": {
    "context": "001451",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "そう言う宇喜多に渡瀬はうなずき、堂島たちと共に部屋を飛び出していった。"
  },
  "001525": {
    "context": "001525",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "だが、それから渡瀬たちは、なかなか戻ってこなかった。"
  },
  "001530": {
    "context": "001530",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "ケースＮも発令されない。"
  },
  "001536": {
    "context": "001536",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "（遅い……！　何をやってるんだ！？）"
  },
  "001542": {
    "context": "001542",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "（４０００ｍＳｖを超えてしまったら、本当に危険だと言うのに！）"
  },
  "001547": {
    "context": "001547",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "そう思った時、突如けたたましいサイレンが響いた。"
  },
  "001572": {
    "context": "001572",
    "code": "0x45",
    "developer_comment": "自動放送",
    "string": "「――ただいま、『ケースＮ』が発生しました」"
  },
  "001580": {
    "context": "001580",
    "code": "0x45",
    "developer_comment": "自動放送",
    "string": "「緊急事態につき、セキュリティを『封鎖モード』に移行」"
  },
  "001588": {
    "context": "001588",
    "code": "0x45",
    "developer_comment": "自動放送",
    "string": "「施設内の全ゲートを閉鎖・ロックすると共に、隔壁を封鎖します」"
  },
  "001593": {
    "context": "001593",
    "code": "0x45",
    "developer_comment": "自動放送",
    "string": "はっとして時計を見ると、渡瀬たちが出て行ってから、約２０分が過ぎていた。"
  },
  "001599": {
    "context": "001599",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "（ま……間に合ったか？）"
  },
  "001604": {
    "context": "001604",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "後ほど渡瀬たちが無事か確認しようと思いつつ、残りの爆弾を全て爆破する。"
  },
  "001671": {
    "context": "001671",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "中には不発だったものもあるようだが、それは仕方ない。"
  },
  "001676": {
    "context": "001676",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "そしてあらかじめ用意しておいたＡＤを投与した時、渡瀬たちがやってきた。"
  },
  "001718": {
    "context": "001718",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "「遅かったじゃないか！　３人ともなんともないか！？」"
  },
  "001729": {
    "context": "001729",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「あ、ああ……とりあえず、大丈夫そうだが……」"
  },
  "001740": {
    "context": "001740",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "「本当か……？」"
  },
  "001745": {
    "context": "001745",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "宇喜多が心配の声をかけると、渡瀬は不愉快そうに答えた。"
  },
  "001776": {
    "context": "001776",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「大丈夫だと言っているだろう。それより――」"
  },
  "001805": {
    "context": "001805",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「大変だ宇喜多さん！　Ｚｅｒｏエリアには、被験体が１人しかいなかった！」"
  },
  "001816": {
    "context": "001816",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "「なに！？」"
  },
  "001827": {
    "context": "001827",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "そう尋ねると、渡瀬は事情を説明した。"
  },
  "001832": {
    "context": "001832",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "被験体ＹⅡの姿は、どこにもなかったという。"
  },
  "001837": {
    "context": "001837",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "さらに２名の上級研究員の姿も見えない事に気付き、それに対して問いを重ねると――信じ難い答えが返ってきた。"
  },
  "001843": {
    "context": "001843",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "（『２人とも、Ｚｅｒｏエリアで殺してしまった』だと！？）"
  },
  "001850": {
    "context": "001850",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "思わず宇喜多は、渡瀬に食ってかかった。"
  },
  "001913": {
    "context": "001913",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "「隊長さん、話が違うじゃないか！」"
  },
  "001924": {
    "context": "001924",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "「人的被害は出さないんじゃなかったのか！？」"
  },
  "001953": {
    "context": "001953",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「ま、待て！　落ち着け宇喜多さん！」"
  },
  "001964": {
    "context": "001964",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "「誰も傷つけないという話だったじゃないか！　騙していたのか！？」"
  },
  "001975": {
    "context": "001975",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "「もしそうだと言うなら、僕は君たちを――」"
  },
  "001998": {
    "context": "001998",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "思わず宇喜多が渡瀬に掴み掛ろうとした、そのとたん――"
  },
  "002059": {
    "context": "002059",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "「っ！」"
  },
  "002064": {
    "context": "002064",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "後頭部を痛打された。"
  },
  "002082": {
    "context": "002082",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "「な、ぜ……」"
  },
  "002087": {
    "context": "002087",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "薄れゆく意識の中、渡瀬の声が聞こえる。"
  },
  "002159": {
    "context": "002159",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「……すまない、宇喜多さん」"
  },
  "002170": {
    "context": "002170",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「俺たちは……もう引き返すことができないんだ」"
  },
  "002175": {
    "context": "002175",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "『俺たち』と渡瀬は言った。"
  },
  "002182": {
    "context": "002182",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "どうやらそこに宇喜多は含まれていないらしい。"
  },
  "002188": {
    "context": "002188",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "（僕は……裏切られたのか……？）"
  },
  "002193": {
    "context": "002193",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "失望感に溺れるように、宇喜多は意識を失った。"
  },
  "002208": {
    "context": "002208",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "――――――"
  },
  "002213": {
    "context": "002213",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "………………"
  },
  "002218": {
    "context": "002218",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "………それからどれくらい、時間が過ぎたのだろう。"
  },
  "002229": {
    "context": "002229",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "「う……む……」"
  },
  "002258": {
    "context": "002258",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "「僕は、いったい…………？」"
  },
  "002269": {
    "context": "002269",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "「そ、そうだ……僕は、隊長さんに殴られて……」"
  },
  "002323": {
    "context": "002323",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "気付けば首から下げていたセキュリティカードと、ポケットにあったカードが、どちらも消えていた。"
  },
  "002339": {
    "context": "002339",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "（彼らはどこへ……！？）"
  },
  "002345": {
    "context": "002345",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "宇喜多は現状を確認するために、監視モニターをチェックした。"
  },
  "002350": {
    "context": "002350",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "だが爆破の影響で大半のカメラが破損しており、とても役立ちそうにない。"
  },
  "002356": {
    "context": "002356",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "（くそ、隊長さんたちはどこに行ったんだ！　僕を置き去りにして！）"
  },
  "002362": {
    "context": "002362",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "（まさか……初めから僕を、スケープゴートにするつもりだったのか！？）"
  },
  "002367": {
    "context": "002367",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "宇喜多の中には、まだ渡瀬を『信じたい』という思いが残っている。"
  },
  "002372": {
    "context": "002372",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "だが彼らが殺人を犯した事と、宇喜多を気絶させた事――その２つの事実が、宇喜多の心に波紋を起こす。"
  },
  "002383": {
    "context": "002383",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "「……い、いや……今はまず、できる事をしなければ……！」"
  },
  "002388": {
    "context": "002388",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "とにかく万が一に備え、自分がこの事件に関与した証拠を抹消しなければならない。"
  },
  "002399": {
    "context": "002399",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "そう思い、まずは監視カメラの映像記録を消し始めたのだが――"
  },
  "002449": {
    "context": "002449",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "――しばらくして、信じ難い映像を発見してしまった。"
  },
  "002514": {
    "context": "002514",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "「こ、これは……！」"
  },
  "002519": {
    "context": "002519",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "宇喜多は慌てて、映像を停止した。"
  },
  "002540": {
    "context": "002540",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "「なんだ、この映像は……！？　なんで彼らがここにいるんだ？」"
  },
  "002545": {
    "context": "002545",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "いるはずのない夏彦たちが、ここにいる！"
  },
  "002550": {
    "context": "002550",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "爆破の影響で映像に乱れはあるが、そこに映っていたのは、間違いなくあの３人の子供だった。"
  },
  "002555": {
    "context": "002555",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "それは宇喜多を激しく動揺させた。"
  },
  "002561": {
    "context": "002561",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "（ま、まさか……あの後、ラボに直接やってきたのか！？）"
  },
  "002567": {
    "context": "002567",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "（そして僕らのテロに巻き込まれて――！）"
  },
  "002574": {
    "context": "002574",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "数時間前、宇喜多は『人がいない場所』を狙って爆破を行った。"
  },
  "002579": {
    "context": "002579",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "だが忙しすぎて、モニターに映りこむのが誰かまでは確認しきれなかった。"
  },
  "002584": {
    "context": "002584",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "そのせいで夏彦たちがいるのに、気付かなかったのだ。"
  },
  "002590": {
    "context": "002590",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "（まずい……まずい、まずいぞ！）"
  },
  "002596": {
    "context": "002596",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "（もし彼らが脱出できていなかったとしたら、それは紛れもなく僕の責任だ！）"
  },
  "002601": {
    "context": "002601",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "――宇喜多がそう思った時。"
  },
  "002606": {
    "context": "002606",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "部屋の外から声が聞こえた。"
  },
  "002664": {
    "context": "002664",
    "code": "0x45",
    "developer_comment": "？？",
    "string": "「おーい！　ここにいるのか！？」"
  },
  "002677": {
    "context": "002677",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "「なっ！？」"
  },
  "002688": {
    "context": "002688",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "「その声は――隊長さんか！？」"
  },
  "002699": {
    "context": "002699",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「なに！？　あ、あんたオレを知ってるのか！？」"
  },
  "002710": {
    "context": "002710",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "「し、知ってるも何も……」"
  },
  "002748": {
    "context": "002748",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "扉を開けると、そこには渡瀬がいた。"
  },
  "002753": {
    "context": "002753",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "だが渡瀬以外にも、２人のレスキュー隊員と――全く見知らぬ女が、２人いた。"
  },
  "002782": {
    "context": "002782",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "「私は椿山恵那。この施設に呼ばれてやってきた、高校教師よ」"
  },
  "002811": {
    "context": "002811",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「わたし、ユウリ……ラボの研究員の、家族です」"
  },
  "002816": {
    "context": "002816",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "その２人の存在を、宇喜多は訝しく思った。"
  },
  "002822": {
    "context": "002822",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "（……おかしい。どんな事情があるにせよ、職員以外がラボに立ち入る事は禁止されているはずなのに）"
  },
  "002828": {
    "context": "002828",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "（いや、それより『ユウリ』という少女……もしや、彼女が被験体ＹⅡなのではないか！？）"
  },
  "002927": {
    "context": "002927",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "だが現れた５人の中で宇喜多を最も慌てさせたのは、渡瀬だった。"
  },
  "002932": {
    "context": "002932",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "彼は何らかの記憶障害を起こしたらしく、宇喜多の事も覚えていないらしいのだ。"
  },
  "002938": {
    "context": "002938",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "（いったい、どういう事なんだ！？　いるはずのない部下に遭遇して、とっさに嘘をついたのか！？）"
  },
  "002944": {
    "context": "002944",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "（だがそうだとしても、堂島と桧山はどうなったんだ！？　被験体Ｎは！？　彼らはどこに行ったのだ！？）"
  },
  "002949": {
    "context": "002949",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "予想外の出来事の連続に、宇喜多は混乱したが――"
  },
  "003003": {
    "context": "003003",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "だが眼の前にいる被験体ＹⅡの存在が、"
  },
  "003053": {
    "context": "003053",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "地下に取り残されているかもしれない夏彦たちの存在が、"
  },
  "003092": {
    "context": "003092",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "――宇喜多を冷静に立ち返らせた。"
  },
  "003098": {
    "context": "003098",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "（そうだ、落ち着け。そして思い出すんだ。僕がなぜ今日の行動を起こしたかを）"
  },
  "003103": {
    "context": "003103",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "何が起きてるのかはわからない。渡瀬たちに裏切られたのかもしれない。"
  },
  "003108": {
    "context": "003108",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "だが宇喜多は被験体を救うために、夏彦を守るために、今日の行動を起こしたのだった。"
  },
  "003114": {
    "context": "003114",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "（だから僕さえ、僕の信念さえ揺るがなければ――きっと彼らをラボから救い出す事が出来るはずだ）"
  },
  "003119": {
    "context": "003119",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "宇喜多はあの雨の夜と同じように、心に熱い火を入れる。"
  },
  "003173": {
    "context": "003173",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "そうして、宇喜多たちは歩き出した。"
  },
  "003224": {
    "context": "003224",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "要救助者とＡＤと、そして脱出口を求めて……。"
  }
}