{
  "000078": {
    "context": "000078",
    "code": "0x55",
    "string": "√Before Chapter2"
  },
  "000092": {
    "context": "000092",
    "code": "0x45",
    "developer_comment": "",
    "string": "――軋んだ音を立てて、閉ざされていたゲートが開いた。"
  },
  "000111": {
    "context": "000111",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「わっ！」"
  },
  "000160": {
    "context": "000160",
    "code": "0x45",
    "developer_comment": "",
    "string": "夏彦は思わず、勢い余って転んだ。"
  },
  "000202": {
    "context": "000202",
    "code": "0x45",
    "developer_comment": "",
    "string": "床に背中をしたたか打ち付けてしまった。"
  },
  "000277": {
    "context": "000277",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「いててて……！？」"
  },
  "000334": {
    "context": "000334",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「夏彦、大丈夫？」"
  },
  "000356": {
    "context": "000356",
    "code": "0x45",
    "developer_comment": "",
    "string": "ましろが声をかけてくる。"
  },
  "000424": {
    "context": "000424",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「……」"
  },
  "000429": {
    "context": "000429",
    "code": "0x45",
    "developer_comment": "",
    "string": "サリュは黙って、夏彦を見下ろしている。"
  },
  "000434": {
    "context": "000434",
    "code": "0x45",
    "developer_comment": "",
    "string": "夏彦は一瞬何が起きてるのか、戸惑った。"
  },
  "000486": {
    "context": "000486",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "（……そ、そうだ……ぼくはラボの中を逃げてたんだ）"
  },
  "000491": {
    "context": "000491",
    "code": "0x45",
    "developer_comment": "",
    "string": "そして開かないゲートを開けようと力を込め、ようやくこじ開けたのだ。"
  },
  "000525": {
    "context": "000525",
    "code": "0x45",
    "developer_comment": "",
    "string": "悠里が夏彦を見て、心配げに言う。"
  },
  "000536": {
    "context": "000536",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「ね、立てる？」"
  },
  "000547": {
    "context": "000547",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「あ、ああ」"
  },
  "000637": {
    "context": "000637",
    "code": "0x45",
    "developer_comment": "",
    "string": "夏彦は立ち上がり、ゲートの向こうを見た。"
  },
  "000648": {
    "context": "000648",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「……それより行こう、ここから先がたぶん『第２エリア』だ」"
  },
  "000728": {
    "context": "000728",
    "code": "0x45",
    "developer_comment": "",
    "string": "夏彦たちはうなずき合い、第２エリアに駆け込んだ。"
  },
  "000896": {
    "context": "000896",
    "code": "0x45",
    "developer_comment": "",
    "string": "それにしても――と夏彦は思う。"
  },
  "000901": {
    "context": "000901",
    "code": "0x45",
    "developer_comment": "",
    "string": "こんなに切羽詰った状況なのに、さっき一瞬、回想にふけってしまっていた。"
  },
  "000953": {
    "context": "000953",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "（どうして今さら、何日も前の出来事を……？）"
  },
  "001158": {
    "context": "001158",
    "code": "0x45",
    "developer_comment": "",
    "string": "あたかも時間を巻き戻したかのように、鮮明にあの日の出来事を思い出していた。"
  },
  "001348": {
    "context": "001348",
    "code": "0x45",
    "developer_comment": "",
    "string": "公園でましろと交わした会話も、サリュが家にやってきた時の事も、詳細に思い出せた。"
  },
  "001464": {
    "context": "001464",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "（……いや、過去を振り返っている場合じゃない）"
  },
  "001470": {
    "context": "001470",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "（いまは逃げる事に集中しろ！）"
  },
  "001487": {
    "context": "001487",
    "code": "0x45",
    "developer_comment": "",
    "string": "夏彦がそう思った時、"
  },
  "001756": {
    "context": "001756",
    "code": "0x45",
    "developer_comment": "",
    "string": "通路の向こうから、異形の人影が２つ現れた。"
  },
  "001767": {
    "context": "001767",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「な……なにあれ？」"
  },
  "001778": {
    "context": "001778",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「……！」"
  },
  "001783": {
    "context": "001783",
    "code": "0x45",
    "developer_comment": "",
    "string": "夏彦たちは立ちすくんだ。"
  },
  "001788": {
    "context": "001788",
    "code": "0x45",
    "developer_comment": "",
    "string": "顔はマスクに隠れ、何者かわからない。"
  },
  "001870": {
    "context": "001870",
    "code": "0x45",
    "developer_comment": "",
    "string": "２人はこちらに歩み寄ってきて、口々に言った。"
  },
  "001878": {
    "context": "001878",
    "code": "0x45",
    "developer_comment": "？？",
    "string": "「……君たちは職員じゃないな？　どうしてこんなところに？」"
  },
  "001889": {
    "context": "001889",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「あなたたちは……？」"
  },
  "001897": {
    "context": "001897",
    "code": "0x45",
    "developer_comment": "？？",
    "string": "「我々はラボから要請を受け、救助活動に来たレスキュー隊だ」"
  },
  "001909": {
    "context": "001909",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「レスキュー隊！？」"
  },
  "001914": {
    "context": "001914",
    "code": "0x45",
    "developer_comment": "",
    "string": "その言葉を聞き、夏彦も安堵の息をついた。"
  },
  "002095": {
    "context": "002095",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "（良かった……悠里たちをこの人に任せれば、一安心だ……）"
  },
  "002104": {
    "context": "002104",
    "code": "0x45",
    "developer_comment": "隊員１",
    "string": "「君たちは要救助者だね？」"
  },
  "002115": {
    "context": "002115",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「え、ええ……偶然ここにいて、巻き込まれてしまったんです」"
  },
  "002141": {
    "context": "002141",
    "code": "0x45",
    "developer_comment": "隊員１",
    "string": "「他に連れは？」"
  },
  "002152": {
    "context": "002152",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「いえ、ここにいる４人で全員です」"
  },
  "002178": {
    "context": "002178",
    "code": "0x45",
    "developer_comment": "隊員２",
    "string": "「――何人だって？」"
  },
  "002207": {
    "context": "002207",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「ええ、４人です。特に体力のない女の子がいるので、その子から救助をお願いできますか？」"
  },
  "002233": {
    "context": "002233",
    "code": "0x45",
    "developer_comment": "隊員２",
    "string": "「わかった、ならばその娘を……」"
  },
  "002304": {
    "context": "002304",
    "code": "0x45",
    "developer_comment": "",
    "string": "そう言いながらレスキュー隊員が、夏彦に手を伸ばした時――"
  },
  "002450": {
    "context": "002450",
    "code": "0x45",
    "developer_comment": "",
    "string": "彼の腹に、誰かの足がめり込んだ。"
  },
  "002589": {
    "context": "002589",
    "code": "0x45",
    "developer_comment": "隊員２",
    "string": "「ぐっ！」"
  },
  "002618": {
    "context": "002618",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「！？」"
  },
  "002630": {
    "context": "002630",
    "code": "0x45",
    "developer_comment": "",
    "string": "腹を蹴られたレスキュー隊員が、床に崩れ落ちる。"
  },
  "002642": {
    "context": "002642",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "（え……？）"
  },
  "002812": {
    "context": "002812",
    "code": "0x45",
    "developer_comment": "",
    "string": "夏彦は呆然と、傍らの少女を見る。"
  },
  "002817": {
    "context": "002817",
    "code": "0x45",
    "developer_comment": "",
    "string": "蹴ったのはサリュだった。"
  },
  "002822": {
    "context": "002822",
    "code": "0x45",
    "developer_comment": "",
    "string": "彼女は冷徹な目で、夏彦を見ていた。"
  },
  "002894": {
    "context": "002894",
    "code": "0x45",
    "developer_comment": "隊員２",
    "string": "「な、何をする！？」"
  },
  "003031": {
    "context": "003031",
    "code": "0x45",
    "developer_comment": "",
    "string": "別のレスキュー隊員が、サリュの肩につかみかかる。"
  },
  "003208": {
    "context": "003208",
    "code": "0x45",
    "developer_comment": "",
    "string": "サリュはその手を瞬時に掴み――投げ飛ばした。"
  },
  "003253": {
    "context": "003253",
    "code": "0x45",
    "developer_comment": "隊員１",
    "string": "「がッ！」"
  },
  "003279": {
    "context": "003279",
    "code": "0x45",
    "developer_comment": "",
    "string": "隊員が床に叩きつけられ、気を失う。"
  },
  "003398": {
    "context": "003398",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「サリュ、どうして……！？」"
  },
  "003403": {
    "context": "003403",
    "code": "0x45",
    "developer_comment": "",
    "string": "ましろが空ろな声をかけると、サリュは鼻を鳴らして言った。"
  },
  "003432": {
    "context": "003432",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「夏彦、ましろ。あなたたちは甘い……心を許すのが早すぎる」"
  },
  "003443": {
    "context": "003443",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「え！？」"
  },
  "003472": {
    "context": "003472",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「なぜそうやって、すぐに人を信じようとするの？」"
  },
  "003483": {
    "context": "003483",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「……っ」"
  },
  "003597": {
    "context": "003597",
    "code": "0x45",
    "developer_comment": "",
    "string": "サリュの冷たい視線――それを見た時、夏彦の脳裏にかつて見た光景がよぎった。"
  },
  "003602": {
    "context": "003602",
    "code": "0x45",
    "developer_comment": "",
    "string": "初めて出会った時の彼女の目。"
  },
  "003607": {
    "context": "003607",
    "code": "0x45",
    "developer_comment": "",
    "string": "事故の騒ぎの中、無表情に、ただ夏彦だけを見据えていた瞳――"
  },
  "003613": {
    "context": "003613",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "（そんな……どうして……？）"
  },
  "003660": {
    "context": "003660",
    "code": "0x45",
    "developer_comment": "",
    "string": "夏彦がそう思った時、"
  },
  "003733": {
    "context": "003733",
    "code": "0x45",
    "developer_comment": "",
    "string": "――傍らの悠里が囁いた。"
  },
  "003744": {
    "context": "003744",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「……ひこりん、忘れたの？　サリュちゃんがどういう子だったか」"
  },
  "003761": {
    "context": "003761",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「え……？」"
  },
  "003790": {
    "context": "003790",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「今日まで一緒にいたんでしょ？」"
  },
  "003819": {
    "context": "003819",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「あの子の言葉に隠された真実……それがどういう意味を持っていたのかわからない？」"
  },
  "003854": {
    "context": "003854",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「それを少しずつでいいから、思い出して欲しいの」"
  },
  "003883": {
    "context": "003883",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「何が本当で、何が嘘なのか」"
  },
  "003918": {
    "context": "003918",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「信じるべきなのは誰なのか……ひこりんはもう知っているはずだよ」"
  },
  "003923": {
    "context": "003923",
    "code": "0x45",
    "developer_comment": "",
    "string": "その言葉の真意はつかめなかった。"
  },
  "003952": {
    "context": "003952",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「答えは記憶の中にあるの。あの日の記憶の中に――」"
  },
  "003963": {
    "context": "003963",
    "code": "0x45",
    "developer_comment": "",
    "string": "そう言われた時、視界の端に時計が映った。"
  },
  "004212": {
    "context": "004212",
    "code": "0x45",
    "developer_comment": "",
    "string": "小さな文字盤の表示が、奇妙なほどにはっきり見える。"
  },
  "004284": {
    "context": "004284",
    "code": "0x47",
    "developer_comment": "",
    "string": "『９　月　１　２　日』"
  },
  "004292": {
    "context": "004292",
    "code": "0x47",
    "developer_comment": "",
    "string": "『６　時　３　９　分』"
  },
  "004326": {
    "context": "004326",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "（この日に、何が！？）"
  },
  "004429": {
    "context": "004429",
    "code": "0x45",
    "developer_comment": "",
    "string": "夏彦は悠里の真意を確かめようと――"
  },
  "004434": {
    "context": "004434",
    "code": "0x45",
    "developer_comment": "",
    "string": "声を搾り出した。"
  }
}