{
  "000068": {
    "context": "000068",
    "code": "0x45",
    "developer_comment": "",
    "string": "――それはもう１６年も前の事。"
  },
  "000073": {
    "context": "000073",
    "code": "0x45",
    "developer_comment": "",
    "string": "恵那がまだ、８歳の頃――"
  },
  "000078": {
    "context": "000078",
    "code": "0x45",
    "developer_comment": "",
    "string": "彼女は大好きな父と共に、街を散歩していた。"
  },
  "000117": {
    "context": "000117",
    "code": "0x45",
    "developer_comment": "恵那の父",
    "string": "「恵那、ここがお父さんの新しい職場だよ」"
  },
  "000125": {
    "context": "000125",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "「わー、りっぱなたてもの！」"
  },
  "000130": {
    "context": "000130",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "恵那は父に連れられ、彼の職場を訪れていた。"
  },
  "000135": {
    "context": "000135",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "恵那の無邪気な声に、父は優しく答える。"
  },
  "000143": {
    "context": "000143",
    "code": "0x45",
    "developer_comment": "恵那の父",
    "string": "「超心理学研究所っていうんだ。以前から超能力とかを研究してた、ちょっと変わった施設だったんだけど――」"
  },
  "000151": {
    "context": "000151",
    "code": "0x45",
    "developer_comment": "恵那の父",
    "string": "「超能力っていうものが、どうやら実際に存在するものだと、最近になってわかってね」"
  },
  "000159": {
    "context": "000159",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "「ちょうのうりょく？　すてき！」"
  },
  "000167": {
    "context": "000167",
    "code": "0x45",
    "developer_comment": "恵那の父",
    "string": "「ワクワクするだろ？　大人たちもその発見に、すんごくワクワクしたんだよ」"
  },
  "000175": {
    "context": "000175",
    "code": "0x45",
    "developer_comment": "恵那の父",
    "string": "「それで人員を急増する事が決まり、お父さんもここに転属になったってわけさ」"
  },
  "000183": {
    "context": "000183",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "「じゃあお父さんも、ちょうのうりょくをけんきゅうするの？」"
  },
  "000191": {
    "context": "000191",
    "code": "0x45",
    "developer_comment": "恵那の父",
    "string": "「しないしない。お父さんは研究者じゃなくて役人だからね」"
  },
  "000199": {
    "context": "000199",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "「じゃあお父さんはなにをするの？」"
  },
  "000207": {
    "context": "000207",
    "code": "0x45",
    "developer_comment": "恵那の父",
    "string": "「超能力者たちを、護るのさ」"
  },
  "000215": {
    "context": "000215",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "「まもる……？　だれからまもるの？」"
  },
  "000223": {
    "context": "000223",
    "code": "0x45",
    "developer_comment": "恵那の父",
    "string": "「悪い人たちからだよ」"
  },
  "000231": {
    "context": "000231",
    "code": "0x45",
    "developer_comment": "恵那の父",
    "string": "「超能力者たちはこれまで、よそですごくいじめられててね……しかも誰からも護ってもらえなかったんだ」"
  },
  "000239": {
    "context": "000239",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "「かわいそう……」"
  },
  "000247": {
    "context": "000247",
    "code": "0x45",
    "developer_comment": "恵那の父",
    "string": "「そうだろ。だからお父さんたちが護ってあげなきゃならないって思って、超能力者を護るための部署を立ち上げたのさ」"
  },
  "000255": {
    "context": "000255",
    "code": "0x45",
    "developer_comment": "恵那の父",
    "string": "「超能力者をいじめる悪者がいたら、お父さんたちが捕まえる」"
  },
  "000263": {
    "context": "000263",
    "code": "0x45",
    "developer_comment": "恵那の父",
    "string": "「逆に超能力が悪用されたら、その時もお父さんたちが止めるのさ。すごくやりがいのある仕事だよ」"
  },
  "000271": {
    "context": "000271",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "「かっこいい、なんかおまわりさんみたい」"
  },
  "000279": {
    "context": "000279",
    "code": "0x45",
    "developer_comment": "恵那の父",
    "string": "「はは、そうだね。さしずめお父さんたちは、『ＢＣ警察』ってところかな」"
  },
  "000287": {
    "context": "000287",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "「えなもおとなになったら、おとうさんみたいなおしごとしたい！」"
  },
  "000295": {
    "context": "000295",
    "code": "0x45",
    "developer_comment": "恵那の父",
    "string": "「おー、いいね。一緒の職場で働ける日を、楽しみにしておこうかな」"
  },
  "000300": {
    "context": "000300",
    "code": "0x45",
    "developer_comment": "恵那の父",
    "string": "そう言って父は微笑んだ。"
  },
  "000359": {
    "context": "000359",
    "code": "0x45",
    "developer_comment": "恵那の父",
    "string": "――恵那もいつかその日が来る事を楽しみにしていた。"
  },
  "000364": {
    "context": "000364",
    "code": "0x45",
    "developer_comment": "恵那の父",
    "string": "だが、決してそれが叶わないと思い知ったのは――"
  },
  "000369": {
    "context": "000369",
    "code": "0x45",
    "developer_comment": "恵那の父",
    "string": "それから約半年後。"
  },
  "000374": {
    "context": "000374",
    "code": "0x45",
    "developer_comment": "恵那の父",
    "string": "２０１４年９月１６日の事だった。"
  },
  "000507": {
    "context": "000507",
    "code": "0x85",
    "developer_comment": "恵那の父",
    "string": "　%N"
  },
  "000510": {
    "context": "000510",
    "code": "0x45",
    "developer_comment": "恵那の父",
    "string": "――『鹿鳴市同時多発火災』――"
  },
  "000514": {
    "context": "000514",
    "code": "0x85",
    "developer_comment": "恵那の父",
    "string": "　%N"
  },
  "000517": {
    "context": "000517",
    "code": "0x45",
    "developer_comment": "恵那の父",
    "string": "あの夜起きた出来事は、後にそう呼ばれた。"
  },
  "000559": {
    "context": "000559",
    "code": "0x85",
    "developer_comment": "恵那の父",
    "string": "　%N"
  },
  "000562": {
    "context": "000562",
    "code": "0x45",
    "developer_comment": "恵那の父",
    "string": "鹿鳴市中の実に１５箇所で、同時に火災が発生したのだ。"
  },
  "000566": {
    "context": "000566",
    "code": "0x85",
    "developer_comment": "恵那の父",
    "string": "　"
  },
  "000569": {
    "context": "000569",
    "code": "0x45",
    "developer_comment": "恵那の父",
    "string": "鹿鳴市立病院をはじめ、公共施設、雑居ビル、マンション。"
  },
  "000572": {
    "context": "000572",
    "code": "0x45",
    "developer_comment": "恵那の父",
    "string": "様々な建物に無差別に火が放たれ、"
  },
  "000575": {
    "context": "000575",
    "code": "0x45",
    "developer_comment": "恵那の父",
    "string": "一夜のうちにそれらは焼け落ちた。"
  },
  "000579": {
    "context": "000579",
    "code": "0x85",
    "developer_comment": "恵那の父",
    "string": "　%N"
  },
  "000582": {
    "context": "000582",
    "code": "0x45",
    "developer_comment": "恵那の父",
    "string": "死者１１８名。"
  },
  "000586": {
    "context": "000586",
    "code": "0x85",
    "developer_comment": "恵那の父",
    "string": "　"
  },
  "000589": {
    "context": "000589",
    "code": "0x45",
    "developer_comment": "恵那の父",
    "string": "その内訳は一酸化炭素等による中毒死が６７名、焼死が３４名。"
  },
  "000592": {
    "context": "000592",
    "code": "0x45",
    "developer_comment": "恵那の父",
    "string": "高所からの転落死が１１名、"
  },
  "000595": {
    "context": "000595",
    "code": "0x45",
    "developer_comment": "恵那の父",
    "string": "ショック死が３名、"
  },
  "000598": {
    "context": "000598",
    "code": "0x45",
    "developer_comment": "恵那の父",
    "string": "苦痛による自殺が２名。"
  },
  "000602": {
    "context": "000602",
    "code": "0x85",
    "developer_comment": "恵那の父",
    "string": "　"
  },
  "000605": {
    "context": "000605",
    "code": "0x45",
    "developer_comment": "恵那の父",
    "string": "そして――"
  },
  "000608": {
    "context": "000608",
    "code": "0x45",
    "developer_comment": "恵那の父",
    "string": "放火犯に遭遇して、刺殺された者が１名。"
  },
  "000651": {
    "context": "000651",
    "code": "0x85",
    "developer_comment": "恵那の父",
    "string": "　%N"
  },
  "000654": {
    "context": "000654",
    "code": "0x45",
    "developer_comment": "恵那の父",
    "string": "――刺殺されたのは、平凡な公務員だった。"
  },
  "000658": {
    "context": "000658",
    "code": "0x85",
    "developer_comment": "恵那の父",
    "string": "　%N"
  },
  "000661": {
    "context": "000661",
    "code": "0x45",
    "developer_comment": "恵那の父",
    "string": "彼を知らぬ者たちにとっては、"
  },
  "000664": {
    "context": "000664",
    "code": "0x45",
    "developer_comment": "恵那の父",
    "string": "その夜に命を落とした１１８名のうちの１人に過ぎない。"
  },
  "000668": {
    "context": "000668",
    "code": "0x85",
    "developer_comment": "恵那の父",
    "string": "　%N"
  },
  "000671": {
    "context": "000671",
    "code": "0x45",
    "developer_comment": "恵那の父",
    "string": "だが恵那にとっては違う。"
  },
  "000674": {
    "context": "000674",
    "code": "0x45",
    "developer_comment": "恵那の父",
    "string": "彼女にとって、彼は――かけがえのない、最愛の父だった。"
  },
  "000794": {
    "context": "000794",
    "code": "0x45",
    "developer_comment": "恵那の父",
    "string": "火災から数日後。"
  },
  "000799": {
    "context": "000799",
    "code": "0x45",
    "developer_comment": "恵那の父",
    "string": "恵那は父の職場を、１人で訪れていた。"
  },
  "000807": {
    "context": "000807",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "「お父さん……」"
  },
  "000812": {
    "context": "000812",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "そう呼びかけても、答えはない。"
  },
  "000817": {
    "context": "000817",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "あの優しい声を聞く事は、もう２度とないのだ。"
  },
  "000822": {
    "context": "000822",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "葬儀の日からたびたび流れていた涙が、またも頬を伝った。"
  },
  "000865": {
    "context": "000865",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "父はあの日、放火犯から誰かを庇って殺されたのだという。"
  },
  "000870": {
    "context": "000870",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "その誰かが誰なのかは、恵那には知らされていない。"
  },
  "000875": {
    "context": "000875",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "だが知らされずとも、彼女にはわかった。"
  },
  "000881": {
    "context": "000881",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "（お父さんはきっと、ちょうのうりょくしゃをまもろうとして……）"
  },
  "000887": {
    "context": "000887",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "（わるいひとたちに……！）"
  },
  "000931": {
    "context": "000931",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "恵那は空を見上げて想う。"
  },
  "000936": {
    "context": "000936",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "父が護ろうとしたものを、自分が護ってみせようと。"
  },
  "000941": {
    "context": "000941",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "いつか父と同じ仕事に就こうと――。"
  },
  "000949": {
    "context": "000949",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "「お父さん……やくそくまもるからね」"
  },
  "000957": {
    "context": "000957",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "「それまでてんごくから、えなをみててね」"
  },
  "000962": {
    "context": "000962",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "幼い彼女には、まだ大きすぎる望み。"
  },
  "000967": {
    "context": "000967",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "だが確かにそれを胸に留め、恵那は涙を拭った。"
  }
}