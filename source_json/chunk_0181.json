{
  "000040": {
    "context": "000040",
    "code": "0x55",
    "developer_comment": "",
    "string": "√Before Chapter5"
  },
  "000067": {
    "context": "000067",
    "code": "0x45",
    "developer_comment": "",
    "string": "――鹿鳴学園のような特科学校は、第２・第４土曜日も、フルタイムで授業がある。"
  },
  "000072": {
    "context": "000072",
    "code": "0x45",
    "developer_comment": "",
    "string": "夏彦は午後の授業のために、購買でパンを買い、教室でそれを食べた。"
  },
  "000077": {
    "context": "000077",
    "code": "0x45",
    "developer_comment": "",
    "string": "サリュと２人で、ぼそぼそと。"
  },
  "000083": {
    "context": "000083",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「……」"
  },
  "000107": {
    "context": "000107",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「……」"
  },
  "000118": {
    "context": "000118",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「……カツサンド、うまい？」"
  },
  "000147": {
    "context": "000147",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「普通」"
  },
  "000158": {
    "context": "000158",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「そうか……」"
  },
  "000163": {
    "context": "000163",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "ムードメーカーであるましろがいないと、会話が盛り上がらないようだ。"
  },
  "000169": {
    "context": "000169",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "（まさかこんな形で、ましろのありがたみを知る事になるとは……）"
  },
  "000174": {
    "context": "000174",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "夏彦がそう思った時――"
  },
  "000186": {
    "context": "000186",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "教室の壁に備え付けられたスピーカーから、校内放送を告げる音が鳴った。"
  },
  "000194": {
    "context": "000194",
    "code": "0x45",
    "developer_comment": "男性教師",
    "string": "「……き、緊急放送です」"
  },
  "000202": {
    "context": "000202",
    "code": "0x45",
    "developer_comment": "男性教師",
    "string": "「先ほど、この校舎内に、危険物が仕掛けられたとの通報がありました……！」"
  },
  "000210": {
    "context": "000210",
    "code": "0x45",
    "developer_comment": "男性教師",
    "string": "「生徒の皆様は、落ち着いて教員の指導に従い、ただちに校庭に避難してください」"
  },
  "000221": {
    "context": "000221",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「――！？」"
  },
  "000226": {
    "context": "000226",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "夏彦は息を呑む。"
  },
  "000231": {
    "context": "000231",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "同時にクラス全体に動揺が走った。"
  },
  "000251": {
    "context": "000251",
    "code": "0x45",
    "developer_comment": "男子生徒１",
    "string": "「危険物！？　マジかよ？」"
  },
  "000259": {
    "context": "000259",
    "code": "0x45",
    "developer_comment": "男子生徒２",
    "string": "「お、おいこれ……ニュースでやってた、一連の騒ぎと関係あんじゃねぇの……？」"
  },
  "000267": {
    "context": "000267",
    "code": "0x45",
    "developer_comment": "女子生徒１",
    "string": "「やだ、何それ！　なんでうちの学校が狙われるの！？」"
  },
  "000272": {
    "context": "000272",
    "code": "0x45",
    "developer_comment": "女子生徒１",
    "string": "どよめきがさらに生徒たちの不安を煽り、教室がにわかに騒がしくなる。"
  },
  "000283": {
    "context": "000283",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「危険物……だって？　一体何が……！」"
  },
  "000312": {
    "context": "000312",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「全校生徒を避難させなければならないという事は、ガスもしくは爆弾だと思う」"
  },
  "000323": {
    "context": "000323",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「ばっ……！」"
  },
  "000328": {
    "context": "000328",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "夏彦は言葉を失った。"
  },
  "000334": {
    "context": "000334",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "（まさか……ましろのヨタ話が、本当になったっていうのか？）"
  },
  "000339": {
    "context": "000339",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "そう思った時、恵那が教室に駆け込んできた。"
  },
  "000448": {
    "context": "000448",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "「みんな、今の放送聴いたわね！？」"
  },
  "000453": {
    "context": "000453",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "いつもとは全く違う力強い声、凛とした表情だった。"
  },
  "000482": {
    "context": "000482",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "「長い人生、今だけでいいから落ち着いて！　まず全員、一度自分の席に着席して！」"
  },
  "000561": {
    "context": "000561",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "生徒たちは言われるままに、自分の席に座る。"
  },
  "000566": {
    "context": "000566",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "恵那はざっと教室を見回し、空席を確認した。"
  },
  "000688": {
    "context": "000688",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "「……うん、鳥羽さん以外はみんな揃ってるわね」"
  },
  "000717": {
    "context": "000717",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "「それじゃ避難を始めます。出席番号順に並んで、私についてきて！」"
  },
  "000795": {
    "context": "000795",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "生徒たちがざわつきながら、おずおずと立ち上がる。"
  },
  "000801": {
    "context": "000801",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "（少なくとも危険物の件は、冗談じゃないみたいだな……）"
  },
  "000807": {
    "context": "000807",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "（じゃあ仮にテロリストなんてものも本当にいて……それを探していたましろと遭遇したら……？）"
  },
  "000813": {
    "context": "000813",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "（バカな、そんな偶然あるわけないだろ！）"
  },
  "000818": {
    "context": "000818",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "だがその考えと裏腹に、不安が膨らんでくる。"
  },
  "000823": {
    "context": "000823",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "夏彦は思わず足を止めた。"
  },
  "000867": {
    "context": "000867",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "「……？　どうしたの天川くん、避難の列に並んで」"
  },
  "000878": {
    "context": "000878",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「は、はい……」"
  },
  "000883": {
    "context": "000883",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "そう答えかけた時――夏彦の耳に、声が響いた。"
  },
  "000941": {
    "context": "000941",
    "code": "0x85",
    "developer_comment": "夏彦",
    "string": "《それじゃひこりん、後悔するかもしれないよ？》"
  },
  "000942": {
    "context": "000942",
    "code": "0x86",
    "developer_comment": "夏彦",
    "string": "悠里"
  },
  "000943": {
    "context": "000943",
    "code": "0x86",
    "developer_comment": "夏彦",
    "string": "《それじゃひこりん、後悔するかもしれないよ？》"
  },
  "000971": {
    "context": "000971",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "その言葉に、夏彦は息を呑んだ。"
  },
  "000976": {
    "context": "000976",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "今朝聞いた、悠里の声。"
  },
  "000981": {
    "context": "000981",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "そして一瞬後、夏彦の口から――"
  },
  "000992": {
    "context": "000992",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「……いえ、先生」"
  },
  "000997": {
    "context": "000997",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "思いもよらない言葉が溢れた。"
  },
  "001008": {
    "context": "001008",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「すみません、早退してもいいですか？」"
  },
  "001052": {
    "context": "001052",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "「え！？」"
  },
  "001057": {
    "context": "001057",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "恵那が、そしてサリュが目を見開いた。"
  },
  "001068": {
    "context": "001068",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「夏彦？」"
  },
  "001097": {
    "context": "001097",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "「な、何を言ってるの！？　いいからまず避難しましょう！」"
  },
  "001108": {
    "context": "001108",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「その……確かに、それもそうなんですが……」"
  },
  "001119": {
    "context": "001119",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「……ましろの安否が気になるんです」"
  },
  "001124": {
    "context": "001124",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "それはいつもの夏彦としては、ありえない言葉だった。"
  },
  "001135": {
    "context": "001135",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「本当すみません、行きます」"
  },
  "001164": {
    "context": "001164",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "「え、あ！　ちょっと！」"
  },
  "001169": {
    "context": "001169",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "恵那の声が聞こえたが、構ってはいられない。"
  },
  "001174": {
    "context": "001174",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "ましろを探すため、夏彦は教室を飛び出した。"
  },
  "001329": {
    "context": "001329",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "――外に出ると、雨足は多少弱まっていた。"
  },
  "001334": {
    "context": "001334",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "これなら傘がなくても、外を探し回れそうだ。"
  },
  "001393": {
    "context": "001393",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "だが、ましろがどこにいるかは全く解らない。"
  },
  "001398": {
    "context": "001398",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "とりあえずＰＤＡを取り出し、彼女に電話をかけてみたが――"
  },
  "001409": {
    "context": "001409",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「くそっ、繋がらない！」"
  },
  "001414": {
    "context": "001414",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "呼び出しはかかっているが、ましろは出なかった。"
  },
  "001420": {
    "context": "001420",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "（昨日の事を怒っているのか？）"
  },
  "001425": {
    "context": "001425",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "それならまだいいのだが、彼女の身に何かが起きて、『電話に出られない』という可能性もないとは言えない。"
  },
  "001431": {
    "context": "001431",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "（しかたない、足で探すしか……）"
  },
  "001436": {
    "context": "001436",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "そう思った時、背後でサリュの声が聞こえた。"
  },
  "001447": {
    "context": "001447",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「夏彦、何をしようとしてるの？」"
  },
  "001571": {
    "context": "001571",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「え？　なんだ、ついてきたのか？」"
  },
  "001600": {
    "context": "001600",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「夏彦が動いたから。それよりどうしておとなしく避難しないの？」"
  },
  "001611": {
    "context": "001611",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「決まっているさ。ましろを探すんだ」"
  },
  "001640": {
    "context": "001640",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「ましろを？　なぜ？」"
  },
  "001651": {
    "context": "001651",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「実は昨夜、あいつがこんな事を――」"
  },
  "001656": {
    "context": "001656",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "夏彦は簡単に、昨夜の事を説明した。"
  },
  "001685": {
    "context": "001685",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「――テロリスト。そんな事が……」"
  },
  "001696": {
    "context": "001696",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「まぁ何かの勘違いだとは思うけど、一応ね」"
  },
  "001725": {
    "context": "001725",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「でももし本当だとしたら、危険な事も起こりうる。ならば私は夏彦を護らなければならない」"
  },
  "001736": {
    "context": "001736",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「そうか……」"
  },
  "001741": {
    "context": "001741",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "相変わらず彼女の言う事はよくわからなかったが、議論している暇はない。"
  },
  "001752": {
    "context": "001752",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「わかった、じゃあ一緒に探してくれ」"
  },
  "001781": {
    "context": "001781",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「了解。ましろの行く先にアテは？」"
  },
  "001792": {
    "context": "001792",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「ないよ、でもとにかく探さなきゃ」"
  },
  "001797": {
    "context": "001797",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "夏彦たちはそう言って、雨の中を駆け出した。"
  }
}