{
  "000072": {
    "context": "000072",
    "code": "0x55",
    "string": "√Before Chapter2"
  },
  "000086": {
    "context": "000086",
    "code": "0x45",
    "developer_comment": "",
    "string": "――もう１０年も前になる、あの日。"
  },
  "000091": {
    "context": "000091",
    "code": "0x45",
    "developer_comment": "",
    "string": "その日に限って、ましろを初めとした遊び友達は、みんな都合が悪かった。"
  },
  "000102": {
    "context": "000102",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「ちぇっ、せっかくにちようびだってのに……」"
  },
  "000107": {
    "context": "000107",
    "code": "0x45",
    "developer_comment": "",
    "string": "つまらないという気持ちを抱え、夏彦は公園に１人で遊びにいった。"
  },
  "000112": {
    "context": "000112",
    "code": "0x45",
    "developer_comment": "",
    "string": "そこで同じく、１人でいる悠里と会ったのだ。"
  },
  "000202": {
    "context": "000202",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「……うぅ～……」"
  },
  "000207": {
    "context": "000207",
    "code": "0x45",
    "developer_comment": "",
    "string": "なぜか悠里は、ぽつんと独りで泣いていた。"
  },
  "000212": {
    "context": "000212",
    "code": "0x45",
    "developer_comment": "",
    "string": "ほとんど声を出さず、だけどすごく悲しそうに。"
  },
  "000217": {
    "context": "000217",
    "code": "0x45",
    "developer_comment": "",
    "string": "それを見た夏彦は、気になって悠里に尋ねた。"
  },
  "000273": {
    "context": "000273",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「なぁなぁ、なんで泣いてるの？」"
  },
  "000302": {
    "context": "000302",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「ふぇ……？」"
  },
  "000307": {
    "context": "000307",
    "code": "0x45",
    "developer_comment": "",
    "string": "それが、夏彦と悠里の初めての会話だった。"
  },
  "000336": {
    "context": "000336",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「……あ、あの……だれ？」"
  },
  "000347": {
    "context": "000347",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「ぼく、夏彦」"
  },
  "000376": {
    "context": "000376",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「……なつひこくん？」"
  },
  "000387": {
    "context": "000387",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「おまえ、『ことのゆうり』だろ？　ちょっとまえに、ちかくにひっこしてきた」"
  },
  "000416": {
    "context": "000416",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「……うん」"
  },
  "000427": {
    "context": "000427",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「ともだちになろうよ。せっかく、ごきんじょなんだからさ」"
  },
  "000456": {
    "context": "000456",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「……いいの？」"
  },
  "000467": {
    "context": "000467",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「へ？　ともだちになっちゃダメなのか？」"
  },
  "000496": {
    "context": "000496",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「だってわたし、ちょうのうりょくしゃだから……」"
  },
  "000507": {
    "context": "000507",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「ちょうのうりょくしゃ？　あぁ、コミュニケーターってこと？」"
  },
  "000536": {
    "context": "000536",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「う、うん……」"
  },
  "000547": {
    "context": "000547",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「ふぅん、それで？」"
  },
  "000576": {
    "context": "000576",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「えっ……？」"
  },
  "000605": {
    "context": "000605",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「ひとのきもちとかよめちゃうかもしれないよ？　こわくないの……？」"
  },
  "000616": {
    "context": "000616",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「べつにそんなことないよ。なーなー、いっしょにあそぼうぜ」"
  },
  "000645": {
    "context": "000645",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「あ……」"
  },
  "000674": {
    "context": "000674",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「ううん……ありがとう……」"
  },
  "000685": {
    "context": "000685",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「へへっ」"
  },
  "000754": {
    "context": "000754",
    "code": "0x45",
    "developer_comment": "",
    "string": "そうして２人は一緒に遊び始めた。"
  },
  "000759": {
    "context": "000759",
    "code": "0x45",
    "developer_comment": "",
    "string": "――もっとも２人でできる遊びなど、あまり思いつかなかった。"
  },
  "000765": {
    "context": "000765",
    "code": "0x45",
    "developer_comment": "",
    "string": "『ケンケンパ』や『Ｓケン』といった、シンプルな遊びをする２人。"
  },
  "000770": {
    "context": "000770",
    "code": "0x45",
    "developer_comment": "",
    "string": "だが芝生の上で飛び跳ねているうち、悠里の顔には笑顔が浮かぶようになった。"
  },
  "000860": {
    "context": "000860",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「ふぅ～……なかなかやるな、ゆうり」"
  },
  "000871": {
    "context": "000871",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「そんなことないよー、夏彦くんのほうがケンケンパもＳケンも、ずっとうまいよ」"
  },
  "000882": {
    "context": "000882",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「でもさすがにちょっとくたびれた。きゅうけいしよう」"
  },
  "000911": {
    "context": "000911",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「うん！」"
  },
  "000952": {
    "context": "000952",
    "code": "0x45",
    "developer_comment": "",
    "string": "２人は並んで芝生に腰を下ろした。"
  },
  "001037": {
    "context": "001037",
    "code": "0x45",
    "developer_comment": "",
    "string": "夏彦は大きく息をつきながら、悠里に尋ねる。"
  },
  "001048": {
    "context": "001048",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「ところでゆうりはさ、なんでこのまちにひっこしてきたの？」"
  },
  "001077": {
    "context": "001077",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「うーん……おとうさんが、そのほうがわたしのためだって……」"
  },
  "001088": {
    "context": "001088",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「え？」"
  },
  "001117": {
    "context": "001117",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「『ろくめいしは、ちょうのうりょくしゃがおおいから』って」"
  },
  "001128": {
    "context": "001128",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「『ここならわたしも、きみわるがられないはず』って」"
  },
  "001133": {
    "context": "001133",
    "code": "0x45",
    "developer_comment": "",
    "string": "そう言われて、夏彦も思い出した。"
  },
  "001138": {
    "context": "001138",
    "code": "0x45",
    "developer_comment": "",
    "string": "この街はコミュニケーターが、他の街よりずっと多いと、母が言っていた事を。"
  },
  "001149": {
    "context": "001149",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「そうだね。ぼくも『てきせい』があるらしいし」"
  },
  "001178": {
    "context": "001178",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「えっ？　な、夏彦くんもコミュニケーターなの？」"
  },
  "001189": {
    "context": "001189",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「うん、なんかそうみたい。テレパシーもまだつかえないけどね」"
  },
  "001218": {
    "context": "001218",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「そうなんだ……じつは、わたしも」"
  },
  "001229": {
    "context": "001229",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「あはっ、おんなじだ」"
  },
  "001252": {
    "context": "001252",
    "code": "0x45",
    "developer_comment": "",
    "string": "夏彦が笑うと、悠里も笑みを返す。"
  },
  "001263": {
    "context": "001263",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「それにね、ぼくだけじゃないよ。しりあいのましろってやつも、てきせいが高いってじまんしてたし」"
  },
  "001274": {
    "context": "001274",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「このまちはコミュニケーターがいっぱいだよ。いてあたりまえってくらい」"
  },
  "001285": {
    "context": "001285",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「だからだれも、悠里をきみわるがったりしないよ？」"
  },
  "001314": {
    "context": "001314",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「夏彦くんも？」"
  },
  "001325": {
    "context": "001325",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「もちろん」"
  },
  "001354": {
    "context": "001354",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「ぜったい？」"
  },
  "001365": {
    "context": "001365",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「ぜったいだって。ゆうりはしんぱいしょーだなぁ」"
  },
  "001388": {
    "context": "001388",
    "code": "0x45",
    "developer_comment": "",
    "string": "夏彦が笑うと、悠里もにっこり微笑んだ。"
  },
  "001417": {
    "context": "001417",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「よかったぁ～……わたし、ほんとはね、すごくふあんだったの」"
  },
  "001428": {
    "context": "001428",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「え、なんで？」"
  },
  "001457": {
    "context": "001457",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「おひっこししてから、ともだちできるかなって。なんかしんじられなかったから」"
  },
  "001468": {
    "context": "001468",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「でも、もうできたよね？」"
  },
  "001497": {
    "context": "001497",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「うん！」"
  },
  "001508": {
    "context": "001508",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「このこうえんには、こどもがいっぱいあつまるからさ。ここにかよえば、ほかのともだちもできるよ」"
  },
  "001537": {
    "context": "001537",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「ありがとう。わたしいっぱいかようね」"
  },
  "001566": {
    "context": "001566",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「はじめてきたのに、夏彦くんとともだちになれたし……」"
  },
  "001571": {
    "context": "001571",
    "code": "0x45",
    "developer_comment": "",
    "string": "そこまで言ってから、悠里はふと何かに気づいたような顔をした。"
  },
  "001600": {
    "context": "001600",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「あっ、そういえば……」"
  },
  "001611": {
    "context": "001611",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「ん？」"
  },
  "001640": {
    "context": "001640",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「なんで夏彦くんは、わたしにこえかけてくれたの？」"
  },
  "001669": {
    "context": "001669",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「みたことない子なのに……それも、おんなの子なのに」"
  },
  "001680": {
    "context": "001680",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「え～？　う～ん、それは……」"
  },
  "001709": {
    "context": "001709",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「それは……？」"
  },
  "001720": {
    "context": "001720",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「なんとなく！」"
  },
  "001749": {
    "context": "001749",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「え～？　なんとなくかぁ」"
  },
  "001754": {
    "context": "001754",
    "code": "0x45",
    "developer_comment": "",
    "string": "悠里がちょっと残念そうに言う。"
  },
  "001821": {
    "context": "001821",
    "code": "0x47",
    "developer_comment": "",
    "string": "だが――それは嘘だった。"
  },
  "001828": {
    "context": "001828",
    "code": "0x47",
    "developer_comment": "",
    "string": "本当の理由は、悠里が泣いていたからだ。"
  },
  "001844": {
    "context": "001844",
    "code": "0x47",
    "developer_comment": "",
    "string": "だから夏彦は声をかけた。"
  },
  "001851": {
    "context": "001851",
    "code": "0x47",
    "developer_comment": "",
    "string": "彼女と一緒にいてあげたいと思って――"
  }
}
