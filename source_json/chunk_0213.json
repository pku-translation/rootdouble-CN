{
  "000007": {
    "context": "000007",
    "code": "0x55",
    "developer_comment": "",
    "string": "√Before Chapter6"
  },
  "000023": {
    "context": "000023",
    "code": "0x45",
    "developer_comment": "",
    "string": "ラボに続く道は、１種類しかない。"
  },
  "000028": {
    "context": "000028",
    "code": "0x45",
    "developer_comment": "",
    "string": "『ラボ専用道路』。その入口には検問があり、そこを通る車は全てチェックされる。"
  },
  "000033": {
    "context": "000033",
    "code": "0x45",
    "developer_comment": "",
    "string": "原子炉を擁する施設なのだ。それも当然の警備体制だった。"
  },
  "000071": {
    "context": "000071",
    "code": "0x45",
    "developer_comment": "",
    "string": "そしてその入口で――"
  },
  "000076": {
    "context": "000076",
    "code": "0x45",
    "developer_comment": "",
    "string": "夏彦たちも、チェックに引っかかってしまった。"
  },
  "000123": {
    "context": "000123",
    "code": "0x45",
    "developer_comment": "監視員",
    "string": "「む！？　誰だ君たちは！？」"
  },
  "000128": {
    "context": "000128",
    "code": "0x45",
    "developer_comment": "監視員",
    "string": "検問を通ろうとした時、厳しい顔をした監視員に引き止められた。"
  },
  "000139": {
    "context": "000139",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「ラボに行きたいんです！　通してください！」"
  },
  "000147": {
    "context": "000147",
    "code": "0x45",
    "developer_comment": "監視員",
    "string": "「何を言ってるんだ、君は？　ダメに決まってるだろう」"
  },
  "000155": {
    "context": "000155",
    "code": "0x45",
    "developer_comment": "監視員",
    "string": "「だいたい、行くってどうやって行くんだ。ここからラボまで１０ｋｍ近くあるぞ？」"
  },
  "000163": {
    "context": "000163",
    "code": "0x45",
    "developer_comment": "監視員",
    "string": "「バカなこと言ってないで、帰った帰った」"
  },
  "000199": {
    "context": "000199",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「で、でも私たち、ここに勤めている天川博士の知り合いなんですけど……」"
  },
  "000207": {
    "context": "000207",
    "code": "0x45",
    "developer_comment": "監視員",
    "string": "「そんな事は関係ない。この検問はいかなる人間であろうと、正当な理由無しに通すわけにはいかん」"
  },
  "000215": {
    "context": "000215",
    "code": "0x45",
    "developer_comment": "監視員",
    "string": "「ラボに行きたいのであれば、鹿鳴市保安課もしくはラボの渉外担当者を通じ、許可を取りたまえ」"
  },
  "000226": {
    "context": "000226",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「許可？　それはどのくらいかかるんですか？」"
  },
  "000234": {
    "context": "000234",
    "code": "0x45",
    "developer_comment": "監視員",
    "string": "「３日から１週間ほどかかるだろう」"
  },
  "000242": {
    "context": "000242",
    "code": "0x45",
    "developer_comment": "監視員",
    "string": "「まぁ単なる学生が申請しても、通るわけもないがね」"
  },
  "000271": {
    "context": "000271",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「そ、そんな……！」"
  },
  "000282": {
    "context": "000282",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「ラボでテロが起きるかもしれないんですよ！？」"
  },
  "000290": {
    "context": "000290",
    "code": "0x45",
    "developer_comment": "監視員",
    "string": "「テロ？　あぁ、近頃市街を騒がせているあれか」"
  },
  "000298": {
    "context": "000298",
    "code": "0x45",
    "developer_comment": "監視員",
    "string": "「それについては我々も警戒している。充分すぎるほどね」"
  },
  "000327": {
    "context": "000327",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「違うんです！　それだけじゃ足りないんです！」"
  },
  "000363": {
    "context": "000363",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「ラボにはテロリストの内通者がいる可能性が高い。その事については考えてある？」"
  },
  "000371": {
    "context": "000371",
    "code": "0x45",
    "developer_comment": "監視員",
    "string": "「内通者？　何をわけのわからないことを言っているんだ？」"
  },
  "000379": {
    "context": "000379",
    "code": "0x45",
    "developer_comment": "監視員",
    "string": "「もういいからはやく帰れ。そうしなければ警察に通報するぞ」"
  },
  "000384": {
    "context": "000384",
    "code": "0x45",
    "developer_comment": "監視員",
    "string": "そう言って監視員は、電話の受話器を手に取った。"
  },
  "000390": {
    "context": "000390",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "（警察……！）"
  },
  "000395": {
    "context": "000395",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "保安課からの疑いは晴れたものの、警察を呼ばれたら、また面倒な事になりかねない。"
  },
  "000400": {
    "context": "000400",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "サリュがそっと夏彦に耳打ちする。"
  },
  "000429": {
    "context": "000429",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「帰ろう２人とも。これ以上騒ぎ立てるのは得策じゃない」"
  },
  "000458": {
    "context": "000458",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「で、でも……！」"
  },
  "000469": {
    "context": "000469",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「今やるべきは、あくまでテロの阻止。警察や保安課を敵に回すべきじゃない」"
  },
  "000480": {
    "context": "000480",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「……！」"
  },
  "000485": {
    "context": "000485",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "そこまで言われて、夏彦たちはようやく納得した。"
  },
  "000496": {
    "context": "000496",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「……わかったよ、別の方策を考えよう」"
  },
  "000525": {
    "context": "000525",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「それがいい」"
  },
  "000554": {
    "context": "000554",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「……失礼しました」"
  },
  "000562": {
    "context": "000562",
    "code": "0x45",
    "developer_comment": "監視員",
    "string": "「ふんっ。間違ってももう来ないようにな」"
  },
  "000610": {
    "context": "000610",
    "code": "0x45",
    "developer_comment": "監視員",
    "string": "その声を背に、夏彦たちはその場を去った。"
  },
  "000639": {
    "context": "000639",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「まずいよ……どうする？」"
  },
  "000650": {
    "context": "000650",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「しかたない……まずは公衆電話を探そう」"
  },
  "000661": {
    "context": "000661",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「警察や保安課、それからラボ関係者に連絡して、注意を促すんだ」"
  },
  "000666": {
    "context": "000666",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "夏彦たちはうなずき、また歩き出した。"
  },
  "000716": {
    "context": "000716",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "――街道を歩くうち、すぐに２つ並んだ公衆電話が見つかった。"
  },
  "000721": {
    "context": "000721",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "さっそく夏彦は保安課の番号を調べ、電話をかけた。"
  },
  "000746": {
    "context": "000746",
    "code": "0x45",
    "developer_comment": "保安課員",
    "string": "「はい、鹿鳴市役所保安課、武橋が承ります」"
  },
  "000757": {
    "context": "000757",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「さきほどはお世話になりました。鹿鳴学園生徒の天川と申しますが」"
  },
  "000765": {
    "context": "000765",
    "code": "0x45",
    "developer_comment": "保安課員",
    "string": "「……なんだ、ハウスキーパー君か。どうしたね？」"
  },
  "000776": {
    "context": "000776",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「いや、実は……」"
  },
  "000781": {
    "context": "000781",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "夏彦はかいつまんで、保安課員にサリュの予想を伝えた。"
  },
  "000786": {
    "context": "000786",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "しかし――"
  },
  "000797": {
    "context": "000797",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「つまり、テロリストがラボを狙ってる可能性がですね……」"
  },
  "000805": {
    "context": "000805",
    "code": "0x45",
    "developer_comment": "保安課員",
    "string": "「あぁ、それはもうわかった。ご協力ありがとう」"
  },
  "000810": {
    "context": "000810",
    "code": "0x45",
    "developer_comment": "保安課員",
    "string": "電話の向こうの保安課員の声には、いま一つ真剣味が感じられなかった。"
  },
  "000818": {
    "context": "000818",
    "code": "0x45",
    "developer_comment": "保安課員",
    "string": "「ラボの方には注意を促しておくから、安心してくれ」"
  },
  "000829": {
    "context": "000829",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「いや、それだけじゃ不充分な気が……警備を強化するとかできないんですか？」"
  },
  "000837": {
    "context": "000837",
    "code": "0x45",
    "developer_comment": "保安課員",
    "string": "「しかし、そのために他の各施設の警備を薄くするわけにもいかないんだ」"
  },
  "000848": {
    "context": "000848",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「それも解るんですけど、そこをなんとか……」"
  },
  "000856": {
    "context": "000856",
    "code": "0x45",
    "developer_comment": "保安課員",
    "string": "「うちも可能性だけじゃ動けないんだよ。それこそ怪文書でも届いていればいいんだが」"
  },
  "000862": {
    "context": "000862",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「……」"
  },
  "000870": {
    "context": "000870",
    "code": "0x45",
    "developer_comment": "保安課員",
    "string": "「ともかく、市内の警戒態勢を強化しておくよ」"
  },
  "000881": {
    "context": "000881",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「わかりました……じゃあお願いします……」"
  },
  "000930": {
    "context": "000930",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "夏彦は肩を落とし、電話を切った。"
  },
  "000995": {
    "context": "000995",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「どうだった？」"
  },
  "001006": {
    "context": "001006",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「一応、話は聞いてくれたけど……そっちは？」"
  },
  "001011": {
    "context": "001011",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "夏彦が保安課に通報している間、ましろは警察に連絡していた。"
  },
  "001016": {
    "context": "001016",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "しかし彼女も、沈んだ表情で首を振る。"
  },
  "001045": {
    "context": "001045",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「だめだよ全然。『警備は充分に行っています』の一点張り」"
  },
  "001056": {
    "context": "001056",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「話が大きすぎて、信用してもらえないのか……！」"
  },
  "001086": {
    "context": "001086",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「……どうする？」"
  },
  "001092": {
    "context": "001092",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "サリュがそう呟いた時――"
  },
  "001097": {
    "context": "001097",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "遠くから、足音が近づいてくるのが聞こえた。"
  }
}