{
  "000061": {
    "context": "000061",
    "code": "0x45",
    "developer_comment": "",
    "string": "――それは約３年前のある日。"
  },
  "000066": {
    "context": "000066",
    "code": "0x45",
    "developer_comment": "",
    "string": "洵は、風見の様子に違和感を持ち始めていた。"
  },
  "000151": {
    "context": "000151",
    "code": "0x45",
    "developer_comment": "",
    "string": "――その日、洵は風見と共に、街に遊びに出てきていた。"
  },
  "000156": {
    "context": "000156",
    "code": "0x45",
    "developer_comment": "",
    "string": "だがなぜか今日の風見は、どこか上の空なのだ。"
  },
  "000167": {
    "context": "000167",
    "code": "0x45",
    "developer_comment": "洵",
    "string": "「ねーお姉ちゃん、お昼なに食べよっか？」"
  },
  "000196": {
    "context": "000196",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「そうね……なにがいいかしらね」"
  },
  "000201": {
    "context": "000201",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "話しかけても、こうして生返事が返って来るばかりである。"
  },
  "000206": {
    "context": "000206",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "洵はいい加減焦れて、声を上げた。"
  },
  "000217": {
    "context": "000217",
    "code": "0x45",
    "developer_comment": "洵",
    "string": "「もう！　どうしたのお姉ちゃん！？」"
  },
  "000281": {
    "context": "000281",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「えっ？」"
  },
  "000292": {
    "context": "000292",
    "code": "0x45",
    "developer_comment": "洵",
    "string": "「あたし、なんかお姉ちゃん怒らせるようなことしたっけ？　言いたい事があったら言ってよ」"
  },
  "000321": {
    "context": "000321",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「あ、いや……別に怒ったりはしてないんだけど……」"
  },
  "000326": {
    "context": "000326",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "風見はおどおどと口ごもった後、ぽつりと呟いた。"
  },
  "000355": {
    "context": "000355",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「……実はちょっと、あなたに聞きたい事があって……」"
  },
  "000366": {
    "context": "000366",
    "code": "0x45",
    "developer_comment": "洵",
    "string": "「え、なに？」"
  },
  "000395": {
    "context": "000395",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「……洵、あなた……」"
  },
  "000406": {
    "context": "000406",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「男の人とお付き合いしたこと、ある？」"
  },
  "000432": {
    "context": "000432",
    "code": "0x45",
    "developer_comment": "洵",
    "string": "「えっ！？　な、なにいきなり！？」"
  },
  "000461": {
    "context": "000461",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「あ、えっと……あなたの姉的存在として、そう言う事も知っておいた方がいいのかなって……」"
  },
  "000472": {
    "context": "000472",
    "code": "0x45",
    "developer_comment": "洵",
    "string": "「いや、ないけど……」"
  },
  "000483": {
    "context": "000483",
    "code": "0x45",
    "developer_comment": "洵",
    "string": "「で、でも、あたしも別にモテないわけじゃないんだよ？」"
  },
  "000494": {
    "context": "000494",
    "code": "0x45",
    "developer_comment": "洵",
    "string": "「ラブレターもらった事もあるし、告られた事もあるし……」"
  },
  "000523": {
    "context": "000523",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「そう……」"
  },
  "000528": {
    "context": "000528",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "風見はそう言って、ため息をついた。"
  },
  "000534": {
    "context": "000534",
    "code": "0x45",
    "developer_comment": "洵",
    "string": "（な……なんだろう？）"
  },
  "000539": {
    "context": "000539",
    "code": "0x45",
    "developer_comment": "洵",
    "string": "風見の口から恋愛関連の話が出るなど、めったにない事だった。"
  },
  "000544": {
    "context": "000544",
    "code": "0x45",
    "developer_comment": "洵",
    "string": "雨でも降るんじゃないかと思っていると、風見が問いを重ねる。"
  },
  "000573": {
    "context": "000573",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「じゃあ、別の質問だけど……洵、あなた、クラシック音楽に詳しい？」"
  },
  "000584": {
    "context": "000584",
    "code": "0x45",
    "developer_comment": "洵",
    "string": "「へ？　クラシック？」"
  },
  "000613": {
    "context": "000613",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「もし詳しければ、色々と知識を伝授してほしいんだけど……」"
  },
  "000624": {
    "context": "000624",
    "code": "0x45",
    "developer_comment": "洵",
    "string": "「ごめん、全然わかんない。あたしロックしか聴かないし」"
  },
  "000653": {
    "context": "000653",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「……そう……」"
  },
  "000658": {
    "context": "000658",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "風見はまたも、深いため息をついた。"
  },
  "000663": {
    "context": "000663",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "洵は中学に上がったあたりから、父親に反発する事が更に多くなっていた。"
  },
  "000668": {
    "context": "000668",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "そして父の趣味は、クラシック鑑賞である。"
  },
  "000674": {
    "context": "000674",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "『坊主憎けりゃ袈裟まで憎し』で、洵は父と共にクラシックをも嫌い、その反動でロックに傾倒したのだ。"
  },
  "000680": {
    "context": "000680",
    "code": "0x45",
    "developer_comment": "洵",
    "string": "（でもなんでお姉ちゃん、いきなり恋愛とかクラシックの話を？　今まで全く興味なかったのに――）"
  },
  "000685": {
    "context": "000685",
    "code": "0x45",
    "developer_comment": "洵",
    "string": "そこまで考えて、洵ははっと息を呑んだ。"
  },
  "000696": {
    "context": "000696",
    "code": "0x45",
    "developer_comment": "洵",
    "string": "「まさか、お姉ちゃん！　好きな男ができたんじゃ！？」"
  },
  "000760": {
    "context": "000760",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「えっ！？　な、なんでそんな事を！？」"
  },
  "000771": {
    "context": "000771",
    "code": "0x45",
    "developer_comment": "洵",
    "string": "「いや、好きな男がクラシック好きで、話を合わせるために急遽クラシック聴こうと思ったとか」"
  },
  "000835": {
    "context": "000835",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「ち、違うわ！　そんなのじゃなくて……」"
  },
  "000864": {
    "context": "000864",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「えっと、つまり……そう、リラクゼーション効果よ！」"
  },
  "000875": {
    "context": "000875",
    "code": "0x45",
    "developer_comment": "洵",
    "string": "「リラクゼーション効果ぁ？」"
  },
  "000904": {
    "context": "000904",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「クラシックには心の疲れを取る効果があるのよ、確か」"
  },
  "000933": {
    "context": "000933",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「私は毎日張り詰めた緊張感の中で仕事をしているから、クラシックを聴く事で、疲れを癒したいと思って……」"
  },
  "000944": {
    "context": "000944",
    "code": "0x45",
    "developer_comment": "洵",
    "string": "「ふぅ～ん……」"
  },
  "000949": {
    "context": "000949",
    "code": "0x45",
    "developer_comment": "洵",
    "string": "嘘っぽいとは思ったが、とりあえず追及はしない事にした。"
  },
  "000954": {
    "context": "000954",
    "code": "0x45",
    "developer_comment": "洵",
    "string": "代わりに釘を刺しておく。"
  },
  "000965": {
    "context": "000965",
    "code": "0x45",
    "developer_comment": "洵",
    "string": "「でもお姉ちゃん、これだけは言っておくけど――」"
  },
  "000976": {
    "context": "000976",
    "code": "0x45",
    "developer_comment": "洵",
    "string": "「クラシック聴くような男に、ロクな奴はいないよ？」"
  },
  "001040": {
    "context": "001040",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「な、なんですって！？」"
  },
  "001051": {
    "context": "001051",
    "code": "0x45",
    "developer_comment": "洵",
    "string": "「だってうちの父親がそうだもん。お姉ちゃんにも話したよね？　うちの父親がロクでもない奴だって」"
  },
  "001080": {
    "context": "001080",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「だ、だからってそれは、あまりに短絡的な考えだと思うけど……」"
  },
  "001091": {
    "context": "001091",
    "code": "0x45",
    "developer_comment": "洵",
    "string": "「いーや、音楽の趣味には人となりが表れるの」"
  },
  "001102": {
    "context": "001102",
    "code": "0x45",
    "developer_comment": "洵",
    "string": "「男を選ぶ時は、そういうのも踏まえて選ばなきゃだめだよ？」"
  },
  "001131": {
    "context": "001131",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「わ……わかったわ」"
  },
  "001136": {
    "context": "001136",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "神妙に答える風見を見て、洵は内心思った。"
  },
  "001142": {
    "context": "001142",
    "code": "0x45",
    "developer_comment": "洵",
    "string": "（これでよし……お姉ちゃんが変な男にひっかかったら、マズイからね）"
  },
  "001147": {
    "context": "001147",
    "code": "0x45",
    "developer_comment": "洵",
    "string": "風見にもし恋人ができたら、洵としては少し寂しい気もするが、むろん最終的には祝福するだろう。"
  },
  "001152": {
    "context": "001152",
    "code": "0x45",
    "developer_comment": "洵",
    "string": "しかし風見の恋人となる男は、洵が認める最高の男でなければならないとも思う。"
  },
  "001158": {
    "context": "001158",
    "code": "0x45",
    "developer_comment": "洵",
    "string": "（……あたしは凪沙の代わりに、お姉ちゃんを護るって決めたんだ）"
  },
  "001164": {
    "context": "001164",
    "code": "0x45",
    "developer_comment": "洵",
    "string": "（悪い虫から護るのも、その一環だよ）"
  },
  "001169": {
    "context": "001169",
    "code": "0x45",
    "developer_comment": "洵",
    "string": "洵は密かに、その決意を新たにした。"
  }
}