{
  "000066": {
    "context": "000066",
    "code": "0x45",
    "developer_comment": "",
    "string": "――悠里の死を知った後――"
  },
  "000071": {
    "context": "000071",
    "code": "0x45",
    "developer_comment": "",
    "string": "ましろは丸１日泣き続けた。"
  },
  "000076": {
    "context": "000076",
    "code": "0x45",
    "developer_comment": "",
    "string": "悠里の葬儀が行われ、それが終わり、さらに一週間が経っても――"
  },
  "000081": {
    "context": "000081",
    "code": "0x45",
    "developer_comment": "",
    "string": "彼女の事を思い出すたび、涙が滲んだ。"
  },
  "000160": {
    "context": "000160",
    "code": "0x45",
    "developer_comment": "",
    "string": "病院行きのバスを待ちながら、ましろはすすり泣いていた。"
  },
  "000168": {
    "context": "000168",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「う……ぅ……」"
  },
  "000219": {
    "context": "000219",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "どれだけ泣いても、親友を失った悲しみは癒えない。"
  },
  "000224": {
    "context": "000224",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "いつも穏やかに微笑んでいた悠里。"
  },
  "000229": {
    "context": "000229",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "誰からも女の子扱いされない自分を、『可愛い』と言ってくれた悠里。"
  },
  "000234": {
    "context": "000234",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "おままごとのようなましろの料理を、美味しいと褒めてくれた悠里。"
  },
  "000239": {
    "context": "000239",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "――本当に心優しく、誰よりも可愛らしい、ましろの憧れの女の子……。"
  },
  "000323": {
    "context": "000323",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "彼女との想い出が、脳裏に浮かんでは消える。"
  },
  "000329": {
    "context": "000329",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "（ねぇ、悠里……悠里とあったのは、ほんの１年くらいまえなんだよね）"
  },
  "000335": {
    "context": "000335",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "（はじめてあったときから、私は悠里にあこがれてたよ）"
  },
  "000341": {
    "context": "000341",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "（悠里といっしょにいれば、私もいつか、すてきな女の子になれるかとおもって……）"
  },
  "000347": {
    "context": "000347",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "（なのに……ほんの１年でおわかれになるなんて、おもわなかったね……！）"
  },
  "000355": {
    "context": "000355",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "その彼女がもう存在しないという事が、ましろにはまだ信じられなかった。"
  },
  "000360": {
    "context": "000360",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "だが悲しみに満たされたましろの心に、唯一残されたのは――"
  },
  "000365": {
    "context": "000365",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "夏彦を心配する気持ちだった。"
  },
  "000371": {
    "context": "000371",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "（……夏彦、だいじょうぶかな……）"
  },
  "000376": {
    "context": "000376",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "夏彦は、悠里の葬儀に出席していなかった。"
  },
  "000381": {
    "context": "000381",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "まだ退院できないらしい。学校にもずっと来ていない。"
  },
  "000387": {
    "context": "000387",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "（おみまいにいかなきゃ……！）"
  },
  "000398": {
    "context": "000398",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "ましろが涙を拭ったとき、バスがやってきた。"
  },
  "000465": {
    "context": "000465",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "――病院の前でバスを降りたましろは、受付を通り、夏彦の病室に向かった。"
  },
  "000533": {
    "context": "000533",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "病室では夏彦が、ベッドに横たわっていた。"
  },
  "000538": {
    "context": "000538",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "その傍らに、天川博士がいる。"
  },
  "000546": {
    "context": "000546",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「はかせ……」"
  },
  "000575": {
    "context": "000575",
    "code": "0x45",
    "developer_comment": "美夜子",
    "string": "「あ、ましろちゃん……」"
  },
  "000604": {
    "context": "000604",
    "code": "0x45",
    "developer_comment": "美夜子",
    "string": "「お見舞いに来てくれたのね？　わざわざありがとう」"
  },
  "000612": {
    "context": "000612",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「は、はい……」"
  },
  "000620": {
    "context": "000620",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「あの、夏彦くんは……おやすみちゅうですか……？」"
  },
  "000644": {
    "context": "000644",
    "code": "0x45",
    "developer_comment": "美夜子",
    "string": "「……」"
  },
  "000673": {
    "context": "000673",
    "code": "0x45",
    "developer_comment": "美夜子",
    "string": "「夏彦はね……」"
  },
  "000678": {
    "context": "000678",
    "code": "0x45",
    "developer_comment": "美夜子",
    "string": "博士はちらりと夏彦を見た。"
  },
  "000683": {
    "context": "000683",
    "code": "0x45",
    "developer_comment": "美夜子",
    "string": "ましろは夏彦の顔を覗きこみ――そして息を呑んだ。"
  },
  "000886": {
    "context": "000886",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「！！」"
  },
  "000891": {
    "context": "000891",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "夏彦は眠っていなかった。"
  },
  "000901": {
    "context": "000901",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "横たわったまま、目を開けていた。"
  },
  "000906": {
    "context": "000906",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "だがその目には光が無く、ただ虚空を見つめている。"
  },
  "000914": {
    "context": "000914",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「……夏彦？」"
  },
  "000919": {
    "context": "000919",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "返事なし。"
  },
  "000927": {
    "context": "000927",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「夏彦」"
  },
  "000932": {
    "context": "000932",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "返事なし。"
  },
  "000940": {
    "context": "000940",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「ね、ねぇ！　どうしちゃったの夏彦！」"
  },
  "000948": {
    "context": "000948",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「私のこえきこえないの！？　ねぇ！」"
  },
  "000953": {
    "context": "000953",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "戸惑いながら叫ぶましろに、天川博士が答える。"
  },
  "000961": {
    "context": "000961",
    "code": "0x45",
    "developer_comment": "美夜子",
    "string": "「……聞こえてないわ」"
  },
  "000969": {
    "context": "000969",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「え？」"
  },
  "000977": {
    "context": "000977",
    "code": "0x45",
    "developer_comment": "美夜子",
    "string": "「悠里ちゃんの死を知ってから、ずっとこうなの……」"
  },
  "000985": {
    "context": "000985",
    "code": "0x45",
    "developer_comment": "美夜子",
    "string": "「ましろちゃんだけじゃない。私の声も、医師の声も……誰の声も届かない」"
  },
  "000993": {
    "context": "000993",
    "code": "0x45",
    "developer_comment": "美夜子",
    "string": "「事実を認められず……心を閉ざしてしまったんだわ……」"
  },
  "001001": {
    "context": "001001",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「……そんな」"
  },
  "001046": {
    "context": "001046",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "ましろは呆然として、夏彦を見る。"
  },
  "001051": {
    "context": "001051",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "彼の視線は、どこか遠いところに向けられていた……。"
  },
  "001101": {
    "context": "001101",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "――ましろは信じられない思いのまま、病院を後にした。"
  },
  "001167": {
    "context": "001167",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "家路を歩く間、頭の中を様々な思考と感情が渦巻く。"
  },
  "001172": {
    "context": "001172",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "しかし、その中でただ１つはっきりしていた感情は――後悔だった。"
  },
  "001178": {
    "context": "001178",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "（……悠里は私のかわりにラボにいって、死んじゃったんだ……）"
  },
  "001184": {
    "context": "001184",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "（だから夏彦も、あんなふうに……）"
  },
  "001189": {
    "context": "001189",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "取り返しのつかない思いが、胸に募る。"
  },
  "001194": {
    "context": "001194",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "その後悔を晴らす方法は思いつかないが――"
  },
  "001200": {
    "context": "001200",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "（せめて、夏彦がもとにもどるまでは……！）"
  },
  "001205": {
    "context": "001205",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "精一杯見守り続けよう。"
  },
  "001210": {
    "context": "001210",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "悠里の代わりにでもいい。なんでも構わない。"
  },
  "001215": {
    "context": "001215",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "ましろは１人、そう誓った。"
  }
}