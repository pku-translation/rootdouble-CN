  },
  "005334": {
    "context": "005334",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「……さて、オレたちも行くか」"
  },
  "005378": {
    "context": "005378",
    "code": "0x45",
    "developer_comment": "宇喜多",
