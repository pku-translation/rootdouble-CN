{
  "000069": {
    "context": "000069",
    "code": "0x45",
    "developer_comment": "",
    "string": "――それは今から９年前。"
  },
  "000074": {
    "context": "000074",
    "code": "0x45",
    "developer_comment": "",
    "string": "風見が１９歳のときの記憶だった……。"
  },
  "000115": {
    "context": "000115",
    "code": "0x45",
    "developer_comment": "",
    "string": "春風が頬を撫で、芝生を波立たせる。"
  },
  "000120": {
    "context": "000120",
    "code": "0x45",
    "developer_comment": "",
    "string": "暖かな風の中に立ちながら、風見は傍らの少女に語りかけた。"
  },
  "000128": {
    "context": "000128",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「いい天気ね、凪沙」"
  },
  "000136": {
    "context": "000136",
    "code": "0x45",
    "developer_comment": "凪沙",
    "string": "「ね、お姉ちゃん」"
  },
  "000141": {
    "context": "000141",
    "code": "0x45",
    "developer_comment": "凪沙",
    "string": "そう答えたのは、風見の妹『凪沙』だ。"
  },
  "000146": {
    "context": "000146",
    "code": "0x45",
    "developer_comment": "凪沙",
    "string": "今日は風見の休日という事で、久しぶりに公園に遊びに来ていたのだ。"
  },
  "000151": {
    "context": "000151",
    "code": "0x45",
    "developer_comment": "凪沙",
    "string": "遠くで子供たちの遊ぶ声が聞こえる。"
  },
  "000195": {
    "context": "000195",
    "code": "0x45",
    "developer_comment": "少年",
    "string": "「あー、ましろ足うごいたー！　おまえのまけー」"
  },
  "000258": {
    "context": "000258",
    "code": "0x45",
    "developer_comment": "少女",
    "string": "「まってまって、ふいうちだよ！　もういっかいはじめからね！」"
  },
  "000287": {
    "context": "000287",
    "code": "0x45",
    "developer_comment": "少年",
    "string": "「ふふーん、なんどやってもおなじさ」"
  },
  "000316": {
    "context": "000316",
    "code": "0x45",
    "developer_comment": "少女",
    "string": "「なにをー！？」"
  },
  "000321": {
    "context": "000321",
    "code": "0x45",
    "developer_comment": "少女",
    "string": "――少年と少女が、尻相撲をやっているらしい。"
  },
  "000326": {
    "context": "000326",
    "code": "0x45",
    "developer_comment": "少女",
    "string": "少年の方には見覚えがある。恐らくこの辺りに住んでいる子供だろう。"
  },
  "000334": {
    "context": "000334",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「あなたと同じくらいの年の子ね。混ざってくる？」"
  },
  "000415": {
    "context": "000415",
    "code": "0x45",
    "developer_comment": "凪沙",
    "string": "「う、うぅん……あの遊び、ちょっとむずかしそう……」"
  },
  "000423": {
    "context": "000423",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「あ、いいのよ。激しい遊びはやめておいた方がいいものね」"
  },
  "000431": {
    "context": "000431",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「のんびりお散歩でもしましょ」"
  },
  "000460": {
    "context": "000460",
    "code": "0x45",
    "developer_comment": "凪沙",
    "string": "「うん！」"
  },
  "000465": {
    "context": "000465",
    "code": "0x45",
    "developer_comment": "凪沙",
    "string": "にっこり笑う凪沙の手を引いて、風見は歩き出した。"
  },
  "000555": {
    "context": "000555",
    "code": "0x45",
    "developer_comment": "凪沙",
    "string": "――凪沙は生まれつき、体が少し弱かった。"
  },
  "000560": {
    "context": "000560",
    "code": "0x45",
    "developer_comment": "凪沙",
    "string": "持病があるわけではないのだが、心肺機能が薄弱で、風邪なども引きやすい。"
  },
  "000565": {
    "context": "000565",
    "code": "0x45",
    "developer_comment": "凪沙",
    "string": "なのでこの公園に来るのも、久しぶりの事だった。"
  },
  "000571": {
    "context": "000571",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "（この子はお父さんたちの血を、色濃く引いてしまったのね……）"
  },
  "000576": {
    "context": "000576",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "風見たちには両親がいない。"
  },
  "000581": {
    "context": "000581",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "母を５年前に、父を３年前に、相次いで癌で失ってしまったのだ。"
  },
  "000586": {
    "context": "000586",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "今は風見が花屋で働いて、凪沙を養っている。"
  },
  "000591": {
    "context": "000591",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "だが互いにたった一人の家族であるが故、姉妹の絆はとても強かった。"
  },
  "000687": {
    "context": "000687",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「凪沙、もうすぐ小学校の入学式ね」"
  },
  "000716": {
    "context": "000716",
    "code": "0x45",
    "developer_comment": "凪沙",
    "string": "「うん。友達たくさんできるかな？」"
  },
  "000724": {
    "context": "000724",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「できるわよ、きっとあの学校ならね」"
  },
  "000753": {
    "context": "000753",
    "code": "0x45",
    "developer_comment": "凪沙",
    "string": "「そうだね。うふふ、楽しみ」"
  },
  "000758": {
    "context": "000758",
    "code": "0x45",
    "developer_comment": "凪沙",
    "string": "微笑む凪沙を見て、風見の胸は詰まった。"
  },
  "000829": {
    "context": "000829",
    "code": "0x45",
    "developer_comment": "凪沙",
    "string": "両親が死んだ時、凪沙はまだ３歳だった。"
  },
  "000834": {
    "context": "000834",
    "code": "0x45",
    "developer_comment": "凪沙",
    "string": "その子を独りで育て上げるのは、当時わずか１６歳だった風見にとって、容易な事ではない。"
  },
  "000839": {
    "context": "000839",
    "code": "0x45",
    "developer_comment": "凪沙",
    "string": "だが決して苦ではなかった。凪沙がとても優しい子だったから。"
  },
  "000844": {
    "context": "000844",
    "code": "0x45",
    "developer_comment": "凪沙",
    "string": "仕事から帰ってくると、いつも凪沙が笑顔で出迎えてくれる。"
  },
  "000849": {
    "context": "000849",
    "code": "0x45",
    "developer_comment": "凪沙",
    "string": "まだ幼いのに、風見をいつも慮ってくれる彼女は、風見に癒しを与えてくれる。"
  },
  "000854": {
    "context": "000854",
    "code": "0x45",
    "developer_comment": "凪沙",
    "string": "凪沙は、両親が風見に遺してくれた、何より大切な宝物だった。"
  },
  "000929": {
    "context": "000929",
    "code": "0x45",
    "developer_comment": "凪沙",
    "string": "妹であると同時に、娘のような存在――"
  },
  "000934": {
    "context": "000934",
    "code": "0x45",
    "developer_comment": "凪沙",
    "string": "その子を風見は、小学生になるまでちゃんと育てる事ができた。"
  },
  "000939": {
    "context": "000939",
    "code": "0x45",
    "developer_comment": "凪沙",
    "string": "風見の心を、しみじみとした想いが満たす。"
  },
  "000947": {
    "context": "000947",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「……それにしても、もうすっかり春ね」"
  },
  "000976": {
    "context": "000976",
    "code": "0x45",
    "developer_comment": "凪沙",
    "string": "「うん、ぽかぽかあったかいよ」"
  },
  "000987": {
    "context": "000987",
    "code": "0x45",
    "developer_comment": "凪沙",
    "string": "「お姉ちゃん、日向ぼっこしない？」"
  },
  "000995": {
    "context": "000995",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「凪沙、それはグッドアイディアよ」"
  },
  "001000": {
    "context": "001000",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "風見たちは芝生に寝転んだ。"
  },
  "001087": {
    "context": "001087",
    "code": "0x45",
    "developer_comment": "凪沙",
    "string": "「お空、きれいだね～」"
  },
  "001095": {
    "context": "001095",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「うーん、気持ちいいわねぇ」"
  },
  "001100": {
    "context": "001100",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "春の陽光が、２人を照らす。"
  },
  "001105": {
    "context": "001105",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "暖かな光に包まれながら、風見は思っていた。"
  },
  "001111": {
    "context": "001111",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "（……凪沙を養うようになってから、もう３年か……）"
  },
  "001117": {
    "context": "001117",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "（この街だからこそ、ここまでやってこれたのかもしれないわね）"
  },
  "001122": {
    "context": "001122",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "鹿鳴市が政令指定機密都市になってから、早７年。"
  },
  "001127": {
    "context": "001127",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "その間に市の福祉は充実し、風見たち姉妹もその恩恵を受けている。"
  },
  "001132": {
    "context": "001132",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "原子力とＢＣを中心とした、各種科学技術産業によって潤う街――その豊かさが、風見たちを護ってくれているのだ。"
  },
  "001138": {
    "context": "001138",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "（ありがたい事だわ……私はほとんど適性を持っていないのに）"
  },
  "001143": {
    "context": "001143",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "風見も一応適性はあるが、それは『Ｄ』という、非常に低い値だった。"
  },
  "001148": {
    "context": "001148",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "かなり努力をしても、テレパシーが使えるか否かという、コミュニケーターと言うにはおこがましい程度の値。"
  },
  "001153": {
    "context": "001153",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "風見もテレパシーさえ使えないし、鹿鳴学園に入学もしていない。"
  },
  "001158": {
    "context": "001158",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "――だが、それでも気になる事はある。"
  },
  "001163": {
    "context": "001163",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "風見が１２歳の時、この街を襲った事件の事だ。"
  },
  "001168": {
    "context": "001168",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "『鹿鳴市同時多発火災』――犯人はコミュニケーターを憎む、過激派団体だったという。"
  },
  "001173": {
    "context": "001173",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "橘一家は幸い火災の被害を受けなかったが、その事実は風見の心に、充分な恐怖を与えた。"
  },
  "001178": {
    "context": "001178",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "故に彼女は用心のため、自分にも妹にも、ＢＣの話はあまりしないよう言い聞かせてきたのだ。"
  },
  "001184": {
    "context": "001184",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "（まぁ、でも、それももうすぐ終わり……）"
  },
  "001190": {
    "context": "001190",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "（……これからは、少なくとも、凪沙には……）"
  },
  "001195": {
    "context": "001195",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "ぼーっとそんな事を考えているうち、まどろみが襲ってきた。"
  },
  "001219": {
    "context": "001219",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "（う……いけない。寝たら凪沙が退屈してしまうわ……）"
  },
  "001224": {
    "context": "001224",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "だが日々の仕事の疲れから来る眠気は、とても抗いがたかった。"
  },
  "001232": {
    "context": "001232",
    "code": "0x45",
    "developer_comment": "凪沙",
    "string": "「……あれ、お姉ちゃん？　おねむなの……？」"
  },
  "001237": {
    "context": "001237",
    "code": "0x45",
    "developer_comment": "凪沙",
    "string": "凪沙の声も、すごく遠く聞こえる。"
  },
  "001271": {
    "context": "001271",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "（眠い……いや、寝ちゃだめ……でも眠い……）"
  },
  "001276": {
    "context": "001276",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "そう思っているうち、風見は眠りに落ちた。"
  },
  "001320": {
    "context": "001320",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "――その短い眠りの間。"
  },
  "001325": {
    "context": "001325",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "風見は、悲しい夢を見た。"
  },
  "001330": {
    "context": "001330",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "凪沙がどこかへ行ってしまう夢。"
  },
  "001361": {
    "context": "001361",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "白い闇の中に消えていく、凪沙の後姿……。"
  },
  "001372": {
    "context": "001372",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "（凪沙！　待って、行っちゃだめ！）"
  },
  "001378": {
    "context": "001378",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "（そっちはダメなの！　戻ってこられなくなるのよ！）"
  },
  "001383": {
    "context": "001383",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "そう叫ぼうとしたが、声は出なかった。"
  },
  "001408": {
    "context": "001408",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "それでも凪沙は、そっと振り返り――"
  },
  "001469": {
    "context": "001469",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "風見に、微笑みかけた。"
  },
  "001539": {
    "context": "001539",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「――凪沙！」"
  },
  "001544": {
    "context": "001544",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "そこで風見は、目を覚ました。"
  },
  "001602": {
    "context": "001602",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「あ、あれ……！？　凪沙は！？」"
  },
  "001607": {
    "context": "001607",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "風見は愕然とした。"
  },
  "001612": {
    "context": "001612",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "一瞬、夢が現実になったのかとでも思ったが――"
  },
  "001650": {
    "context": "001650",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "見回すと少し離れた所に、凪沙の姿があった。"
  },
  "001655": {
    "context": "001655",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "そばには近所の女の子『洵』もいる。"
  },
  "001660": {
    "context": "001660",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "風見が寝入った後、凪沙は洵と会い、２人で遊んでいたらしい。"
  },
  "001666": {
    "context": "001666",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "（いけない、私ったら！）"
  },
  "001678": {
    "context": "001678",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "風見は慌てて立ち上がり、２人の元に駆け寄った。"
  },
  "001771": {
    "context": "001771",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「じゅ、洵ちゃん！　ごめんなさいね、凪沙を見ててくれたのね！」"
  },
  "001800": {
    "context": "001800",
    "code": "0x45",
    "developer_comment": "洵",
    "string": "「あ、風見ちゃん。いいよいいよ、あたしも楽しいから」"
  },
  "001829": {
    "context": "001829",
    "code": "0x45",
    "developer_comment": "凪沙",
    "string": "「お姉ちゃん、ぐっすりだったから、起こしちゃいけないとおもって……」"
  },
  "001837": {
    "context": "001837",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「つい、うとうとしちゃったわ……あんまりぽかぽかしてるものだから……」"
  },
  "001842": {
    "context": "001842",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "恥ずかしげに言う風見に、凪沙と洵は笑った。"
  },
  "001850": {
    "context": "001850",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「……いつの間にか、お昼過ぎちゃったわね。家に帰ってご飯にしましょうか」"
  },
  "001879": {
    "context": "001879",
    "code": "0x45",
    "developer_comment": "凪沙",
    "string": "「うん！　おなかすいた！」"
  },
  "001884": {
    "context": "001884",
    "code": "0x45",
    "developer_comment": "凪沙",
    "string": "そう言う凪沙と、そして洵に手を握られ、風見は家に向けて歩き出す。"
  },
  "001889": {
    "context": "001889",
    "code": "0x45",
    "developer_comment": "凪沙",
    "string": "このささやかな平穏が、いつまでも続くものと、風見は信じていた……。"
  }
}