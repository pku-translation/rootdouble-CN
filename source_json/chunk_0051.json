  "001174": {
    "context": "001174",
    "code": "0x45",
    "developer_comment": "ユウリ",
    "string": "「は、はい……」"
  },
  "001203": {
    "context": "001203",
    "code": "0x45",
    "developer_comment": "ユウリ",
