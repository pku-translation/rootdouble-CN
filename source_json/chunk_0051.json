  "002426": {
    "context": "002426",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "（！！）"
  },
  "002431": {
    "context": "002431",
    "code": "0x45",
    "developer_comment": "",
    "string": "完全に破壊されていた。"
  },
  "002436": {
    "context": "002436",
    "code": "0x45",
    "developer_comment": "",
    "string": "まるで誰かが、意図的に壊したかのように！"
  },
  "002442": {
    "context": "002442",
