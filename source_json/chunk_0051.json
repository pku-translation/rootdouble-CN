  "001694": {
    "context": "001694",
    "code": "0x45",
    "developer_comment": "",
    "string": "――だが、"
  },
  "001699": {
    "context": "001699",
    "code": "0x45",
    "developer_comment": "",
    "string": "その直後――"
  },
