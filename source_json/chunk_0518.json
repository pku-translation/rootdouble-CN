{
  "000053": {
    "context": "000053",
    "code": "0x45",
    "developer_comment": "",
    "string": "――それは、悠里が鹿鳴市に引っ越してきた最初の夏。"
  },
  "000103": {
    "context": "000103",
    "code": "0x45",
    "developer_comment": "",
    "string": "小学２年生になった悠里には、心待ちにしているものがあった。"
  },
  "000108": {
    "context": "000108",
    "code": "0x45",
    "developer_comment": "",
    "string": "それは、夏休み。"
  },
  "000113": {
    "context": "000113",
    "code": "0x45",
    "developer_comment": "",
    "string": "夏彦やましろたちといっぱい遊べると思い、悠里はずっと楽しみにしていたのだ。"
  },
  "000118": {
    "context": "000118",
    "code": "0x45",
    "developer_comment": "",
    "string": "そして、その日がついに来た。"
  },
  "000147": {
    "context": "000147",
    "code": "0x45",
    "developer_comment": "",
    "string": "夏休みの初日――悠里は、公園に向かっていた。"
  },
  "000158": {
    "context": "000158",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「夏彦くんとましろちゃん、いるかな？」"
  },
  "000169": {
    "context": "000169",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「……いなかったらどうしよう。ご本でもよもうかな……」"
  },
  "000174": {
    "context": "000174",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "そんな不安が心をよぎる。"
  },
  "000265": {
    "context": "000265",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "しかし、ちゃんと夏彦とましろは公園で遊んでいた。"
  },
  "000276": {
    "context": "000276",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「夏彦くん、ましろちゃん、こんにちは」"
  },
  "000305": {
    "context": "000305",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「あ、悠里だ」"
  },
  "000334": {
    "context": "000334",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「あれ？　しょうがっこーにいかなくていいの？」"
  },
  "000345": {
    "context": "000345",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「うん、もう夏やすみに入ったの。きょうからまいにち、３人であそぼうね」"
  },
  "000374": {
    "context": "000374",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「そっか！　よし、それなら夏やすみのけいかくをたてようぜ！」"
  },
  "000385": {
    "context": "000385",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「よていをたてないと、夏なんてあっというまにおわっちゃうからな」"
  },
  "000396": {
    "context": "000396",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「あ、それいいね」"
  },
  "000407": {
    "context": "000407",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「夏彦くん、何かいいアイディアとかある？」"
  },
  "000436": {
    "context": "000436",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「う～ん、そうだなぁ……」"
  },
  "000441": {
    "context": "000441",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "夏彦が腕を組み、やがてカッと目を見開く。"
  },
  "000470": {
    "context": "000470",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「これだ！　鹿鳴市さいきょーベーゴマつかいけっていせん！」"
  },
  "000481": {
    "context": "000481",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「べーごま？」"
  },
  "000510": {
    "context": "000510",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「むぅ！　それはまさか、よにいう『貝独楽』！」"
  },
  "000539": {
    "context": "000539",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「なにぃ！？　し、しってるのか、ましろ！？」"
  },
  "000568": {
    "context": "000568",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「しってるよ、ふつうにベーゴマでしょ」"
  },
  "000597": {
    "context": "000597",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「ぬぅ……」"
  },
  "000608": {
    "context": "000608",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「ねぇねぇ、夏彦くん。けっていせんってどういうこと？」"
  },
  "000637": {
    "context": "000637",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「このまちでいちばんベーゴマがつよいやつをきめる、いちだいイベントさ」"
  },
  "000666": {
    "context": "000666",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「どうだ、すごいだろ！？」"
  },
  "000677": {
    "context": "000677",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「うん！　夏彦くん、すごい！」"
  },
  "000682": {
    "context": "000682",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "悠里はぱちぱちと拍手を送る。"
  },
  "000687": {
    "context": "000687",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "しかし、ましろは嘲るような笑みを浮かべた。"
  },
  "000716": {
    "context": "000716",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「ふ～ん。夏彦のアイディアも、いがいとたいしたことないね」"
  },
  "000745": {
    "context": "000745",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「なんだと！？　じゃあもっといいあんだしてみろよ」"
  },
  "000774": {
    "context": "000774",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「よ～し、きいておどろくな！」"
  },
  "000803": {
    "context": "000803",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「ズバリ！　鹿鳴市さいきょーメンコつかいけっていせん！」"
  },
  "000832": {
    "context": "000832",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「ぼくのマネじゃん！」"
  },
  "000843": {
    "context": "000843",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「あ、でもそれもかわいくていいかも」"
  },
  "000854": {
    "context": "000854",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「ましろちゃん、やっぱり女の子だね」"
  },
  "000883": {
    "context": "000883",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「えへへ～」"
  },
  "000921": {
    "context": "000921",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「へん！　そんなのぜんぜんすごくないやい！　ぼくのほうがすごい！」"
  },
  "000959": {
    "context": "000959",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「なによ、私のほうがすごいもん！」"
  },
  "000997": {
    "context": "000997",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「ぼく！」"
  },
  "001035": {
    "context": "001035",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「私！」"
  },
  "001073": {
    "context": "001073",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「う～……！」"
  },
  "001111": {
    "context": "001111",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「がぅ……！」"
  },
  "001116": {
    "context": "001116",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "２人が、威嚇するように睨み合う。いつも通りの光景だ。"
  },
  "001127": {
    "context": "001127",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「２人とも、ケンカはやめてよぅ……」"
  },
  "001138": {
    "context": "001138",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「もう、さいきん２人はなかよくなったと思ったのに……」"
  },
  "001176": {
    "context": "001176",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「だってましろが……」"
  },
  "001214": {
    "context": "001214",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「だって～夏彦が～……」"
  },
  "001225": {
    "context": "001225",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「う、う～ん……」"
  },
  "001230": {
    "context": "001230",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "夏彦もましろも互いに譲らない。悠里は深く肩を落とした。"
  },
  "001235": {
    "context": "001235",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "２人の間には、ライバル意識のようなものがあるのだろう。"
  },
  "001283": {
    "context": "001283",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "（このままじゃせっかくの夏休みも、だいなしになっちゃうかも。どうしよう……）"
  },
  "001289": {
    "context": "001289",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "（……あ、そうだ）"
  },
  "001328": {
    "context": "001328",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「２人であだなとかつけて、それでよびあったらどうかな？」"
  },
  "001339": {
    "context": "001339",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「そうすればすこし、きょりが近くなるかもしれないよ」"
  },
  "001368": {
    "context": "001368",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「あだなって、たとえばどんな？」"
  },
  "001379": {
    "context": "001379",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「う～ん、たとえば～……」"
  },
  "001384": {
    "context": "001384",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "悠里はそう言ってしばらく黙った。"
  },
  "001389": {
    "context": "001389",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "２人には良いあだ名をつけてあげたいとは思うが、なかなか思いつかない。"
  },
  "001394": {
    "context": "001394",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "しばらく考えた後、悠里の頭の中に閃くものがあった。"
  },
  "001405": {
    "context": "001405",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「あ！　そ……そうだっ！」"
  },
  "001416": {
    "context": "001416",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「『ひこりん』と『ましろん』ってのはどうかな？」"
  },
  "001454": {
    "context": "001454",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「え～！？　ひこりん～～～……？」"
  },
  "001483": {
    "context": "001483",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「なによ夏彦、きにいらないの？」"
  },
  "001512": {
    "context": "001512",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「いや、きにいらないってわけじゃないけどさぁ……」"
  },
  "001541": {
    "context": "001541",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「ひこりんに、ましろん……うぅ～イヤだな～……」"
  },
  "001579": {
    "context": "001579",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「悠里がつけてくれたんだから、もんくいわないの！」"
  },
  "001608": {
    "context": "001608",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「かわいいじゃん。ね、夏彦？」"
  },
  "001637": {
    "context": "001637",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「ましろだって、ひこりんってよんでないじゃないか……」"
  },
  "001675": {
    "context": "001675",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「あっ……」"
  },
  "001680": {
    "context": "001680",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "そう言って、夏彦とましろは黙った。"
  },
  "001685": {
    "context": "001685",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "その様子を見ながら、クスクスと笑う悠里。"
  },
  "001691": {
    "context": "001691",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "（うん。ケンカになってないから、さっきよりはマシだよね）"
  },
  "001696": {
    "context": "001696",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "色々違うような気もするが、とにかくそう思うことにした。"
  },
  "001701": {
    "context": "001701",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "本音を言えば、やっぱりましろと夏彦には、あだ名で呼び合ってもらいたい。"
  },
  "001706": {
    "context": "001706",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "悠里としては『ひこりん』と『ましろん』というあだ名は相当に気にいっているのだ。"
  },
  "001711": {
    "context": "001711",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "だが、とりあえず贅沢は言わないことにした。"
  },
  "001764": {
    "context": "001764",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "（いつかそんな日が来るよね）"
  },
  "001770": {
    "context": "001770",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "（いつか、きっと……）"
  },
  "001775": {
    "context": "001775",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "悠里には、その願いだけで十分だった。"
  },
  "001821": {
    "context": "001821",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "それから――ことあるごとに悠里は夏彦たちを『ましろん、ひこりん』と呼んだ。"
  },
  "001826": {
    "context": "001826",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "呼び続ければ、もっと仲良くなれると思ったからだ。"
  },
  "001831": {
    "context": "001831",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "しかし、いつまで経っても夏彦とましろに関しては相変わらずで――"
  },
  "001842": {
    "context": "001842",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「ねぇねぇひこりん、どうしてましろちゃんのこと『ましろん』ってよばないの？」"
  },
  "001871": {
    "context": "001871",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「……なんか、くすぐったいんだもん」"
  },
  "001882": {
    "context": "001882",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「え～？　ま、ましろんは？」"
  },
  "001911": {
    "context": "001911",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「……の、のーこめんと」"
  },
  "001922": {
    "context": "001922",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「うぅ～……２人とも、わたしによばれるのはイヤがらないのに～……」"
  },
  "001951": {
    "context": "001951",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「悠里はいいんだ、悠里は。おねえさんだからな」"
  },
  "001980": {
    "context": "001980",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「うん、夏彦のいうとおりだね」"
  },
  "001991": {
    "context": "001991",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「なんでこんなときばっかり、いきピッタリなのよぅ……」"
  },
  "002002": {
    "context": "002002",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「もう……」"
  },
  "002007": {
    "context": "002007",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "悠里は苦笑いを浮かべたが、悪い気はしなかった。"
  },
  "002012": {
    "context": "002012",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "夏彦とましろの仲が、少しずつ良くなっているような気がしたし――"
  },
  "002017": {
    "context": "002017",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "３人の距離が、より近づいた気がしたからだ。"
  },
  "002023": {
    "context": "002023",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "（……ずっとずっとおとなになっても、３人なかよしでいられたらいいな）"
  },
  "002029": {
    "context": "002029",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "（おとなになっても、あだなでよんだらへんかな……？）"
  },
  "002034": {
    "context": "002034",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "そんなことを思いながら、悠里は夏彦とましろの頭を撫でる。"
  },
  "002039": {
    "context": "002039",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "くすぐったそうにする２人の顔が、悠里にはとても可愛く見えた。"
  },
  "002090": {
    "context": "002090",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "――そのあだ名は結局、悠里以外には定着しなかったが――"
  },
  "002170": {
    "context": "002170",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "ましろがニックネーム魔になったのは、その時からだったように思う。"
  },
  "002175": {
    "context": "002175",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "仲良くなりたいと思う相手に、あだ名をつけるのだ。"
  },
  "002180": {
    "context": "002180",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "だがましろは悠里の事だけは、『もう親友だから』という理由で、ずっと名前で呼んでいた。"
  },
  "002196": {
    "context": "002196",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "それも嬉しく、誇らしかった。"
  },
  "002220": {
    "context": "002220",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "――そして、当の悠里は――"
  },
