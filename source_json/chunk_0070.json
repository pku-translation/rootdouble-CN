{
  "000140": {
    "context": "000140",
    "code": "0x55",
    "developer_comment": "",
    "string": "√After Chapter5"
  },
  "000150": {
    "context": "000150",
    "code": "0x45",
    "developer_comment": "",
    "string": "――第１エリア地下２階、内周棟――"
  },
  "000155": {
    "context": "000155",
    "code": "0x45",
    "developer_comment": "",
    "string": "その一室に、渡瀬は負傷した洵を横たえた。"
  },
  "000166": {
    "context": "000166",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「大丈夫か守部！　聞こえてるか！？」"
  },
  "000172": {
    "context": "000172",
    "code": "0x45",
    "developer_comment": "洵",
    "string": "「……」"
  },
  "000177": {
    "context": "000177",
    "code": "0x45",
    "developer_comment": "洵",
    "string": "声をかけても、洵は目を覚まさなかった。"
  },
  "000182": {
    "context": "000182",
    "code": "0x45",
    "developer_comment": "洵",
    "string": "背の傷から血が溢れているのが、服越しにわかる。"
  },
  "000193": {
    "context": "000193",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「くそ、早く手当てしなければ……！」"
  },
  "000198": {
    "context": "000198",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "そう言った時、宇喜多が張り詰めた声を上げた。"
  },
  "000281": {
    "context": "000281",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "「よ……よし、隊長さん！　だったら君が守部くんを手当てしている間に、僕が捜索を続けておこう！」"
  },
  "000292": {
    "context": "000292",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「なに！？」"
  },
  "000321": {
    "context": "000321",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "「さっき隊長さんが聞いたと言う『足音』、それを確認したいんだよ」"
  },
  "000332": {
    "context": "000332",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "「隊長さんは、守部さんの治療に専念したまえ」"
  },
  "000343": {
    "context": "000343",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「ダメだおっさん！　単独行動は危険過ぎる！」"
  },
  "000372": {
    "context": "000372",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "「それはわかるが、捜索の手を止めるのも危険だろう！？　時間は常に出血し続けているんだぞ！」"
  },
  "000383": {
    "context": "000383",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「……！」"
  },
  "000388": {
    "context": "000388",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "宇喜多の言う事はもっともだった。"
  },
  "000393": {
    "context": "000393",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "黙り込む渡瀬に、宇喜多はなおも続ける。"
  },
  "000422": {
    "context": "000422",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "「隊長さん。確かに君たちから見れば、僕はただの素人かもしれない」"
  },
  "000433": {
    "context": "000433",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "「だが僕だって、みんなを救いたいという気持ちは同じだ」"
  },
  "000444": {
    "context": "000444",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「……しかし、だったらオレが行くべきだと……」"
  },
  "000473": {
    "context": "000473",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "「いや、傷の手当は君が適役だ。守部くんが目覚めた時、君がそばにいた方が彼女も安心するだろう」"
  },
  "000484": {
    "context": "000484",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "「それに僕は、君たちよりもこのラボに慣れているからね。１人だってそうそうヘマはしないさ」"
  },
  "000507": {
    "context": "000507",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "宇喜多はそう言って笑った。"
  },
  "000512": {
    "context": "000512",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "強がっているようにも見えるが――渡瀬はその言葉を信じる事にした。"
  },
  "000523": {
    "context": "000523",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「わかった……だがあまり遠くに行くんじゃないぞ」"
  },
  "000528": {
    "context": "000528",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "渡瀬は宇喜多に、予備のプロキオンを渡した。"
  },
  "000557": {
    "context": "000557",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "「では第１エリアは僕が捜索を済ませておこう。後ほど第２エリアで落ち合おうじゃないか」"
  },
  "000568": {
    "context": "000568",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「あぁ……くれぐれも気をつけろよ、おっさん」"
  },
  "000579": {
    "context": "000579",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "「承知した」"
  },
  "000599": {
    "context": "000599",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "宇喜多はそう言うと、早足で研究室を出て行った。"
  },
  "000644": {
    "context": "000644",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "渡瀬は洵と２人きりになった。"
  },
  "000649": {
    "context": "000649",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "さっそく彼女の背に目を向ける。"
  },
  "000655": {
    "context": "000655",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "（カイローンを塗りたいが……塗るには、服を脱がすしかない……）"
  },
  "000660": {
    "context": "000660",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "渡瀬は一瞬ためらった後、洵の体をわずかに起こし――"
  },
  "000671": {
    "context": "000671",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「すまん、守部」"
  },
  "000676": {
    "context": "000676",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "そう言って制服のファスナーを下ろそうとした時、洵が声を上げた。"
  },
  "000785": {
    "context": "000785",
    "code": "0x45",
    "developer_comment": "洵",
    "string": "「ぎゃっ！！」"
  },
  "000796": {
    "context": "000796",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「わっ！？」"
  },
  "000840": {
    "context": "000840",
    "code": "0x45",
    "developer_comment": "洵",
    "string": "「だ、誰！？　なにやってんの！」"
  },
  "000845": {
    "context": "000845",
    "code": "0x45",
    "developer_comment": "洵",
    "string": "じたばたと暴れる洵に、渡瀬は慌てて答えた。"
  },
  "000856": {
    "context": "000856",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「落ち着けオレだ！　背中に怪我をしたお前を、手当てしようとしてたんだ！」"
  },
  "000907": {
    "context": "000907",
    "code": "0x45",
    "developer_comment": "洵",
    "string": "「服を脱がして！？　隊長だからって越権行為だよ、この弩エロ！」"
  },
  "000918": {
    "context": "000918",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「弩エロだと！？　お前意外と元気だな、心配して損したぜ！」"
  },
  "000947": {
    "context": "000947",
    "code": "0x45",
    "developer_comment": "洵",
    "string": "「なに勝手なこと言ってんだよ！　だいたい服脱がさなくても、傷のとこだけハサミで切って開けばいいじゃん！」"
  },
  "000958": {
    "context": "000958",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「む……それもそうだな」"
  },
  "001001": {
    "context": "001001",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "室内の机を調べると、ハサミはすぐに見つかった。"
  },
  "001012": {
    "context": "001012",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「あったぞ、守部。こっちに背中を向けてくれ」"
  },
  "001078": {
    "context": "001078",
    "code": "0x45",
    "developer_comment": "洵",
    "string": "「余計なところ切るんじゃないよ」"
  },
  "001083": {
    "context": "001083",
    "code": "0x45",
    "developer_comment": "洵",
    "string": "レスキュー服にハサミを入れると、直径２センチほどの傷口があらわになった。"
  },
  "001088": {
    "context": "001088",
    "code": "0x45",
    "developer_comment": "洵",
    "string": "そこにカイローンを塗りこむと、洵がびくんと身を震わせた。"
  },
  "001152": {
    "context": "001152",
    "code": "0x45",
    "developer_comment": "洵",
    "string": "「ふぁっ」"
  },
  "001163": {
    "context": "001163",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「ちょっと動くなって！」"
  },
  "001192": {
    "context": "001192",
    "code": "0x45",
    "developer_comment": "洵",
    "string": "「で、でも……くすぐったくて……」"
  },
  "001203": {
    "context": "001203",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「優しくしてるんじゃないかよ。とにかくもう少し我慢しろ」"
  },
  "001208": {
    "context": "001208",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "カイローンを半分使い、ようやく洵の傷はふさがった。"
  },
  "001264": {
    "context": "001264",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「ふぅ……これで、ひとまず安心だな」"
  },
  "001293": {
    "context": "001293",
    "code": "0x45",
    "developer_comment": "洵",
    "string": "「……」"
  },
  "001298": {
    "context": "001298",
    "code": "0x45",
    "developer_comment": "洵",
    "string": "だが手当てが終わったと言うのに、洵は暗い表情を浮かべていた。"
  },
  "001309": {
    "context": "001309",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「どうしたんだ守部、そんな顔して？　傷が痛むか？」"
  },
  "001338": {
    "context": "001338",
    "code": "0x45",
    "developer_comment": "洵",
    "string": "「ううん、そうじゃないけど……」"
  },
  "001343": {
    "context": "001343",
    "code": "0x45",
    "developer_comment": "洵",
    "string": "洵はそう呟いてから、室内を見回して続けた。"
  },
  "001372": {
    "context": "001372",
    "code": "0x45",
    "developer_comment": "洵",
    "string": "「ところで隊長、宇喜多さんは？」"
  },
  "001383": {
    "context": "001383",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「オレが手当てをしている間に捜索を進めるというから、先に行かせたよ」"
  },
  "001412": {
    "context": "001412",
    "code": "0x45",
    "developer_comment": "洵",
    "string": "「え……１人で？」"
  },
  "001417": {
    "context": "001417",
    "code": "0x45",
    "developer_comment": "洵",
    "string": "その言葉に、洵の表情が険しくなった。"
  },
  "001428": {
    "context": "001428",
    "code": "0x45",
    "developer_comment": "洵",
    "string": "「なにやってんの隊長？　素人を単独行動させるなんて、危険でしょ？」"
  },
  "001439": {
    "context": "001439",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「オレもそう言ったんだが、聞かなくてな」"
  },
  "001468": {
    "context": "001468",
    "code": "0x45",
    "developer_comment": "洵",
    "string": "「だったら隊長も早く追いなよ。あたしも行くから」"
  },
  "001500": {
    "context": "001500",
    "code": "0x45",
    "developer_comment": "洵",
    "string": "そう言って立ち上がろうとする洵だったが、その脚はふらついていた。"
  },
  "001511": {
    "context": "001511",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「いや、お前は頭を打ったばかりだ。ここで少し休んでからにしろ」"
  },
  "001535": {
    "context": "001535",
    "code": "0x45",
    "developer_comment": "洵",
    "string": "「……」"
  },
  "001540": {
    "context": "001540",
    "code": "0x45",
    "developer_comment": "洵",
    "string": "洵は一瞬ためらったが、すぐにうなずいた。"
  },
  "001551": {
    "context": "001551",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「オレは第２エリア方面に向かう。回復したら追ってきてくれ」"
  },
  "001580": {
    "context": "001580",
    "code": "0x45",
    "developer_comment": "洵",
    "string": "「了解」"
  },
  "001591": {
    "context": "001591",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「あ、それと守部……」"
  },
  "001602": {
    "context": "001602",
    "code": "0x45",
    "developer_comment": "洵",
    "string": "「なに？」"
  },
  "001613": {
    "context": "001613",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「さっきは助けてくれて、ありがとな」"
  },
  "001618": {
    "context": "001618",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "その言葉に、洵はようやく微笑んだ。"
  },
  "001647": {
    "context": "001647",
    "code": "0x45",
    "developer_comment": "洵",
    "string": "「んーん、気にしないで」"
  },
  "001652": {
    "context": "001652",
    "code": "0x45",
    "developer_comment": "洵",
    "string": "渡瀬はその言葉に笑みを返し、研究室を後にした。"
  },
  "001749": {
    "context": "001749",
    "code": "0x45",
    "developer_comment": "洵",
    "string": "――渡瀬は宇喜多を探し、第１エリア内を一回りした。"
  },
  "001754": {
    "context": "001754",
    "code": "0x45",
    "developer_comment": "洵",
    "string": "だが、宇喜多の姿はない。"
  },
  "001765": {
    "context": "001765",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「おっさん！　いるか！？」"
  },
  "001770": {
    "context": "001770",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "そう声を張り上げても、答えはない。"
  },
  "001776": {
    "context": "001776",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "（先に行ったのか……）"
  },
  "001824": {
    "context": "001824",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "渡瀬はそう思い、宇喜多を追って第２エリアに向かった。"
  }
}