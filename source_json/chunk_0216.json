{
  "000015": {
    "context": "000015",
    "code": "0x55",
    "developer_comment": "",
    "string": "√Before Chapter7"
  },
  "000079": {
    "context": "000079",
    "code": "0x45",
    "developer_comment": "",
    "string": "星の明かりもない闇の中――"
  },
  "000084": {
    "context": "000084",
    "code": "0x45",
    "developer_comment": "",
    "string": "そこに、夏彦は伏していた。"
  },
  "000089": {
    "context": "000089",
    "code": "0x45",
    "developer_comment": "",
    "string": "時計の音がかすかに辺りに響く。"
  },
  "000094": {
    "context": "000094",
    "code": "0x45",
    "developer_comment": "",
    "string": "その音を聞きつけ、夏彦は目を覚ました。"
  },
  "000105": {
    "context": "000105",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「……う……」"
  },
  "000116": {
    "context": "000116",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「……こ……ここは……？」"
  },
  "000166": {
    "context": "000166",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "そこは例の廃工場だった。"
  },
  "000316": {
    "context": "000316",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "どうやら気絶させられたまま、放置されたようだ。"
  },
  "000321": {
    "context": "000321",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "あれからどのくらい時間が経っているのだろうか。"
  },
  "000326": {
    "context": "000326",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "そう思った時、うめくような声がそばで響いた。"
  },
  "000337": {
    "context": "000337",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「……ぅ」"
  },
  "000348": {
    "context": "000348",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「な……夏彦？」"
  },
  "000353": {
    "context": "000353",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "ましろとサリュの声だった。"
  },
  "000358": {
    "context": "000358",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "２人とも同じく目覚めたところらしい。"
  },
  "000475": {
    "context": "000475",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「あ、２人とも！　無事だったか！？」"
  },
  "000486": {
    "context": "000486",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「ん……無事、かな？　どこも痛くないし……」"
  },
  "000497": {
    "context": "000497",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「でも、身動きは取れない模様」"
  },
  "000508": {
    "context": "000508",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「え……？」"
  },
  "000513": {
    "context": "000513",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "そう言いかけた時、夏彦も気付いた。"
  },
  "000524": {
    "context": "000524",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "両手と両足が、固く縄で縛られている事に。"
  },
  "000529": {
    "context": "000529",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "見ればましろとサリュも、同じように縛られていた。"
  },
  "000540": {
    "context": "000540",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「あっ、なにこれ！？」"
  },
  "000551": {
    "context": "000551",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「私たちを逃さないためだと思う」"
  },
  "000562": {
    "context": "000562",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「むっ……！　くっ……！」"
  },
  "000567": {
    "context": "000567",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "いくら腕や脚に力を入れても、縄はビクともしなかった。"
  },
  "000573": {
    "context": "000573",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "（くそ……早く縄を解いて、あいつらを止めないと……！）"
  },
  "000578": {
    "context": "000578",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "夏彦の焦りに反応するように、緊迫した鳴き声が闇に響いた。"
  },
  "000586": {
    "context": "000586",
    "code": "0x45",
    "developer_comment": "アリス",
    "string": "「きゅう～……！」"
  },
  "000597": {
    "context": "000597",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「あ、アリスは壊されなかったんだね……よかった」"
  },
  "000608": {
    "context": "000608",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「アリス、こっちへ」"
  },
  "000655": {
    "context": "000655",
    "code": "0x45",
    "developer_comment": "アリス",
    "string": "「きゅうきゅう」"
  },
  "000660": {
    "context": "000660",
    "code": "0x45",
    "developer_comment": "アリス",
    "string": "サリュの声に応えるように、アリスがサリュの元へ走る。"
  },
  "000671": {
    "context": "000671",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「……ん、待て。アリスって自律歩行ができるのか？」"
  },
  "000682": {
    "context": "000682",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「簡易的な機能だけど、できる。口頭で命令して歩かせる事や、順路データを入力する事も」"
  },
  "000693": {
    "context": "000693",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「なるほど……！　それならサリュ、アリスに何かこの縄を切るものを取ってきてもらえないか？」"
  },
  "000704": {
    "context": "000704",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「あ、その手があったか」"
  },
  "000709": {
    "context": "000709",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "見れば窓際に、ガラスの破片らしきものが落ちている。"
  },
  "000720": {
    "context": "000720",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「ではアリス、以下命令」"
  },
  "000731": {
    "context": "000731",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「『１５ｍ直進後、そこにある砕片を拾い、現座標に回帰』。以上命令」"
  },
  "000739": {
    "context": "000739",
    "code": "0x45",
    "developer_comment": "アリス",
    "string": "「きゅう！」"
  },
  "000783": {
    "context": "000783",
    "code": "0x45",
    "developer_comment": "アリス",
    "string": "頼もしい鳴き声でアリスが応え、窓のそばに向かう。"
  },
  "000788": {
    "context": "000788",
    "code": "0x45",
    "developer_comment": "アリス",
    "string": "しばらくそこで、サリュの命じたものに該当する物体を探していたが――"
  },
  "000793": {
    "context": "000793",
    "code": "0x45",
    "developer_comment": "アリス",
    "string": "やがて割れたガラスを咥え、戻ってきた。"
  },
  "000840": {
    "context": "000840",
    "code": "0x45",
    "developer_comment": "アリス",
    "string": "「きゅう！」"
  },
  "000851": {
    "context": "000851",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「やった！　でかしたアリス！」"
  },
  "000856": {
    "context": "000856",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "夏彦は身をよじり、アリスからガラスを受け取った。"
  },
  "000867": {
    "context": "000867",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「ちょっと待ってろ、とりあえずぼくが抜け出してみんなの縄も切る……」"
  },
  "000872": {
    "context": "000872",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "夏彦は縄にガラスを当てて、力を込めてこすり――"
  },
  "000921": {
    "context": "000921",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "なんとか手足の縄を切る事に成功した。"
  },
  "000932": {
    "context": "000932",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「よしっ！」"
  },
  "000937": {
    "context": "000937",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "さらにましろとサリュの縄も切ってやる。"
  },
  "001041": {
    "context": "001041",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「ふぅ……ようやく解放されたよ」"
  },
  "001061": {
    "context": "001061",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「ありがとう、夏彦」"
  },
  "001066": {
    "context": "001066",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "２人ともやはり、どこにも怪我はないようだ。"
  },
  "001071": {
    "context": "001071",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "テロリストの目的は、あくまで夏彦たちを足止めするだけだったらしい。"
  },
  "001100": {
    "context": "001100",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「……それにしても、テロリストたちの意図がわからない」"
  },
  "001111": {
    "context": "001111",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「私たちを生かしておく意味はないはずなのに……」"
  },
  "001122": {
    "context": "001122",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「いや、奴らの目的はあくまでもラボだ。ぼくらを殺したりして、目立つのを恐れたんだろう」"
  },
  "001151": {
    "context": "001151",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「なんでわかるの？」"
  },
  "001162": {
    "context": "001162",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「実際に聞いたからだよ。奴らの心の声を」"
  },
  "001191": {
    "context": "001191",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「声を聞いたって？　エンパシーで？」"
  },
  "001202": {
    "context": "001202",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「あぁ、多分……」"
  },
  "001207": {
    "context": "001207",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "そう思いつつも、夏彦は若干の違和感を覚えていた。"
  },
  "001212": {
    "context": "001212",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "あの時、気絶しかけた夏彦が発動した能力――それはエンパシーとは少し違う気がした。"
  },
  "001217": {
    "context": "001217",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "声だけではなく、映像も見えたのだ。あれがエンパシーと言えるだろうか。"
  },
  "001222": {
    "context": "001222",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "いつか聞いた恵那の言葉が、胸に蘇る。"
  },
  "001312": {
    "context": "001312",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "「ＢＣの可能性は無限なの」"
  },
  "001323": {
    "context": "001323",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "「いつかはテレパシーでもないエンパシーでもない、第３の能力が現れるかもしれないのよ」"
  },
  "001422": {
    "context": "001422",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "（第３の能力……！？）"
  },
  "001427": {
    "context": "001427",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "その可能性はありうる。"
  },
  "001432": {
    "context": "001432",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "夏彦はここ数日、急速にＢＣ能力が高まっていた。"
  },
  "001437": {
    "context": "001437",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "まだ制御しきれてないとはいえ、ほんの数日でテレパシーとエンパシーを使えるようになっていたのだ。"
  },
  "001443": {
    "context": "001443",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "（本当に、そんなものが……？）"
  },
  "001448": {
    "context": "001448",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "だがその可能性について、じっくり考えている時間はない。"
  },
  "001453": {
    "context": "001453",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "その能力によって得た情報の方が、今は遥かに重要だった。"
  },
  "001464": {
    "context": "001464",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「……とにかく奴らの心の声によると、ラボが狙われているのは本当らしい」"
  },
  "001475": {
    "context": "001475",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「しかも、決行は今日だ……！」"
  },
  "001504": {
    "context": "001504",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「今日！？　じゃ、じゃあすぐ止めなきゃ！」"
  },
  "001533": {
    "context": "001533",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「でもどうすれば？　警察などに通報しても、昨日と同じ結果になりかねない」"
  },
  "001544": {
    "context": "001544",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「確かに……！」"
  },
  "001599": {
    "context": "001599",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "保安課や警察を今から説得しても、間に合うとは思えなかった。"
  },
  "001604": {
    "context": "001604",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "夏彦は押し黙り、考え込む。"
  },
  "001610": {
    "context": "001610",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "（ラボに直接行って、注意を促すか……？）"
  },
  "001615": {
    "context": "001615",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "だがそれは危険だ。"
  },
  "001620": {
    "context": "001620",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "内通者がいる可能性も高いし、そのままテロに巻き込まれるかもしれない。"
  },
  "001625": {
    "context": "001625",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "そうなれば今度こそ、殺されかねない――！"
  },
  "001631": {
    "context": "001631",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "（……だったら……）"
  },
  "001637": {
    "context": "001637",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "（すぐにでも、逃げるべきじゃないのか……？）"
  },
  "001642": {
    "context": "001642",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "ましろたちとすぐ家に帰り、悠里を連れてこの街から脱出する。"
  },
  "001647": {
    "context": "001647",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "それが最も妥当なやり方に思えた。"
  },
  "001652": {
    "context": "001652",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "夏彦だけならまだしも、ましろたちまで危険にさらすわけにはいかない。"
  },
  "001657": {
    "context": "001657",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "しかし――"
  },
  "001663": {
    "context": "001663",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "（そうすれば、この街にいる全ての人を見捨てる事になる）"
  },
  "001669": {
    "context": "001669",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "（自分たちに関係ない人々は、どうでもいいって考えるべきか……？）"
  },
  "001674": {
    "context": "001674",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "いつだって彼はそうしてきた。"
  },
  "001679": {
    "context": "001679",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "それが夏彦の処世術だった。"
  },
  "001684": {
    "context": "001684",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "例えばテレビで報道される、この街で起きた『火事』や『事故』や『事件』。"
  },
  "001689": {
    "context": "001689",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "それらは全て、自分とは関係ない事。自ら関わる必要もない。"
  },
  "001694": {
    "context": "001694",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "大切なのは自分と悠里と、せいぜいましろやサリュくらいのものだ。"
  },
  "001699": {
    "context": "001699",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "ずっとそう思ってきたのに……"
  },
  "001704": {
    "context": "001704",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "脳裏に浮かぶのは、この街に住む、『関係ないはずの人々』の事だった。"
  },
  "001709": {
    "context": "001709",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "距離を置いてつきあってきたクラスメートたち。"
  },
  "001714": {
    "context": "001714",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "夏彦がコミュニケーターだと知るなり、離れていった幼い頃の友達。"
  },
  "001719": {
    "context": "001719",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "そして、夏彦たちに心を開いてくれなかった恵那。"
  },
  "001725": {
    "context": "001725",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "（……くそ、だからどうだっていうんだ）"
  },
  "001731": {
    "context": "001731",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "（仮に原子炉がメルトダウンしたって、皆が死ぬわけじゃない）"
  },
  "001737": {
    "context": "001737",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "（この街に誰も住めなくなるってだけだろ？）"
  },
  "001743": {
    "context": "001743",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "（だったらまず、自分たちが逃げることを優先するのが当たり前じゃないか……！）"
  },
  "001748": {
    "context": "001748",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "だがそのとき、不意に――"
  },
  "001753": {
    "context": "001753",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "今まで聞いた様々な言葉が、夏彦の耳に蘇った。"
  },
  "001845": {
    "context": "001845",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「それじゃひこりん、後悔するかもしれないよ」"
  },
  "001955": {
    "context": "001955",
    "code": "0x45",
    "developer_comment": "洵",
    "string": "「あたしはあたしなりのやり方で、この街を護る。あんたたちもやれる事をやるんだ。いいかい？」"
  },
  "002036": {
    "context": "002036",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「そうして過去を乗り越えて。ひこりんならできるはずだから」"
  },
  "002179": {
    "context": "002179",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "（……！）"
  },
  "002184": {
    "context": "002184",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "その時、夏彦は気づいた。"
  },
  "002189": {
    "context": "002189",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "今すぐ逃げようとしても、逃げおおせるとは限らない事に。"
  },
  "002195": {
    "context": "002195",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "（この街は、住人の出入りにも、厳重なチェックと手続きがいる……）"
  },
  "002201": {
    "context": "002201",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "（なのに悠里を連れて、逃げられるものか？）"
  },
  "002206": {
    "context": "002206",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "それは難しいだろう。"
  },
  "002211": {
    "context": "002211",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "ラボに行くのと逃げるのと、どちらも危険な事には変わりないのだ。"
  },
  "002216": {
    "context": "002216",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "だったら――"
  },
  "002222": {
    "context": "002222",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "（ラボに行くべきだ……！）"
  },
  "002227": {
    "context": "002227",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "弾き出された結論に、夏彦自身驚いた。"
  },
  "002232": {
    "context": "002232",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "だが急速に、その意思は膨らんでいく。"
  },
  "002237": {
    "context": "002237",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "９年前、夏彦と悠里は、ラボに行って事故に遭った。"
  },
  "002242": {
    "context": "002242",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "あの恐怖は今も消えていない。"
  },
  "002247": {
    "context": "002247",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "また死ぬような目に遭うかもしれないし、何かを失うかもしれない。"
  },
  "002252": {
    "context": "002252",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "だが、それでもなお。"
  },
  "002258": {
    "context": "002258",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "（行かなきゃならないんだな、悠里……）"
  },
  "002264": {
    "context": "002264",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "（そうしなきゃぼくは、過去を乗り越えられないんだな？）"
  },
  "002282": {
    "context": "002282",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "夏彦がそう思った瞬間、ましろがはっと息を呑んだ。"
  },
  "002311": {
    "context": "002311",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「……夏彦、もしかして……」"
  },
  "002322": {
    "context": "002322",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「今から直接、ラボに行くつもり？」"
  },
  "002348": {
    "context": "002348",
    "code": "0x45",
    "developer_comment": "夏彦・サリュ",
    "string": "「っ！？」"
  },
  "002353": {
    "context": "002353",
    "code": "0x45",
    "developer_comment": "夏彦・サリュ",
    "string": "その言葉に、夏彦もサリュも目を見開いた。"
  },
  "002364": {
    "context": "002364",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「どうしてそれを！？」"
  },
  "002393": {
    "context": "002393",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「エンパシーを使ったの？」"
  },
  "002422": {
    "context": "002422",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「使わなくてもわかるよ……長い付き合いなんだもん」"
  },
  "002428": {
    "context": "002428",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "（そうか……ましろには、隠し通せないか……）"
  },
  "002433": {
    "context": "002433",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "夏彦は諦めて笑い、彼女に答えた。"
  },
  "002444": {
    "context": "002444",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「……なぁましろ。３日前の夜のお前の気持ち、ようやくわかったよ」"
  },
  "002473": {
    "context": "002473",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「え？」"
  },
  "002484": {
    "context": "002484",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「これからラボでテロが起こる事を知っているのは、今この街でぼくらだけだ」"
  },
  "002495": {
    "context": "002495",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「そんな状況になると、どうしたって……使命感って湧いてくるもんなんだな？」"
  },
  "002524": {
    "context": "002524",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「っ……」"
  },
  "002535": {
    "context": "002535",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「……ぼくが行かなきゃ、この街が滅んじまう」"
  },
  "002546": {
    "context": "002546",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「それがテレビの向こうの出来事なんかじゃなくて、自分に関係のある出来事なら――」"
  },
  "002557": {
    "context": "002557",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「やっぱり、行かなきゃいけないよな」"
  },
  "002562": {
    "context": "002562",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "危険を避けてさえいれば、穏やかに暮らせると思っていた。"
  },
  "002567": {
    "context": "002567",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "だがどれだけ避けようとしても、平穏が崩れる事はある。"
  },
  "002572": {
    "context": "002572",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "ようやくその事に気付いた夏彦に、サリュがかすかな声で言う。"
  },
  "002601": {
    "context": "002601",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「……夏彦がラボに行ったからって、何もできないかもしれない」"
  },
  "002612": {
    "context": "002612",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「警備関係者に注意を促しても、信じてもらえない可能性は高い」"
  },
  "002623": {
    "context": "002623",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「だけど行かなかったら、ぼくはたぶん後悔する」"
  },
  "002634": {
    "context": "002634",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「もう後悔なんて、したくないんだよ」"
  },
  "002639": {
    "context": "002639",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "夏彦は今日まで、幾度となく後悔してきた。"
  },
  "002644": {
    "context": "002644",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "あの日、ラボに行きさえしなければ……そう思った事は数知れない。"
  },
  "002649": {
    "context": "002649",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "だが今回は逆だ。"
  },
  "002654": {
    "context": "002654",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "後悔しないためにラボに行くのだ。"
  },
  "002683": {
    "context": "002683",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「だったら、私たちも――」"
  },
  "002694": {
    "context": "002694",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「いや、ましろたちまで来る必要はない。１人いれば充分だ」"
  },
  "002723": {
    "context": "002723",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「じゃあ私たちはどうすれば？」"
  },
  "002734": {
    "context": "002734",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「頼みがあるんだ」"
  },
  "002745": {
    "context": "002745",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「すぐに家に帰って、悠里を連れて、少しでも遠くに逃げてくれ」"
  },
  "002774": {
    "context": "002774",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「っ……」"
  },
  "002785": {
    "context": "002785",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「悠里が泣いても叫んでも、引きずっていってくれ」"
  },
  "002796": {
    "context": "002796",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「万が一の事が起きた時のために……！」"
  },
  "002801": {
    "context": "002801",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "夏彦は２人を見据えて言う。"
  },
  "002806": {
    "context": "002806",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "だがましろは――夏彦を睨んで答えた。"
  },
  "002835": {
    "context": "002835",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「……冗談じゃないよ」"
  },
  "002846": {
    "context": "002846",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「ここまで来て『帰れ』って、私が素直に帰ると思ってんの？」"
  },
  "002857": {
    "context": "002857",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「え……？」"
  },
  "002868": {
    "context": "002868",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「９年前、夏彦と悠里は、ラボで事故にあったよね？」"
  },
  "002897": {
    "context": "002897",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「それで私が、どんな気持ちになったと思う？」"
  },
  "002926": {
    "context": "002926",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「死ぬほど後悔したよ、止めりゃよかったって」"
  },
  "002937": {
    "context": "002937",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「鈍い鈍い夏彦だって、そのくらいわかってんでしょ？」"
  },
  "002948": {
    "context": "002948",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「……！」"
  },
  "002977": {
    "context": "002977",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「今だって止めたいよ……逃げるなら逃げるで、夏彦とも一緒に逃げたいよ！」"
  },
  "003006": {
    "context": "003006",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「でも夏彦、どうせ止めても、ラボに行くのやめないでしょ！？　９年前と同じにさ！」"
  },
  "003012": {
    "context": "003012",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「……」"
  },
  "003041": {
    "context": "003041",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「だったら……だったら私も、せめて一緒に行きたい」"
  },
  "003070": {
    "context": "003070",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「２度と後悔したくないのは、私も同じなんだよ……！」"
  },
  "003075": {
    "context": "003075",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "ましろの声は震えていた。"
  },
  "003080": {
    "context": "003080",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "それは恐らく、ましろが長年胸に押し込めてきた言葉だっただろう。"
  },
  "003091": {
    "context": "003091",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「でもましろ、やっぱりそれは……！」"
  },
  "003096": {
    "context": "003096",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "それでも止めようとする夏彦に、サリュがため息混じりに言った。"
  },
  "003125": {
    "context": "003125",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「私も同感。夏彦は言っても止まらない」"
  },
  "003136": {
    "context": "003136",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「え？」"
  },
  "003147": {
    "context": "003147",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「危険を避けているなどと言いつつ、ましろや悠里が絡むと、簡単にその禁が破れる」"
  },
  "003158": {
    "context": "003158",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「い、いや、まぁ……」"
  },
  "003187": {
    "context": "003187",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「矛盾だらけで御しがたい。それはこの数日でよくわかった」"
  },
  "003216": {
    "context": "003216",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「止めても無駄なら仕方がない。私も一緒に行く」"
  },
  "003221": {
    "context": "003221",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "その言葉に、夏彦たちは息を呑んだ。"
  },
  "003232": {
    "context": "003232",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「ちょっとおい、なに言ってんだ！？　今の話聞いてなかったのか？」"
  },
  "003261": {
    "context": "003261",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「そ、そうだよ危険なんだよ！？」"
  },
  "003272": {
    "context": "003272",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「私は９年前の事があるからともかくとしても、なんでサリュまで一緒に来なきゃならないの？」"
  },
  "003301": {
    "context": "003301",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「いや、むしろ行くのが当然」"
  },
  "003330": {
    "context": "003330",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「あなたたち２人でラボに行って、もしテロリストに遭遇したら、対抗する術がない。違う？」"
  },
  "003341": {
    "context": "003341",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「む……」"
  },
  "003370": {
    "context": "003370",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「そりゃそうだけど……サリュだって昨夜、テロリストにＫＯされたじゃん……」"
  },
  "003399": {
    "context": "003399",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「そう、私が後悔しているのは昨夜の事」"
  },
  "003410": {
    "context": "003410",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「私が奴らを倒していれば、今こんなに悩む事はなかった」"
  },
  "003439": {
    "context": "003439",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「だから私も行く。これは決定事項。覆す事はできない」"
  },
  "003468": {
    "context": "003468",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「それに何より――私の役目は、夏彦を護る事だから」"
  },
  "003473": {
    "context": "003473",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "サリュは一息に言った。出会った時と同じく、冷徹な口調で。"
  },
  "003478": {
    "context": "003478",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "だがその言葉の中に、夏彦たちを案じる気持ちがある事ははっきりとわかった。"
  },
  "003489": {
    "context": "003489",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「あぁもう……どうしてそんなに２人とも、わからず屋なんだ……！」"
  },
  "003494": {
    "context": "003494",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "苦々しい気持ちで言う夏彦に、ましろたちが言い返す。"
  },
  "003523": {
    "context": "003523",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「夏彦に言われたくないよ。３人とも考えが一緒なだけだよ」"
  },
  "003552": {
    "context": "003552",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「全く同感」"
  },
  "003557": {
    "context": "003557",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "その言葉を聞き、夏彦は苦笑する。"
  },
  "003563": {
    "context": "003563",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "（止めても無駄なのは、お互い様か……！）"
  },
  "003568": {
    "context": "003568",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "女の子を２人連れて、これからテロが起こる施設に向かう。"
  },
  "003573": {
    "context": "003573",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "全く馬鹿げた話で、危険極まりない。"
  },
  "003578": {
    "context": "003578",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "なのに、あろう事か夏彦は――それもいいと思い始めていた。"
  },
  "003584": {
    "context": "003584",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "（ちくしょう、どうして……）"
  },
  "003590": {
    "context": "003590",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "（９年前の恐怖が、ずっと消えなかったのに……）"
  },
  "003596": {
    "context": "003596",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "（どうして３人で行くと思うと、怖さが和らいじまうんだ？）"
  },
  "003601": {
    "context": "003601",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "ますますましろの気持ちがわかった。"
  },
  "003606": {
    "context": "003606",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "『１人ではない』という事が、これほどまでに心強い事だとは。"
  },
  "003611": {
    "context": "003611",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "夏彦は２人に笑いかけ、そしてことさら元気よく言う。"
  },
  "003622": {
    "context": "003622",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「……わかった、わかったよ２人とも！　ぼくの負けだ！」"
  },
  "003651": {
    "context": "003651",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「夏彦、それじゃ！？」"
  },
  "003662": {
    "context": "003662",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「こうして言い争ってる時間がもったいない、仕方ないから３人で行こう！」"
  },
  "003691": {
    "context": "003691",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「って何それ、態度悪いなぁ！」"
  },
  "003720": {
    "context": "003720",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「私より弱いくせに……」"
  },
  "003731": {
    "context": "003731",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「ははっ、確かにな！」"
  },
  "003736": {
    "context": "003736",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "そう笑いつつも、夏彦は心に誓っていた。"
  },
  "003741": {
    "context": "003741",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "これから何があっても、絶対２人を護ろうと。"
  },
  "003746": {
    "context": "003746",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "サリュより腕っ節が弱かろうと、ましろほどＢＣが上手く使えなかろうと。"
  },
  "003751": {
    "context": "003751",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "２人がくれた勇気に対して、報いる方法はそれしかない。"
  },
  "003762": {
    "context": "003762",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「……それじゃいいかい、２人とも」"
  },
  "003791": {
    "context": "003791",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「了解」"
  },
  "003820": {
    "context": "003820",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「ん、いつでも行けるよ」"
  },
  "003831": {
    "context": "003831",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「行こう、この街を護りに……！」"
  },
  "003836": {
    "context": "003836",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "３人はうなずき合い、そして駆け出した。"
  }
}