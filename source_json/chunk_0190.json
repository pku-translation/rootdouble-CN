{
  "000041": {
    "context": "000041",
    "code": "0x55",
    "developer_comment": "",
    "string": "√Before Chapter5"
  },
  "000064": {
    "context": "000064",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「！」"
  },
  "000069": {
    "context": "000069",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "夏彦の全身の毛が逆立った。"
  },
  "000152": {
    "context": "000152",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "見れば屈強そうな男が、窓から部屋に入ってくるところだった。"
  },
  "000160": {
    "context": "000160",
    "code": "0x45",
    "developer_comment": "警備員",
    "string": "「こらッ！　何やってるんだお前ら！」"
  },
  "000165": {
    "context": "000165",
    "code": "0x45",
    "developer_comment": "警備員",
    "string": "格好から察するに、この施設の警備員らしい。"
  },
  "000170": {
    "context": "000170",
    "code": "0x45",
    "developer_comment": "警備員",
    "string": "外を巡回していたところ、夏彦たちの声を聞きつけてやってきたようだ。"
  },
  "000175": {
    "context": "000175",
    "code": "0x45",
    "developer_comment": "警備員",
    "string": "夏彦はいくらかほっとしつつ、警備員に言う。"
  },
  "000186": {
    "context": "000186",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「すみません。この施設がテロリストに狙われてる可能性があると聞いて、それをお伝えしに来たんです」"
  },
  "000194": {
    "context": "000194",
    "code": "0x45",
    "developer_comment": "警備員",
    "string": "「テロリスト……？」"
  },
  "000199": {
    "context": "000199",
    "code": "0x45",
    "developer_comment": "警備員",
    "string": "その言葉に、警備員の声が強張った。"
  },
  "000207": {
    "context": "000207",
    "code": "0x45",
    "developer_comment": "警備員",
    "string": "「君、どこでそんな話を聞いたんだ？」"
  },
  "000218": {
    "context": "000218",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「あ、いや……」"
  },
  "000223": {
    "context": "000223",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "事実をありのままに伝えても、信じてもらえるか微妙だろう。"
  },
  "000228": {
    "context": "000228",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "夏彦は少し考えてから答えた。"
  },
  "000239": {
    "context": "000239",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「そんな噂を、友達から聞いたんです」"
  },
  "000250": {
    "context": "000250",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「そ、そうです。近頃この街では、テロリストが送った怪文書が飛び交っていて……」"
  },
  "000261": {
    "context": "000261",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「どうもＢＣ関連施設が狙われているらしいんですよ。それでここも危ないかと思って」"
  },
  "000269": {
    "context": "000269",
    "code": "0x45",
    "developer_comment": "警備員",
    "string": "「そうか、なるほど……」"
  },
  "000277": {
    "context": "000277",
    "code": "0x45",
    "developer_comment": "警備員",
    "string": "「ではちょっと詳しい話を聞かせてもらおうか」"
  },
  "000289": {
    "context": "000289",
    "code": "0x45",
    "developer_comment": "警備員",
    "string": "警備員はそう言って、夏彦の腕を乱暴に掴んだ。"
  },
  "000300": {
    "context": "000300",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「痛っ……なにを！？」"
  },
  "000308": {
    "context": "000308",
    "code": "0x45",
    "developer_comment": "警備員",
    "string": "「悪いが、警備室に来てくれ」"
  },
  "000313": {
    "context": "000313",
    "code": "0x45",
    "developer_comment": "警備員",
    "string": "警備員が押し殺した声で言う。"
  },
  "000318": {
    "context": "000318",
    "code": "0x45",
    "developer_comment": "警備員",
    "string": "それを聞いた瞬間、夏彦は訝しく思った。"
  },
  "000324": {
    "context": "000324",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "（なんだ……！？）"
  },
  "000329": {
    "context": "000329",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "夏彦たちは善意の通報者だ。"
  },
  "000334": {
    "context": "000334",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "なのにこの警備員が、夏彦を見る目は――"
  },
  "000340": {
    "context": "000340",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "（まるで犯人を見る目じゃないか！）"
  },
  "000345": {
    "context": "000345",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "夏彦ははっとして、警備員の手を振りほどいた。"
  },
  "000356": {
    "context": "000356",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「ちょ、ちょっとなんですか警備室に来いって！？」"
  },
  "000364": {
    "context": "000364",
    "code": "0x45",
    "developer_comment": "警備員",
    "string": "「いや、ここのところ街中で、不穏な事が起こっているのは聞いていたのでね」"
  },
  "000372": {
    "context": "000372",
    "code": "0x45",
    "developer_comment": "警備員",
    "string": "「こちらも警戒を強めているんだよ」"
  },
  "000377": {
    "context": "000377",
    "code": "0x45",
    "developer_comment": "警備員",
    "string": "警備員はそう言って、夏彦にさらに詰め寄ってきた。"
  },
  "000388": {
    "context": "000388",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「ちょ、ちょっとやめて！　夏彦、痛がってるじゃないですか！」"
  },
  "000399": {
    "context": "000399",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「そう、乱暴はやめるべき」"
  },
  "000407": {
    "context": "000407",
    "code": "0x45",
    "developer_comment": "警備員",
    "string": "「乱暴！？　こっちは話を聞こうというだけだ！」"
  },
  "000415": {
    "context": "000415",
    "code": "0x45",
    "developer_comment": "警備員",
    "string": "「だいたい勝手に侵入しておいて、その言い草はなんだ！　大人に向かって！」"
  },
  "000420": {
    "context": "000420",
    "code": "0x45",
    "developer_comment": "警備員",
    "string": "警備員が怒声を上げる。"
  },
  "000425": {
    "context": "000425",
    "code": "0x45",
    "developer_comment": "警備員",
    "string": "夏彦が２人を庇おうとした、その時――"
  },
  "000436": {
    "context": "000436",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「やめろと言っている」"
  },
  "000441": {
    "context": "000441",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "サリュが言った。初めて聞くような、怒りと冷静さの入り混じった声で。"
  },
  "000449": {
    "context": "000449",
    "code": "0x45",
    "developer_comment": "警備員",
    "string": "「……なに！？」"
  },
  "000508": {
    "context": "000508",
    "code": "0x45",
    "developer_comment": "警備員",
    "string": "警備員が言った時には、サリュはもう動いていた。"
  },
  "000516": {
    "context": "000516",
    "code": "0x45",
    "developer_comment": "警備員",
    "string": "「！」"
  },
  "000521": {
    "context": "000521",
    "code": "0x45",
    "developer_comment": "警備員",
    "string": "彼に駆け寄り、手を掴む。"
  },
  "000526": {
    "context": "000526",
    "code": "0x45",
    "developer_comment": "警備員",
    "string": "そしてダンスを踊るように、優雅に身を翻すと――"
  },
  "000534": {
    "context": "000534",
    "code": "0x45",
    "developer_comment": "警備員",
    "string": "「うおッ！？」"
  },
  "000546": {
    "context": "000546",
    "code": "0x45",
    "developer_comment": "警備員",
    "string": "警備員の巨体が宙を舞った。"
  },
  "000560": {
    "context": "000560",
    "code": "0x45",
    "developer_comment": "警備員",
    "string": "「がっ！」"
  },
  "000625": {
    "context": "000625",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「！？」"
  },
  "000630": {
    "context": "000630",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "一瞬夏彦たちには、何が起こったのか理解できなかった。"
  },
  "000635": {
    "context": "000635",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "床に叩きつけられた警備員は、白目をむいて気絶していた。"
  },
  "000664": {
    "context": "000664",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「え……今、サリュが投げ飛ばしたの？」"
  },
  "000736": {
    "context": "000736",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「そう」"
  },
  "000747": {
    "context": "000747",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「……っ」"
  },
  "000752": {
    "context": "000752",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "夏彦は絶句した。"
  },
  "000758": {
    "context": "000758",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "（こんな小さな子が、大の男を投げ飛ばすなんて……）"
  },
  "000763": {
    "context": "000763",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "サリュは知力と身体能力が高いだけではなく、武術まで身につけているらしい。"
  },
  "000792": {
    "context": "000792",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「す、すごいね……合気道か何か？」"
  },
  "000821": {
    "context": "000821",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「そう。夏彦に危害を加えようとしたから、ついカッとなって技が出た」"
  },
  "000832": {
    "context": "000832",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「カッとなってって……もうちょっとやりようとかあったんじゃないのか？」"
  },
  "000861": {
    "context": "000861",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「む……」"
  },
  "000872": {
    "context": "000872",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「不愉快。夏彦を護ったのに、なぜ怒られるの」"
  },
  "000901": {
    "context": "000901",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「あ、そうだよ。サリュだって夏彦のためにやったのに」"
  },
  "000912": {
    "context": "000912",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「いや、その気持ちは嬉しいけどさ……」"
  },
  "000917": {
    "context": "000917",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "夏彦は足元でのびている警備員を見下ろした。"
  },
  "000928": {
    "context": "000928",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「大丈夫なのか、この人？」"
  },
  "000957": {
    "context": "000957",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「手加減はした。頭も打っていないし、やがて目覚める」"
  },
  "000968": {
    "context": "000968",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「でも怪我がないにしろ、気絶させちゃったのは事実だからな……起きたら面倒な事になりそうだぞ」"
  },
  "000997": {
    "context": "000997",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「……それじゃ、逃げちゃおっか？」"
  },
  "001026": {
    "context": "001026",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「それがいい。後からこの男に何か言われても、正当防衛が成り立つ」"
  },
  "001037": {
    "context": "001037",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「そ、そうかな……？」"
  },
  "001042": {
    "context": "001042",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "夏彦が戸惑っていると、サリュが饒舌に話し始めた。"
  },
  "001053": {
    "context": "001053",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「日本の法律は私も知っている。刑法３６条１項、正当防衛について」"
  },
  "001064": {
    "context": "001064",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「『急迫不正の侵害に対して、自己または他人の権利を防衛するために、やむを得ずした行為は、犯罪に問われない』」"
  },
  "001093": {
    "context": "001093",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「『侵害者が官憲またはそれに準ずる者であった場合も、これを例外としない』」"
  },
  "001104": {
    "context": "001104",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「夏彦はこの男の恐るべき暴力行為によって、権利侵害の危機にあった。ならば投げの１発や２発、実にやむを得ない事」"
  },
  "001133": {
    "context": "001133",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「り、理路整然だね……！」"
  },
  "001144": {
    "context": "001144",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「なんか言いくるめられているような気もするけど……」"
  },
  "001149": {
    "context": "001149",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "だが、ここで法律談義をしているヒマはなさそうだ。"
  },
  "001154": {
    "context": "001154",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "曲がりなりにも、テロの一件は伝える事ができたし、最低限の責務は果たしただろう。"
  },
  "001160": {
    "context": "001160",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "（これをきっかけに、この施設の警備が増強されれば、結果オーライかな……）"
  },
  "001171": {
    "context": "001171",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「……仕方ない、とりあえずここから離れよう。騒ぎになる前に」"
  },
  "001200": {
    "context": "001200",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「了解」"
  },
  "001229": {
    "context": "001229",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「らじゃ！」"
  },
  "001234": {
    "context": "001234",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "夏彦たちはそう言って、こそこそと部屋を出た。"
  },
  "001350": {
    "context": "001350",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "外に出るなり、夏彦たちは駆け出した。"
  },
  "001415": {
    "context": "001415",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "遠く離れた住宅街まで逃げて、ようやく一息つく。"
  },
  "001426": {
    "context": "001426",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「はぁぁ～……」"
  },
  "001470": {
    "context": "001470",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「な、何とか逃げれたかな……？」"
  },
  "001481": {
    "context": "001481",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「大丈夫。走りながら後ろは確認していた。誰もついてきていない」"
  },
  "001486": {
    "context": "001486",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "サリュは息も切らさずに言った。"
  },
  "001491": {
    "context": "001491",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "この娘は筋力や格闘技術だけではなく、肺活量まで鍛えられているらしい。"
  },
  "001502": {
    "context": "001502",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「あー、久しぶりに走った……ノドがカラカラだ……」"
  },
  "001531": {
    "context": "001531",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「だ、だね。ちょっと待って、ジュース買ってくるから……」"
  },
  "001554": {
    "context": "001554",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "ましろはそばにあった自販機に、よろよろと歩み寄る。"
  },
  "001559": {
    "context": "001559",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "しばらくして、３本のビンを抱えて戻ってきた。"
  },
  "001604": {
    "context": "001604",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「はい、夏彦、サリュ。私のおごりだよ」"
  },
  "001718": {
    "context": "001718",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「なんだこれ、『レジスタゴールド』……？」"
  },
  "001729": {
    "context": "001729",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「栄養ドリンクだよ。恐ろしい量のタウリンが入ってんの。肉体疲労時の栄養補給にもってこいだよ」"
  },
  "001740": {
    "context": "001740",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「逆にのどが渇きそうだけど……」"
  },
  "001745": {
    "context": "001745",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "夏彦はぶつぶつ言いながらも、それを一口飲んだ。"
  },
  "001830": {
    "context": "001830",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "のどが潤ったところで、本題に入る。"
  },
  "001841": {
    "context": "001841",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「しかし……あの警備員、何か様子が変だったな？」"
  },
  "001870": {
    "context": "001870",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「うん、それは私も感じたよ。警戒を強めてるって言ったって、あんなに態度悪くなるもんかな？」"
  },
  "001899": {
    "context": "001899",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「この市の警備員というものは、みんなあんなに高圧的なの？」"
  },
  "001910": {
    "context": "001910",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「いや、そんなわけはないと思うけど。何か理由があるのかな」"
  },
  "001939": {
    "context": "001939",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「実はあの施設にも、脅迫状が届いてたとか？」"
  },
  "001950": {
    "context": "001950",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「あるいは、それ以外の何かが……？」"
  },
  "001955": {
    "context": "001955",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "おかしな点がいくつかあるような気がしたが、どれもよくわからず、推察もできない。"
  },
  "002075": {
    "context": "002075",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "――そうしてあれこれ話し合っているうちに、やがて日が暮れてきた。"
  },
  "002166": {
    "context": "002166",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「……暗くなってきた」"
  },
  "002177": {
    "context": "002177",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「だな。そろそろ帰ろうか、２人とも」"
  },
  "002206": {
    "context": "002206",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「え～、もう！？」"
  },
  "002235": {
    "context": "002235",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「いや、それは妥当。ただでも危険なことをしてるのに、夜になったらなお危ない」"
  },
  "002246": {
    "context": "002246",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「それに悠里が心配するといけないし、今日の調査はここで打ち切りにしよう」"
  },
  "002275": {
    "context": "002275",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「う～……そっかぁ……」"
  },
  "002280": {
    "context": "002280",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "ましろはそう言いつつも、まだ何か言いたげだった。"
  },
  "002291": {
    "context": "002291",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「大丈夫だよ、明日は日曜日だ。なんなら明日も付き合うから」"
  },
  "002320": {
    "context": "002320",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「……そっか、そうだね！」"
  },
  "002325": {
    "context": "002325",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "夏彦の言葉に、彼女はようやく笑顔でうなずいた。"
  }
}