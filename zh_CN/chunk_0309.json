  "002681": {
    "context": "002681",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "（！？）"
  },
  "002686": {
    "context": "002686",
    "code": "0x45",
    "developer_comment": "",
    "string": "这个想法让夏彦清醒了过来。"
