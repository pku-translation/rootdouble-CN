  "000866": {
    "context": "000866",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「谢谢」"
  },
  "000988": {
    "context": "000988",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「什么啊，也不是什么大不了的嘛。贴张创可贴就好了」"
