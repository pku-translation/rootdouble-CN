  "000583": {
    "context": "000583",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「唔，挺疼的样子……贴张创可贴吧」"
  },
  "000781": {
    "context": "000781",
    "code": "0x45",
    "developer_comment": "",
    "string": "夏彦从药箱里取出创可贴，用它把真白的手指包了起来。"
  },
  "000792": {
    "context": "000792",
    "code": "0x45",
    "developer_comment": "夏彦",
