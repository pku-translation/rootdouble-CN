  },
  "001900": {
    "context": "001900",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「……」"
  },
  "001931": {
    "context": "001931",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "@import 0035 001585"
  },
  "001960": {
