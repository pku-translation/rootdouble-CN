  "000351": {
    "context": "000351",
    "code": "0x45",
    "developer_comment": "女子生徒１",
    "string": "「到——」"
  },
  "000362": {
    "context": "000362",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "「小田同学——」"
  },
  "000367": {
    "context": "000367",
    "code": "0x45",
    "developer_comment": "",
