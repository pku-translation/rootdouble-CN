  "002171": {
    "context": "002171",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「我因为不擅长这个，才总是没有自信……」"
  },
  "002200": {
    "context": "002200",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「今天大家看起来好像都很愉快。夏彦也很开心吗？」"
  },
  "002219": {
    "context": "002219",
    "code": "0x45",
    "developer_comment": "夏彦",
