  },
  "002248": {
    "context": "002248",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「……那个」"
  },
  "002277": {
    "context": "002277",
    "code": "0x45",
    "developer_comment": "サリュ",
