  },
  "000999": {
    "context": "000999",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "「自分の職場が非人道的行為を行っているのであれば、止めたいと思ったのさ」"
  },
  "001010": {
    "context": "001010",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「純粋な正義感からか？」"
  },
  "001039": {
    "context": "001039",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "「まぁそんなところだ」"
  },
  "001068": {
    "context": "001068",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "「なにしろあのラボの職員たちは、誰一人自分の職場の異常さについて改めようとしない」"
  },
  "001097": {
    "context": "001097",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "「他に誰もやろうとしないのなら、否が応にも正義感が湧いてくるものだろう？」"
  },
  "001108": {
    "context": "001108",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「いかにも人間的な理由だな。科学者らしくもない」"
  },
  "001137": {
    "context": "001137",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "「科学者の大半は、存外に人間的なものだよ。そういう人間は、ラボの職員に向いていないというだけで」"
  },
