  "003671": {
    "context": "003671",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「不明瞭な情報を妄信するましろと、アテもないのにそれを追う夏彦は、私から見れば似たりよったり」"
  },
  "003679": {
    "context": "003679",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「う……まぁ確かに」"
  },
  "003708": {
    "context": "003708",
    "code": "0x45",
    "developer_comment": "サリュ",
