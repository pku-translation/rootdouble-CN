  },
  "004399": {
    "context": "004399",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "「没、没事的……只是肋骨多少有点疼……」"
  },
  "004410": {
    "context": "004410",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「太好了，我们真的很担心啊」"
  },
  "004439": {
    "context": "004439",
    "code": "0x45",
    "developer_comment": "宇喜多",
