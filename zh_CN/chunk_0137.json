  },
  "003997": {
    "context": "003997",
    "code": "0x45",
    "developer_comment": "",
    "string": "真白注视着这名女队员，欢呼道。"
  },
  "004008": {
    "context": "004008",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「我们也过去吧，我很担心叔叔」"
