  "004037": {
    "context": "004037",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「啊、嗯！」"
  },
  "004093": {
    "context": "004093",
    "code": "0x45",
    "developer_comment": "",
    "string": "夏彦和真白跑向救援队抬着的担架。"
  },
  "004106": {
    "context": "004106",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「叔叔！」"
  },
  "004117": {
    "context": "004117",
    "code": "0x45",
    "developer_comment": "ましろ",
