  "000957": {
    "context": "000957",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「你是说，不只是我……洵也有杀人的能力吗！」"
  },
  "000968": {
    "context": "000968",
    "code": "0x45",
    "developer_comment": "被験体Ｎ",
    "string": "「当然了？你才知道吗？」"
