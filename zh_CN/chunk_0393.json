  "000142": {
    "context": "000142",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "（除了把渡濑的记忆再全部破坏一次以外，别无他法了！）"
  },
  "000247": {
    "context": "000247",
    "code": "0x45",
