  },
  "003812": {
    "context": "003812",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「え？」"
  },
  "003862": {
    "context": "003862",
    "code": "0x45",
    "developer_comment": "美夜子",
    "string": "@auto-import 0226 000820"
  },
  "003873": {
