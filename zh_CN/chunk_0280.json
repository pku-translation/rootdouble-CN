    "developer_comment": "美夜子",
    "string": "@auto-import 0226 000804"
  },
  "003812": {
    "context": "003812",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "@auto-import 0005 001803"
  },
  "003862": {
    "context": "003862",
