  "002239": {
    "context": "002239",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「咳啊！！」"
  },
  "002268": {
    "context": "002268",
    "code": "0x45",
    "developer_comment": "",
    "string": "渡濑急忙用手挡住了这一下。"
