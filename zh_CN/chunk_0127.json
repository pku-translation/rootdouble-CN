  "004005": {
    "context": "004005",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "（这，这样下去会被杀掉的……！）"
  },
  "004011": {
    "context": "004011",
    "code": "0x45",
    "developer_comment": "渡瀬",
