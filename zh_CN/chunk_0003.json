  "002029": {
    "context": "002029",
    "code": "0x45",
    "developer_comment": "女隊員",
    "string": "「诶呀，感觉队长没问题啊？所以也让他帮忙吧！」"
  },
  "002040": {
    "context": "002040",
    "code": "0x45",
    "developer_comment": "男",
    "string": "「不，不是为了这个！只是想让你们告诉我！」"
  },
  "002051": {
    "context": "002051",
    "code": "0x45",
    "developer_comment": "男",
    "string": "「刚才那是什么声音？！出了什么事？！」"
  },
  "002080": {
    "context": "002080",
    "code": "0x45",
    "developer_comment": "副隊長",
    "string": "「诶！？」"
  },
  "002109": {
    "context": "002109",
    "code": "0x45",
    "developer_comment": "女隊員",
    "string": "「队，队长你在说什么？！不知道现在是什么状况吗？！」"
