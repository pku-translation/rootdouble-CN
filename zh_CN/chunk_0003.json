  },
  "000094": {
    "context": "000094",
    "code": "0x45",
    "developer_comment": "",
    "string": "不懂。"
  },
  "000099": {
    "context": "000099",
    "code": "0x45",
    "developer_comment": "",
    "string": "脑海空无一物，一片虚无。"
  },
  "000105": {
    "context": "000105",
    "code": "0x45",
    "developer_comment": "？？",
    "string": "（是什么……？）"
  },
