    "string": "@import 0173 003132"
  },
  "003499": {
    "context": "003499",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「嘛，虽然没有那种特别像旅游胜地一样的地方，但也是一座稍微有点奇妙的城市」"
  },
  "003510": {
    "context": "003510",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "@import 0173 003154"
  },
  "003521": {
    "context": "003521",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "@import 0173 003205"
  },
  "003532": {
    "context": "003532",
