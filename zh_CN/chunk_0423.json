  "002369": {
    "context": "002369",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "@auto-import 0014 002122"
  },
  "002398": {
    "context": "002398",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「……睡着了吗。累了吧……」"
  },
  "002455": {
    "context": "002455",
    "code": "0x45",
    "developer_comment": "風見",
    "string": "「也难怪吧……这孩子为了救我们，一直磨损着内心战斗」"
