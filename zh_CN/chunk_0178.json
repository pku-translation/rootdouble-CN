  },
  "001379": {
    "context": "001379",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「好，那么出发吧！」"
  },
  "001384": {
    "context": "001384",
    "code": "0x45",
    "developer_comment": "",
