  },
  "001198": {
    "context": "001198",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「这，这是……！」"
  },
  "001227": {
    "context": "001227",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「小彦，该怎么办……？」"
