  "003111": {
    "context": "003111",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「没有。谁都没见过」"
  },
  "003140": {
    "context": "003140",
    "code": "0x45",
    "developer_comment": "サリュ",
