  "003030": {
    "context": "003030",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「——！」"
  },
  "003035": {
    "context": "003035",
    "code": "0x45",
    "developer_comment": "",
    "string": "纱瑠被吓得退了一步。"
