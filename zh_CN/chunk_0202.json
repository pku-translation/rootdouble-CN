  "001330": {
    "context": "001330",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「……这样啊」"
  },
  "001359": {
    "context": "001359",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「怎么想都不像是防身术，倒是有不少感觉都是在哪儿听过的招式名」"
  },
  "001418": {
    "context": "001418",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「这种事无所谓了。话说回来，现在几点了？」"
