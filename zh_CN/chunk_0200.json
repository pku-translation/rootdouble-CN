  "004603": {
    "context": "004603",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「怎么会！妈妈你好过分，悠里——」"
  },
  "004633": {
    "context": "004633",
    "code": "0x45",
    "developer_comment": "美夜子",
    "string": "「所以说！」"
  },
  "004638": {
    "context": "004638",
    "code": "0x45",
