  "001084": {
    "context": "001084",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "@auto-import 0162 000503"
  },
  "001165": {
    "context": "001165",
    "code": "0x45",
    "developer_comment": "夏彦",
