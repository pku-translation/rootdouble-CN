  "002251": {
    "context": "002251",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「我们可以寄希望于这一点，大家一起等待隔离墙自然开放！」"
  },
  "002281": {
    "context": "002281",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「说、说的没错……！这是个好办法，悠里！」"
