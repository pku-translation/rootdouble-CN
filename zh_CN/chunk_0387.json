  "006092": {
    "context": "006092",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "（……还有其他的办法吗……）"
  },
  "006105": {
    "context": "006105",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "（必须要有人死吗……！）"
  },
  "006117": {
    "context": "006117",
    "code": "0x45",
    "developer_comment": "",
    "string": "决断的时刻迫近了。"
  },
  "006129": {
    "context": "006129",
    "code": "0x45",
    "developer_comment": "",
    "string": "“要舍弃的人是谁?”作出选择的瞬间迫在眉睫……！"
