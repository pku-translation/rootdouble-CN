  "003495": {
    "context": "003495",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "『力所能及的事是什么？』"
  },
  "003522": {
    "context": "003522",
    "code": "0x45",
    "developer_comment": "美夜子",
