  "003259": {
    "context": "003259",
    "code": "0x45",
    "developer_comment": "美夜子",
    "string": "『夺取权限的时间，总共也只有30秒……在那之前，无论如何也要解除 Zero 区域的安全系统』"
  },
  "003270": {
    "context": "003270",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "『啊啊，知道了』"
  },
  "003275": {
    "context": "003275",
    "code": "0x45",
    "developer_comment": "",
