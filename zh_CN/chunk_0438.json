  "000518": {
    "context": "000518",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「ん？」"
  },
  "000606": {
    "context": "000606",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「覚えてる？　あの頃のこと」"
  },
  "000614": {
    "context": "000614",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「悠里と一緒に３人で、ここを駆け回ってた頃のこと」"
  },
  "000649": {
    "context": "000649",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「あぁ、もちろん……懐かしい思い出だよ」"
  },
  "000657": {
    "context": "000657",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「……たぶんあの頃、私ね……」"
  },
  "000665": {
    "context": "000665",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「夏彦のこと、好きだったよ」"
  },
  "000670": {
    "context": "000670",
    "code": "0x45",
    "developer_comment": "",
    "string": "その言葉に、夏彦の目が見開かれた。"
  },
  "000699": {
    "context": "000699",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "@import 0060 001114"
  },
  "000707": {
    "context": "000707",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「意地悪だけど、いざという時は優しくてさ。他の男の子とは、ちょっと違う感じでさ」"
  },
  "000715": {
    "context": "000715",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「ケンカばっかりしてたけど、ホントはもっと素直に、仲良くしたかった」"
  },
  "000723": {
    "context": "000723",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「子供だったからよくわかんなかったけど、きっとあれが初恋だったんだと思う」"
  },
  "000752": {
    "context": "000752",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "@import 0005 000751"
  },
  "000781": {
    "context": "000781",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「初恋って……今は？」"
  },
  "000789": {
    "context": "000789",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「今も好きだよ、友達として」"
  },
  "000797": {
    "context": "000797",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「でも夏彦には、悠里がいるじゃない？」"
  },
  "000826": {
    "context": "000826",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "@import 0024 002121"
  },
  "000834": {
    "context": "000834",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「だから、まぁ……そういう事だよ」"
  },
  "000842": {
    "context": "000842",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「今の私はね、夏彦と悠里を見守る事の方が……なんか、嬉しいんだ」"
  },
  "000847": {
    "context": "000847",
    "code": "0x45",
    "developer_comment": "",
    "string": "――そんな形でしか、想いを告げる事はできない。"
  },
  "000852": {
    "context": "000852",
    "code": "0x45",
    "developer_comment": "",
    "string": "夏彦は目を伏せ、寂しそうに笑う。"
  },
  "000881": {
    "context": "000881",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "@import 0022 001466"
  },
  "000886": {
    "context": "000886",
    "code": "0x45",
    "developer_comment": "",
    "string": "彼がそう呟いた時――穏やかな風が吹き抜け、２人を包んだ。"
  },
  "000970": {
    "context": "000970",
    "code": "0x45",
    "developer_comment": "",
    "string": "@import 0229 000583"
  },
  "000975": {
    "context": "000975",
    "code": "0x45",
    "developer_comment": "",
    "string": "いつか、このかりそめの告白を、本物にできればいいと……。"
  },
