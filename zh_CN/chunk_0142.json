  "004550": {
    "context": "004550",
    "code": "0x45",
    "developer_comment": "三ノ宮",
    "string": "「随便哪个都行——」"
  },
  "004579": {
    "context": "004579",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「我想到了！把这三个缩写到一起，叫你“纱瑠”怎么样？！」"
