  "003837": {
    "context": "003837",
    "code": "0x45",
    "developer_comment": "",
    "string": "夏彦屏着气，侧眼看着这场交锋。"
  },
  "003866": {
    "context": "003866",
    "code": "0x45",
    "developer_comment": "三ノ宮",
    "string": "「……你有什么事吗？」"
  },
  "003895": {
    "context": "003895",
    "code": "0x45",
