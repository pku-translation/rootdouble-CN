  "003739": {
    "context": "003739",
    "code": "0x45",
    "developer_comment": "三ノ宮",
    "string": "「这样」"
  },
  "003768": {
    "context": "003768",
    "code": "0x45",
    "developer_comment": "ましろ",
    "string": "「那本书我也读过喔。很有名呢，少年科幻系列的代表作吧？」"
