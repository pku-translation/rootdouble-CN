  "002485": {
    "context": "002485",
    "code": "0x45",
    "developer_comment": "洵",
    "string": "「因为只有工作上的联系啊……」"
  },
  "002535": {
    "context": "002535",
    "code": "0x45",
    "developer_comment": "渡瀬",
