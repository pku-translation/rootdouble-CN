  "003113": {
    "context": "003113",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「……被験体Ｎ……」"
  },
  "003124": {
    "context": "003124",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「すまなかった。君の言う通りだ」"
  },
  "003154": {
    "context": "003154",
    "code": "0x45",
    "developer_comment": "被験体Ｎ",
    "string": "「そうか……ようやくわかってくれたか」"
  },
  "003159": {
    "context": "003159",
    "code": "0x45",
    "developer_comment": "",
    "string": "そう言って彼女は、嬉しそうに微笑んだ。"
  },
  "003189": {
    "context": "003189",
    "code": "0x45",
    "developer_comment": "被験体Ｎ",
    "string": "「では現実に帰りたまえ。『それ』をするのは、肉体を持つ君の役目だ」"
