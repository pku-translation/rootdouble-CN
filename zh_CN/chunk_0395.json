  "003230": {
    "context": "003230",
    "code": "0x45",
    "developer_comment": "被験体Ｎ",
    "string": "「頼むぞ、君の正義を貫いてくれ」"
  },
  "003295": {
    "context": "003295",
    "code": "0x45",
