  "007059": {
    "context": "007059",
    "code": "0x45",
    "developer_comment": "",
    "string": "——这时，不知第几次的广播响起。"
  },
  "007125": {
    "context": "007125",
    "code": "0x85",
    "string": "「……区域为止的楼梯，已经全部垮塌！"
  },
  "007127": {
    "context": "007127",
    "code": "0x85",
    "string": "但第 5、第 6 区域的楼梯仍然完好！」"
  },
  "007131": {
    "context": "007131",
    "code": "0x86",
    "string": "「……区域为止的楼梯，已经全部垮塌！但第 5、第 6 区域的楼梯仍然完好！」"
  },
  "007140": {
    "context": "007140",
    "code": "0x85",
    "string": "「设施内的工作人员，请尽快避难！」"
