  "002122": {
    "context": "002122",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「…………」"
  },
  "002127": {
    "context": "002127",
    "code": "0x45",
    "developer_comment": "",
    "string": "渡濑无言以对。"
  },
  "002132": {
    "context": "002132",
    "code": "0x45",
