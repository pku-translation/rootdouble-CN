  "000582": {
    "context": "000582",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「隊長さんの記憶を消したくない気持ち、わかるよ……うぅん、消す必要なんか無いんだよ」"
  },
  "000612": {
    "context": "000612",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「だってこれは初めから、約束してた事なんだから」"
  },
  "000642": {
    "context": "000642",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「そうですよね、隊長さん……わたし、約束守りますから」"
  },
  "000655": {
    "context": "000655",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「……」"
  },
