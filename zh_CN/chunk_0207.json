  },
  "005766": {
    "context": "005766",
    "code": "0x45",
    "developer_comment": "",
    "string": "过了一会儿，他想到了一个人。"
  },
  "005795": {
    "context": "005795",
    "code": "0x45",
    "developer_comment": "ましろ",
