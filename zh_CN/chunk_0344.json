  },
  "000517": {
    "context": "000517",
    "code": "0x45",
    "developer_comment": "",
    "string": "あの夜起きた出来事は、後にそう呼ばれた。"
  },
  "000559": {
    "context": "000559",
    "code": "0x85",
    "string": "　%N"
  },
  "000562": {
    "context": "000562",
