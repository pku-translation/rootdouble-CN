  "002963": {
    "context": "002963",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「啊……没，没有」"
  },
  "002992": {
    "context": "002992",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「其实……从刚刚开始我就非常困」"
