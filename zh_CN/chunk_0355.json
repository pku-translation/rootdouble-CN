  "000244": {
    "context": "000244",
    "code": "0x45",
    "developer_comment": "美夜子",
    "string": "「反正也就是一瞬间的事情」"
  },
  "000249": {
    "context": "000249",
    "code": "0x45",
    "developer_comment": "",
    "string": "确实正如博士所说。"
  },
  "000254": {
    "context": "000254",
    "code": "0x45",
