  "000119": {
    "context": "000119",
    "code": "0x45",
    "developer_comment": "",
    "string": "仿佛是祈求他“醒来”一般——"
  },
  "000130": {
    "context": "000130",
    "code": "0x45",
    "developer_comment": "オレ",
    "string": "「……」"
  },
  "000141": {
    "context": "000141",
    "code": "0x45",
    "developer_comment": "オレ",
    "string": "「……唔，咳……」"
  },
  "000287": {
    "context": "000287",
    "code": "0x45",
    "developer_comment": "",
    "string": "……我在不知是何处的地方醒来了。"
  },
  "000293": {
    "context": "000293",
    "code": "0x45",
    "developer_comment": "オレ",
    "string": "（……？）"
  },
  "000307": {
    "context": "000307",
    "code": "0x45",
