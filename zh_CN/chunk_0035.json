  },
  "002108": {
    "context": "002108",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「没事，维持部下的心情也是队长的任务啊」"
  },
  "002113": {
    "context": "002113",
    "code": "0x45",
    "developer_comment": "",
