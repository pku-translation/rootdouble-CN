  "001963": {
    "context": "001963",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "「真的吗……？」"
  },
  "001968": {
    "context": "001968",
    "code": "0x45",
    "developer_comment": "",
    "string": "宇喜多用怀疑的眼神打量着渡濑。他的眼神非常令人不快。"
  },
  "001979": {
    "context": "001979",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「都说了没事了。话说回来——」"
  },
  "001990": {
    "context": "001990",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「麻烦了宇喜多！Zero 区域里只有一个实验对象！」"
  },
  "002019": {
    "context": "002019",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "「什么！？」"
  },
  "002030": {
    "context": "002030",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「哪里都找不到 YⅡ！你知道她在哪里吗！？」"
