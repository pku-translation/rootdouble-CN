  "003593": {
    "context": "003593",
    "code": "0x45",
    "developer_comment": "",
    "string": "渡濑自己也不知道自己在想什么。"
  },
  "003687": {
    "context": "003687",
    "code": "0x45",
    "developer_comment": "",
