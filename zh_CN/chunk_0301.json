  "005301": {
    "context": "005301",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "（刚刚的研究员干部，也一瞬间就变成了那个样子）"
  },
  "005307": {
    "context": "005307",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "（为什么我要救这种家伙……？）"
  },
  "005329": {
    "context": "005329",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「那，那还用说吗……我要带他们离开这里！」"
