  "001342": {
    "context": "001342",
    "code": "0x45",
    "developer_comment": "",
    "string": "被砰的一下猛揍一拳一样的冲击。"
  },
  "001388": {
    "context": "001388",
    "code": "0x45",
    "developer_comment": "悠里",
    "string": "「……！」"
  },
  "001393": {
    "context": "001393",
    "code": "0x45",
    "developer_comment": "",
    "string": "悠里和真白的表情僵住了。"
