  "001243": {
    "context": "001243",
    "code": "0x45",
    "developer_comment": "",
    "string": "黑影的手中举着什么。"
  },
  "001248": {
    "context": "001248",
    "code": "0x45",
    "developer_comment": "",
    "string": "并且把它指向了夏彦——"
  },
  "001321": {
    "context": "001321",
    "code": "0x45",
    "developer_comment": "",
    "string": "冷淡的声音响起。"
  },
  "001332": {
    "context": "001332",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「！！」"
  },
