  "000805": {
    "context": "000805",
    "code": "0x45",
    "developer_comment": "",
    "string": "结果惠那就被关在了地下。"
  },
  "000864": {
    "context": "000864",
    "code": "0x45",
    "developer_comment": "",
    "string": "没能和黑田他们汇合，就在连接通道里面被火焰追赶。"
  },
  "000872": {
    "context": "000872",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "「咳咳！咳咳！！」"
  },
  "000880": {
    "context": "000880",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "「唔，可……可恶！」"
  },
