  "000097": {
    "context": "000097",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「为，为什么……！」"
  },
  "000102": {
    "context": "000102",
    "code": "0x45",
    "developer_comment": "",
    "string": "在场的所有人都愣住了。"
  },
  "000131": {
    "context": "000131",
    "code": "0x45",
    "developer_comment": "洵",
    "string": "「糟糕了……现在手里的 AD，只够每个人 1 支的……！」"
  },
  "000142": {
    "context": "000142",
    "code": "0x45",
    "developer_comment": "洵",
    "string": "「如果没有 AD 的话，我们……无论如何都会受到照射！」"
  },
  "000153": {
    "context": "000153",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「……」"
  },
  "000158": {
    "context": "000158",
    "code": "0x45",
    "developer_comment": "",
    "string": "@import 0073 000564"
  },
  "000263": {
    "context": "000263",
