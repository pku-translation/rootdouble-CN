  "000731": {
    "context": "000731",
    "code": "0x45",
    "developer_comment": "",
    "string": "但是，进了消防局没多久——"
  },
  "000737": {
    "context": "000737",
    "code": "0x45",
    "developer_comment": "渡瀬",
