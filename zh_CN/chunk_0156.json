  },
  "000336": {
    "context": "000336",
    "code": "0x86",
    "string": "『那么、开始了哦信……指向性传心的秘密特训』"
  },
  "000344": {
    "context": "000344",
    "code": "0x85",
    "string": "『好，来吧红崎』"
  },
  "000355": {
    "context": "000355",
    "code": "0x86",
    "string": "冷静的男生"
  },
  "000356": {
    "context": "000356",
    "code": "0x86",
    "string": "@import 0156 000344"
  },
  "000362": {
    "context": "000362",
    "code": "0x85",
    "string": "『来了哦！"
  },
  "000364": {
    "context": "000364",
    "code": "0x85",
    "string": "“惠那老师的胸围"
  },
  "000366": {
    "context": "000366",
