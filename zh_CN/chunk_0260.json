    "string": "@import 0088 001069"
  },
  "001154": {
    "context": "001154",
    "code": "0x45",
    "developer_comment": "",
    "string": "さらに向けられる厳しい追及。"
  },
  "001183": {
    "context": "001183",
    "code": "0x45",
    "developer_comment": "宇喜多",
    "string": "@import 0088 001381"
  },
  "001194": {
    "context": "001194",
