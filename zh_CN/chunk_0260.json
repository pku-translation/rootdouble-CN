  "000500": {
    "context": "000500",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "（しまった……！　こうなることは、当然予想しておくべきだったのに……！）"
  },
  "000543": {
    "context": "000543",
    "code": "0x45",
    "developer_comment": "",
    "string": "そして夏彦が手をこまねいている内に、さらに新たな問題が勃発し――"
  },
  "000603": {
    "context": "000603",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "（っ！？　ＡＤがなくなってる！？）"
  },
  "000614": {
    "context": "000614",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "@import 0085 002400"
  },
  "000625": {
