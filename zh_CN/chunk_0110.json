  "002324": {
    "context": "002324",
    "code": "0x45",
    "developer_comment": "",
    "string": "宇喜多从右肩到胸口，有一道巨大的割裂伤。"
  },
  "002335": {
    "context": "002335",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「大叔！喂！振作一点！」"
