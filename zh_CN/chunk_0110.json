  "002833": {
    "context": "002833",
    "code": "0x45",
    "developer_comment": "",
    "string": "渡濑突然注意到了。"
  },
  "002844": {
    "context": "002844",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「——这样的话，橘和老师很危险！」"
