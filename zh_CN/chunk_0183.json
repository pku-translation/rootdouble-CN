  "000255": {
    "context": "000255",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「用那个的话就算不知道真白在哪里，也能联络上吧？」"
  },
  "000266": {
    "context": "000266",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「……传心啊」"
