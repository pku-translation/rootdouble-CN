  "000292": {
    "context": "000292",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「我能做到吗……？」"
  },
  "000321": {
    "context": "000321",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「真白说过，传心的诀窍是，“在心底里想着你想要说的话”」"
  },
  "000350": {
    "context": "000350",
    "code": "0x45",
    "developer_comment": "サリュ",
    "string": "「虽然只是外行人的想法，但是现在你从心底里担心真白的话，我觉得或许可以做到」"
  },
  "000361": {
    "context": "000361",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "「嗯……说不定有可能呢」"
  },
  "000372": {
    "context": "000372",
    "code": "0x45",
    "developer_comment": "夏彦",
