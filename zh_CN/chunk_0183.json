  "000619": {
    "context": "000619",
    "code": "0x45",
    "developer_comment": "夏彦",
    "string": "（……？）"
  },
  "000624": {
    "context": "000624",
    "code": "0x45",
    "developer_comment": "",
    "string": "一瞬间，他以为是雨停了。"
