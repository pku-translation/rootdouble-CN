  "002230": {
    "context": "002230",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「老，老师……这里写的是真的吗？」"
  },
  "002259": {
    "context": "002259",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "「诶？」"
  },
  "002264": {
    "context": "002264",
    "code": "0x45",
    "developer_comment": "",
    "string": "惠那凑过来看了看那本书。"
  },
  "002275": {
    "context": "002275",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "「这个，读心这种能力……真的存在吗？」"
  },
  "002304": {
    "context": "002304",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "「对，是啊。我之前没说吗」"
  },
  "002315": {
    "context": "002315",
    "code": "0x45",
    "developer_comment": "恵那",
    "string": "「“是一种能跟远处的人相互传达想法的能力，除此之外没有作用”，这么说过吧」"
