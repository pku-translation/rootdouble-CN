  "004314": {
    "context": "004314",
    "code": "0x45",
    "developer_comment": "",
    "string": "他也有些在意刚才看到的奇怪的幻影。"
  },
  "004367": {
    "context": "004367",
    "code": "0x87",
    "string": "@ignore"
  },
  "004373": {
    "context": "004373",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "（就这么回去真的好吗！？）"
  },
  "004390": {
    "context": "004390",
    "code": "0x45",
    "developer_comment": "渡瀬",
    "string": "（不，好不容易才来到这里……！）"
